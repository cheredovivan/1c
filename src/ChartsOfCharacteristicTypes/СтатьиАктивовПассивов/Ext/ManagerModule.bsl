// Дополняет таблицу объектов финансового учета свойствами для обращения к данным.
// Если какой-либо параметр является выражением языка запросов, то описания полей в нем должны содержать
// псевдоним источника данных, полученный через функцию ФинансовыйУчетПоДаннымБалансовыхРегистров.ПсевдонимИсточникаДанныхПоУмолчанию().
// Если параметр является только именем поля,в том числе разыменованным через точку, его можно указывать без псевдонима источника данных.
// Допускается использовать параметры запроса из функции МеждународныйУчетПоДаннымФинансовыхРегистров.ЗапросДанныхФинансовыхРегистров().
// Допускается использовать строковые шаблоны из функции МеждународныйУчетПоДаннымФинансовыхРегистров.ТекстыЗапросовЗаписейРегистров().
// 
// Параметры:
//  НастройкиУчета - см. НастройкиФинансовогоУчета
//  ОписаниеОбъектовУчета - см. НоваяТаблицаОбъектовФинансовогоУчета
// 
Процедура ДополнитьОписаниеОбъектовУчетаДляПолученияДанных(НастройкиУчета, ОписаниеОбъектовУчета) Экспорт
	
	МетаРегистрыНакопления = Метаданные.РегистрыНакопления;
	МетаФункциональныеОпции = Метаданные.ФункциональныеОпции;
	
	ОписаниеНеиспользуемыхДанных = ОписаниеНеиспользуемыхДанных(ОписаниеОбъектовУчета,
		МетаРегистрыНакопления.СебестоимостьТоваров.Имя);
	ЗаполнитьТаблицуРесурсов(ОписаниеНеиспользуемыхДанных.РесурсыУпр, РесурсыУчетаСобственныхЗапасовУпр());
	ЗаполнитьТаблицуРесурсов(ОписаниеНеиспользуемыхДанных.РесурсыРегл, РесурсыУчетаСобственныхЗапасовРегл());
	
	Если ОписаниеОбъектовУчета.Найти(Истина, "НесобственныеЦенности") <> Неопределено Тогда
		ОписаниеНеиспользуемыхДанных = ОписаниеНеиспользуемыхДанных(ОписаниеОбъектовУчета,
			МетаРегистрыНакопления.СебестоимостьТоваров.Имя);
		ЗаполнитьТаблицуРесурсов(ОписаниеНеиспользуемыхДанных.РесурсыУпр, РесурсыУчетаПринятыхЗапасовУпр());
		ЗаполнитьТаблицуРесурсов(ОписаниеНеиспользуемыхДанных.РесурсыРегл, РесурсыУчетаПринятыхЗапасовРегл());
	КонецЕсли;
	
	Если ПолучитьФункциональнуюОпцию(МетаФункциональныеОпции.ФормироватьРезервыПодОбесценениеЗапасов.Имя) Тогда
		ОписаниеНеиспользуемыхДанных = ОписаниеНеиспользуемыхДанных(ОписаниеОбъектовУчета, МетаРегистрыНакопления.СебестоимостьТоваров.Имя);
		ЗаполнитьТаблицуРесурсов(ОписаниеНеиспользуемыхДанных.РесурсыУпр, РесурсыУчетаРезерваПодОбесценениеЗапасовУпр());
		ЗаполнитьТаблицуРесурсов(ОписаниеНеиспользуемыхДанных.РесурсыРегл, РесурсыУчетаРезерваПодОбесценениеЗапасовРегл());
		ЗаполнитьТаблицуРесурсов(ОписаниеНеиспользуемыхДанных.РесурсыКоличество, "Количество", 0);
	КонецЕсли;
	
	ОписаниеОбъектаУчета = ОписаниеОбъектовУчета.Найти(ОбязательстваПоНеотфактурованномуНДС, "ОбъектУчета");
	Если ОписаниеОбъектаУчета <> Неопределено Тогда
		ОписаниеОбъектаУчета.ПутьКДаннымИдентификаторФинЗаписи = "&ИдентификаторНеиспользуемойФинЗаписи";
//		ОписаниеНеиспользуемыхДанных = ОписаниеНеиспользуемыхДанных(ОписаниеОбъектовУчета, МетаРегистрыНакопления.НДСПредъявленный.Имя);
//		ЗаполнитьТаблицуРесурсов(ОписаниеНеиспользуемыхДанных.РесурсыКоличество, "0", 0);
	КонецЕсли;
	
	ОписаниеОбъектаУчета = ОписаниеОбъектовУчета.Найти(МатериалыРаботыВНезавершенномПроизводстве, "ОбъектУчета");
	Если ОписаниеОбъектаУчета <> Неопределено Тогда
		ОписаниеОбъектаУчета.ПутьКДаннымОбъектНастройки = "АналитикаФинансовогоУчета";
	КонецЕсли;
	
	ОписаниеОбъектаУчета = ОписаниеОбъектовУчета.Найти(ТоварыМатериалыПринятыеНаХранениеВПроизводстве, "ОбъектУчета");
	Если ОписаниеОбъектаУчета <> Неопределено Тогда
		ОписаниеОбъектаУчета.ПутьКДаннымОбъектНастройки = "АналитикаФинансовогоУчета";
	КонецЕсли;
	
	ОписаниеОбъектаУчета = ОписаниеОбъектовУчета.Найти(МатериалыПринятыеВПереработкуВПроизводстве, "ОбъектУчета");
	Если ОписаниеОбъектаУчета <> Неопределено Тогда
		ОписаниеОбъектаУчета.ПутьКДаннымОбъектНастройки = "АналитикаФинансовогоУчета";
	КонецЕсли;
	
	ОписаниеОбъектаУчета = ОписаниеОбъектовУчета.Найти(РезервыПодОбесценениеНезавершенногоПроизводства, "ОбъектУчета");
	Если ОписаниеОбъектаУчета <> Неопределено Тогда
		ОписаниеОбъектаУчета.ПутьКДаннымОбъектНастройки = "АналитикаФинансовогоУчета";
	КонецЕсли;
	
	ОписаниеОбъектаУчета = ОписаниеОбъектовУчета.Найти(ОбязательстваПоНеотфактурованнымПоставкам, "ОбъектУчета");
	Если ОписаниеОбъектаУчета <> Неопределено Тогда
		ОписаниеОбъектаУчета.ПутьКДаннымОбъектНастройки = "НЕОПРЕДЕЛЕНО";
	КонецЕсли;
	
	ОписаниеОбъектаУчета = ОписаниеОбъектовУчета.Найти(НДССАвансовПолученных, "ОбъектУчета");
	Если ОписаниеОбъектаУчета <> Неопределено Тогда
		ОписаниеОбъектаУчета.УчетНДС = Истина;
	КонецЕсли;
	
	ОписаниеОбъектаУчета = ОписаниеОбъектовУчета.Найти(НДССАвансовВыданных, "ОбъектУчета");
	Если ОписаниеОбъектаУчета <> Неопределено Тогда
		ОписаниеОбъектаУчета.УчетНДС = Истина;
	КонецЕсли;
	
	ОписаниеОбъектаУчета = ОписаниеОбъектовУчета.Найти(НДСПоПриобретеннымЦенностям, "ОбъектУчета");
	Если ОписаниеОбъектаУчета <> Неопределено Тогда
		ОписаниеОбъектаУчета.УчетНДС = Истина;
	КонецЕсли;
	
	ОписаниеОбъектаУчета = ОписаниеОбъектовУчета.Найти(Налоги, "ОбъектУчета");
	Если ОписаниеОбъектаУчета <> Неопределено Тогда
		ОписаниеОбъектаУчета.ПутьКДаннымОбъектНастройки = "Аналитика";
	КонецЕсли;
	
	//++ Локализация
	
	ОписаниеОбъектаУчета = ОписаниеОбъектовУчета.Найти(РезервыПоВознаграждениям, "ОбъектУчета");
	Если ОписаниеОбъектаУчета <> Неопределено Тогда
		ОписаниеОбъектаУчета.ПутьКДаннымОбъектНастройки = "Аналитика";
	КонецЕсли;
	
	ОписаниеОбъектаУчета = ОписаниеОбъектовУчета.Найти(РезервыПоСтраховымВзносам, "ОбъектУчета");
	Если ОписаниеОбъектаУчета <> Неопределено Тогда
		ОписаниеОбъектаУчета.ПутьКДаннымОбъектНастройки = "Аналитика";
	КонецЕсли;
	
	//-- Локализация
	
	ОписаниеОбъектаУчета = ОписаниеОбъектовУчета.Найти(БилетыБронирование, "ОбъектУчета");
	Если ОписаниеОбъектаУчета <> Неопределено Тогда
		КопияОписанияОбъектаУчета = КопияОписанияОбъектаУчета(ОписаниеОбъектовУчета, ОписаниеОбъектаУчета);
		ЗаполнитьТаблицуРесурсов(ОписаниеОбъектаУчета.РесурсыУпр, "ПредоплатаУпр");
		ЗаполнитьТаблицуРесурсов(ОписаниеОбъектаУчета.РесурсыРегл, "ПредоплатаРегл");
		ЗаполнитьТаблицуРесурсов(ОписаниеОбъектаУчета.РесурсыВал, "Предоплата");
		ЗаполнитьТаблицуРесурсов(КопияОписанияОбъектаУчета.РесурсыУпр, "ДолгУпр");
		ЗаполнитьТаблицуРесурсов(КопияОписанияОбъектаУчета.РесурсыРегл, "ДолгРегл");
		ЗаполнитьТаблицуРесурсов(КопияОписанияОбъектаУчета.РесурсыВал, "Долг");
	КонецЕсли;
	
	ОписаниеОбъектаУчета = ОписаниеОбъектовУчета.Найти(РасчетыПоНДС, "ОбъектУчета");
	Если ОписаниеОбъектаУчета <> Неопределено Тогда
		ОписаниеОбъектаУчета.УчетНДС = Истина;
		Если НастройкиУчета.УчетВыручкиСНДС И НастройкиУчета.УчетВыручкиБезНДС Тогда
			КопияОписанияОбъектаУчета = КопияОписанияОбъектаУчета(ОписаниеОбъектовУчета, ОписаниеОбъектаУчета);
			КопияОписанияОбъектаУчета.ПутьКДаннымИдентификаторФинЗаписи = СтрЗаменить(
				"ВЫБОР
				|		КОГДА ПсевдонимИсточникаДанных.ОбъектРасчетов <> ЗНАЧЕНИЕ(Справочник.ОбъектыРасчетов.ПустаяСсылка)
				|			ТОГДА ПсевдонимИсточникаДанных.ОбъектРасчетов.УникальныйИдентификатор
				|		КОГДА ТИПЗНАЧЕНИЯ(ПсевдонимИсточникаДанных.Регистратор) В (&ТипыДокументовРозничныхПродаж)
				|		  ИЛИ ТИПЗНАЧЕНИЯ(ПсевдонимИсточникаДанных.Регистратор) В (&ТипыДокументовРозничныхВозвратов)
				|			ТОГДА ПОДСТРОКА(ПсевдонимИсточникаДанных.ИдентификаторФинЗаписи, 29, 8) +
				|				  ПОДСТРОКА(ПсевдонимИсточникаДанных.ИдентификаторФинЗаписи, 9, 20) +
				|				  ПОДСТРОКА(ПсевдонимИсточникаДанных.ИдентификаторФинЗаписи, 1, 8)
				|		ИНАЧЕ &ИдентификаторНеиспользуемойФинЗаписи
				|	КОНЕЦ",
				"ПсевдонимИсточникаДанных",
				ФинансовыйУчетПоДаннымБалансовыхРегистров.ПсевдонимИсточникаДанныхПоУмолчанию());
			КопияОписанияОбъектаУчета.ПутьКДаннымИдентификаторСтроки = "ИдентификаторФинЗаписи";
			ЗаполнитьТаблицуРесурсов(КопияОписанияОбъектаУчета.РесурсыКоличество, "0", 0);
			ЗаполнитьПризнакиУчетаКорреспонденций(КопияОписанияОбъектаУчета, "ВыручкаБезНДС", Истина);
		ИначеЕсли НастройкиУчета.УчетВыручкиБезНДС Тогда
			ОписаниеОбъектаУчета.ПутьКДаннымИдентификаторФинЗаписи = СтрЗаменить(
				"ВЫБОР
				|		КОГДА ПсевдонимИсточникаДанных.ОбъектРасчетов <> ЗНАЧЕНИЕ(Справочник.ОбъектыРасчетов.ПустаяСсылка)
				|			ТОГДА ПсевдонимИсточникаДанных.ОбъектРасчетов.УникальныйИдентификатор
				|		ИНАЧЕ ПсевдонимИсточникаДанных.ИдентификаторФинЗаписи
				|	КОНЕЦ",
				"ПсевдонимИсточникаДанных",
				ФинансовыйУчетПоДаннымБалансовыхРегистров.ПсевдонимИсточникаДанныхПоУмолчанию());
			ОписаниеОбъектаУчета.ПутьКДаннымИдентификаторСтроки = "ИдентификаторФинЗаписи";
		КонецЕсли;
	КонецЕсли;
	
	ОписаниеОбъектаУчета = ОписаниеОбъектовУчета.Найти(НДСПоОтгрузкамБезПереходаПраваСобственности, "ОбъектУчета");
	Если ОписаниеОбъектаУчета <> Неопределено Тогда
		ОписаниеОбъектаУчета.УчетНДС = Истина;
		Если НастройкиУчета.УчетВыручкиСНДС И НастройкиУчета.УчетВыручкиБезНДС Тогда
			КопияОписанияОбъектаУчета = КопияОписанияОбъектаУчета(ОписаниеОбъектовУчета, ОписаниеОбъектаУчета);
			КопияОписанияОбъектаУчета.ПутьКДаннымИдентификаторФинЗаписи = СтрЗаменить(
				"ВЫБОР
				|		КОГДА ПсевдонимИсточникаДанных.ОбъектРасчетов <> ЗНАЧЕНИЕ(Справочник.ОбъектыРасчетов.ПустаяСсылка)
				|			ТОГДА ПсевдонимИсточникаДанных.ОбъектРасчетов.УникальныйИдентификатор
				|		ИНАЧЕ &ИдентификаторНеиспользуемойФинЗаписи
				|	КОНЕЦ",
				"ПсевдонимИсточникаДанных",
				ФинансовыйУчетПоДаннымБалансовыхРегистров.ПсевдонимИсточникаДанныхПоУмолчанию());
			КопияОписанияОбъектаУчета.ПутьКДаннымНастройкаХозОперации = СтрЗаменить(
				"ВЫБОР
				|		КОГДА ПсевдонимИсточникаДанных.ДебетКредит = ЗНАЧЕНИЕ(Перечисление.ВидыДвиженийПрочихАктивовПассивов.Дебет)
