//
Функция ПараметрыЗаполненияСуммыНДСРеглУпр(ВалютаДокумента, ВалютаРегл, ВалютаУпр) Экспорт
	
	Результат = Новый Структура;
	Результат.Вставить("ВалютаДокумента", ВалютаДокумента);
	Результат.Вставить("ВалютаРегл", ВалютаРегл);
