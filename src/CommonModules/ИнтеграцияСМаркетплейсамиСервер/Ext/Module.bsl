Функция ЕстьВключенноеРегламентноеЗаданиеРучногоИмпортаТоварногоКаталога(МетаданныеЗадания, КлючЗадания, ДанныеЗапуска)
	
	Результат = Ложь;
	
	Отбор = Новый Структура;
	
	Отбор.Вставить("Использование", Истина);
	Отбор.Вставить("Метаданные",    МетаданныеЗадания);
	Отбор.Вставить("Ключ",          КлючЗадания);
	
	УстановитьПривилегированныйРежим(Истина);
	РезультатПоиска = РегламентныеЗаданияСервер.НайтиЗадания(Отбор);
	УстановитьПривилегированныйРежим(Ложь);
	
	Результат = (РезультатПоиска.Количество() > 0);
	
	Если Результат Тогда
		
		ТекущаяДата = ТекущаяДатаСеанса();
		
		ЧасВСекундах = 60 * 60;
		
		// Если регламентное задание создано больше часа назад, но так и не было удалено, считаем, что его нет.
		//
		Если ТекущаяДата - ДанныеЗапуска.ДатаСозданияРучногоИмпортаТоварногоКаталога > ЧасВСекундах Тогда
			Результат = Ложь;
		КонецЕсли;
		
	КонецЕсли;
	
	Возврат Результат;
	
КонецФункции

