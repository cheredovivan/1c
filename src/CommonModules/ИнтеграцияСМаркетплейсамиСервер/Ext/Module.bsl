Функция НовоеОписаниеДанныхДляСозданияУпаковок()
	
	Результат = Новый Структура;
	
	Реквизиты = Новый Структура;
	
	Реквизиты.Вставить("Ссылка",       Справочники.УпаковкиЕдиницыИзмерения.ПустаяСсылка());
