// Конструктор параметров замены номеров отправлений по заказу.
// 
// Возвращаемое значение:
//  Структура:
//   * ИмяРегистра - Строка - полное имя регистра сведений, в котором необходимо заменить номер отправления.
//                   Поддерживаются ДополнительныеСведенияПоТоварамЗаказовТорговыхПлощадок и ЗаказыТорговыхПлощадок.
//   * ИмяСобытияЖурналаРегистрации - Строка - имя события, с которым будет произведена запись в журнал регистрации.
//   * Заказ - ДокументСсылка.ЗаказКлиента - заказ клиента, номера отправлений которого нужно изменить.
//   * ДанныеДляЗамены - Соответствие из КлючИЗначение:
//      ** Ключ - Строка - старый номер отправления.
//      ** Значение - см. НовыеДанныеЗаписиДляЗаменыНомераОтправленияПоЗаказу
//
Функция НовыеПараметрыЗаменыНомеровОтправленийПоЗаказу() Экспорт
	
	Результат = Новый Структура;
	
	Результат.Вставить("ИмяРегистра", "");
	Результат.Вставить("ИмяСобытияЖурналаРегистрации", "");
	Результат.Вставить("Заказ", Документы.ЗаказКлиента.ПустаяСсылка());
	Результат.Вставить("ДанныеДляЗамены", Новый Соответствие);
	
	Возврат Результат;
	
КонецФункции

// Конструктор данных записи для замены номера отправления по заказу.
// 
// Возвращаемое значение:
//  Структура:
//   * НомерОтправления - Строка - новый номер отправления из сервиса.
//   * ИдентификаторОтправления - Строка - идентификатор отправления из сервиса.
//   * ИдентификаторОтгрузки - Строка - идентификатор отгрузки из сервиса.
//   * Статус - ПеречислениеСсылка.СтатусыЗаказовТорговыхПлощадок - новый статус заказа.
//   * ДатаУстановкиСтатуса - Дата - дата установки или изменения статуса заказа. Используется, если передан статус.
//   * ТребуетсяПолучениеЭтикеток - Булево - признак необходимости получения этикеток по номеру отправления.
//                                  Используется, если передан статус.
//
Функция НовыеДанныеЗаписиДляЗаменыНомераОтправленияПоЗаказу() Экспорт
	
	Результат = Новый Структура;
	
	Результат.Вставить("НомерОтправления", "");
	Результат.Вставить("ИдентификаторОтправления", "");
	Результат.Вставить("ИдентификаторОтгрузки", "");
	Результат.Вставить("Статус", Перечисления.СтатусыЗаказовТорговыхПлощадок.ПустаяСсылка());
	Результат.Вставить("ДатаУстановкиСтатуса", Дата(1, 1, 1));
	Результат.Вставить("ТребуетсяПолучениеЭтикеток", Ложь);
	
	Возврат Результат;
	
КонецФункции

// Заменяет номера отправлений для заказа после подтверждения сборки выделенных товаров.
// 
// Параметры:
//  ПараметрыЗамены - см. НовыеПараметрыЗаменыНомеровОтправленийПоЗаказу
// 
// Возвращаемое значение:
//  Строка - текст ошибки.
//
Функция ЗаменитьНомераОтправленийПоЗаказу(ПараметрыЗамены) Экспорт
	
	ТекстОшибки = "";
	
	НомераОтправлений = Новый Массив; // Массив из Строка
	Для Каждого КлючЗначение Из ПараметрыЗамены.ДанныеДляЗамены Цикл
		НомераОтправлений.Добавить(КлючЗначение.Ключ);
	КонецЦикла;
	
	НачатьТранзакцию();
	
	Попытка
		Блокировка = Новый БлокировкаДанных;
		ЭлементБлокировки = Блокировка.Добавить(ПараметрыЗамены.ИмяРегистра);
		ЭлементБлокировки.УстановитьЗначение("Заказ", ПараметрыЗамены.Заказ);

		Блокировка.Заблокировать();
		
		Выборка = ВыборкаДанныхДляЗаменыНомеровОтправленийПоЗаказу(
			ПараметрыЗамены.ИмяРегистра, ПараметрыЗамены.Заказ, НомераОтправлений);
		
		МенеджерРегистра = ОбщегоНазначения.МенеджерОбъектаПоПолномуИмени(ПараметрыЗамены.ИмяРегистра);
		НаборЗаписей = МенеджерРегистра.СоздатьНаборЗаписей();
		
		Пока Выборка.Следующий() Цикл
			
			ИзменятьНомерРодительскогоОтправления = Ложь;
			
			НовыеДанныеЗаписи = ПараметрыЗамены.ДанныеДляЗамены.Получить(Выборка.СтарыйНомерОтправления);
			НовыеДанныеЗаписиРодителя = ПараметрыЗамены.ДанныеДляЗамены.Получить(Выборка.НомерРодительскогоОтправления);
			
			Если НовыеДанныеЗаписи = Неопределено Или НовыеДанныеЗаписиРодителя <> Неопределено Тогда
				ИзменятьНомерРодительскогоОтправления = Истина;
			КонецЕсли;
			
			ДанныеЗаполнения = ДанныеЗаполненияПоНовымДаннымЗаписи(
				НовыеДанныеЗаписи,
				(Выборка.СтарыйНомерОтправления = Выборка.НомерРодительскогоОтправления));
			
			Если ИзменятьНомерРодительскогоОтправления Тогда
				ДанныеЗаполнения.Вставить("НомерРодительскогоОтправления", НовыеДанныеЗаписиРодителя.НомерОтправления);
			КонецЕсли;
			
			НоваяЗапись = НаборЗаписей.Добавить();
			ЗаполнитьЗначенияСвойств(НоваяЗапись, Выборка);
			ЗаполнитьЗначенияСвойств(НоваяЗапись, ДанныеЗаполнения);
			
		КонецЦикла;
		
		Если НаборЗаписей.Количество() > 0 Тогда
			НаборЗаписей.Записать(РежимЗамещения.Обновление);
		КонецЕсли;
		ЗафиксироватьТранзакцию();
		
	Исключение
		ОтменитьТранзакцию();
		ТекстОшибки = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(
			НСтр("ru = 'Не удалось изменить номера отправлений для заказа <%1> по причине: %2.';
				|en = 'Cannot change the shipment numbers for the <%1> order. Reason: %2.'",
				ОбщегоНазначения.КодОсновногоЯзыка()),
			Строка(ПараметрыЗамены.Заказ),
			ОбработкаОшибок.ПодробноеПредставлениеОшибки(ИнформацияОбОшибке()));
		
		ОбщегоНазначения.СообщитьПользователю(ТекстОшибки);
		ЗаписьЖурналаРегистрации(
			ПараметрыЗамены.ИмяСобытияЖурналаРегистрации,
			УровеньЖурналаРегистрации.Ошибка,
			ПараметрыЗамены.Заказ.Метаданные(),
			ПараметрыЗамены.Заказ,
			ТекстОшибки);
	КонецПопытки;
	
	Возврат ТекстОшибки;

КонецФункции

