	КомандаФормы = Форма.Команды.Найти(АдресКомандЭДОВоВременномХранилище);
	Если КомандаФормы <> Неопределено Тогда
		Если ТипЗнч(Форма) = Тип("ФормаКлиентскогоПриложения") Тогда
			ИмяФормы = Форма.ИмяФормы;
		Иначе
			ИмяФормы = Форма;
		КонецЕсли;
		
		ОбъектМетаданных = Метаданные.НайтиПоПолномуИмени(ИмяФормы);
		Если ОбъектМетаданных <> Неопределено 
				И Не Метаданные.ОбщиеФормы.Содержит(ОбъектМетаданных) Тогда
			ОбъектМетаданных = ОбъектМетаданных.Родитель();
		КонецЕсли;
		
		СоставКоманд = Новый Структура;
		СоставКоманд.Вставить("Исходящие", Новый Массив);
		СоставКоманд.Вставить("Входящие",  Новый Массив);
		
		НаправлениеЭД = ПараметрыРазмещенияКоманд.Направление;
		КомандыЭДО    = ПолучитьИзВременногоХранилища(КомандаФормы.Действие);
		
		НовыеКомандыЭДО = КомандыЭДО.Скопировать();
		НовыеКомандыЭДО.Очистить();
		ПриОпределенииСпискаКомандЭДО(СоставКоманд, ОбъектМетаданных.ПолноеИмя(), НаправлениеЭД, НовыеКомандыЭДО);
		
		// Размещение команд на форме
		МестоРазмещенияКомандПоУмолчанию = ПараметрыРазмещенияКоманд.МестоРазмещенияКоманд;
		
		ТаблицаКоманд = НовыеКомандыЭДО.Скопировать(, "МестоРазмещения");
		ТаблицаКоманд.Свернуть("МестоРазмещения");
		МестаРазмещения = ТаблицаКоманд.ВыгрузитьКолонку("МестоРазмещения");
	
		Для Каждого МестоРазмещения Из МестаРазмещения Цикл
			НайденныеКоманды = НовыеКомандыЭДО.НайтиСтроки(
				Новый Структура("МестоРазмещения, СкрытаФункциональнымиОпциями, Отключена", МестоРазмещения, Ложь, Ложь));
			ЭлементФормыДляРазмещения = Форма.Элементы.Найти(МестоРазмещения); // Массив из СтрокаТаблицыЗначений
			Если ЭлементФормыДляРазмещения = Неопределено Тогда
				ЭлементФормыДляРазмещения = МестоРазмещенияКомандПоУмолчанию;
			КонецЕсли;

			Если НайденныеКоманды.Количество() > 0 Тогда
				ДобавитьКомандыЭДО(Форма, НайденныеКоманды, ЭлементФормыДляРазмещения,
					ПараметрыРазмещенияКоманд.ИсточникКомандЭДО);
			КонецЕсли;
		КонецЦикла;
		
		ОбщегоНазначенияКлиентСервер.ДополнитьТаблицу(НовыеКомандыЭДО, КомандыЭДО);
		КомандаФормы.Действие = ПоместитьВоВременноеХранилище(КомандыЭДО, Форма.УникальныйИдентификатор);
	КонецЕсли;
	
КонецПроцедуры

// Дополняет список команд ЭДО командами других подсистем.
//
// Параметры:
//   СоставКоманд  - Структура - состав команд ЭДО:
//     * Исходящие    - Массив из Строка - объекты для команд работы с исходящими документами.
//     * Входящие     - Массив из Строка - объекты для команд работы со входящими документами.
//     * БезПодписи   - Массив из Строка - объекты для команд выполнения ЭДО без подписи.
//     * Внутренние   - Массив из Строка - объекты для команд внутреннего ЭДО.
//     * Интеркампани - Массив из Строка - объекты для команд обмена между организациями в рамках одной базы.
//     * Контрагенты  - Массив из Строка - объекты для команд работы с контрагентами.
//     * Организации  - Массив из Строка - объекты для команд работы с организациями.
//     * Договоры     - Массив из Строка - объекты для команд работы с договорами.
//   ПолноеИмя     - Строка - имя объекта, например "Документ.РеализацияТоваровУслуг".
//   НаправлениеЭД - ПеречислениеСсылка.НаправленияЭДО - параметр отбора входящих или исходящих документов.
//   КомандыЭДО    - ТаблицаЗначений - подготавливаемый список команд ЭДО.
//
Процедура ПриОпределенииСпискаКомандЭДО(Знач СоставКоманд, Знач ПолноеИмя, Знач НаправлениеЭД, КомандыЭДО) Экспорт
	
	ИспользоватьОбменЭД  = ПолучитьФункциональнуюОпцию("ИспользоватьОбменЭД");
	ЕстьПравоОбработкиЭД = ОбменСКонтрагентами.ЕстьПравоОбработкиЭД();
	ЕстьПравоОбменаЭД    = ОбменСКонтрагентами.ЕстьПравоВыполненияОбмена();
	
	СоставКоманд = Новый Структура;
	СоставКоманд.Вставить("Исходящие", Новый Массив);
	СоставКоманд.Вставить("Входящие",  Новый Массив);

	ПриОпределенииСоставаКомандЭДО(СоставКоманд);
	
	Если СоставКоманд.Входящие.Найти(ПолноеИмя) <> Неопределено 
			И НаправлениеЭД <> Перечисления.НаправленияЭДО.Исходящий Тогда 
		Идентификатор = "СверитьДокументСТорговойПлощадкой";
		
		Если ИспользоватьОбменЭД
				И ЕстьПравоОбменаЭД
				И КомандыЭДО.Найти(Идентификатор, "Идентификатор") = Неопределено Тогда
			КомандаЭДО = КомандыЭДО.Добавить();
			КомандаЭДО.Обработчик                  = "ИнтеграцияСМаркетплейсамиКлиент.СверитьДокументСТорговойПлощадкой";
			КомандаЭДО.СписокФорм                  = "";
			КомандаЭДО.Идентификатор               = Идентификатор;
			КомандаЭДО.Представление               = НСтр("ru = 'Сверить документ с получателем';
															|en = 'Reconcile the document with the recipient'");
			КомандаЭДО.Порядок                     = 100;
			КомандаЭДО.Картинка                    = БиблиотекаКартинок.ЗагрузкаИзВнешнегоИсточника;
			КомандаЭДО.РежимИспользованияПараметра = РежимИспользованияПараметраКоманды.Одиночный;
			КомандаЭДО.МестоРазмещения             = "КомандыЭДОСервис";
			КомандаЭДО.ТолькоВоВсехДействиях       = Ложь;
			КомандаЭДО.Недоступна                  = Не ЕстьПравоОбработкиЭД;
		КонецЕсли;
	КонецЕсли;
	
КонецПроцедуры

// Определяет параметры прикладного электронного документа по данным файла.
//
// Параметры:
//   ДвоичныеДанные      - ДвоичныеДанные - данные файла прикладного электронного документа.
//   РасширениеФайла     - Строка - расширение файла без точки.
//   Параметры           - см. НовыйПрикладнойТипЭлектронногоДокумента.
//   ВидТорговойПлощадки - Неопределено, ПеречислениеСсылка.ВидыМаркетплейсов - вид торговой площадки.
//   ТекстОшибки         - Строка - представление ошибки.
//
Процедура ПриОпределенииПараметровФайлаПрикладногоЭлектронногоДокумента(Знач ДвоичныеДанные, Знач РасширениеФайла, 
			Параметры, ВидТорговойПлощадки = Неопределено, ТекстОшибки = "") Экспорт
	
	РасширениеФайла = НРег(СтрЗаменить(РасширениеФайла, ".", ""));
	
	Если НРег(РасширениеФайла) <> "xls" 
			И НРег(РасширениеФайла) <> "xlsx" 
			И НРег(РасширениеФайла) <> "ods" Тогда
		ТекстОшибки = НСтр("ru = 'Некорректный формат файла. Выберите файл с расширением ""xml"" или ""xls"".';
							|en = 'Incorrect file format. Select a file with ""xml"" or ""xls"" extension.'");
		Возврат;
	КонецЕсли;
	
	ДанныеФайла = ПрочитатьТабличныйДокумент(ДвоичныеДанные, РасширениеФайла, ТекстОшибки);
	Если ДанныеФайла = Неопределено Тогда
		Возврат;
	КонецЕсли;
	
	Параметры.Вставить("ЕстьМаркировка",      Ложь);
	Параметры.Вставить("ВидТорговойПлощадки", Неопределено);
	Параметры.Вставить("Представление",       Новый ТабличныйДокумент);
	Параметры.Вставить("ДанныеРазбора",       Новый ТаблицаЗначений);
	
	Если ДанныеФайла.Области.Количество() > 1 Тогда
		ТабличныйДокумент = Новый ТабличныйДокумент;
		
		Для Каждого Лист Из ДанныеФайла.Области Цикл
			Область = ТабличныйДокумент.Область(ТабличныйДокумент.ВысотаТаблицы + 1 
				+ ?(ТабличныйДокумент.ВысотаТаблицы > 0, 1, 0), 1);
			Область.Текст                 = СтрЗаменить(СтрЗаменить(Лист.Имя, "__", ", "), "_", " ");
			Область.Шрифт                 = ШрифтыСтиля.ВажнаяНадписьШрифт;
			Область.ВертикальноеПоложение = ВертикальноеПоложение.Верх;
			Область.ВысотаСтроки          = 15;
			
			ТабличныйДокумент.Вывести(ДанныеФайла.ПолучитьОбласть(Лист.Имя));
		КонецЦикла;
		
		Параметры.Вставить("Представление", ТабличныйДокумент);
		
	Иначе
		Параметры.Вставить("Представление", ДанныеФайла);
	КонецЕсли;
	
	// Акт приемки товаров
	Если Не ЗначениеЗаполнено(ВидТорговойПлощадки)
			Или ВидТорговойПлощадки = Перечисления.ВидыМаркетплейсов.МаркетплейсOzon Тогда
		Обработки.УправлениеПродажамиНаOzon.ПрочитатьАктСверки(ДанныеФайла, Параметры);
		Если ЗначениеЗаполнено(Параметры.ВидТорговойПлощадки) Тогда
			Возврат;
		КонецЕсли;
	КонецЕсли;
	
	Если Не ЗначениеЗаполнено(ВидТорговойПлощадки)
			Или ВидТорговойПлощадки = Перечисления.ВидыМаркетплейсов.МаркетплейсЯндексМаркет Тогда
		Обработки.УправлениеПродажамиНаЯндексМаркет.ПрочитатьАктСверки(ДанныеФайла, Параметры);
		Если ЗначениеЗаполнено(Параметры.ВидТорговойПлощадки) Тогда
			Возврат;
		КонецЕсли;
	КонецЕсли;
	
	Если Не ЗначениеЗаполнено(ВидТорговойПлощадки)
			Или ВидТорговойПлощадки = Перечисления.ВидыМаркетплейсов.МаркетплейсWildberries Тогда
		Обработки.УправлениеПродажамиНаМаркетплейсах.ПрочитатьАктСверки(ДанныеФайла, Параметры);
		Если ЗначениеЗаполнено(Параметры.ВидТорговойПлощадки) Тогда
			Возврат;
		КонецЕсли;
	КонецЕсли;
	
	Параметры.ТипЭлектронногоДокумента = Справочники.ИдентификаторыОбъектовМетаданных.ПустаяСсылка();
	ТекстОшибки = НСтр("ru = 'Содержимое выбранного файла не соответствует требуемому формату.';
						|en = 'The content of the selected file does not match the required format.'");
	
КонецПроцедуры

// Формирование представления прикладного электронного документа по данным файла.
//
// Параметры:
//   ТабличныйДокумент   - ТабличныйДокумент - визуализация прикладного электронного документа.
//   ДвоичныеДанные      - ДвоичныеДанные - данные файла прикладного электронного документа.
//   ПараметрыЗаполнения - Структура из КлючИЗначение - дополнительные параметры для формирования табличного документа.
//   ТекстОшибки         - Строка - представление ошибки.
//
Процедура ПриЗаполненииТабличногоДокументаИзФайлаПрикладногоЭлектронногоДокумента(ТабличныйДокумент, Знач ДвоичныеДанные, 
			ПараметрыЗаполнения, ТекстОшибки = "") Экспорт
	
	ТабличныйДокумент = Новый ТабличныйДокумент;
	ДанныеФайла       = ПрочитатьТабличныйДокумент(ДвоичныеДанные,, ТекстОшибки);
	
	Если ДанныеФайла <> Неопределено Тогда
		Если ДанныеФайла.Области.Количество() > 1 Тогда
			Для Каждого Лист Из ДанныеФайла.Области Цикл
				Область = ТабличныйДокумент.Область(ТабличныйДокумент.ВысотаТаблицы + 1 
					+ ?(ТабличныйДокумент.ВысотаТаблицы > 0, 1, 0), 1);
				Область.Текст                 = СтрЗаменить(СтрЗаменить(Лист.Имя, "__", ", "), "_", " ");
				Область.Шрифт                 = ШрифтыСтиля.ВажнаяНадписьШрифт;
				Область.ВертикальноеПоложение = ВертикальноеПоложение.Верх;
				Область.ВысотаСтроки          = 15;
				
				ТабличныйДокумент.Вывести(ДанныеФайла.ПолучитьОбласть(Лист.Имя));
			КонецЦикла;
			
		Иначе
			ТабличныйДокумент = ДанныеФайла;
		КонецЕсли;
	КонецЕсли;
	
КонецПроцедуры

// Выполняется при отражении в учете электронного документа, содержащего информацию для сопоставления номенклатуры.
// Позволяет определить необходимость сопоставления номенклатуры для указанного способа обработки электронных документов,
// а также отказаться от отражения в учете, если сопоставление не выполнено.
//
// Параметры:
//   Документ - Структура - данные электронного документа (только чтение):
//     * СпособОбработки - Строка    - способ отражения в учете электронного документа.
//                       - Структура - способ отражения в учете УПД (расширенный):
//       ** ПервичныйДокумент - Строка - способ отражения в учете первичного документа.
//       ** СчетФактура       - Строка - способ отражения в учете счет-фактуры.
//  Настройки - Структура - настройки сопоставления номенклатуры (для изменения):
//   * ОтражатьТолькоСопоставленные - Булево - признак разрешения отражать в учете только документы с сопоставленной 
//                                      номенклатурой.
//
Процедура ПриОтраженииВУчетеЭлектронногоДокументаСНоменклатурой(Знач Документ, Настройки) Экспорт
	
	СпособОбработки = ?(ТипЗнч(Документ.СпособОбработки) = Тип("Структура"),
		Документ.СпособОбработки.ПервичныйДокумент,
		Документ.СпособОбработки);
		
	Если Строка(СпособОбработки) = "СверкаДокументаСТорговойПлощадкой" Тогда
		Настройки.ОтражатьТолькоСопоставленные = Ложь;
	КонецЕсли;
	
КонецПроцедуры

// Получает описание сообщения по двоичным данным.
//
// Параметры:
//   ОписаниеФайла - см. РаботаСФайламиБЭД.НовоеОписаниеФайла.
//   ТекстОшибки   - Строка - представление ошибки.
//
// Возвращаемое значение:
//   - Неопределено - 
//   - Структура - дополненная структура ФорматыЭДО.ПрочитатьСодержаниеДокумента:
//       * ИдентификаторДокумента       - Строка
//       * ТипДокумента                 - ПеречислениеСсылка.ТипыДокументовЭДО
//       * ПрикладнойТипДокумента       - Неопределено
//       * ТипРегламента                - ПеречислениеСсылка.ТипыРегламентовЭДО
//       * НомерДокумента               - Строка
//       * ДатаДокумента                - Дата
//       * СуммаДокумента               - Число
//       * ВалютаДокумента              - Строка 
//       * СтавкаНДСДокумента           - Строка
//       * СуммаНДСДокумента            - Число
//       * ЕстьМаркировка               - Булево
//       * Отправитель                  - см. ФорматыЭДО.НовоеОписаниеУчастникаЭДО
//       * Получатель                   - см. ФорматыЭДО.НовоеОписаниеУчастникаЭДО
//       * Формат                       - Строка
//       * ОтражениеВУчете              - Неопределено
//       * СвойстваДокументовПоФорматам - см. ФорматыЭДО.НовоеОписаниеСвойствДокументовПоФорматам
//       * ДанныеРазбора                - Неопределено, Строка - адрес временного хранилища. 
//
Функция СодержаниеЭлектронногоДокумента(Знач ОписаниеФайла, ТекстОшибки = "") Экспорт
	
	Файл            = Новый Файл(ОписаниеФайла.ИмяФайла);
	РасширениеФайла = НРег(СтрЗаменить(Файл.Расширение, ".", ""));
	
	Если РасширениеФайла = "xls" 
			Или РасширениеФайла = "xlsx" 
			Или РасширениеФайла = "ods" Тогда
		Параметры = НовыйПрикладнойТипЭлектронногоДокумента();
		ПриОпределенииПараметровФайлаПрикладногоЭлектронногоДокумента(ОписаниеФайла.ДвоичныеДанные, 
			РасширениеФайла, 
			Параметры,
			,
			ТекстОшибки);
		
		Если Параметры.ТипЭлектронногоДокумента = Неопределено Тогда
			Возврат Неопределено;
		КонецЕсли;
		
		Результат = ЭлектронныеДокументыЭДО.НовоеСодержаниеСообщения();
		Результат.Вставить("ЕстьМаркировка",      Параметры.ЕстьМаркировка);
		Результат.Вставить("ВидТорговойПлощадки", Параметры.ВидТорговойПлощадки);
		Результат.Вставить("Представление",       Параметры.Представление);
		Результат.Вставить("ДанныеРазбора",       ПоместитьВоВременноеХранилище(Параметры.ДанныеРазбора, 
													Новый УникальныйИдентификатор));
		
		Результат.ТипДокумента           = Перечисления.ТипыДокументовЭДО.Прикладной;
		Результат.ПрикладнойТипДокумента = Параметры.ТипЭлектронногоДокумента;
		Результат.ТипРегламента          = Перечисления.ТипыРегламентовЭДО.Неформализованный;
		Результат.Формат                 = ОбменСКонтрагентами.ФорматыДокументов().Прикладной;
		Результат.НомерДокумента         = Параметры.НомерДокументаОтправителя;
		Результат.ДатаДокумента          = Параметры.ДатаДокументаОтправителя;
		Результат.СуммаДокумента         = Параметры.СуммаДокумента;
		Результат.Отправитель            = Параметры.Отправитель;
		Результат.Получатель             = Параметры.Получатель;
		
	Иначе
		Результат = ЭлектронныеДокументыЭДО.СодержаниеСообщения(ОписаниеФайла);
		
		Если ЗначениеЗаполнено(Результат) Тогда
			Результат.Вставить("ДанныеРазбора", Неопределено);
		КонецЕсли;
	КонецЕсли;
	
	Возврат Результат;
	
КонецФункции

// Определяет предполагаемый вид торговой площадки.
//
// Параметры:
//   ДанныеЭлектронногоДокумента - см. ОтражениеВУчетеЭДО.НовыеДанныеЭлектронногоДокументаДляОтраженияВУчете.
//   ТекстОшибки                 - Строка - представление ошибки.
// 
// Возвращаемое значение:
//   ПеречислениеСсылка.ВидыМаркетплейсов - вид торговой площадки.
//
Функция ОпределитьВидТорговойПлощадкиПоЭлектронномуДокументу(ДанныеЭлектронногоДокумента, ТекстОшибки = "") Экспорт
	
	Попытка
		ДанныеРазбора = ПолучитьДанныеЭлектронногоДокумента(ДанныеЭлектронногоДокумента, ТекстОшибки);
		
		Если ЗначениеЗаполнено(ДанныеРазбора) Тогда
			СтрокиТребующиеСопоставления             = СтрокиТребующиеСопоставления(ДанныеРазбора.НовыйЭД, Истина, ТекстОшибки);
			ТоварыПодчиненыОднойКорневойСтрокеДерева = ТоварыПодчиненыОднойКорневойСтрокеДерева(СтрокиТребующиеСопоставления);
			
			Если ЗначениеЗаполнено(СтрокиТребующиеСопоставления) 
					И ТоварыПодчиненыОднойКорневойСтрокеДерева Тогда
				Константа_Идентификатор = ИнтеграцияСМаркетплейсомOzonКлиентСервер.КонстантыСервиса()["УПД_ИдентификаторОбъекта"];	//@NON-NLS-1
				Константа_Штрихкод      = ИнтеграцияСЯндексМаркетКлиентСервер.КонстантыСервиса()["УПД_Штрихкод"];	//@NON-NLS-1
						
				Для Каждого КлючИЗначение Из СтрокиТребующиеСопоставления Цикл
					ПутьКДопДанным       = КлючИЗначение.Ключ.ПолныйПуть + ".ДопДанныеПодписанные";
					ДопДанныеПодписанные = КлючИЗначение.Ключ.Строки.Найти(ПутьКДопДанным, "ПолныйПуть", Истина);
					
					Если ЗначениеЗаполнено(ДопДанныеПодписанные.Значение) Тогда
						ПутьКДопДанным = ПутьКДопДанным + ".НомерСтроки";
						Для Каждого СтрокаДереваЗначений Из ДопДанныеПодписанные.Строки Цикл
							Имя = ЭлектронноеВзаимодействие.ЗначениеРеквизитаВДереве(СтрокаДереваЗначений, 
								ПутьКДопДанным + ".Идентификатор",);
							Значение = ЭлектронноеВзаимодействие.ЗначениеРеквизитаВДереве(СтрокаДереваЗначений, 
								ПутьКДопДанным + ".Значение");
							
							Если ЗначениеЗаполнено(Имя)
									И ЗначениеЗаполнено(Значение) Тогда
								Если ВРег(Имя) = Константа_Идентификатор Тогда
									Возврат Перечисления.ВидыМаркетплейсов.МаркетплейсOzon;
								ИначеЕсли ВРег(Имя) = Константа_Штрихкод Тогда
									Возврат Перечисления.ВидыМаркетплейсов.МаркетплейсЯндексМаркет;
								КонецЕсли;
							КонецЕсли;
						КонецЦикла;
					КонецЕсли;
					
					Прервать;
				КонецЦикла;
			КонецЕсли;
		КонецЕсли;
		
	Исключение
		ТекстОшибки = ОбработкаОшибок.КраткоеПредставлениеОшибки(ИнформацияОбОшибке());
	КонецПопытки;
	
	Возврат Перечисления.ВидыМаркетплейсов.ПустаяСсылка();
	
КонецФункции

// Отражает данные электронного документа в учете.
//
// Параметры:
//   ПервичныйДокумент           - ДокументСсылка.ПередачаТоваровХранителю - первичный документ.
//   ДанныеЭлектронногоДокумента - см. ОтражениеВУчетеЭДО.НовыеДанныеЭлектронногоДокументаДляОтраженияВУчете.
//   СпособОбработки             - см. ОтражениеВУчетеЭДО.СпособОбработкиДокумента.
//   СопоставленнаяНоменклатура  - Соответствие из КлючИЗначение - сведения о сопоставленной номенклатуре:
//     * Ключ     - Строка - идентификатор номенклатуры контрагента.
//     * Значение - Структура - данные сопоставления:
//       ** НоменклатураИБ          - см. СопоставлениеНоменклатурыКонтрагентовКлиентСервер.НоваяНоменклатураИнформационнойБазы.
//       ** НоменклатураКонтрагента - см. СопоставлениеНоменклатурыКонтрагентовКлиентСервер.НоваяНоменклатураКонтрагента.
//
// Возвращаемое значение:
//   - Неопределено,
//   - Структура - результат анализа расхождений:
//       * ТоварыИзФайла                - ТаблицаЗначений из см. Документ.ПриобретениеТоваровУслуг.Товары.
//       * ШтрихкодыУпаковокИзФайла     - см. ШтрихкодированиеИС.ШтрихкодыУпаковок.
//       * ТоварыИзДокумента            - ТаблицаЗначений из см. Документ.ПередачаТоваровХранителю.Товары.
//       * ШтрихкодыУпаковокИзДокумента - см. ЭлектронноеВзаимодействиеИСМП.ЧастичноеСодержимоеИКодыОСУ.
//       * ТоварыРасхождения            - см. НоваяТаблицаТоварыРасхождения.
//       * ШтрихкодыУпаковокРасхождения - см. НоваяТаблицаШтрихкодыУпаковокРасхождения.
//       * ДанныеРазбора                - Неопределено,
//                                      - ТаблицаЗначений,
//                                      - ТабличныйДокумент,
//                                      - ДвоичныеДанные,
//                                      - Произвольный,
//                                      - Структура:
//         ** ДеревоРазбора               - см. ДеревоЭлектронногоДокументаБЭД.ИнициализироватьДеревоРазбора.
//         ** НовыйЭД                     - см. ДеревоЭлектронногоДокументаБЭД.НайтиСоздатьТипОбъектаВДеревеРазбора.
//         ** Содержание                  - Неопределено.
//
//@skip-check function-return-types
Функция ОтразитьДанныеОРасхожденияхЭлектронногоДокумента(Знач ПервичныйДокумент, ДанныеЭлектронногоДокумента, 
			Знач СпособОбработки = "", Знач СопоставленнаяНоменклатура = Неопределено) Экспорт
	
	Замер = ОценкаПроизводительности.НачатьЗамерДлительнойОперации(
		"ОбщийМодуль.ИнтеграцияСМаркетплейсамиСервер.ОтразитьДанныеОРасхожденияхЭлектронногоДокумента");
	
	ЭтоПрикладнойТипЭлектронногоДокумента = 
		ИнтеграцияЭДО.ПрикладныеТипыЭлектронныхДокументов().Найти(ДанныеЭлектронногоДокумента.ТипДокумента) <> Неопределено
			Или ДанныеЭлектронногоДокумента.ТипДокумента = Перечисления.ТипыДокументовЭДО.Прикладной;
	
	Если Не ЭтоПрикладнойТипЭлектронногоДокумента Тогда
		ПоддерживаемыеФорматы = ОбменСКонтрагентами.ФорматыДокументов();
	КонецЕсли;
		
	ОбъектыУчета  = Новый Массив;
	ДанныеРазбора = Неопределено;
	ТекстОшибки   = "";
	
	ШтрихкодыУпаковок = ЭлектронноеВзаимодействиеИСМП.НоваяТаблицаШтрихкодыУпаковок(Истина);
	ШтрихкодыУпаковок.Колонки.Добавить("ВидУпаковки", Новый ОписаниеТипов("ПеречислениеСсылка.ВидыУпаковокИС"));
	
	// Чтение электронного документа
	Если ТипЗнч(ДанныеЭлектронногоДокумента.ДополнительныеДанные) = Тип("Структура")
			И ДанныеЭлектронногоДокумента.ДополнительныеДанные.Свойство("СпособОбработки")
			И ДанныеЭлектронногоДокумента.ДополнительныеДанные.СпособОбработки = "СверкаДокументаСТорговойПлощадкой" Тогда
		УчетнаяЗапись = ДанныеЭлектронногоДокумента.ДополнительныеДанные.УчетнаяЗапись;	// СправочникСсылка.УчетныеЗаписиМаркетплейсов
		ВидТорговойПлощадки = ДанныеЭлектронногоДокумента.ДополнительныеДанные.ВидТорговойПлощадки;
		Если ЗначениеЗаполнено(УчетнаяЗапись) Тогда
			ВидТорговойПлощадки = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(УчетнаяЗапись, "ВидМаркетплейса");
		КонецЕсли;
		
		Если ЭтоПрикладнойТипЭлектронногоДокумента Тогда
			ИнформацияОТоварах = ИнформацияОТоварахИзТабличногоДокумента(ДанныеЭлектронногоДокумента, ТекстОшибки);
			ДанныеРазбора      = ДанныеЭлектронногоДокумента.ДополнительныеДанные.ДанныеРазбора;
			
			Если ИнформацияОТоварах = Неопределено Тогда
				Если Не ЗначениеЗаполнено(ТекстОшибки) Тогда
					ТекстОшибки = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(
						НСтр("ru = 'Ошибка анализа расхождений по данным файла %1';
							|en = 'Error analyzing discrepancies in the %1 file'"), 
						ДанныеЭлектронногоДокумента.ДанныеОсновногоФайла.ИмяФайла);
				КонецЕсли;
					
				ЗаписьЖурналаРегистрации(СобытиеЖурналаРегистрации(ДанныеЭлектронногоДокумента.ДополнительныеДанные.СпособОбработки), 
					УровеньЖурналаРегистрации.Ошибка,,, ТекстОшибки);
					
				ОценкаПроизводительности.ЗакончитьЗамерДлительнойОперации(Замер, 0);
				Возврат Неопределено;
				
			Иначе
				УстановитьПривилегированныйРежим(Истина);
				ПриобретениеТоваровУслуг = Документы.ПриобретениеТоваровУслуг.СоздатьДокумент();
				УстановитьПривилегированныйРежим(Ложь);
				
				Для Каждого ЭлементКоллекции Из ИнформацияОТоварах Цикл
					СтрокаТабличнойЧасти = ПриобретениеТоваровУслуг.Товары.Добавить();
					ЗаполнитьЗначенияСвойств(СтрокаТабличнойЧасти, ЭлементКоллекции);
					
					ЭлементСопоставления = Неопределено;
					Если ЗначениеЗаполнено(СопоставленнаяНоменклатура)
							И ЗначениеЗаполнено(ЭлементКоллекции.ИД) Тогда
						ЭлементСопоставления = СопоставленнаяНоменклатура.Получить("Идентификатор_" + ЭлементКоллекции.ИД);
						Если ЗначениеЗаполнено(ЭлементСопоставления) Тогда
							ЗаполнитьЗначенияСвойств(СтрокаТабличнойЧасти, ЭлементСопоставления.НоменклатураИБ);
							
							СтрокаТаблицыЗначений = ДанныеРазбора[ИнформацияОТоварах.Индекс(ЭлементКоллекции)];
							ЗаполнитьЗначенияСвойств(СтрокаТаблицыЗначений, ЭлементСопоставления.НоменклатураИБ);
							СтрокаТаблицыЗначений.ВариантУказанияНоменклатуры = 
								ЭлементСопоставления.НоменклатураКонтрагента.ВариантУказанияНоменклатуры;
						КонецЕсли;
					КонецЕсли;
					
					Если ЗначениеЗаполнено(ЭлементКоллекции.КонтрольныеИдентификационныеЗнаки) Тогда
						СтрокаТабличнойЧасти = ПриобретениеТоваровУслуг.ШтрихкодыУпаковок.Добавить();
						СтрокаТабличнойЧасти.ЗначениеШтрихкода = ЭлементКоллекции.КонтрольныеИдентификационныеЗнаки;
						
						СтрокаТаблицыЗначений = ШтрихкодыУпаковок.Добавить();
						СтрокаТаблицыЗначений.ЗначениеШтрихкода = ЭлементКоллекции.КонтрольныеИдентификационныеЗнаки;
						Если ЗначениеЗаполнено(ЭлементСопоставления) Тогда
							ЗаполнитьЗначенияСвойств(СтрокаТаблицыЗначений, ЭлементСопоставления.НоменклатураИБ);
						КонецЕсли;
					КонецЕсли;
				КонецЦикла;
				
				СтруктураПересчетаСуммы = ПакетнаяОбработкаТабличнойЧастиКлиентСервер.ПараметрыПересчетаСуммыНДСВСтрокеТЧ(
					ПриобретениеТоваровУслуг);
				
				СтруктураДействий = Новый Структура;
				СтруктураДействий.Вставить("ПересчитатьКоличествоЕдиниц");
				СтруктураДействий.Вставить("ПересчитатьСумму",     "КоличествоУпаковок");
				СтруктураДействий.Вставить("ПересчитатьСуммуНДС",  СтруктураПересчетаСуммы);
				СтруктураДействий.Вставить("ПересчитатьСуммуСНДС", СтруктураПересчетаСуммы);
				СтруктураДействий.Вставить("ОчиститьСуммуВзаиморасчетов");
				СтруктураДействий.Вставить("ПересчитатьСуммуСУчетомРучнойСкидки", Новый Структура("Очищать", Ложь));
				
				КэшированныеЗначения = ПакетнаяОбработкаТабличнойЧастиКлиентСервер.ПолучитьСтруктуруКэшируемыеЗначения();
				ПакетнаяОбработкаТабличнойЧастиСервер.ОбработатьТЧ(ПриобретениеТоваровУслуг.Товары, 
					СтруктураДействий, 
					КэшированныеЗначения);
					
				ОбъектыУчета.Добавить(ПриобретениеТоваровУслуг);
			КонецЕсли;
			
		ИначеЕсли ЗначениеЗаполнено(СопоставленнаяНоменклатура)
					И ЗначениеЗаполнено(ВидТорговойПлощадки) Тогда
			ДанныеРазбора = ПолучитьДанныеЭлектронногоДокумента(ДанныеЭлектронногоДокумента, ТекстОшибки);
			
			Если Не ЗначениеЗаполнено(ДанныеРазбора) Тогда
				ЗаписьЖурналаРегистрации(СобытиеЖурналаРегистрации(ДанныеЭлектронногоДокумента.ДополнительныеДанные.СпособОбработки), 
					УровеньЖурналаРегистрации.Ошибка,,, ТекстОшибки);
					
				ОценкаПроизводительности.ЗакончитьЗамерДлительнойОперации(Замер, 0);
				Возврат Неопределено;
			КонецЕсли;
			
			СтрокиТребующиеСопоставления = СтрокиТребующиеСопоставления(ДанныеРазбора.НовыйЭД, Ложь, ТекстОшибки);
			Если СтрокиТребующиеСопоставления = Неопределено Тогда
				ЗаписьЖурналаРегистрации(СобытиеЖурналаРегистрации(ДанныеЭлектронногоДокумента.ДополнительныеДанные.СпособОбработки), 
					УровеньЖурналаРегистрации.Ошибка,,, ТекстОшибки);
					
				ОценкаПроизводительности.ЗакончитьЗамерДлительнойОперации(Замер, 0);
				Возврат Неопределено;
			КонецЕсли;
			
			Для Каждого КлючИЗначение Из СтрокиТребующиеСопоставления Цикл
				Идентификатор   = "";
				КодНоменклатуры = "";
				Штрихкод        = "";
				
				ОпределитьКлючевыеРеквизитыСопоставления(ВидТорговойПлощадки, КлючИЗначение.Ключ, 
					Идентификатор, КодНоменклатуры, Штрихкод);
				
				ЭлементСопоставления = Неопределено;
				
				Если ЗначениеЗаполнено(Идентификатор) Тогда
					КлючИЗначение.Значение.Вставить("Идентификатор", Идентификатор);
					
					ПутьКСопоставлению = КлючИЗначение.Ключ.ПолныйПуть + ".Сопоставление";
					ЭлектронноеВзаимодействие.ЗаполнитьЗначениеРеквизитаВДереве(КлючИЗначение.Ключ, 
						ПутьКСопоставлению + ".Идентификатор", Идентификатор);
					
					Если Не ЗначениеЗаполнено(ЭлементСопоставления) Тогда
						ЭлементСопоставления = СопоставленнаяНоменклатура.Получить("Идентификатор_" + Идентификатор);
					КонецЕсли;
				КонецЕсли;
				
				Если ЗначениеЗаполнено(КодНоменклатуры) Тогда
					КлючИЗначение.Значение.Вставить("КодНоменклатуры", КодНоменклатуры);
					
					Если ДанныеЭлектронногоДокумента.Формат = ПоддерживаемыеФорматы.ФНС.УПД.ИнформацияПродавца
							Или ДанныеЭлектронногоДокумента.Формат = ПоддерживаемыеФорматы.ФНС.УПД2019.ИнформацияПродавца Тогда
						ЭлектронноеВзаимодействие.ЗаполнитьЗначениеРеквизитаВДереве(КлючИЗначение.Ключ, 
							КлючИЗначение.Ключ.ПолныйПуть + ".ТоварКод", КодНоменклатуры);
					ИначеЕсли ДанныеЭлектронногоДокумента.Формат = ПоддерживаемыеФорматы.ФНС.УПД_5_02.ИнформацияПродавца Тогда
						ЭлектронноеВзаимодействие.ЗаполнитьЗначениеРеквизитаВДереве(КлючИЗначение.Ключ, 
							КлючИЗначение.Ключ.ПолныйПуть + ".Код", КодНоменклатуры);
					КонецЕсли;
					
					Если Не ЗначениеЗаполнено(ЭлементСопоставления) Тогда
						ЭлементСопоставления = СопоставленнаяНоменклатура.Получить("КодНоменклатуры_" + КодНоменклатуры);
					КонецЕсли;
				КонецЕсли;
				
				Если ЗначениеЗаполнено(Штрихкод)
						И СтроковыеФункцииКлиентСервер.ТолькоЦифрыВСтроке(Штрихкод) Тогда
					КлючИЗначение.Значение.Вставить("Штрихкод", Штрихкод);
					
					ПутьКСопоставлению = КлючИЗначение.Ключ.ПолныйПуть + ".Сопоставление";
					ЭлектронноеВзаимодействие.ЗаполнитьЗначениеРеквизитаВДереве(КлючИЗначение.Ключ, 
						ПутьКСопоставлению + ".Штрихкод", Штрихкод);
					
					Если Не ЗначениеЗаполнено(ЭлементСопоставления) Тогда
						ЭлементСопоставления = СопоставленнаяНоменклатура.Получить("Штрихкод_" + Штрихкод);
					КонецЕсли;
				КонецЕсли;
				
				Если ЗначениеЗаполнено(ЭлементСопоставления) Тогда
					ПутьКСопоставлению = КлючИЗначение.Ключ.ПолныйПуть + ".Сопоставление";
					
					Если Не ЭлектронноеВзаимодействие.СуществуетРеквизитВДереве(КлючИЗначение.Ключ, 
							ПутьКСопоставлению + ".ВариантУказанияНоменклатурыИБ") Тогда
						ДеревоЭлектронногоДокументаБЭД.ДобавитьЗначениеВДерево(
							КлючИЗначение.Ключ.Строки.Найти(ПутьКСопоставлению, "ПолныйПуть", Истина), 
							ПутьКСопоставлению + ".ВариантУказанияНоменклатурыИБ", 
							ЭлементСопоставления.НоменклатураКонтрагента.ВариантУказанияНоменклатуры);
					Иначе
						ЭлектронноеВзаимодействие.ЗаполнитьЗначениеРеквизитаВДереве(КлючИЗначение.Ключ, 
							ПутьКСопоставлению + ".ВариантУказанияНоменклатурыИБ", 
							ЭлементСопоставления.НоменклатураКонтрагента.ВариантУказанияНоменклатуры);
					КонецЕсли;
					КлючИЗначение.Значение.Вставить("ВариантУказанияНоменклатурыИБ", 
						ЭлементСопоставления.НоменклатураКонтрагента.ВариантУказанияНоменклатуры);
					
					ЭлектронноеВзаимодействие.ЗаполнитьЗначениеРеквизитаВДереве(КлючИЗначение.Ключ, 
						ПутьКСопоставлению + ".НоменклатураИБ", ЭлементСопоставления.НоменклатураИБ.Номенклатура);
					КлючИЗначение.Значение.Вставить("НоменклатураИБ", 
						ЭлементСопоставления.НоменклатураИБ.Номенклатура);
					
					ЭлектронноеВзаимодействие.ЗаполнитьЗначениеРеквизитаВДереве(КлючИЗначение.Ключ, 
						ПутьКСопоставлению + ".ХарактеристикаИБ", ЭлементСопоставления.НоменклатураИБ.Характеристика);
					КлючИЗначение.Значение.Вставить("ХарактеристикаИБ", 
						ЭлементСопоставления.НоменклатураИБ.Характеристика);
					
					ЭлектронноеВзаимодействие.ЗаполнитьЗначениеРеквизитаВДереве(КлючИЗначение.Ключ, 
						ПутьКСопоставлению + ".УпаковкаИБ", ЭлементСопоставления.НоменклатураИБ.Упаковка);
					КлючИЗначение.Значение.Вставить("УпаковкаИБ", 
						ЭлементСопоставления.НоменклатураИБ.Упаковка);
				КонецЕсли;
			КонецЦикла;
		КонецЕсли;
	КонецЕсли;
	
	Если Не ЗначениеЗаполнено(ОбъектыУчета) Тогда
		ОтражениеВУчетеЭДО.ОтразитьДанныеЭлектронногоДокумента(ДанныеЭлектронногоДокумента, 
			СпособОбработки, 
			ОбъектыУчета, 
			ДанныеРазбора);
	КонецЕсли;
	
	// Соответствие штрихкодов упаковок
	Если Не ЭтоПрикладнойТипЭлектронногоДокумента Тогда
		Если ДанныеЭлектронногоДокумента.Формат = ПоддерживаемыеФорматы.ФНС.УПД.ИнформацияПродавца
				Или ДанныеЭлектронногоДокумента.Формат = ПоддерживаемыеФорматы.ФНС.УПД2019.ИнформацияПродавца Тогда
			СведенияОТоварах = ДанныеРазбора.НовыйЭД.ЗначениеРеквизита.Строки.Найти("СведенияОТоварах", 
				"ПолныйПуть", Истина);
			
			ПутьКСопоставлению         = "СведенияОТоварах.НомерСтроки.Сопоставление";
			ПутьСведенияОМаркировке    = "СведенияОТоварах.НомерСтроки.СведенияОМаркировке";
			ПутьКонтрольныеЗнаки       = ПутьСведенияОМаркировке + ".КонтрольныеИдентификационныеЗнаки";
			ПутьТранспортныеУпаковки   = ПутьСведенияОМаркировке + ".ТранспортныеУпаковки";
			ПутьИндивидуальныеУпаковки = ПутьСведенияОМаркировке + ".ИндивидуальныеУпаковки";
			
			Для Каждого СведенияОТоваре Из СведенияОТоварах.Строки Цикл
				Штрихкоды = ЭлектронноеВзаимодействиеИСМП.НоваяТаблицаШтрихкодыУпаковок(Истина);
				Штрихкоды.Колонки.Добавить("ВидУпаковки", Новый ОписаниеТипов("ПеречислениеСсылка.ВидыУпаковокИС"));
				
				Номенклатура = ЭлектронноеВзаимодействие.ЗначениеРеквизитаВДереве(СведенияОТоваре,
					ПутьКСопоставлению + ".НоменклатураИБ");
				Характеристика = ЭлектронноеВзаимодействие.ЗначениеРеквизитаВДереве(СведенияОТоваре,
					ПутьКСопоставлению + ".ХарактеристикаИБ");
				
				ДанныеМаркировки = СведенияОТоваре.Строки.Найти(ПутьСведенияОМаркировке, "ПолныйПуть", Истина);
				
				Если ДанныеМаркировки <> Неопределено И ЗначениеЗаполнено(ДанныеМаркировки.Значение) Тогда
					КонтрольныеИдентификационныеЗнаки = ДанныеМаркировки.Строки.Найти(ПутьКонтрольныеЗнаки, "ПолныйПуть", Истина);
					Если КонтрольныеИдентификационныеЗнаки <> Неопределено Тогда
						Для Каждого СтрокаДереваЗначений Из КонтрольныеИдентификационныеЗнаки.Строки Цикл
							ЗначениеШтрихкода = ЭлектронноеВзаимодействие.ЗначениеРеквизитаВДереве(СтрокаДереваЗначений, 
								ПутьКонтрольныеЗнаки + ".НомерСтроки.Код");
							ДобавитьШтрихкодУпаковкиИзСтрокиДерева(Штрихкоды, ЗначениеШтрихкода, 
								Номенклатура, Характеристика, Перечисления.ВидыУпаковокИС.Потребительская);
						КонецЦикла;
					КонецЕсли;
					
					ТранспортныеУпаковки = ДанныеМаркировки.Строки.Найти(ПутьТранспортныеУпаковки, "ПолныйПуть", Истина);
					Если ТранспортныеУпаковки <> Неопределено Тогда
						Для Каждого СтрокаДереваЗначений Из ТранспортныеУпаковки.Строки Цикл
							ЗначениеШтрихкода = ЭлектронноеВзаимодействие.ЗначениеРеквизитаВДереве(СтрокаДереваЗначений,
								ПутьТранспортныеУпаковки + ".НомерСтроки.Код");
							ДобавитьШтрихкодУпаковкиИзСтрокиДерева(Штрихкоды, ЗначениеШтрихкода, 
								Номенклатура, Характеристика, Перечисления.ВидыУпаковокИС.Логистическая);
						КонецЦикла;
					КонецЕсли;
					
					ИндивидуальныеУпаковки = ДанныеМаркировки.Строки.Найти(ПутьИндивидуальныеУпаковки, "ПолныйПуть", Истина);
					Если ИндивидуальныеУпаковки <> Неопределено Тогда
						Для Каждого СтрокаДереваЗначений Из ИндивидуальныеУпаковки.Строки Цикл
							ЗначениеШтрихкода = ЭлектронноеВзаимодействие.ЗначениеРеквизитаВДереве(СтрокаДереваЗначений,
								ПутьИндивидуальныеУпаковки + ".НомерСтроки.Код");
							ДобавитьШтрихкодУпаковкиИзСтрокиДерева(Штрихкоды, ЗначениеШтрихкода, 
								Номенклатура, Характеристика, Перечисления.ВидыУпаковокИС.Групповая);
						КонецЦикла;
					КонецЕсли;
				КонецЕсли;
				
				ОбщегоНазначенияКлиентСервер.ДополнитьТаблицу(Штрихкоды, ШтрихкодыУпаковок);
			КонецЦикла;
			
		ИначеЕсли ДанныеЭлектронногоДокумента.Формат = ПоддерживаемыеФорматы.ФНС.УПД_5_02.ИнформацияПродавца Тогда
			СведенияОТоварах = ДанныеРазбора.НовыйЭД.ЗначениеРеквизита.Строки.Найти("ТаблицаСчетаФактуры.СведенияОбОтгруженныхПозициях", 
				"ПолныйПуть", Истина);
			
			ПутьКСопоставлению         = "ТаблицаСчетаФактуры.СведенияОбОтгруженныхПозициях.НомерСтроки.Сопоставление";
			ПутьСведенияОМаркировке    = "ТаблицаСчетаФактуры.СведенияОбОтгруженныхПозициях.НомерСтроки.СведенияОМаркировке";
			ПутьКонтрольныеЗнаки       = ПутьСведенияОМаркировке + ".НомерСтроки.Идентификатор.КонтрольныеЗнаки";
			ПутьТранспортнаяУпаковка   = ПутьСведенияОМаркировке + ".НомерСтроки.ИдентификаторУпаковки";
			ПутьИндивидуальныеУпаковки = ПутьСведенияОМаркировке + ".НомерСтроки.Идентификатор.ИдентификаторыУпаковок";
			ПутьКоличествоОСУ          = ПутьСведенияОМаркировке + ".НомерСтроки.Количество";
			ПутьГТИН                   = "ТаблицаСчетаФактуры.СведенияОбОтгруженныхПозициях.НомерСтроки.ГТИН";
			
			Для Каждого СведенияОТоваре Из СведенияОТоварах.Строки Цикл
				Штрихкоды = ЭлектронноеВзаимодействиеИСМП.НоваяТаблицаШтрихкодыУпаковок(Истина);
				Штрихкоды.Колонки.Добавить("ВидУпаковки", Новый ОписаниеТипов("ПеречислениеСсылка.ВидыУпаковокИС"));
				
				Номенклатура = ЭлектронноеВзаимодействие.ЗначениеРеквизитаВДереве(СведенияОТоваре,
					ПутьКСопоставлению + ".НоменклатураИБ");
				Характеристика = ЭлектронноеВзаимодействие.ЗначениеРеквизитаВДереве(СведенияОТоваре,
					ПутьКСопоставлению + ".ХарактеристикаИБ");
				
				ДанныеМаркировки = СведенияОТоваре.Строки.Найти(ПутьСведенияОМаркировке, "ПолныйПуть", Истина);
				ДанныеГТИН       = СведенияОТоваре.Строки.Найти(ПутьГТИН, "ПолныйПуть", Истина);
				
				Если ДанныеМаркировки <> Неопределено И ЗначениеЗаполнено(ДанныеМаркировки.Значение) Тогда
					Для Каждого СтрокаМаркировки Из ДанныеМаркировки.Строки Цикл
						КонтрольныеИдентификационныеЗнаки = СтрокаМаркировки.Строки.Найти(ПутьКонтрольныеЗнаки, "ПолныйПуть", Истина);
						Если КонтрольныеИдентификационныеЗнаки <> Неопределено Тогда
							Для Каждого СтрокаДереваЗначений Из КонтрольныеИдентификационныеЗнаки.Строки Цикл
								ЗначениеШтрихкода = ЭлектронноеВзаимодействие.ЗначениеРеквизитаВДереве(СтрокаДереваЗначений, 
									ПутьКонтрольныеЗнаки + ".НомерСтроки.КонтрольныйЗнак");
								ДобавитьШтрихкодУпаковкиИзСтрокиДерева(Штрихкоды, ЗначениеШтрихкода, 
									Номенклатура, Характеристика, Перечисления.ВидыУпаковокИС.Потребительская);
							КонецЦикла;
						КонецЕсли;
						
						ТранспортнаяУпаковка = ЭлектронноеВзаимодействие.ЗначениеРеквизитаВДереве(СтрокаМаркировки,
							ПутьТранспортнаяУпаковка, Ложь);
						Если ТранспортнаяУпаковка <> Неопределено Тогда
							ДобавитьШтрихкодУпаковкиИзСтрокиДерева(Штрихкоды, ТранспортнаяУпаковка, 
								Номенклатура, Характеристика, Перечисления.ВидыУпаковокИС.Логистическая);
						КонецЕсли;
						
						ИндивидуальныеУпаковки = СтрокаМаркировки.Строки.Найти(ПутьИндивидуальныеУпаковки, "ПолныйПуть", Истина);
						Если ИндивидуальныеУпаковки <> Неопределено Тогда
							Для Каждого СтрокаДереваЗначений Из ИндивидуальныеУпаковки.Строки Цикл
								ЗначениеШтрихкода = ЭлектронноеВзаимодействие.ЗначениеРеквизитаВДереве(СтрокаДереваЗначений,
									ПутьИндивидуальныеУпаковки + ".НомерСтроки.ИдентификаторУпаковки");
								ДобавитьШтрихкодУпаковкиИзСтрокиДерева(Штрихкоды, ЗначениеШтрихкода, 
									Номенклатура, Характеристика, Перечисления.ВидыУпаковокИС.Групповая);
							КонецЦикла;
						КонецЕсли; 
						
						КоличествоПоОСУ = ЭлектронноеВзаимодействие.ЗначениеРеквизитаВДереве(СтрокаМаркировки,
							ПутьКоличествоОСУ, Ложь);
						Если Не КоличествоПоОСУ = Неопределено 
								И ЗначениеЗаполнено(КоличествоПоОСУ) 
								И Не ДанныеГТИН = Неопределено Тогда
							ЗначениеШтрихкода = ЭлектронноеВзаимодействиеИСМП.СформироватьКодОСУПоКоличествуИGTIN(ДанныеГТИН, КоличествоПоОСУ);
							ДобавитьШтрихкодУпаковкиИзСтрокиДерева(Штрихкоды, ЗначениеШтрихкода, 
								Номенклатура, Характеристика, Перечисления.ВидыУпаковокИС.ОбъемноСортовойУчет);
						КонецЕсли;
					КонецЦикла;
				КонецЕсли;
				
				ОбщегоНазначенияКлиентСервер.ДополнитьТаблицу(Штрихкоды, ШтрихкодыУпаковок);
			КонецЦикла;
		КонецЕсли;
	КонецЕсли;
	
	Если Не ЗначениеЗаполнено(ОбъектыУчета) Тогда
		ОценкаПроизводительности.ЗакончитьЗамерДлительнойОперации(Замер, 0);
		Возврат Неопределено;
	КонецЕсли;
	
	ОбъектУчета   = ОбъектыУчета[0];	// ДокументОбъект.ПередачаТоваровХранителю
	ТоварыИзФайла = ОбъектУчета.Товары.Выгрузить();
	
	ШтрихкодыУпаковокИзФайла = ШтрихкодированиеИС.ДанныеШтрихкодовУпаковок(ШтрихкодыУпаковок.ВыгрузитьКолонку("ЗначениеШтрихкода"),, Истина);
	ШтрихкодыУпаковокИзФайла.Колонки.Штрихкод.Имя = "ЗначениеШтрихкода";
	ШтрихкодыУпаковокИзФайла.Колонки.ШтрихкодУпаковки.Имя = "Ссылка";
	ШтрихкодыУпаковокИзФайла.Колонки.Добавить("Обработан",   Новый ОписаниеТипов("Булево"));
	ШтрихкодыУпаковокИзФайла.Колонки.Добавить("ВидУпаковки", Новый ОписаниеТипов("ПеречислениеСсылка.ВидыУпаковокИС"));
	
	Для Каждого СтрокаТаблицыЗначений Из ШтрихкодыУпаковок Цикл
		Запись = ШтрихкодыУпаковокИзФайла.Найти(СтрокаТаблицыЗначений.ЗначениеШтрихкода);
		Если Запись = Неопределено Тогда
			Запись = ШтрихкодыУпаковокИзФайла.Добавить();
			ЗаполнитьЗначенияСвойств(Запись, СтрокаТаблицыЗначений);
			Запись.Количество = 1;
		Иначе
			ЗаполнитьЗначенияСвойств(Запись, СтрокаТаблицыЗначений, "Номенклатура, Характеристика, ВидУпаковки");
		КонецЕсли;
	КонецЦикла;
	
	ТоварыИзДокумента            = Новый ТаблицаЗначений;
	ШтрихкодыУпаковокИзДокумента = Новый ТаблицаЗначений;
	ПолучитьТабличныеЧастиИзДокумента(ПервичныйДокумент, ТоварыИзДокумента, ШтрихкодыУпаковокИзДокумента);
	
	ОценкаПроизводительности.ЗафиксироватьЗамерДлительнойОперации(Замер, 
		ТоварыИзФайла.Количество() / 100,
		"ЧтениеЭлектронногоДокумента");
	
	// Анализ расхождений
	Расхождения = АнализРасхожденийДокументаИТорговойПлощадки(ТоварыИзФайла, 
		ШтрихкодыУпаковокИзФайла, 
		ТоварыИзДокумента, 
		ШтрихкодыУпаковокИзДокумента);
	ЗаполнитьСлужебныеРеквизитыПоНоменклатуре(ПервичныйДокумент, Расхождения.ТоварыРасхождения);
	
	ОценкаПроизводительности.ЗакончитьЗамерДлительнойОперации(Замер, 
		ТоварыИзФайла.Количество() / 100);
	
	Результат = Новый Структура;
	Результат.Вставить("ДанныеРазбора",                ДанныеРазбора);
	Результат.Вставить("ТоварыИзФайла",                ТоварыИзФайла);
	Результат.Вставить("ШтрихкодыУпаковокИзФайла",     ШтрихкодыУпаковокИзФайла);
	Результат.Вставить("ТоварыИзДокумента",            ТоварыИзДокумента);
	Результат.Вставить("ШтрихкодыУпаковокИзДокумента", ШтрихкодыУпаковокИзДокумента);
	Результат.Вставить("ТоварыРасхождения",            Расхождения.ТоварыРасхождения);
	Результат.Вставить("ШтрихкодыУпаковокРасхождения", Расхождения.ШтрихкодыУпаковокРасхождения);
	
	Возврат Результат;
	
КонецФункции

// Получает преобразованные табличные части первичного документа для анализа расхождений.
//
// Параметры:
//   ПервичныйДокумент            - ДокументСсылка.ПередачаТоваровХранителю - первичный документ.
//   ТоварыИзДокумента            - ТаблицаЗначений - см. Документ.ПередачаТоваровХранителю.Товары.
//   ШтрихкодыУпаковокИзДокумента - см. ЭлектронноеВзаимодействиеИСМП.ЧастичноеСодержимоеИКодыОСУ.
//
Процедура ПолучитьТабличныеЧастиИзДокумента(Знач ПервичныйДокумент, ТоварыИзДокумента, ШтрихкодыУпаковокИзДокумента) Экспорт
	
	РеквизитыДокумента = ОбщегоНазначения.ЗначенияРеквизитовОбъекта(ПервичныйДокумент, "Товары, ШтрихкодыУпаковок");
	ТоварыИзДокумента  = РеквизитыДокумента.Товары.Выгрузить();
	
	ШтрихкодыУпаковок = РеквизитыДокумента.ШтрихкодыУпаковок.Выгрузить();
	ШтрихкодыУпаковок.Колонки.ШтрихкодУпаковки.Имя = "Штрихкод";
	
	ТаблицаТоварыДляЭДО = ИнтеграцияИСМПУТ.ТаблицаТоварыДляЭДО(ТоварыИзДокумента, "Количество");
	Если ТаблицаТоварыДляЭДО.Количество() Тогда
		ПараметрыСканирования = ШтрихкодированиеОбщегоНазначенияИС.ПараметрыСканирования(ПервичныйДокумент);
	КонецЕсли;
	ШтрихкодыУпаковокИзДокумента = ЭлектронноеВзаимодействиеИСМП.ЧастичноеСодержимоеИКодыОСУ(ШтрихкодыУпаковок,
		ТаблицаТоварыДляЭДО,
		ПараметрыСканирования);
	ШтрихкодыУпаковокИзДокумента.Колонки.Удалить("Ссылка");
	ШтрихкодыУпаковокИзДокумента.Колонки.Штрихкод.Имя = "Ссылка";
	
КонецПроцедуры

// Заполняет служебные реквизиты табличной части расхождений по товарам.
//
// Параметры:
//   ПервичныйДокумент - ДокументСсылка.ПередачаТоваровХранителю - первичный документ.
//   ТоварыРасхождения - ТаблицаЗначений - с дополнениями для Документ.ПередачаТоваровХранителю.Товары.
//
Процедура ЗаполнитьСлужебныеРеквизитыПоНоменклатуре(Знач ПервичныйДокумент, ТоварыРасхождения) Экспорт
	
	СтруктураДействий = Новый Структура;
	
	Если ТипЗнч(ПервичныйДокумент) = Тип("ДокументСсылка.ПередачаТоваровХранителю") Тогда
		РеквизитыДокумента = ОбщегоНазначения.ЗначенияРеквизитовОбъекта(ПервичныйДокумент, "Склад, ХозяйственнаяОперация");
		Обработчик         = Документы.ПередачаТоваровХранителю.ОбработчикДействий(РеквизитыДокумента.ХозяйственнаяОперация);
		СкладГруппа        = Обработчик.ЭтоГруппаСкладовИСкладыИспользуютсяВТЧДокументовПродажи(РеквизитыДокумента.Склад);
		
		ПараметрыУказанияСерий = НоменклатураСервер.ПараметрыУказанияСерий(ПервичныйДокумент, 
			Документы.ПередачаТоваровХранителю);
		
		ПроверитьСериюРассчитатьСтатус = Новый Структура;
		ПроверитьСериюРассчитатьСтатус.Вставить("Склад",                  РеквизитыДокумента.Склад);
		ПроверитьСериюРассчитатьСтатус.Вставить("ПараметрыУказанияСерий", ПараметрыУказанияСерий);
		
		ПроверитьЗаполнитьСклад = ПакетнаяОбработкаТабличнойЧастиКлиентСервер.ПолучитьСтруктуруЗаполненияСкладаВСтрокеТЧ(
			ПервичныйДокумент,
			СкладГруппа);
		
		СтруктураДействий.Вставить("ПроверитьСериюРассчитатьСтатус", ПроверитьСериюРассчитатьСтатус);
		СтруктураДействий.Вставить("ПроверитьЗаполнитьСклад",        ПроверитьЗаполнитьСклад);
		
	Иначе
		ПараметрыУказанияСерий = НоменклатураСервер.ПараметрыУказанияСерий(ПервичныйДокумент,
			Документы.АктОРасхожденияхПослеОтгрузки);
		НоменклатураСервер.ЗаполнитьСтатусыУказанияСерий(ПервичныйДокумент, ПараметрыУказанияСерий);
	КонецЕсли;
	
	СтруктураДействий.Вставить("ЗаполнитьПризнакТипНоменклатуры", 
		Новый Структура("Номенклатура", "ТипНоменклатуры"));
	СтруктураДействий.Вставить("ЗаполнитьПризнакАртикул", 
		Новый Структура("Номенклатура", "Артикул"));
	СтруктураДействий.Вставить("ЗаполнитьПризнакХарактеристикиИспользуются", 
		Новый Структура("Номенклатура", "ХарактеристикиИспользуются"));
	
	КэшированныеЗначения = ПакетнаяОбработкаТабличнойЧастиКлиентСервер.ПолучитьСтруктуруКэшируемыеЗначения();
	ПакетнаяОбработкаТабличнойЧастиСервер.ОбработатьТЧ(ТоварыРасхождения, СтруктураДействий, КэшированныеЗначения);
	
КонецПроцедуры

// Определяет расхождения между двумя документами:
//   - по таблице товаров для Документ.ПередачаТоваровХранителю.Товары;
//   - по таблице штрихкодов для ЭлектронноеВзаимодействиеИСМП.ЧастичноеСодержимоеИКодыОСУ.
//
// Параметры:
//   ТоварыИзФайла                - ТаблицаЗначений из см. Документ.ПриобретениеТоваровУслуг.Товары.
//   ШтрихкодыУпаковокИзФайла     - см. ШтрихкодированиеИС.ШтрихкодыУпаковок.
//   ТоварыИзДокумента            - ТаблицаЗначений из см. Документ.ПередачаТоваровХранителю.Товары.
//   ШтрихкодыУпаковокИзДокумента - см. ЭлектронноеВзаимодействиеИСМП.ЧастичноеСодержимоеИКодыОСУ.
//
// Возвращаемое значение:
//   Структура - данные по расхождениям:
//     * ТоварыРасхождения            - см. НоваяТаблицаТоварыРасхождения.
//     * ШтрихкодыУпаковокРасхождения - см. НоваяТаблицаШтрихкодыУпаковокРасхождения.
//
Функция АнализРасхожденийДокументаИТорговойПлощадки(Знач ТоварыИзФайла, Знач ШтрихкодыУпаковокИзФайла, 
			Знач ТоварыИзДокумента, Знач ШтрихкодыУпаковокИзДокумента) Экспорт
			
	ШтрихкодыУпаковокИзФайла.Индексы.Добавить("ЗначениеШтрихкода");
	ШтрихкодыУпаковокИзДокумента.Индексы.Добавить("ЗначениеШтрихкода");
	
	// Расхождения по номенклатуре
	ТоварыРасхождения = НоваяТаблицаТоварыРасхождения(ТоварыИзДокумента);
	
	ЕстьКолонкаСумма = ЗначениеЗаполнено(ТоварыИзФайла.Итог("Сумма"));
	
	ВариантыРаспределения = Новый Массив;
	Если ЕстьКолонкаСумма Тогда
		ВариантыРаспределения.Добавить("ПоУпаковкеИСумме");
	КонецЕсли;
	ВариантыРаспределения.Добавить("ПоУпаковке");
	ВариантыРаспределения.Добавить("ПоНоменклатуре");
	
	Для Каждого СтрокаТаблицыЗначений Из ТоварыИзФайла Цикл // СтрокаТаблицыЗначений
		Количество         = СтрокаТаблицыЗначений.Количество;
		КоличествоУпаковок = СтрокаТаблицыЗначений.КоличествоУпаковок;
		Коэффициент        = ?(Количество = 0, 1, Количество / КоличествоУпаковок);
		
		Отбор = Новый Структура;
		Отбор.Вставить("Номенклатура",   СтрокаТаблицыЗначений.Номенклатура);
		Отбор.Вставить("Характеристика", СтрокаТаблицыЗначений.Характеристика);
		
		НайденныеСтроки = ТоварыРасхождения.НайтиСтроки(Отбор);
		Для Каждого ЭлементКоллекции Из НайденныеСтроки Цикл
			Если Количество <= 0 Тогда
				Прервать;
			КонецЕсли;
			
			КоэффициентПоДокументу = ?(ЭлементКоллекции.КоличествоПоДокументу = 0, 1, 
				ЭлементКоллекции.КоличествоПоДокументу / ЭлементКоллекции.КоличествоУпаковокПоДокументу);
			
			// Распределим по указанному варианту с учетом количества в документе
			Для Каждого Вариант Из ВариантыРаспределения Цикл
				Если Количество <= 0 Тогда
					Прервать;
				КонецЕсли;
				
				Если Вариант = "ПоУпаковкеИСумме"
							И СтрокаТаблицыЗначений.Упаковка = ЭлементКоллекции.Упаковка
							И СтрокаТаблицыЗначений.Цена = ЭлементКоллекции.Цена
						Или Вариант = "ПоУпаковке"
							И СтрокаТаблицыЗначений.Упаковка = ЭлементКоллекции.Упаковка
						Или Вариант = "ПоНоменклатуре" Тогда
					КоличествоКРаспределению = Мин(Количество, 
						ЭлементКоллекции.КоличествоПоДокументу - ЭлементКоллекции.Количество);
					
					Если КоличествоКРаспределению > 0 Тогда
						Количество               = Количество - КоличествоКРаспределению;
						КоличествоУпаковок       = КоличествоУпаковок - Окр(КоличествоКРаспределению 
							/ ?(Коэффициент = 0, 1, Коэффициент), 3, РежимОкругления.Окр15как20);
							
						ЭлементКоллекции.Количество         = ЭлементКоллекции.Количество + КоличествоКРаспределению;
						ЭлементКоллекции.КоличествоУпаковок = Окр(ЭлементКоллекции.Количество 
							/ ?(КоэффициентПоДокументу = 0, 1, КоэффициентПоДокументу), 3, РежимОкругления.Окр15как20);
						ЭлементКоллекции.Сумма              = ЭлементКоллекции.КоличествоУпаковок * ЭлементКоллекции.Цена;
					КонецЕсли;
				КонецЕсли;
			КонецЦикла;
			
			// Распределим остаток по первому варианту как излишек
			Если Количество > 0 Тогда
				Вариант = ВариантыРаспределения[0];
				
				Если Вариант = "ПоУпаковкеИСумме"
							И СтрокаТаблицыЗначений.Упаковка = ЭлементКоллекции.Упаковка
							И СтрокаТаблицыЗначений.Цена = ЭлементКоллекции.Цена
						Или Вариант = "ПоУпаковке"
							И СтрокаТаблицыЗначений.Упаковка = ЭлементКоллекции.Упаковка Тогда
					КоличествоКРаспределению = Количество;
					
					Если КоличествоКРаспределению > 0 Тогда
						Количество               = Количество - КоличествоКРаспределению;
						КоличествоУпаковок       = КоличествоУпаковок - Окр(КоличествоКРаспределению 
							/ ?(Коэффициент = 0, 1, Коэффициент), 3, РежимОкругления.Окр15как20);
							
						ЭлементКоллекции.Количество         = ЭлементКоллекции.Количество + КоличествоКРаспределению;
						ЭлементКоллекции.КоличествоУпаковок = Окр(ЭлементКоллекции.Количество 
							/ ?(КоэффициентПоДокументу = 0, 1, КоэффициентПоДокументу), 3, РежимОкругления.Окр15как20);
						ЭлементКоллекции.Сумма              = ЭлементКоллекции.КоличествоУпаковок * ЭлементКоллекции.Цена;
					КонецЕсли;
				КонецЕсли;
			КонецЕсли;
		КонецЦикла;
		
		// Остаток - как новая строка
		Если Количество > 0 Тогда
			Если НайденныеСтроки.Количество() > 0 Тогда
				Запись = ТоварыРасхождения.Вставить(
					ТоварыРасхождения.Индекс(НайденныеСтроки[НайденныеСтроки.ВГраница()]) + 1);
			Иначе
				Запись = ТоварыРасхождения.Добавить();
			КонецЕсли;
			
			ЗаполнитьЗначенияСвойств(Запись, СтрокаТаблицыЗначений, "Номенклатура, Характеристика, Упаковка, Цена");
			
			Запись.Количество                    = Количество;
			Запись.КоличествоУпаковок            = КоличествоУпаковок;
			Запись.Сумма                         = Запись.КоличествоУпаковок * Запись.Цена;
			Запись.КоличествоПоДокументу         = 0;
			Запись.КоличествоУпаковокПоДокументу = 0;
			Запись.СуммаПоДокументу              = 0;
		КонецЕсли;
	КонецЦикла;
	
	МассивНоменклатуры    = ТоварыРасхождения.ВыгрузитьКолонку("Номенклатура");	// Массив из СправочникСсылка.Номенклатура
	РеквизитыНоменклатуры = ОбщегоНазначения.ЗначениеРеквизитаОбъектов(МассивНоменклатуры, "ЕдиницаИзмерения");
		
	// Расхождения по кодам маркировки
	ШтрихкодыУпаковокРасхождения = НоваяТаблицаШтрихкодыУпаковокРасхождения(ШтрихкодыУпаковокИзДокумента);
	
	ЕстьШтрихкодыУпаковокИзФайла = ЗначениеЗаполнено(ШтрихкодыУпаковокИзФайла);
	Если Не ЕстьШтрихкодыУпаковокИзФайла Тогда
		ШтрихкодыУпаковокРасхождения.ЗаполнитьЗначения("КодовМаркировкиНетВФайле",            "Статус");
		ШтрихкодыУпаковокРасхождения.ЗаполнитьЗначения(НСтр("ru = '(в файле нет сведений)';
															|en = '(there are no information records in the file)'"), "ТекстСообщения");
		
		Для Каждого СтрокаТаблицыЗначений Из ШтрихкодыУпаковокРасхождения Цикл
			СтрокаТаблицыЗначений.Количество                        = СтрокаТаблицыЗначений.КоличествоПоДокументу;
			СтрокаТаблицыЗначений.КоличествоПотребительскихУпаковок = СтрокаТаблицыЗначений.КоличествоПотребительскихУпаковокПоДокументу;
		КонецЦикла;
		
	Иначе
		ШтрихкодыУпаковокРасхождения.ЗаполнитьЗначения("НедостачаКодовМаркировки", "Статус");
		
		Для Каждого СтрокаТаблицыЗначений Из ШтрихкодыУпаковокИзФайла Цикл
			Количество                        = СтрокаТаблицыЗначений.Количество;
			КоличествоПотребительскихУпаковок = ?(Не ЗначениеЗаполнено(СтрокаТаблицыЗначений.КоличествоПотребительскихУпаковок),
				1, СтрокаТаблицыЗначений.КоличествоПотребительскихУпаковок);
			Коэффициент                       = ?(Количество = 0, 1, КоличествоПотребительскихУпаковок / Количество);
			
			Отбор = Новый Структура;
			Отбор.Вставить("ЗначениеШтрихкода", СтрокаТаблицыЗначений.ЗначениеШтрихкода);
			
			НайденныеСтроки = ШтрихкодыУпаковокРасхождения.НайтиСтроки(Отбор);
			Для Каждого ЭлементКоллекции Из НайденныеСтроки Цикл
				Если Количество <= 0 Тогда
					Прервать;
				КонецЕсли;
				
				КоличествоКРаспределению = Мин(Количество, 
					ЭлементКоллекции.КоличествоПоДокументу - ЭлементКоллекции.Количество);
				
				Если КоличествоКРаспределению > 0 Тогда
					Количество                        = Количество - КоличествоКРаспределению;
					КоличествоПотребительскихУпаковок = КоличествоПотребительскихУпаковок - Окр(КоличествоКРаспределению 
						/ ?(Коэффициент = 0, 1, Коэффициент), 3, РежимОкругления.Окр15как20);
						
					КоэффициентПоДокументу                             = ?(ЭлементКоллекции.КоличествоПоДокументу = 0, 
						1, ЭлементКоллекции.КоличествоПотребительскихУпаковокПоДокументу / ЭлементКоллекции.КоличествоПоДокументу);
					ЭлементКоллекции.Количество                        = ЭлементКоллекции.Количество + КоличествоКРаспределению;
					ЭлементКоллекции.КоличествоПотребительскихУпаковок = Окр(ЭлементКоллекции.Количество 
						/ ?(КоэффициентПоДокументу = 0, 1, КоэффициентПоДокументу), 3, РежимОкругления.Окр15как20);
						
					Если СтрокаТаблицыЗначений.Номенклатура <> ЭлементКоллекции.Номенклатура
							Или СтрокаТаблицыЗначений.Характеристика <> ЭлементКоллекции.Характеристика Тогда
						ЭлементКоллекции.Статус         = "ПересортицаКодовМаркировки";
						ЭлементКоллекции.ТекстСообщения = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(
							НСтр("ru = '(в файле: %1%2)';
								|en = '(in the file: %1%2)'"), 
							СтрокаТаблицыЗначений.Номенклатура, 
							?(ЗначениеЗаполнено(СтрокаТаблицыЗначений.Характеристика), 
								", " + СтрокаТаблицыЗначений.Характеристика, ""));
					Иначе
						ЭлементКоллекции.Статус = "КодыМаркировкиСовпадают";
					КонецЕсли;
				КонецЕсли;
			КонецЦикла;
			
			// Остаток - как новая строка
			Если Количество > 0 Тогда
				Запись = ШтрихкодыУпаковокРасхождения.Добавить();
				ЗаполнитьЗначенияСвойств(Запись, СтрокаТаблицыЗначений);
				
				Запись.Статус                                        = ?(ЗначениеЗаполнено(СтрокаТаблицыЗначений.Ссылка), 
					"ИзлишекКодовМаркировки", "КодыМаркировкиНеНайдены");
				Запись.Количество                                    = Количество;
				Запись.КоличествоПотребительскихУпаковок             = КоличествоПотребительскихУпаковок;
				Запись.КоличествоПоДокументу                         = 0;
				Запись.КоличествоПотребительскихУпаковокПоДокументу  = 0;
				Запись.КоличествоВПотребительскойУпаковкеПоДокументу = 0;
				Запись.КоличествоРаспределено                        = 0;
			КонецЕсли;
		КонецЦикла;
	КонецЕсли;
	
	ВариантыРаспределения = Новый Массив;
	ВариантыРаспределения.Добавить("ССерией");
	ВариантыРаспределения.Добавить("БезСерии");
	
	Для Каждого СтрокаТаблицыЗначений Из ТоварыРасхождения Цикл
		Если СтрокаТаблицыЗначений.СведенияОМаркировке = Неопределено Тогда
			СтрокаТаблицыЗначений.СведенияОМаркировке     = Новый Соответствие;
			СтрокаТаблицыЗначений.РасхожденийПоМаркировке = ?(ЕстьШтрихкодыУпаковокИзФайла, 0, -1);
		КонецЕсли;
		
		Количество            = СтрокаТаблицыЗначений.Количество;
		КоличествоПоДокументу = СтрокаТаблицыЗначений.КоличествоПоДокументу;
		
		Отбор = Новый Структура;
		Отбор.Вставить("Номенклатура",   СтрокаТаблицыЗначений.Номенклатура);
		Отбор.Вставить("Характеристика", СтрокаТаблицыЗначений.Характеристика);
		
		НайденныеСтроки = ШтрихкодыУпаковокРасхождения.НайтиСтроки(Отбор);
		Для Каждого ЭлементКоллекции Из НайденныеСтроки Цикл
			Если Количество <= 0 Тогда
				Прервать;
			КонецЕсли;
			
			Для Каждого Вариант Из ВариантыРаспределения Цикл
				Если Количество <= 0 Тогда
					Прервать;
				КонецЕсли;
				
				Если Вариант = "ССерией"
							И СтрокаТаблицыЗначений.Серия = ЭлементКоллекции.Серия
						Или Вариант = "БезСерии" Тогда
					КоличествоКРаспределению = Мин(Количество, ЭлементКоллекции.Количество 
						- ЭлементКоллекции.КоличествоРаспределено);
					
					Если КоличествоКРаспределению > 0 Тогда
						Количество                              = Количество - КоличествоКРаспределению;
						ЭлементКоллекции.КоличествоРаспределено = ЭлементКоллекции.КоличествоРаспределено 
							+ КоличествоКРаспределению;
						
						ЭлементКоллекции.Упаковка = ?(ЗначениеЗаполнено(СтрокаТаблицыЗначений.Упаковка),
							СтрокаТаблицыЗначений.Упаковка, 
							РеквизитыНоменклатуры.Получить(СтрокаТаблицыЗначений.Номенклатура));
						
						СведенияОМаркировке = ОбщегоНазначения.СтрокаТаблицыЗначенийВСтруктуру(ЭлементКоллекции);
						СтрокаТаблицыЗначений.СведенияОМаркировке.Вставить(ЭлементКоллекции.ЗначениеШтрихкода, 
							СведенияОМаркировке);
						
						Если ЕстьШтрихкодыУпаковокИзФайла
								И ЭлементКоллекции.Статус <> "КодыМаркировкиСовпадают" Тогда
							СтрокаТаблицыЗначений.РасхожденийПоМаркировке = СтрокаТаблицыЗначений.РасхожденийПоМаркировке 
								+ КоличествоКРаспределению;
						КонецЕсли;
					КонецЕсли;
				КонецЕсли;
			КонецЦикла;
		КонецЦикла;
		
		НайденныеСтроки = ШтрихкодыУпаковокРасхождения.НайтиСтроки(Отбор);
		Для Каждого ЭлементКоллекции Из НайденныеСтроки Цикл
			Если КоличествоПоДокументу <= 0 Тогда
				Прервать;
			КонецЕсли;
			
			Для Каждого Вариант Из ВариантыРаспределения Цикл
				Если КоличествоПоДокументу <= 0 Тогда
					Прервать;
				КонецЕсли;
				
				Если Вариант = "ССерией"
							И СтрокаТаблицыЗначений.Серия = ЭлементКоллекции.Серия
						Или Вариант = "БезСерии" Тогда
					КоличествоКРаспределению = Мин(КоличествоПоДокументу, ЭлементКоллекции.КоличествоПоДокументу 
						- ЭлементКоллекции.КоличествоРаспределеноПоДокументу);
					
					Если КоличествоКРаспределению > 0 Тогда
						КоличествоПоДокументу                              = КоличествоПоДокументу - КоличествоКРаспределению;
						ЭлементКоллекции.КоличествоРаспределеноПоДокументу = ЭлементКоллекции.КоличествоРаспределеноПоДокументу 
							+ КоличествоКРаспределению;
							
						ЭлементКоллекции.Упаковка = ?(ЗначениеЗаполнено(СтрокаТаблицыЗначений.Упаковка),
							СтрокаТаблицыЗначений.Упаковка, 
							РеквизитыНоменклатуры.Получить(СтрокаТаблицыЗначений.Номенклатура));
						
						Если СтрокаТаблицыЗначений.СведенияОМаркировке.Получить(ЭлементКоллекции.ЗначениеШтрихкода) = Неопределено Тогда
							СведенияОМаркировке = ОбщегоНазначения.СтрокаТаблицыЗначенийВСтруктуру(ЭлементКоллекции);
							СтрокаТаблицыЗначений.СведенияОМаркировке.Вставить(ЭлементКоллекции.ЗначениеШтрихкода, 
								СведенияОМаркировке);
							
							Если ЕстьШтрихкодыУпаковокИзФайла
									И ЭлементКоллекции.Статус <> "КодыМаркировкиСовпадают" Тогда
								СтрокаТаблицыЗначений.РасхожденийПоМаркировке = СтрокаТаблицыЗначений.РасхожденийПоМаркировке 
									+ КоличествоКРаспределению;
							КонецЕсли;
						КонецЕсли;
					КонецЕсли;
				КонецЕсли;
			КонецЦикла;
		КонецЦикла;
	КонецЦикла;
	
	// Излишки штрихкодов из документа
	Для Каждого СтрокаТаблицыЗначений Из ШтрихкодыУпаковокРасхождения Цикл
		Если СтрокаТаблицыЗначений.КоличествоРаспределеноПоДокументу < СтрокаТаблицыЗначений.КоличествоПоДокументу Тогда
			Отбор = Новый Структура;
			Отбор.Вставить("Номенклатура",   СтрокаТаблицыЗначений.Номенклатура);
			Отбор.Вставить("Характеристика", СтрокаТаблицыЗначений.Характеристика);
			
			СтрокаНайдена   = Ложь;
			НайденныеСтроки = ТоварыРасхождения.НайтиСтроки(Отбор);
			
			Для Каждого ЭлементКоллекции Из НайденныеСтроки Цикл
				Для Каждого Вариант Из ВариантыРаспределения Цикл
					Если Вариант = "ССерией"
								И СтрокаТаблицыЗначений.Серия = ЭлементКоллекции.Серия
							Или Вариант = "БезСерии" Тогда
						СведенияОМаркировке = ОбщегоНазначения.СтрокаТаблицыЗначенийВСтруктуру(СтрокаТаблицыЗначений);
						СведенияОМаркировке.Вставить("Статус", "ЛишнийКодМаркировки");
						ЭлементКоллекции.СведенияОМаркировке.Вставить(СтрокаТаблицыЗначений.ЗначениеШтрихкода, 
							СведенияОМаркировке);
						
						Если ЕстьШтрихкодыУпаковокИзФайла Тогда
							ЭлементКоллекции.РасхожденийПоМаркировке = ЭлементКоллекции.РасхожденийПоМаркировке 
								+ СтрокаТаблицыЗначений.КоличествоПоДокументу 
								- СтрокаТаблицыЗначений.КоличествоРаспределеноПоДокументу;
						КонецЕсли;
						СтрокаТаблицыЗначений.КоличествоРаспределеноПоДокументу = СтрокаТаблицыЗначений.КоличествоПоДокументу;
						
						СтрокаТаблицыЗначений.Упаковка = ?(ЗначениеЗаполнено(ЭлементКоллекции.Упаковка),
							ЭлементКоллекции.Упаковка, РеквизитыНоменклатуры.Получить(ЭлементКоллекции.Номенклатура));
						
						СтрокаНайдена = Истина;
						Прервать;
					КонецЕсли;
				КонецЦикла;
				
				Если СтрокаНайдена Тогда
					Прервать;
				КонецЕсли;
			КонецЦикла;
		КонецЕсли;
	КонецЦикла;
	
	Для Каждого СтрокаТаблицыЗначений Из ТоварыРасхождения Цикл
		СтрокаТаблицыЗначений.ИндексНабора                  = ?(ЗначениеЗаполнено(СтрокаТаблицыЗначений.НоменклатураНабора), 1, 0);
		СтрокаТаблицыЗначений.КоличествоУпаковокРасхождения = СтрокаТаблицыЗначений.КоличествоУпаковок 
																- СтрокаТаблицыЗначений.КоличествоУпаковокПоДокументу;
		СтрокаТаблицыЗначений.СуммаРасхождения              = СтрокаТаблицыЗначений.Сумма 
																- СтрокаТаблицыЗначений.СуммаПоДокументу;
		СтрокаТаблицыЗначений.ЕстьРасхождения               = ЗначениеЗаполнено(СтрокаТаблицыЗначений.КоличествоУпаковокРасхождения)
																Или ЗначениеЗаполнено(СтрокаТаблицыЗначений.СуммаРасхождения);
		
		Если Не ЗначениеЗаполнено(СтрокаТаблицыЗначений.СведенияОМаркировке) Тогда
			СтрокаТаблицыЗначений.Маркировка = "";
		ИначеЕсли Не ЕстьШтрихкодыУпаковокИзФайла Тогда
			СтрокаТаблицыЗначений.Маркировка = НСтр("ru = 'в файле нет сведений';
													|en = 'There are no information records in the file'");
		ИначеЕсли ЗначениеЗаполнено(СтрокаТаблицыЗначений.РасхожденийПоМаркировке) Тогда
			СтрокаТаблицыЗначений.ЕстьРасхождения = Истина;
			СтрокаТаблицыЗначений.Маркировка = Формат(СтрокаТаблицыЗначений.РасхожденийПоМаркировке, "ЧДЦ=0; ЧН=0; ЧГ=0") + " "
				+ ОбщегоНазначенияУТКлиентСервер.СклонениеСлова(
					СтрокаТаблицыЗначений.РасхожденийПоМаркировке,
					НСтр("ru = 'расхождение, расхождения, расхождений, м';
						|en = 'Discrepancy, discrepancies, discrepancies, м'"));
		Иначе
			СтрокаТаблицыЗначений.Маркировка = НСтр("ru = 'нет расхождений';
													|en = 'No discrepancies'");
		КонецЕсли;
	КонецЦикла;
	
	Результат = Новый Структура;
	Результат.Вставить("ТоварыРасхождения",            ТоварыРасхождения);
	Результат.Вставить("ШтрихкодыУпаковокРасхождения", ШтрихкодыУпаковокРасхождения);
	
	Возврат Результат;
	
КонецФункции

// Возвращает список констант для анализа расхождений.
//
// Возвращаемое значение:
//   Соответствие из КлючИЗначение - имена и значения констант:
//     * Ключ     - Строка - статус.
//     * Значение - Строка - имя картинки в библиотеке.
//
Функция КонстантыРасхождений() Экспорт
	
	Результат = Новый Соответствие;
	
	Результат.Вставить("КодыМаркировкиСовпадают",    "ВыполненоУспешно");
	Результат.Вставить("ЛишнийКодМаркировки",        "Штрихкод");
	Результат.Вставить("КодовМаркировкиНетВФайле",   "Предупреждение");
	Результат.Вставить("ПересортицаКодовМаркировки", "Предупреждение");
	Результат.Вставить("ИзлишекКодовМаркировки",     "КрасныйПлюс");
	Результат.Вставить("НедостачаКодовМаркировки",   "ЗначокМинус");
	Результат.Вставить("КодыМаркировкиНеНайдены",    "ПредупреждениеКрасноеБЭД16");
	
	Возврат Результат;
	
КонецФункции

// Формирует представлений расхождений в виде форматированной строки.
//
// Параметры:
//   СведенияОМаркировке - Соответствие из КлючИЗначение - сведения о маркируемой продукции:
//     * Ключ     - Строка - код маркировки.
//     * Значение - Структура из КлючИЗначение - с дополнениями для ЭлектронноеВзаимодействиеИСМП.ЧастичноеСодержимоеИКодыОСУ.
//   
// Возвращаемое значение:
//   ФорматированнаяСтрока.
//
Функция СведенияОМаркировкеФорматированнойСтрокой(Знач СведенияОМаркировке) Экспорт
	
	Если Не ЗначениеЗаполнено(СведенияОМаркировке) Тогда
		Возврат "";
	КонецЕсли;
	
	КонстантыРасхождений = КонстантыРасхождений();
	Результат = "";
	
	Для Каждого КлючИЗначение Из СведенияОМаркировке Цикл
		Результат = Результат + "<img src=""" + КонстантыРасхождений[КлючИЗначение.Значение.Статус] + """>";
		Результат = Результат + Символы.НПП + КлючИЗначение.Значение.ЗначениеШтрихкода;
		Если Макс(КлючИЗначение.Значение.Количество, КлючИЗначение.Значение.КоличествоПоДокументу) > 1 Тогда
			Результат = Результат + "<span style=""color: ЦветНеАктивнойСтроки"">"
				+ Символы.НПП + "-" + Символы.НПП 
				+ Макс(КлючИЗначение.Значение.Количество, КлючИЗначение.Значение.КоличествоПоДокументу) 
				+ Символы.НПП + СтрЗаменить(КлючИЗначение.Значение.Упаковка, " ", Символы.НПП)
				+ "</span>";
		КонецЕсли;
		Если ЗначениеЗаполнено(КлючИЗначение.Значение.ТекстСообщения) Тогда
			Результат = Результат + "<span style=""color: ЦветНеАктивнойСтроки"">"
				+ Символы.НПП + СтрЗаменить(КлючИЗначение.Значение.ТекстСообщения, " ", Символы.НПП)
				+ "</span>";
		КонецЕсли;
		Результат = Результат + Символы.ПС;
	КонецЦикла;
	
	Возврат СтроковыеФункции.ФорматированнаяСтрока(Результат);
	
КонецФункции

// Конструктор таблицы данных контрагента для создания номенклатуры, характеристики и упаковки.
// 
// Возвращаемое значение:
//  ТаблицаЗначений:
//   * НоменклатураНаименование - Строка - 
//   * ХарактеристикаНаименование - Строка - 
//   * УпаковкаНаименование - Строка - 
//   * Номенклатура - СправочникСсылка.Номенклатура - 
//   * Характеристика - СправочникСсылка.ХарактеристикиНоменклатуры - 
//   * Упаковка - СправочникСсылка.УпаковкиЕдиницыИзмерения - 
//   * ХарактеристикаНайдена - Булево - 
//   * УпаковкаНайдена - Булево - 
//   * НоменклатураКонтрагента - см. СопоставлениеНоменклатурыКонтрагентовКлиентСервер.НоваяНоменклатураКонтрагента
//
Функция НоваяТаблицаДанныхКонтрагентаДляСозданияНоменклатуры() Экспорт
	
	Результат = Новый ТаблицаЗначений;
	
	Результат.Колонки.Добавить("НоменклатураНаименование",   ОбщегоНазначения.ОписаниеТипаСтрока(100));
	Результат.Колонки.Добавить("ХарактеристикаНаименование", ОбщегоНазначения.ОписаниеТипаСтрока(150));
	Результат.Колонки.Добавить("УпаковкаНаименование",       ОбщегоНазначения.ОписаниеТипаСтрока(50));
	
	Результат.Колонки.Добавить("Номенклатура",   Новый ОписаниеТипов("СправочникСсылка.Номенклатура"));
	Результат.Колонки.Добавить("Характеристика", Новый ОписаниеТипов("СправочникСсылка.ХарактеристикиНоменклатуры"));
	Результат.Колонки.Добавить("Упаковка",       Новый ОписаниеТипов("СправочникСсылка.УпаковкиЕдиницыИзмерения"));
	
	Результат.Колонки.Добавить("ХарактеристикаНайдена", Новый ОписаниеТипов("Булево"));
	Результат.Колонки.Добавить("УпаковкаНайдена",       Новый ОписаниеТипов("Булево"));
	
	Результат.Колонки.Добавить("НоменклатураКонтрагента"); // Произвольный
	
	Возврат Результат;
	
КонецФункции

// Возвращает состав полей, требуемых для создания номенклатуры по данным контрагента.
//
// Параметры:
//   ИсточникДанных - Произвольный - источник данных для определения категории сервиса.
//
// Возвращаемое значение:
//  Структура - см. ИнтеграцияСМаркетплейсамиКлиентСервер.РеквизитыНоменклатурыДляСозданияПоДаннымКонтрагента и дополнительно:
//   * ДополнительныеРеквизиты - см. НовыеДополнительныеРеквизитыДляСозданияПоДаннымКонтрагента.
//
Функция НовыеРеквизитыНоменклатурыДляСозданияПоДаннымКонтрагента(ИсточникДанных = Неопределено) Экспорт
	
	Результат = Новый Структура;
	
	ОбщегоНазначенияКлиентСервер.ДополнитьСтруктуру(
		Результат,
		ИнтеграцияСМаркетплейсамиКлиентСервер.РеквизитыНоменклатурыДляСозданияПоДаннымКонтрагента(ИсточникДанных));
	
	Результат.Вставить("ДополнительныеРеквизиты", НовыеДополнительныеРеквизитыДляСозданияПоДаннымКонтрагента());
	
	Возврат Результат;
	
КонецФункции

// Формирует данные для создания элементов справочников Номенклатура, Характеристики номенклатуры,
// Упаковки и единицы измерения по переданным данным номенклатуры контрагентов.
// 
// Параметры:
//  УчетнаяЗапись - СправочникСсылка.УчетныеЗаписиМаркетплейсов - 
//  ДанныеКонтрагента - см. НоваяТаблицаДанныхКонтрагентаДляСозданияНоменклатуры
//  РеквизитыНоменклатуры - см. НовыеРеквизитыНоменклатурыДляСозданияПоДаннымКонтрагента
// 
// Возвращаемое значение:
//  см. НовыеДанныеДляСозданияНоменклатурыПоДаннымКонтрагента
//
Функция ДанныеДляСозданияНоменклатурыПоДаннымКонтрагента(УчетнаяЗапись, ДанныеКонтрагента, РеквизитыНоменклатуры) Экспорт
	
	Результат = НовыеДанныеДляСозданияНоменклатурыПоДаннымКонтрагента();
	
	УстановитьСсылкиНаНоменклатуруВДанныхКонтрагента(ДанныеКонтрагента);
	ВыполнитьПоискХарактеристикВДанныхКонтрагента(ДанныеКонтрагента, РеквизитыНоменклатуры);
	ВыполнитьПоискУпаковокВДанныхКонтрагента(ДанныеКонтрагента, РеквизитыНоменклатуры);
	
	Для Каждого Строка Из ДанныеКонтрагента Цикл
		
		ДобавитьОписаниеДанныхСоздаваемойНоменклатуры(
			Результат.СоздаваемаяНоменклатура, РеквизитыНоменклатуры, Строка);
		
		ДобавитьОписаниеДанныхСоздаваемойХарактеристики(
			Результат.СоздаваемыеХарактеристики, РеквизитыНоменклатуры, Строка);
		
		ДобавитьОписаниеДанныхСоздаваемойУпаковки(
			Результат.СоздаваемыеУпаковки, РеквизитыНоменклатуры, Строка);
		
	КонецЦикла;
	
	РеквизитыУчетнойЗаписи = ОбщегоНазначения.ЗначенияРеквизитовОбъекта(
		УчетнаяЗапись, "ИсточникКатегории, ВидМаркетплейса");
		
	Категория1С = Категория1СДляСозданияНоменклатурыПоДаннымКонтрагента(
		РеквизитыУчетнойЗаписи.ИсточникКатегории, РеквизитыНоменклатуры);
	
	Если РеквизитыУчетнойЗаписи.ВидМаркетплейса = Перечисления.ВидыМаркетплейсов.МаркетплейсOzon Тогда
		
		ИнтеграцияСМаркетплейсомOzonСервер.ПриЗаполненииДанныхДляСозданияНоменклатурыПоДаннымКонтрагента(
			УчетнаяЗапись, Категория1С, ДанныеКонтрагента, Результат);
		
	КонецЕсли;
	
	Возврат Результат;
	
КонецФункции

// Создает элементы справочников Номенклатура, Характеристики номенклатуры, Упаковки и единицы измерения по переданным
// данным номенклатуры контрагентов.
// 
// Параметры:
//  УчетнаяЗапись - СправочникСсылка.УчетныеЗаписиМаркетплейсов - 
//  ДанныеКонтрагента - см. НоваяТаблицаДанныхКонтрагентаДляСозданияНоменклатуры
//  РеквизитыНоменклатуры - см. НовыеРеквизитыНоменклатурыДляСозданияПоДаннымКонтрагента
// 
// Возвращаемое значение:
//  Массив из см. НовыйРезультатСозданияНовойНоменклатуры
//
Функция СоздатьНоменклатуруПоДаннымКонтрагента(УчетнаяЗапись, ДанныеКонтрагента, РеквизитыНоменклатуры) Экспорт
	
	Результат = Новый Массив; // Массив из см. НовыйРезультатСозданияНовойНоменклатуры
	
	ДанныеДляСоздания = ДанныеДляСозданияНоменклатурыПоДаннымКонтрагента(
		УчетнаяЗапись, ДанныеКонтрагента, РеквизитыНоменклатуры);
		
	ПараметрыПрогресса = ПараметрыПрогресса();
	
	ПараметрыПрогресса.ШагИтерацииВПроцентах = 50;
	
	УстановитьПроцентПрогресса(1, 1, 100, ПараметрыПрогресса);
	
	ПараметрыПрогресса.ПрогрессВыполнения = 45;
	
	ПроцентВыполнения = 0;
	ИндексТекущегоОбъекта = 1;
	
	ВсегоСтрок =
		ДанныеДляСоздания.СоздаваемаяНоменклатура.Количество()
		+ ДанныеДляСоздания.СоздаваемыеХарактеристики.Количество()
		+ ДанныеДляСоздания.СоздаваемыеУпаковки.Количество();
	
	ЗначенияКонстантУпаковок = ЗначенияКонстантДляСозданияУпаковок();
	
	НачатьТранзакцию();
	
	Попытка
		
		Для Каждого КлючЗначение Из ДанныеДляСоздания.СоздаваемаяНоменклатура Цикл
			
			ОписаниеДанных = КлючЗначение.Значение;
			
			СоздатьНовыйЭлементСправочникаНоменклатура(ОписаниеДанных);
			
			УстановитьПроцентПрогресса(ВсегоСтрок, ИндексТекущегоОбъекта, ПроцентВыполнения, ПараметрыПрогресса);
			ИндексТекущегоОбъекта = ИндексТекущегоОбъекта + 1;
			
		КонецЦикла;
		
		Для Каждого КлючЗначение Из ДанныеДляСоздания.СоздаваемыеХарактеристики Цикл
			
			ОписаниеДанных = КлючЗначение.Значение;
			
			СоздатьНовыйЭлементСправочникаХарактеристикиНоменклатуры(РеквизитыНоменклатуры, ОписаниеДанных);
			
			УстановитьПроцентПрогресса(ВсегоСтрок, ИндексТекущегоОбъекта, ПроцентВыполнения, ПараметрыПрогресса);
			ИндексТекущегоОбъекта = ИндексТекущегоОбъекта + 1;
			
		КонецЦикла;
		
		Для Каждого КлючЗначение Из ДанныеДляСоздания.СоздаваемыеУпаковки Цикл
			
			ОписаниеДанных = КлючЗначение.Значение;
			
			СоздатьНовыйЭлементСправочникаУпаковкиЕдиницыИзмерения(
				РеквизитыНоменклатуры, ОписаниеДанных, ЗначенияКонстантУпаковок);
			
			УстановитьПроцентПрогресса(ВсегоСтрок, ИндексТекущегоОбъекта, ПроцентВыполнения, ПараметрыПрогресса);
			ИндексТекущегоОбъекта = ИндексТекущегоОбъекта + 1;
			
		КонецЦикла;
		
		ЗафиксироватьТранзакцию();
		
	Исключение
		
		ОтменитьТранзакцию();
		
		ТекстОшибки = ОбработкаОшибок.ПодробноеПредставлениеОшибки(ИнформацияОбОшибке());
		
		ЗаписьЖурналаРегистрации(
			СобытиеЖурналаРегистрации(), УровеньЖурналаРегистрации.Ошибка, , УчетнаяЗапись, ТекстОшибки);
		
	КонецПопытки;
	
	Для Каждого Строка Из ДанныеКонтрагента Цикл
		
		НоменклатураИБ = СопоставлениеНоменклатурыКонтрагентовКлиентСервер.НоваяНоменклатураИнформационнойБазы(
			Строка.Номенклатура, Строка.Характеристика, Строка.Упаковка);
		
		СозданныйЭлемент = НовыйРезультатСозданияНовойНоменклатуры();
		
		СозданныйЭлемент.НоменклатураКонтрагента = Строка.НоменклатураКонтрагента;
		СозданныйЭлемент.НоменклатураИБ          = НоменклатураИБ;
		
		Результат.Добавить(СозданныйЭлемент);
		
	КонецЦикла;
	
	Возврат Результат;
	
КонецФункции

#КонецОбласти

#Область СопоставлениеНоменклатуры

// См. ЭлектронноеВзаимодействиеПереопределяемый.ПриСозданииФормыПодсистемы.
Процедура ПриСозданииФормыСопоставленияНоменклатуры(Контекст, Отказ, СтандартнаяОбработка) Экспорт
	
	Если Контекст.Назначение = "СопоставлениеНоменклатуры"
			И ТипЗнч(Контекст.Форма.ДополнительныеПараметрыПоиска) = Тип("Структура")
			И Контекст.Форма.ДополнительныеПараметрыПоиска.Свойство("СпособОбработки")
			И Контекст.Форма.ДополнительныеПараметрыПоиска.СпособОбработки = "СверкаДокументаСТорговойПлощадкой" Тогда
		Префикс = Контекст.Форма.ДополнительныеПараметрыПоиска.СпособОбработки + "_";
		
		КомандаНазад = Контекст.Форма.Команды.Добавить(Префикс + "Назад");
		КомандаНазад.Действие = "Подключаемый_КомандаДействие";
		КомандаНазад = Контекст.Форма.Команды.Добавить(Префикс + "Далее");
		КомандаНазад.Действие = "Подключаемый_КомандаДействие";
		КомандаЗакрыть = Контекст.Форма.Команды.Добавить(Префикс + "Закрыть");
		КомандаЗакрыть.Действие = "Подключаемый_КомандаДействие";
		КомандаПоказатьПодсказку = Контекст.Форма.Команды.Добавить(Префикс + "ПоказатьПодсказку");
		КомандаПоказатьПодсказку.Действие  = "Подключаемый_КомандаДействие";
		КомандаПоказатьПодсказку.Подсказка = НСтр("ru = 'Показать подсказку';
													|en = 'Show tooltip'");
		КомандаСкрытьПодсказку = Контекст.Форма.Команды.Добавить(Префикс + "СкрытьПодсказку");
		КомандаСкрытьПодсказку.Действие  = "Подключаемый_КомандаДействие";
		КомандаСкрытьПодсказку.Подсказка = НСтр("ru = 'Скрыть подсказку';
												|en = 'Hide tooltip'");
		
		Контекст.Форма.ОтображатьЗаголовок      = Ложь;
		Контекст.Форма.ПоложениеКоманднойПанели = ПоложениеКоманднойПанелиФормы.Нет;
		
		ГруппаИнформация = Контекст.Форма.Элементы.Вставить(Префикс + "ГруппаИнформация", Тип("ГруппаФормы"), 
			Контекст.Форма.Элементы.ГруппаСопоставление, Контекст.Форма.Элементы.ГруппаОшибкаВыполненияПоиска);
		ГруппаИнформация.Вид                      = ВидГруппыФормы.ОбычнаяГруппа;
		ГруппаИнформация.Видимость                = Ложь;
		ГруппаИнформация.ОтображатьЗаголовок      = Ложь;
		ГруппаИнформация.РастягиватьПоГоризонтали = Истина;
		ГруппаИнформация.ЦветФона                 = ЦветаСтиля.ЦветФонаПодсказки;
			ДекорацияИнформация = Контекст.Форма.Элементы.Добавить(Префикс + "ДекорацияИнформация", Тип("ДекорацияФормы"), 
				ГруппаИнформация);
			ДекорацияИнформация.АвтоМаксимальнаяШирина   = Ложь;
			ДекорацияИнформация.Заголовок                = Новый ФорматированнаяСтрока(
				НСтр("ru = '1. Проверьте и при необходимости измените подобранный вариант сопоставления номенклатуры из файла с данными информационной базы.
					|2. Переход к следующему шагу помощника возможен после сопоставления всех позиций номенклатуры.
					|3. Запись позиций, которые были найдены по данным публикации товаров торговых площадок (выделены шрифтом), не производится. Изменения действуют только при работе с текущим помощником.
					|4. При нажатии кнопки ""Назад"" выполненные изменения аннулируются. Повторный поиск вариантов сопоставления будет выполнен без учета выполненных изменений.
					|5. Нажмите кнопку ""Далее"", чтобы перейти к следующему шагу помощника ""Обработка расхождений"".';
					|en = '1. Check and, if necessary, change the selected mapping option of items from the file with the infobase data.
					|2. You can proceed to the next step of the wizard after mapping all the items.
					|3. Items that were found based on the publication data of marketplace goods (in bold) are not recorded. Changes are valid only when using the current wizard.
					|4. When you click Back, the changes are canceled. A repeated search for mapping options will be performed without taking into account the changes made.
					|5. Click Next to proceed to the next ""Handling discrepancies"" wizard step.'"));
			ДекорацияИнформация.РастягиватьПоГоризонтали = Истина;
			ДекорацияИнформация.ЦветТекста               = ЦветаСтиля.ПоясняющийТекст;
			
			КнопкаСкрытьПодсказку = Контекст.Форма.Элементы.Добавить(Префикс + "СкрытьПодсказку", Тип("КнопкаФормы"), 
				ГруппаИнформация);
			КнопкаСкрытьПодсказку.Вид                            = ВидКнопкиФормы.Гиперссылка;
			КнопкаСкрытьПодсказку.ГоризонтальноеПоложениеВГруппе = ГоризонтальноеПоложениеЭлемента.Право;
			КнопкаСкрытьПодсказку.ИмяКоманды                     = Префикс + "СкрытьПодсказку";
			КнопкаСкрытьПодсказку.Картинка                       = БиблиотекаКартинок.ПолеВводаОчистить;
			КнопкаСкрытьПодсказку.Отображение                    = ОтображениеКнопки.Картинка;
			
		Контекст.Форма.Элементы.ВладелецОтбор.ТолькоПросмотр = Истина;
		Контекст.Форма.Элементы.ВладелецОтбор.КнопкаОчистки  = Ложь;
		
		ГруппаОтбор = Контекст.Форма.Элементы.ГруппаОтбор;
		ГруппаОтбор.РастягиватьПоГоризонтали = Истина;
			ДекорацияНадпись = Контекст.Форма.Элементы.Вставить(Префикс + "ДекорацияРазделитель1", Тип("ДекорацияФормы"), 
				ГруппаОтбор, Контекст.Форма.Элементы.СопоставленоОтбор);
			ДекорацияНадпись.РастягиватьПоГоризонтали = Ложь;
			
			КомандаСоздатьНоменклатуру = Контекст.Форма.Элементы.КомандаСоздатьНоменклатуру;
			Контекст.Форма.Элементы.Переместить(КомандаСоздатьНоменклатуру, ГруппаОтбор);
			
			ДекорацияНадпись = Контекст.Форма.Элементы.Вставить(Префикс + "ДекорацияРазделитель2", Тип("ДекорацияФормы"), 
				ГруппаОтбор, КомандаСоздатьНоменклатуру);
			ДекорацияНадпись.РастягиватьПоГоризонтали = Ложь;
			
			КнопкаПоказатьПодсказку = Контекст.Форма.Элементы.Добавить(Префикс + "ПоказатьПодсказку", Тип("КнопкаФормы"), 
				ГруппаОтбор);
			КнопкаПоказатьПодсказку.Вид                            = ВидКнопкиФормы.Гиперссылка;
			КнопкаПоказатьПодсказку.ГоризонтальноеПоложениеВГруппе = ГоризонтальноеПоложениеЭлемента.Право;
			КнопкаПоказатьПодсказку.ИмяКоманды                     = Префикс + "ПоказатьПодсказку";
			КнопкаПоказатьПодсказку.Картинка                       = БиблиотекаКартинок.Справка;
			КнопкаПоказатьПодсказку.Отображение                    = ОтображениеКнопки.Картинка;
		
		ГруппаКоманднаяПанель = Контекст.Форма.Элементы.Добавить(Префикс + "ГруппаКоманднаяПанель", Тип("ГруппаФормы"), 
			Контекст.Форма);
		ГруппаКоманднаяПанель.Вид = ВидГруппыФормы.ОбычнаяГруппа;
			ГруппаКоманднаяПанельЛево = Контекст.Форма.Элементы.Добавить(Префикс + "ГруппаКоманднаяПанельЛево", 
				Тип("ГруппаФормы"), ГруппаКоманднаяПанель);
			ГруппаКоманднаяПанельЛево.Вид = ВидГруппыФормы.КоманднаяПанель;    
			ГруппаКоманднаяПанельЛево.ГоризонтальноеПоложение = ГоризонтальноеПоложениеЭлемента.Лево;
				КнопкаНазад = Контекст.Форма.Элементы.Добавить(Префикс + "КнопкаНазад", Тип("КнопкаФормы"), 
					ГруппаКоманднаяПанельЛево);
				КнопкаНазад.Заголовок = НСтр("ru = '< Назад';
											|en = '< Back'");
				КнопкаНазад.ИмяКоманды = Префикс + "Назад";
			ГруппаКоманднаяПанельПраво = Контекст.Форма.Элементы.Добавить(Префикс + "ГруппаКоманднаяПанельПраво", 
				Тип("ГруппаФормы"), ГруппаКоманднаяПанель);
			ГруппаКоманднаяПанельПраво.Вид = ВидГруппыФормы.КоманднаяПанель;    
			ГруппаКоманднаяПанельПраво.ГоризонтальноеПоложение = ГоризонтальноеПоложениеЭлемента.Право;
				КнопкаДалее = Контекст.Форма.Элементы.Добавить(Префикс + "КнопкаДалее", Тип("КнопкаФормы"), 
					ГруппаКоманднаяПанельПраво);
				КнопкаДалее.Заголовок = НСтр("ru = 'Далее >';
											|en = 'Next >'");
				КнопкаДалее.ИмяКоманды = Префикс + "Далее";
				КнопкаДалее.КнопкаПоУмолчанию = Истина;
				КнопкаЗакрыть = Контекст.Форма.Элементы.Добавить(Префикс + "КнопкаЗакрыть", Тип("КнопкаФормы"), 
					ГруппаКоманднаяПанельПраво);
				КнопкаЗакрыть.Заголовок = НСтр("ru = 'Закрыть';
												|en = 'Close'");
				КнопкаЗакрыть.ИмяКоманды = Префикс + "Закрыть";
				
		УсловноеОформление = Контекст.Форма.УсловноеОформление;
		ЭлементУсловногоОформления = УсловноеОформление.Элементы.Добавить();

		ПолеЭлемента = ЭлементУсловногоОформления.Поля.Элементы.Добавить();
		ПолеЭлемента.Поле = Новый ПолеКомпоновкиДанных("СопоставлениеНоменклатура");
		ПолеЭлемента = ЭлементУсловногоОформления.Поля.Элементы.Добавить();
		ПолеЭлемента.Поле = Новый ПолеКомпоновкиДанных("СопоставлениеХарактеристика");
		ПолеЭлемента = ЭлементУсловногоОформления.Поля.Элементы.Добавить();
		ПолеЭлемента.Поле = Новый ПолеКомпоновкиДанных("СопоставлениеУпаковка");
		
		ОтборЭлемента = ЭлементУсловногоОформления.Отбор.Элементы.Добавить(Тип("ЭлементОтбораКомпоновкиДанных"));
		ОтборЭлемента.ЛевоеЗначение  = Новый ПолеКомпоновкиДанных("Объект.Сопоставление.ВариантУказанияНоменклатуры");
		ОтборЭлемента.ВидСравнения   = ВидСравненияКомпоновкиДанных.Равно;
		ОтборЭлемента.ПравоеЗначение = СопоставлениеНоменклатурыКонтрагентовКлиентСервер.ВариантУказанияНоменклатураОрганизации();

		ЭлементУсловногоОформления.Оформление.УстановитьЗначениеПараметра("Шрифт", ШрифтыСтиля.ВажнаяНадписьШрифт);
	КонецЕсли;
	
КонецПроцедуры

// Позволяет провести поиск вариантов по дополнительным натуральным ключам.
//
// Параметры:
//   ТаблицаНоменклатурыКонтрагентов - ТаблицаЗначений из см. СопоставлениеНоменклатурыКонтрагентовСлужебный.ПреобразоватьНаборНоменклатурыКонтрагентовВТаблицу.
//   ДополнительныеПараметры         - см. СопоставлениеНоменклатурыКонтрагентовКлиентСервер.НовыеПараметрыСопоставленияНоменклатуры.
// 
Процедура ПодобратьВариантыСопоставленияНоменклатуры(ТаблицаНоменклатурыКонтрагентов, 
			Знач ДополнительныеПараметры) Экспорт
	
	НайтиВариантыСопоставленияНоменклатуры(ТаблицаНоменклатурыКонтрагентов, 
		ДополнительныеПараметры.ДополнительныеПараметрыПоиска, 
		Истина);
	
КонецПроцедуры

// Формирует список номенклатуры для сопоставления.
//
// Параметры:
//   ДанныеЭлектронногоДокумента - см. ОтражениеВУчетеЭДО.НовыеДанныеЭлектронногоДокументаДляОтраженияВУчете.
// 
// Возвращаемое значение:
//   Структура - данные для сопоставления:
//     * НаборНоменклатурыКонтрагентов - Массив из см. СопоставлениеНоменклатурыКонтрагентовКлиентСервер.НоваяНоменклатураКонтрагента.
//     * ДанныеРазбора                 - Неопределено,
//                                     - ТабличныйДокумент,
//                                     - ДвоичныеДанные,
//                                     - Произвольный,
//                                     - Структура - данные разбора:
//       ** ДеревоРазбора               - см. ДеревоЭлектронногоДокументаБЭД.ИнициализироватьДеревоРазбора.
//       ** НовыйЭД                     - см. ДеревоЭлектронногоДокументаБЭД.НайтиСоздатьТипОбъектаВДеревеРазбора.
//       ** Содержание                  - Неопределено.
//                                     - см. ПолучитьДанныеЭлектронногоДокумента,
//
Функция НоменклатураЭлектронногоДокументаДляСопоставления(ДанныеЭлектронногоДокумента) Экспорт
	
	Если ДанныеЭлектронногоДокумента.ТипДокумента = Перечисления.ТипыДокументовЭДО.Прикладной Тогда
		ИнформацияОТоварах = ИнформацияОТоварахИзТабличногоДокумента(ДанныеЭлектронногоДокумента);
		ДанныеРазбора      = ДанныеЭлектронногоДокумента.ДополнительныеДанные.ДанныеРазбора;
		
	Иначе
		УчетнаяЗапись = ДанныеЭлектронногоДокумента.ДополнительныеДанные.УчетнаяЗапись;	// СправочникСсылка.УчетныеЗаписиМаркетплейсов
		ВидТорговойПлощадки = ДанныеЭлектронногоДокумента.ДополнительныеДанные.ВидТорговойПлощадки;
		Если ЗначениеЗаполнено(УчетнаяЗапись) Тогда
			ВидТорговойПлощадки = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(УчетнаяЗапись, "ВидМаркетплейса");
		КонецЕсли;
		
		ПараметрыПолучения = ЭлектронныеДокументыЭДО.НовыеПараметрыПолученияДанныхДокумента();
		ПараметрыПолучения.ОсновнойФайл = ДанныеЭлектронногоДокумента.ДанныеОсновногоФайла;
		
		ИнформацияОТоварах = ЭлектронныеДокументыЭДО.ДанныеНоменклатурыДокумента(ПараметрыПолучения);
		
		// Изменение параметров сопоставления
		Если ЗначениеЗаполнено(ВидТорговойПлощадки) Тогда
			Отказ = Истина;
			ДанныеРазбора = ПолучитьДанныеЭлектронногоДокумента(ДанныеЭлектронногоДокумента);
			
			Если ЗначениеЗаполнено(ДанныеРазбора) Тогда
				СтрокиТребующиеСопоставления             = СтрокиТребующиеСопоставления(ДанныеРазбора.НовыйЭД, Истина);
				ТоварыПодчиненыОднойКорневойСтрокеДерева = ТоварыПодчиненыОднойКорневойСтрокеДерева(СтрокиТребующиеСопоставления);
				
				Если ЗначениеЗаполнено(СтрокиТребующиеСопоставления) 
						И ТоварыПодчиненыОднойКорневойСтрокеДерева Тогда
					Для Каждого КлючИЗначение Из СтрокиТребующиеСопоставления Цикл
						Идентификатор         = "";
						КодНоменклатуры       = "";
						Штрихкод              = "";
						СтрокаТаблицыЗначений = Неопределено;
						
						ОпределитьКлючевыеРеквизитыСопоставления(ВидТорговойПлощадки, КлючИЗначение.Ключ, 
							Идентификатор, КодНоменклатуры, Штрихкод);
						
						Если ЗначениеЗаполнено(Идентификатор)
								Или ЗначениеЗаполнено(КодНоменклатуры)
								Или ЗначениеЗаполнено(Штрихкод) Тогда
							СтрокаТаблицыЗначений = ИнформацияОТоварах[КлючИЗначение.Ключ.Значение - 1];
							
							Если ЗначениеЗаполнено(Идентификатор) Тогда
								СтрокаТаблицыЗначений.ИД = Идентификатор;
								СтрокаТаблицыЗначений.Сопоставление.Вставить("Идентификатор", Идентификатор);
							КонецЕсли;
							
							Если ЗначениеЗаполнено(КодНоменклатуры) Тогда
								СтрокаТаблицыЗначений.КодТовара = КодНоменклатуры;
								СтрокаТаблицыЗначений.Сопоставление.Вставить("КодНоменклатуры", КодНоменклатуры);
							КонецЕсли;
							
							Если ЗначениеЗаполнено(Штрихкод)
									И СтроковыеФункцииКлиентСервер.ТолькоЦифрыВСтроке(Штрихкод) Тогда
								СтрокаТаблицыЗначений.Штрихкод = Штрихкод;
								СтрокаТаблицыЗначений.Сопоставление.Вставить("ШтрихкодКомбинации", Штрихкод);
							КонецЕсли;
						КонецЕсли;
						
						ПутьКСопоставлению = КлючИЗначение.Ключ.ПолныйПуть + ".Сопоставление";
						ВариантУказанияНоменклатурыИБ = ЭлектронноеВзаимодействие.ЗначениеРеквизитаВДереве(КлючИЗначение.Ключ, 
							ПутьКСопоставлению + ".ВариантУказанияНоменклатурыИБ", Ложь);
						НоменклатураИБ = ЭлектронноеВзаимодействие.ЗначениеРеквизитаВДереве(КлючИЗначение.Ключ, 
							ПутьКСопоставлению + ".НоменклатураИБ", Ложь);
						ХарактеристикаИБ = ЭлектронноеВзаимодействие.ЗначениеРеквизитаВДереве(КлючИЗначение.Ключ, 
							ПутьКСопоставлению + ".ХарактеристикаИБ", Ложь);
						УпаковкаИБ = ЭлектронноеВзаимодействие.ЗначениеРеквизитаВДереве(КлючИЗначение.Ключ, 
							ПутьКСопоставлению + ".УпаковкаИБ", Ложь);
						
						Если ЗначениеЗаполнено(СтрокаТаблицыЗначений)
								И ЗначениеЗаполнено(НоменклатураИБ) Тогда
							СтрокаТаблицыЗначений.Сопоставление.Вставить("ВариантУказанияНоменклатурыИБ", ВариантУказанияНоменклатурыИБ);
							СтрокаТаблицыЗначений.Сопоставление.Вставить("НоменклатураИБ",                НоменклатураИБ);
							СтрокаТаблицыЗначений.Сопоставление.Вставить("ХарактеристикаИБ",              ХарактеристикаИБ);
							СтрокаТаблицыЗначений.Сопоставление.Вставить("УпаковкаИБ",                    УпаковкаИБ);
						КонецЕсли;
					КонецЦикла;
					
					Отказ = Ложь;
				КонецЕсли;
			КонецЕсли;
			
			Если Отказ Тогда
				Для Каждого СтрокаТаблицыЗначений Из ИнформацияОТоварах Цикл
					Если Не ЗначениеЗаполнено(СтрокаТаблицыЗначений.ИД) 
							И ЗначениеЗаполнено(СтрокаТаблицыЗначений.КодТовара) Тогда
						СтрокаТаблицыЗначений.ИД = СтрокаТаблицыЗначений.КодТовара;
						СтрокаТаблицыЗначений.Сопоставление.Вставить("Идентификатор", СтрокаТаблицыЗначений.КодТовара);
					КонецЕсли;
				КонецЦикла;
			КонецЕсли;
		КонецЕсли;
	КонецЕсли;
	
	Владелец = СопоставлениеНоменклатурыКонтрагентовСлужебный.ВладелецНоменклатурыКонтрагента(
		ДанныеЭлектронногоДокумента.Отправитель);
	
	НаборНоменклатурыКонтрагентов   = НаборНоменклатурыКонтрагентовПоИнформацииОТоварах(ИнформацияОТоварах, Владелец);
	ТаблицаНоменклатурыКонтрагентов = НоваяТаблицаНоменклатурыКонтрагентов(НаборНоменклатурыКонтрагентов);
	
	НайтиВариантыСопоставленияНоменклатуры(ТаблицаНоменклатурыКонтрагентов, 
		ДанныеЭлектронногоДокумента.ДополнительныеДанные, 
		Ложь);
		
	ТаблицаНоменклатурыКонтрагентов.Колонки.НоменклатураИБ.Имя   = "Номенклатура";
	ТаблицаНоменклатурыКонтрагентов.Колонки.Характеристика.Имя   = "НаименованиеХарактеристики";
	ТаблицаНоменклатурыКонтрагентов.Колонки.ХарактеристикаИБ.Имя = "Характеристика";
	ТаблицаНоменклатурыКонтрагентов.Колонки.УпаковкаИБ.Имя       = "Упаковка";
	
	НаборНоменклатурыКонтрагентов = ОбщегоНазначения.ТаблицаЗначенийВМассив(ТаблицаНоменклатурыКонтрагентов);
	
	Результат = Новый Структура;
	Результат.Вставить("НаборНоменклатурыКонтрагентов", НаборНоменклатурыКонтрагентов);
	Результат.Вставить("ДанныеРазбора",                 ДанныеРазбора);
	
	Возврат Результат;
	
КонецФункции

// Возвращает настройки, по которым выполняется поиск номенклатуры партнера и сопоставление ее полей.
//
// Параметры:
//   ВидМаркетплейса - ПеречислениеСсылка.ВидыМаркетплейсов - вид торговой площадки.
//
// Возвращаемое значение:
//   Соответствие из Строка - настройки, по которым выполняется поиск номенклатуры партнера и сопоставление ее полей:
//     * ПоляСопоставления - Соответствие Из Строка - сопоставление полей поиска:
//         ** Ключ     - Строка - поле источника данных.
//         ** Значение - Строка - поле номенклатуры партнера.
//     * ПоляПоиска        - Соответствие из Строка - сопоставление полей заполнения номенклатуры партнера:
//         ** Ключ     - Строка - поле источника данных.
//         ** Значение - Строка - поле номенклатуры партнера.
//
Функция НастройкиСопоставленияНоменклатуры(ВидМаркетплейса) Экспорт

	Настройки = Новый Структура;
	Настройки.Вставить("ПоляПоиска",        Новый Соответствие);
	Настройки.Вставить("ПоляСопоставления", Новый Соответствие);
	
	Если ВидМаркетплейса = Перечисления.ВидыМаркетплейсов.МаркетплейсOzon Тогда
		Настройки.ПоляСопоставления.Вставить("ПредставлениеОбъектаМаркетплейса", "ПредставлениеТовара");
		Настройки.ПоляСопоставления.Вставить("ИдентификаторПубликации",          "Идентификатор");
		Настройки.ПоляСопоставления.Вставить("ИдентификаторПубликации",          "ИдентификаторКакАртикул");
		Настройки.ПоляСопоставления.Вставить("ИдентификаторОбъектаМаркетплейса", "ИдентификаторТовара");
		
		СтатусНедействителен = "Статус В (ЗНАЧЕНИЕ(Перечисление.СтатусыОбъектовМаркетплейсаOzon.ВАрхиве),
								|			ЗНАЧЕНИЕ(Перечисление.СтатусыОбъектовМаркетплейсаOzon.НеИспользуется))";
		Настройки.ПоляСопоставления.Вставить(СтатусНедействителен, "Недействителен");
		
		Настройки.ПоляПоиска.Вставить("ИдентификаторПубликации",          "Идентификатор");
		Настройки.ПоляПоиска.Вставить("ИдентификаторОбъектаМаркетплейса", "ИдентификаторНоменклатуры");
	КонецЕсли;
	
	Возврат Настройки;

КонецФункции

// Подготавливает шаблон текста запроса поиска номенклатуры партнера по переданным настройкам.
//
// Параметры:
//   НастройкиСопоставленияНоменклатуры - см. НастройкиСопоставленияНоменклатуры.
//   ДополнительноеУсловие - Строка - дополнительное условие соединений для поиска.
//                                    Например: "И ИсточникДанных.НоменклатураПартнера = НЕОПРЕДЕЛЕНО".
//
// Возвращаемое значение:
//   Структура - настройки текста запроса:
//     * ПоляНоменклатурыПартнера - Строка - описание полей для заполнения номенклатуры партнера.
//     * РезультатПоиска          - Строка - строка заполнения значения поля по результатам применения соединений для поиска.
//     * СоединенияПоиска         - Строка - строка соединений для поиска.
//
Функция НастройкиЗапросаПоискаНоменклатуры(НастройкиСопоставленияНоменклатуры, ДополнительноеУсловие = "") Экспорт

	Настройки = Новый Структура;
	Настройки.Вставить("ПоляНоменклатурыПартнера", "");
	Настройки.Вставить("РезультатПоиска",          "");
	Настройки.Вставить("СоединенияПоиска",         "");
	
	Если НастройкиСопоставленияНоменклатуры.ПоляСопоставления.Количество() = 0 Тогда
		Настройки.ПоляНоменклатурыПартнера = "НЕОПРЕДЕЛЕНО";
	Иначе
		ШаблонПоля = "ИсточникДанных.%1" + " " + "КАК" + " " + "%2";
		
		ОписаниеПолей = Новый Массив;
		Для Каждого Поле Из НастройкиСопоставленияНоменклатуры.ПоляСопоставления Цикл
			ОписаниеПолей.Добавить(СтрШаблон(ШаблонПоля, Поле.Ключ, Поле.Значение));
		КонецЦикла;
		
		Настройки.ПоляНоменклатурыПартнера = СтрСоединить(ОписаниеПолей, "," + Символы.ПС + "	");
	КонецЕсли;
	
	Если НастройкиСопоставленияНоменклатуры.ПоляПоиска.Количество() > 0 Тогда
		ПолеНоменклатурыПартнера = "НЕОПРЕДЕЛЕНО";
		
		ИндексПоиска = 0;
		ШаблонСоединения = "
		|		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.НоменклатураКонтрагентов КАК СправочникНоменклатураКонтрагентов%1
		|		ПО (СправочникНоменклатураКонтрагентов%1.Владелец = &ВладелецКаталога)
		|			И ИсточникДанных.%2 = СправочникНоменклатураКонтрагентов%1.%3";
		
		Если Не ПустаяСтрока(ДополнительноеУсловие) Тогда
			ШаблонСоединения = ШаблонСоединения + " " + ДополнительноеУсловие;
		КонецЕсли;
		
		СоединенияПоиска = Новый Массив;
		Для Каждого Поле Из НастройкиСопоставленияНоменклатуры.ПоляПоиска Цикл
			ИндексПоиска = ИндексПоиска + 1;
			
			ШаблонПоля = СтрШаблон("ЕСТЬNULL(СправочникНоменклатураКонтрагентов%1.Ссылка, НЕОПРЕДЕЛЕНО)", ИндексПоиска);
			
			ПолеНоменклатурыПартнера = СтрЗаменить(ПолеНоменклатурыПартнера, "НЕОПРЕДЕЛЕНО", ШаблонПоля);
			СоединенияПоиска.Добавить(СтрШаблон(ШаблонСоединения, ИндексПоиска, Поле.Ключ, Поле.Значение));
		КонецЦикла;
		
		Если СоединенияПоиска.Количество() > 0 Тогда
			Настройки.РезультатПоиска = ПолеНоменклатурыПартнера;
			Настройки.СоединенияПоиска = СтрСоединить(СоединенияПоиска, Символы.ПС);
		КонецЕсли;
	КонецЕсли;
	
	Возврат Настройки;

КонецФункции

// Параметры:
//   УчетнаяЗапись                 - СправочникСсылка.УчетныеЗаписиМаркетплейсов - учетная запись торговой площадки.
//   НаборНоменклатурыКонтрагентов - Массив из см. СопоставлениеНоменклатурыКонтрагентовКлиентСервер.НоваяНоменклатураКонтрагента
//
// Возвращаемое значение:
//  СправочникСсылка.ВидыНоменклатуры - вид номенклатуры.
//
Функция ВидНовойНоменклатуры(УчетнаяЗапись, НаборНоменклатурыКонтрагентов) Экспорт

	ВидНоменклатуры = Справочники.ВидыНоменклатуры.ПустаяСсылка();
	
	ИдентификаторыТоваров = Новый Массив;
	Для Каждого НоменклатураКонтрагента Из НаборНоменклатурыКонтрагентов Цикл
		ИдентификаторыТоваров.Добавить(НоменклатураКонтрагента.Артикул);
	КонецЦикла;
	
	РеквизитыУчетнойЗаписи = ОбщегоНазначения.ЗначенияРеквизитовОбъекта(
		УчетнаяЗапись,
		"ВидМаркетплейса, ИсточникКатегории");
	
	ТекстЗапроса = ТекстЗапросаПоискаВидаНоменклатуры(
		РеквизитыУчетнойЗаписи.ВидМаркетплейса,
		РеквизитыУчетнойЗаписи.ИсточникКатегории);
	
	Если Не ПустаяСтрока(ТекстЗапроса) Тогда
		Запрос = Новый Запрос;
		Запрос.Текст = ТекстЗапроса;
		Запрос.УстановитьПараметр("УчетнаяЗапись",         УчетнаяЗапись);
		Запрос.УстановитьПараметр("ИдентификаторыТоваров", ИдентификаторыТоваров);
		
		Выборка = Запрос.Выполнить().Выбрать();
		Если Выборка.Следующий() Тогда
			Если Выборка.ИсточникЭтоГруппа Тогда
				ВидНоменклатуры = ВидНоменклатурыПодчиненныхЭлементов(
					Выборка.Источник,
					РеквизитыУчетнойЗаписи.ИсточникКатегории);
			Иначе
				ВидНоменклатуры = Выборка.ВидНоменклатуры;
			КонецЕсли;
		КонецЕсли;
	КонецЕсли;
	
	Возврат ВидНоменклатуры;

КонецФункции

#КонецОбласти

// Получение двоичных данных по адресу URL, например изображения.
