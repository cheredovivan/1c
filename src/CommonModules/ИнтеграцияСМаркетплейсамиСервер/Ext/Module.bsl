			|	ТаблицаКодовНоменклатуры.ШтрихкодКомбинации КАК ШтрихкодКомбинации,
			|	СтатусыПубликацииОбъектов.Номенклатура КАК НоменклатураИБ,
			|	СтатусыПубликацииОбъектов.Характеристика КАК ХарактеристикаИБ,
			|	СтатусыПубликацииОбъектов.Упаковка КАК УпаковкаИБ
			|ИЗ
			|	РегистрСведений.СтатусыПубликацииОбъектовМаркетплейсаOzon КАК СтатусыПубликацииОбъектов
			|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ТаблицаКодовНоменклатуры КАК ТаблицаКодовНоменклатуры
			|		ПО (ТаблицаКодовНоменклатуры.КодНоменклатуры = СтатусыПубликацииОбъектов.ИдентификаторFBOSKU)
			|ГДЕ
			|	СтатусыПубликацииОбъектов.УчетнаяЗаписьМаркетплейса = &УчетнаяЗапись
			|	И СтатусыПубликацииОбъектов.ВидОбъектаМаркетплейса = &ВидОбъектаТорговойПлощадки
			|	И СтатусыПубликацииОбъектов.Номенклатура <> &ПустаяНоменклатура";
		
		ТекстыПриоритетов.Добавить(ТекстЗапроса);
		
	ИначеЕсли ВидМаркетплейса = Перечисления.ВидыМаркетплейсов.МаркетплейсЯндексМаркет Тогда
		
		ТекстЗапроса =
			"ВЫБРАТЬ
			|	1 КАК Приоритет,
			|	ТаблицаИдентификаторов.НоменклатураПубликации КАК НоменклатураПубликации,
			|	ТаблицаИдентификаторов.ИдентификаторПубликации КАК ИдентификаторПубликации,
			|	ТаблицаИдентификаторов.КодНоменклатуры КАК КодНоменклатуры,
			|	ТаблицаИдентификаторов.ШтрихкодКомбинации КАК ШтрихкодКомбинации,
			|	СтатусыПубликацииОбъектов.Номенклатура КАК НоменклатураИБ,
			|	СтатусыПубликацииОбъектов.Характеристика КАК ХарактеристикаИБ,
			|	ВЫБОР
			|		КОГДА СтатусыПубликацииОбъектов.Упаковка = СправочникНоменклатура.ЕдиницаИзмерения
			|			ТОГДА &ПустаяУпаковка
			|		ИНАЧЕ СтатусыПубликацииОбъектов.Упаковка
			|	КОНЕЦ КАК УпаковкаИБ
			|ИЗ
			|	РегистрСведений.СтатусыПубликацииТоваровЯндексМаркет КАК СтатусыПубликацииОбъектов
			|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.Номенклатура КАК СправочникНоменклатура
			|		ПО (СправочникНоменклатура.Ссылка = СтатусыПубликацииОбъектов.Номенклатура)
			|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ТаблицаИдентификаторов КАК ТаблицаИдентификаторов
			|		ПО (ТаблицаИдентификаторов.ИдентификаторПубликации = СтатусыПубликацииОбъектов.ИдентификаторПредложения)
			|ГДЕ
			|	СтатусыПубликацииОбъектов.УчетнаяЗапись = &УчетнаяЗапись";
		
		ТекстыПриоритетов.Добавить(ТекстЗапроса);
		
		ТекстЗапроса =
			"ВЫБРАТЬ
			|	2 КАК Приоритет,
			|	ТаблицаПоНоменклатуре.НоменклатураПубликации КАК НоменклатураПубликации,
			|	ТаблицаПоНоменклатуре.ИдентификаторПубликации КАК ИдентификаторПубликации,
			|	ТаблицаПоНоменклатуре.КодНоменклатуры КАК КодНоменклатуры,
			|	ТаблицаПоНоменклатуре.ШтрихкодКомбинации КАК ШтрихкодКомбинации,
			|	СтатусыПубликацииОбъектов.Номенклатура КАК НоменклатураИБ,
			|	СтатусыПубликацииОбъектов.Характеристика КАК ХарактеристикаИБ,
			|	ВЫБОР
			|		КОГДА СтатусыПубликацииОбъектов.Упаковка = СправочникНоменклатура.ЕдиницаИзмерения
			|			ТОГДА &ПустаяУпаковка
			|		ИНАЧЕ СтатусыПубликацииОбъектов.Упаковка
			|	КОНЕЦ КАК УпаковкаИБ
			|ИЗ
			|	РегистрСведений.СтатусыПубликацииТоваровЯндексМаркет КАК СтатусыПубликацииОбъектов
			|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.Номенклатура КАК СправочникНоменклатура
			|		ПО (СправочникНоменклатура.Ссылка = СтатусыПубликацииОбъектов.Номенклатура)
			|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ТаблицаПоНоменклатуре КАК ТаблицаПоНоменклатуре
			|		ПО (СтатусыПубликацииОбъектов.УчетнаяЗапись = &УчетнаяЗапись)
			|			И (ТаблицаПоНоменклатуре.НоменклатураИБ = СтатусыПубликацииОбъектов.Номенклатура)
			|			И (ТаблицаПоНоменклатуре.ХарактеристикаИБ = СтатусыПубликацииОбъектов.Характеристика)
			|			И (ТаблицаПоНоменклатуре.УпаковкаИБ = ВЫБОР
			|				КОГДА СтатусыПубликацииОбъектов.Упаковка = СправочникНоменклатура.ЕдиницаИзмерения
			|					ТОГДА &ПустаяУпаковка
			|				ИНАЧЕ СтатусыПубликацииОбъектов.Упаковка
			|			КОНЕЦ)
			|ГДЕ
			|	НЕ СтатусыПубликацииОбъектов.НоменклатураПартнера = &ПустаяНоменклатураКонтрагентов";
		
		ТекстыПриоритетов.Добавить(ТекстЗапроса);
		
	КонецЕсли;
	
	ТекстЗапроса =
		"ВЫБРАТЬ
		|	3 КАК Приоритет,
		|	ТаблицаИдентификаторов.НоменклатураПубликации КАК НоменклатураПубликации,
		|	ТаблицаИдентификаторов.ИдентификаторПубликации КАК ИдентификаторПубликации,
		|	ТаблицаИдентификаторов.КодНоменклатуры КАК КодНоменклатуры,
		|	ТаблицаИдентификаторов.ШтрихкодКомбинации КАК ШтрихкодКомбинации,
		|	СтатусыПубликацииОбъектов.Номенклатура КАК НоменклатураИБ,
		|	СтатусыПубликацииОбъектов.Характеристика КАК ХарактеристикаИБ,
		|	СтатусыПубликацииОбъектов.Упаковка КАК УпаковкаИБ
		|ИЗ
		|	СтатусыПубликацииТоваровЯндексМаркет КАК СтатусыПубликацииОбъектов
		|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ТаблицаИдентификаторов КАК ТаблицаИдентификаторов
		|		ПО (ТаблицаИдентификаторов.ИдентификаторПубликации = СтатусыПубликацииОбъектов.ИдентификаторПредложения)
		|ГДЕ
		|	СтатусыПубликацииОбъектов.УчетнаяЗапись <> &УчетнаяЗапись
		|	И СтатусыПубликацииОбъектов.Номенклатура <> &ПустаяНоменклатура";
	
	ТекстыПриоритетов.Добавить(ТекстЗапроса);
	
	ТекстЗапроса =
		"ВЫБРАТЬ
		|	3 КАК Приоритет,
		|	ТаблицаИдентификаторов.НоменклатураПубликации КАК НоменклатураПубликации,
		|	ТаблицаИдентификаторов.ИдентификаторПубликации КАК ИдентификаторПубликации,
		|	ТаблицаИдентификаторов.КодНоменклатуры КАК КодНоменклатуры,
		|	ТаблицаИдентификаторов.ШтрихкодКомбинации КАК ШтрихкодКомбинации,
		|	СтатусыПубликацииОбъектов.Номенклатура КАК НоменклатураИБ,
		|	СтатусыПубликацииОбъектов.Характеристика КАК ХарактеристикаИБ,
		|	СтатусыПубликацииОбъектов.Упаковка КАК УпаковкаИБ
		|ИЗ
		|	СтатусыПубликацииОбъектовМаркетплейсаOzon КАК СтатусыПубликацииОбъектов
		|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ТаблицаИдентификаторов КАК ТаблицаИдентификаторов
		|		ПО (ТаблицаИдентификаторов.ИдентификаторПубликации = СтатусыПубликацииОбъектов.ИдентификаторПубликации)
		|ГДЕ
		|	СтатусыПубликацииОбъектов.УчетнаяЗаписьМаркетплейса <> &УчетнаяЗапись
		|	И СтатусыПубликацииОбъектов.ВидОбъектаМаркетплейса = &ВидОбъектаТорговойПлощадки
		|	И СтатусыПубликацииОбъектов.Номенклатура <> &ПустаяНоменклатура";
	
	ТекстыПриоритетов.Добавить(ТекстЗапроса);
	
	ТекстЗапроса = СтрСоединить(ТекстыПриоритетов, ОбщегоНазначения.ТекстОбъединитьВсе());
	
	ТекстЗапроса = ТекстЗапроса + "
		|
		|ИНДЕКСИРОВАТЬ ПО
		|	ИдентификаторПубликации,
		|	КодНоменклатуры";
	
	Тексты.Добавить(ТекстЗапроса);
	
#КонецОбласти
	
	Для Каждого ИмяУдаляемойТаблицы Из МассивУничтожаемыхТаблиц Цикл
		Тексты.Добавить("УНИЧТОЖИТЬ " + ИмяУдаляемойТаблицы);
	КонецЦикла;
	
	ТекстЗапроса =
		"ВЫБРАТЬ
		|	ВариантыСопоставления.НоменклатураПубликации КАК НоменклатураПубликации,
		|	ВариантыСопоставления.ИдентификаторПубликации КАК ИдентификаторПубликации,
		|	ВариантыСопоставления.КодНоменклатуры КАК КодНоменклатуры,
		|	ВариантыСопоставления.ШтрихкодКомбинации КАК ШтрихкодКомбинации,
		|	ВариантыСопоставления.НоменклатураИБ КАК НоменклатураИБ,
		|	ВариантыСопоставления.ХарактеристикаИБ КАК ХарактеристикаИБ,
		|	ВариантыСопоставления.УпаковкаИБ КАК УпаковкаИБ,
		|	ВариантыСопоставления.Приоритет КАК Приоритет
		|ИЗ
		|	ВариантыСопоставления КАК ВариантыСопоставления
		|
		|УПОРЯДОЧИТЬ ПО
		|	ИдентификаторПубликации,
		|	КодНоменклатуры,
		|	Приоритет";
	
	Тексты.Добавить(ТекстЗапроса);
	
	ТекстЗапроса = СтрСоединить(Тексты, ОбщегоНазначения.РазделительПакетаЗапросов());
	
	Запрос       = Новый Запрос;
	Запрос.Текст = ТекстЗапроса;
	
	Запрос.УстановитьПараметр("ТаблицаНоменклатурыКонтрагентов", ТаблицаНоменклатурыКонтрагентов);
	Запрос.УстановитьПараметр("ПустаяНоменклатураКонтрагентов",  Справочники.НоменклатураКонтрагентов.ПустаяСсылка());
	Запрос.УстановитьПараметр("ПустаяНоменклатура",              Справочники.Номенклатура.ПустаяСсылка());
	Запрос.УстановитьПараметр("ПустаяУпаковка",                  Справочники.УпаковкиЕдиницыИзмерения.ПустаяСсылка());
	Запрос.УстановитьПараметр("УчетнаяЗапись",                   УчетнаяЗапись);
	Запрос.УстановитьПараметр("ВидОбъектаТорговойПлощадки",      Перечисления.ВидыОбъектовМаркетплейсов.Товар);
	Запрос.УстановитьПараметр("Владелец",                        Партнер);
	
	УстановитьПривилегированныйРежим(Истина);
	РезультатЗапроса = Запрос.Выполнить();
	УстановитьПривилегированныйРежим(Ложь);
	
	Возврат РезультатЗапроса.Выбрать();
	
КонецФункции

