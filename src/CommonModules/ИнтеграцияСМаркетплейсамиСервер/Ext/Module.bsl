КонецФункции

// Возвращает результат проверки необходимости импорта.
//
// Параметры:
//   Параметры - см. НовыеПараметрыПроверкиНаличияДанныхИЗапускаИмпортаКаталога
//
// Возвращаемое значение:
//   Структура - результат проверки:
//     * ИспользоватьНоменклатуруПартнеров - Булево - признак использования номенклатуры партнеров.
//     * Партнер                           - СправочникСсылка.Партнеры - партнер,
//                                             представляющий торговую площадку.
//     * ДанныеСуществуют                  - Булево - признак наличия данных в товарном каталоге.
//     * ТребуютСопоставления              - Булево - признак наличия требующих сопоставления данных
//                                             в товарном каталоге.
//     * РучнойИмпортРегламентнымЗаданием  - Булево - признак необходимости запуска ручного импорта
//                                             с помощью регламентного задания.
//     * ЗаданиеАктивно                    - Булево - признак активности фонового задания.
//     * ЗаданиеЗавершено                  - Дата   - дата завершения последнего фонового задания.
//
Функция ПроверитьНаличиеДанныхИЗапускИмпортаКаталога(Параметры) Экспорт

	РеквизитыУчетнойЗаписи =
		ОбщегоНазначения.ЗначенияРеквизитовОбъекта(Параметры.УчетнаяЗапись, "ВидМаркетплейса, Партнер");
	
	Результат = Новый Структура;
	Результат.Вставить("ИспользоватьНоменклатуруПартнеров", ПолучитьФункциональнуюОпцию("ИспользоватьНоменклатуруПартнеров"));
	Результат.Вставить("Партнер",                           РеквизитыУчетнойЗаписи.Партнер);
	Результат.Вставить("ДанныеСуществуют",                  Истина);
	Результат.Вставить("ТребуютСопоставления",              Ложь);
	Результат.Вставить("РучнойИмпортРегламентнымЗаданием",  Ложь);
	Результат.Вставить("ЗаданиеАктивно",                    Ложь);
	Результат.Вставить("ЗаданиеЗавершено",                  Дата(1, 1, 1));
	
	ТекстЗапроса =
		"ВЫБРАТЬ ПЕРВЫЕ 1
		|	СтатусыПубликации.УчетнаяЗапись КАК УчетнаяЗапись
		|ИЗ
		|	&Регистр КАК СтатусыПубликации
		|ГДЕ
		|	СтатусыПубликации.УчетнаяЗапись = &УчетнаяЗапись
		|	И &Условие";
	
	ВидМаркетплейса = РеквизитыУчетнойЗаписи.ВидМаркетплейса;
	Если ВидМаркетплейса = Перечисления.ВидыМаркетплейсов.МаркетплейсЯндексМаркет Тогда
		ТекстЗапроса = СтрЗаменить(ТекстЗапроса, "&Регистр", "РегистрСведений.СтатусыПубликацииТоваровЯндексМаркет");
	ИначеЕсли ВидМаркетплейса = Перечисления.ВидыМаркетплейсов.МаркетплейсOzon Тогда
		ТекстЗапроса = СтрЗаменить(ТекстЗапроса, "&Регистр", "РегистрСведений.СтатусыПубликацииОбъектовМаркетплейсаOzon");
		ТекстЗапроса = СтрЗаменить(ТекстЗапроса, "СтатусыПубликации.УчетнаяЗапись", "СтатусыПубликации.УчетнаяЗаписьМаркетплейса");
	Иначе
		Возврат Результат;
	КонецЕсли;
	
	Запрос = Новый Запрос;
	Запрос.Текст = ТекстЗапроса;
	Запрос.УстановитьПараметр("УчетнаяЗапись", Параметры.УчетнаяЗапись);
	
	УстановитьПривилегированныйРежим(Истина);
	
	Запрос.Текст = СтрЗаменить(ТекстЗапроса,
		"&Условие",
		"СтатусыПубликации.Номенклатура <> ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка)");
	РезультатЗапроса = Запрос.Выполнить();
	Если РезультатЗапроса.Пустой() Тогда
		Результат.ДанныеСуществуют = Ложь;
	КонецЕсли;
	
	Запрос.Текст = СтрЗаменить(ТекстЗапроса,
		"&Условие",
		"СтатусыПубликации.Номенклатура = ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка)");
	РезультатЗапроса = Запрос.Выполнить();
	Если Не РезультатЗапроса.Пустой() Тогда
		Результат.ТребуютСопоставления = Истина;
	КонецЕсли;
	
	ДанныеЗапуска = ДанныеЗапускаИмпортаТоварногоКаталога(Параметры.КлючЗадания);
	
	Результат.ЗаданиеЗавершено                  = ДанныеЗапуска.ДатаЗавершения;
	Результат.РучнойИмпортРегламентнымЗаданием = 
		Не РегламентныеЗаданияСервер.РаботаСВнешнимиРесурсамиЗаблокирована()
			И ОбщегоНазначения.ИнформационнаяБазаФайловая();
	
	ИмяРегламентногоЗадания = ИнтеграцияСМаркетплейсамиКлиентСервер.ИмяРегламентногоЗаданияИмпортаТоварногоКаталога(
		ВидМаркетплейса);
	
	МетаданныеЗадания = Неопределено;
	
	Если Не ПустаяСтрока(ИмяРегламентногоЗадания) Тогда
		МетаданныеЗадания = Метаданные.РегламентныеЗадания[ИмяРегламентногоЗадания];
	Иначе
		Результат.РучнойИмпортРегламентнымЗаданием = Ложь;
	КонецЕсли;
	
	Отбор = Новый Структура;
	Отбор.Вставить("Ключ",      Параметры.КлючЗадания);
	Отбор.Вставить("Состояние", СостояниеФоновогоЗадания.Активно);
	
	Если Результат.РучнойИмпортРегламентнымЗаданием Тогда
		Отбор.Вставить("ИмяМетода", МетаданныеЗадания.ИмяМетода);
	КонецЕсли;
	
	Задания = ФоновыеЗадания.ПолучитьФоновыеЗадания(Отбор);
	
	Если Результат.РучнойИмпортРегламентнымЗаданием И Задания.Количество() = 0 Тогда
		
		Отбор.Ключ = ИнтеграцияСМаркетплейсамиКлиентСервер.КлючЗаданияРучногоИмпортаТоварногоКаталога(
			Параметры.КлючЗадания);
		
		Задания = ФоновыеЗадания.ПолучитьФоновыеЗадания(Отбор);
		
	КонецЕсли;
	
	Если Задания.Количество() > 0 Тогда
		Результат.ЗаданиеАктивно = Истина;
	КонецЕсли;
	
	Если Результат.РучнойИмпортРегламентнымЗаданием И Не Результат.ЗаданиеАктивно Тогда
		
		Результат.ЗаданиеАктивно = ЕстьВключенноеРегламентноеЗаданиеРучногоИмпортаТоварногоКаталога(
			МетаданныеЗадания, Отбор.Ключ, ДанныеЗапуска);
		
	КонецЕсли;
	
	УстановитьПривилегированныйРежим(Ложь);
	
	Возврат Результат;

КонецФункции

// Читает сохраненные данные о запуске импорта товаров из сервиса.
//
// Параметры:
//  КлючНастроек - Строка - ключ регламентного задания импорта товарного каталога.
//
// Возвращаемое значение:
//   Структура:
//    * ДатаСозданияРучногоИмпортаТоварногоКаталога - Дата - дата создания регламентного задания по ручному запуску
//                                                    импорта товарного каталога из сервиса.
//    * ДатаЗавершения - Дата - дата последнего успешного завершения импорта.
//
Функция ДанныеЗапускаИмпортаТоварногоКаталога(КлючНастроек) Экспорт
	
	УстановитьПривилегированныйРежим(Истина);
	
	Результат = Новый Структура;
	
	Результат.Вставить("ДатаСозданияРучногоИмпортаТоварногоКаталога", Дата(1, 1, 1));
	Результат.Вставить("ДатаЗавершения",                              Дата(1, 1, 1));
	
	СохраненныеНастройки = Новый Структура;
	
	СохраненныеНастройки = ОбщегоНазначения.ХранилищеСистемныхНастроекЗагрузить(
		ИнтеграцияСМаркетплейсамиКлиентСервер.КлючОбъектаДанныхЗапускаИмпортаТоварногоКаталога(),
		КлючНастроек, 
		СохраненныеНастройки, ,
		"");
	
	УстановитьПривилегированныйРежим(Ложь);
	
	ЗаполнитьЗначенияСвойств(Результат, СохраненныеНастройки);
	
	Возврат Результат;
	
КонецФункции

