Функция СохранитьДанныеСобытияСЭДО(ДанныеСобытия) Экспорт
	
	Результат = Новый Структура;
	Результат.Вставить("Выполнено", Ложь);
	Результат.Вставить("ОписаниеОшибки", "");
	
	Если ДанныеСобытия.ПривестиВремяКМосковскому Тогда
		ТекущаяДатаСЭДОПоМоскве = МосковскоеВремя(ДанныеСобытия.ДатаСобытия);
	Иначе
		ТекущаяДатаСЭДОПоМоскве = ДанныеСобытия.ДатаСобытия;
	КонецЕсли;
	
	Попытка
		Событие 					= ДанныеСобытия.Событие;
		Запись 						= РегистрыСведений.ЖурналСобытийОбменаСЭДО.СоздатьМенеджерЗаписи();
		Запись.Организация			= ДанныеСобытия.Организация;
		Запись.Событие				= Событие;
		Запись.ОбменЧерезОператора	= ДанныеСобытия.ОбменЧерезОператора;
		Запись.Прочитать();
		Если ДанныеСобытия.СохранятьЕслиДатаМаксимальная
			И Запись.ДатаСобытия >= ТекущаяДатаСЭДОПоМоскве Тогда
			Результат.Выполнено = Истина;
			Возврат Результат;
		КонецЕсли;
		Запись.Организация			= ДанныеСобытия.Организация;
		Запись.Событие				= Событие;
		Запись.ОбменЧерезОператора	= ДанныеСобытия.ОбменЧерезОператора;
		Запись.ИдентификаторЗапроса	= ДанныеСобытия.ИдентификаторЗапроса;
		Запись.ДатаСобытия			= ТекущаяДатаСЭДОПоМоскве;
		Запись.Записать();
		Результат.Выполнено = Истина;
	Исключение
		ИнформацияОбОшибке = ИнформацияОбОшибке();
		Шаблон = НСтр("ru = 'Ошибка при сохранении данных события %1:';
						|en = 'Ошибка при сохранении данных события %1:'");
		ЗаголовокОшибки = СтрШаблон(Шаблон, Событие);
		ОписаниеОшибки = ЗаголовокОшибки + Символы.ПС + КраткоеПредставлениеОшибки(ИнформацияОбОшибке);
		Результат.ОписаниеОшибки = ОписаниеОшибки;
		// Запись в журнал.
		Шаблон = НСтр("ru = 'Ошибка при сохранении данных события %1 по организации %2:';
						|en = 'Ошибка при сохранении данных события %1 по организации %2:'");
		ЗаголовокОшибки = СтрШаблон(Шаблон, Событие, ДанныеСобытия.Организация);
		ОписаниеОшибкиЖурнал = ЗаголовокОшибки + Символы.ПС + ПодробноеПредставлениеОшибки(ИнформацияОбОшибке);
		ЗаписатьСобытиеВЖурнал(УровеньЖурналаРегистрации.Ошибка, ОписаниеОшибкиЖурнал);
		Если ДанныеСобытия.ВызватьИсключениеПриОшибкеЗаписи Тогда
			ВызватьИсключение(ОписаниеОшибки);
		КонецЕсли;
	КонецПопытки;
		
	Возврат Результат;
	
КонецФункции

Функция ДатаСобытияСЭДО(Организация, Событие, ОбменЧерезОператора = Неопределено) Экспорт
	
	ДатаСобытия = Дата(1, 1, 1);
	
	Запрос = Новый Запрос;
	
	Запрос.Текст = 
    "ВЫБРАТЬ РАЗРЕШЕННЫЕ ПЕРВЫЕ 1
    |	ЖурналСобытийОбменаСЭДО.ДатаСобытия КАК ДатаСобытия
    |ИЗ
    |	РегистрСведений.ЖурналСобытийОбменаСЭДО КАК ЖурналСобытийОбменаСЭДО
    |ГДЕ
    |	ЖурналСобытийОбменаСЭДО.Организация = &Организация
	|	И ЖурналСобытийОбменаСЭДО.Событие = &Событие
    |	И &УсловиеНаОбменЧерезОператора
    |
    |УПОРЯДОЧИТЬ ПО
    |	ЖурналСобытийОбменаСЭДО.ДатаСобытия УБЫВ";
	
	Если ОбменЧерезОператора = Неопределено Тогда
		Запрос.Текст = СтрЗаменить(Запрос.Текст,
			"&УсловиеНаОбменЧерезОператора",
			"ЖурналСобытийОбменаСЭДО.ОбменЧерезОператора = &ОбменЧерезОператора");
	КонецЕсли;
	
	Запрос.УстановитьПараметр("Организация", 					Организация);
	Запрос.УстановитьПараметр("УсловиеНаОбменЧерезОператора", 	Истина);
	Запрос.УстановитьПараметр("ОбменЧерезОператора", 			ОбменЧерезОператора = Истина);
	Запрос.УстановитьПараметр("Событие", 						Событие);
	
	УстановитьПривилегированныйРежим(Истина);
	Результат = Запрос.Выполнить();
	УстановитьПривилегированныйРежим(Ложь);
	
	Если Результат.Пустой() Тогда
		Возврат ДатаСобытия;
	КонецЕсли;
	
	Выборка = Результат.Выбрать();
	Выборка.Следующий();
	ДатаСобытия = Выборка.ДатаСобытия;
	
	Возврат ДатаСобытия;
	
КонецФункции

Функция НовыеДанныеСобытияСЭДО() Экспорт
	
	Результат = Новый Структура;
	ТекущаяДатаПоМоскве = МосковскоеВремя();
	// Данные регистра.
	Результат.Вставить("Организация", Неопределено);
	Результат.Вставить("Событие", Неопределено);
	Результат.Вставить("ОбменЧерезОператора", Ложь);
	// По умолчанию сохраняется московское время. 
	Результат.Вставить("ДатаСобытия", ТекущаяДатаПоМоскве);
	Результат.Вставить("ИдентификаторЗапроса", "");
	// Параметры сохранения.
	Результат.Вставить("ПривестиВремяКМосковскому", Ложь);
	Результат.Вставить("СохранятьЕслиДатаМаксимальная", Ложь);
	Результат.Вставить("ВызватьИсключениеПриОшибкеЗаписи", Ложь);
	
	Возврат Результат;
	
КонецФункции

Функция КодПодчиненностиФССОрганизации(Организация) Экспорт
	
	СведенияОбОрганизации = РегламентированнаяОтчетностьВызовСервера.ПолучитьСведенияОбОрганизации(
		Организация, ,
		"РегистрационныйНомерФСС, КодПодчиненностиФСС, ИННФЛ, ТипНП");
		
	КодПодчиненности = СведенияОбОрганизации.КодПодчиненностиФСС;
	Если ПустаяСтрока(КодПодчиненности) Тогда
		КодПодчиненности = ПолучитьКодПодчиненностиФСС(СведенияОбОрганизации, СведенияОбОрганизации.РегистрационныйНомерФСС);
	КонецЕсли;
	
	Возврат КодПодчиненности;
	
КонецФункции

