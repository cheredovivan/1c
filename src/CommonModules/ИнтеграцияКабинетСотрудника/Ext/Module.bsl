Процедура ЗарегистрироватьБудущиеСобытияВычетыНДФЛ(ПараметрыОбновления = Неопределено) Экспорт
	
	Если Не ПолучитьФункциональнуюОпцию("ИспользуетсяСервисКабинетСотрудника") Тогда
		ОбновлениеИнформационнойБазыЗарплатаКадрыБазовый.УстановитьПараметрОбновления(ПараметрыОбновления, "ОбработкаЗавершена", Истина);
		Возврат;
	КонецЕсли;
	
	ДатаАктуальности = ТекущаяДатаСеанса();
	НалоговыйПериод = Год(ДатаАктуальности);
	
	Запрос = Новый Запрос;
	Запрос.УстановитьПараметр("НалоговыйПериод", НалоговыйПериод);
	Запрос.УстановитьПараметр("ДатаАктуальности", ДатаАктуальности);
	Запрос.Текст = 
	"ВЫБРАТЬ
	|	ФизическиеЛица.Ссылка КАК Ссылка
	|ПОМЕСТИТЬ ВТФизическиеЛица
	|ИЗ
	|	РегистрСведений.ВыгружаемыеОбъектыКабинетСотрудника КАК ФизическиеЛица
	|		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.БудущиеСобытияУправлениеПерсоналом КАК БудущиеСобытия
	|		ПО ФизическиеЛица.Ссылка = БудущиеСобытия.Объект
	|			И (БудущиеСобытия.Приложение = ЗНАЧЕНИЕ(Перечисление.ПриложенияДляИнтеграции.КабинетСотрудника))
	|			И (БудущиеСобытия.Событие = ЗНАЧЕНИЕ(Перечисление.ВидыБудущихСобытийУправлениеПерсоналом.ВычетыНДФЛ))
	|ГДЕ
	|	ФизическиеЛица.Ссылка ССЫЛКА Справочник.ФизическиеЛица
	|	И БудущиеСобытия.ДатаСобытия ЕСТЬ NULL
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	Таблица.ФизическоеЛицо КАК ФизическоеЛицо,
	|	Таблица.ДатаНачала КАК ДатаНачала
	|ИЗ
	|	(ВЫБРАТЬ
	|		Таблица.Сотрудник КАК ФизическоеЛицо,
	|		Таблица.ПрименятьВычетыС КАК ДатаНачала
	|	ИЗ
	|		Документ.УведомлениеОПравеНаИмущественныйВычетДляНДФЛ КАК Таблица
	|			ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТФизическиеЛица КАК ФизическиеЛица
	|			ПО Таблица.Сотрудник = ФизическиеЛица.Ссылка
	|	ГДЕ
	|		Таблица.НалоговыйПериод >= &НалоговыйПериод
	|		И Таблица.Проведен
	|	
	|	ОБЪЕДИНИТЬ ВСЕ
	|	
	|	ВЫБРАТЬ
	|		Таблица.Сотрудник,
	|		Таблица.ПрименятьВычетыС
	|	ИЗ
	|		Документ.ЕжегодныеСтандартныеВычеты КАК Таблица
	|			ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТФизическиеЛица КАК ФизическиеЛица
	|			ПО Таблица.Сотрудник = ФизическиеЛица.Ссылка
	|	ГДЕ
	|		Таблица.НалоговыйПериод >= &НалоговыйПериод
	|		И Таблица.Проведен) КАК Таблица
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	Вычеты.ФизическоеЛицо КАК ФизическоеЛицо,
	|	Вычеты.МесяцРегистрации КАК ДатаНачала,
	|	Вычеты.ДействуетДо КАК ДатаОкончания
	|ИЗ
	|	РегистрСведений.СтандартныеВычетыНаДетейНДФЛ КАК Вычеты
	|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТФизическиеЛица КАК ФизическиеЛица
	|		ПО Вычеты.ФизическоеЛицо = ФизическиеЛица.Ссылка
	|ГДЕ
	|	(Вычеты.ДействуетДо >= &ДатаАктуальности
	|			ИЛИ Вычеты.МесяцРегистрации >= &ДатаАктуальности)";
	РезультатЗапроса = Запрос.ВыполнитьПакет();
	
	Если РезультатЗапроса[1].Пустой() И РезультатЗапроса[2].Пустой() Тогда
		ОбновлениеИнформационнойБазыЗарплатаКадрыБазовый.УстановитьПараметрОбновления(ПараметрыОбновления, "ОбработкаЗавершена", Истина);
		Возврат;
	КонецЕсли;
	
	БудущиеСобытияВычеты = ОбщегоНазначенияБЗК.ТаблицаЗначенийПоИмениРегистраСведений("БудущиеСобытияУправлениеПерсоналом");
	ДлинаСуток = 86400;
	Событие = Перечисления.ВидыБудущихСобытийУправлениеПерсоналом.ВычетыНДФЛ;
	Приложение = Перечисления.ПриложенияДляИнтеграции.КабинетСотрудника;
	
	БудущиеСобытияФизическихЛиц = Новый Соответствие;
	
	ДанныеВычетов = РезультатЗапроса[1].Выгрузить();
	Для каждого СтрокаТЗ Из ДанныеВычетов Цикл
		
		БудущиеСобытия = БудущиеСобытияФизическихЛиц[СтрокаТЗ.ФизическоеЛицо];
		Если БудущиеСобытия = Неопределено Тогда
			БудущиеСобытия = БудущиеСобытияВычеты.СкопироватьКолонки();
		КонецЕсли;
		
		Если СтрокаТЗ.ДатаНачала <= ДатаАктуальности Тогда
			НоваяСтрока = БудущиеСобытия.Добавить();
			НоваяСтрока.Объект 		= СтрокаТЗ.ФизическоеЛицо;
			НоваяСтрока.ДатаСобытия = КонецГода(ДатаАктуальности) + ДлинаСуток;
			НоваяСтрока.Событие 	= Событие;
			НоваяСтрока.Приложение 	= Приложение;
		Иначе
			НоваяСтрока = БудущиеСобытия.Добавить();
			НоваяСтрока.Объект 		= СтрокаТЗ.ФизическоеЛицо;
			НоваяСтрока.ДатаСобытия = СтрокаТЗ.ДатаНачала;
			НоваяСтрока.Событие 	= Событие;
			НоваяСтрока.Приложение 	= Приложение;
			НоваяСтрока = БудущиеСобытия.Добавить();
			НоваяСтрока.Объект 		= СтрокаТЗ.ФизическоеЛицо;
			НоваяСтрока.ДатаСобытия = КонецГода(ДатаАктуальности)+ ДлинаСуток;
			НоваяСтрока.Событие 	= Событие;
			НоваяСтрока.Приложение 	= Приложение;
		КонецЕсли;
		
		БудущиеСобытияФизическихЛиц.Вставить(СтрокаТЗ.ФизическоеЛицо, БудущиеСобытия);
		
	КонецЦикла;
	
	ДанныеВычетов = РезультатЗапроса[2].Выгрузить();
	Для каждого СтрокаТЗ Из ДанныеВычетов Цикл
		
		БудущиеСобытия = БудущиеСобытияФизическихЛиц[СтрокаТЗ.ФизическоеЛицо];
		Если БудущиеСобытия = Неопределено Тогда
			БудущиеСобытия = БудущиеСобытияВычеты.СкопироватьКолонки();
		КонецЕсли;
		
		Если СтрокаТЗ.ДатаНачала > ДатаАктуальности Тогда
			НоваяСтрока = БудущиеСобытия.Добавить();
			НоваяСтрока.Объект 		= СтрокаТЗ.ФизическоеЛицо;
			НоваяСтрока.ДатаСобытия = СтрокаТЗ.ДатаНачала;
			НоваяСтрока.Событие 	= Событие;
			НоваяСтрока.Приложение 	= Приложение;
		КонецЕсли;
		Если СтрокаТЗ.ДатаОкончания > ДатаАктуальности Тогда
			НоваяСтрока = БудущиеСобытия.Добавить();
			НоваяСтрока.Объект 		= СтрокаТЗ.ФизическоеЛицо;
			НоваяСтрока.ДатаСобытия = ДобавитьМесяц(СтрокаТЗ.ДатаОкончания, 1);
			НоваяСтрока.Событие 	= Событие;
			НоваяСтрока.Приложение 	= Приложение;
		КонецЕсли;
		
		БудущиеСобытияФизическихЛиц.Вставить(СтрокаТЗ.ФизическоеЛицо, БудущиеСобытия);
		
	КонецЦикла;
	
	ОбработкаЗавершена = Истина;
	Для каждого КлючИЗначение Из БудущиеСобытияФизическихЛиц Цикл
		
		ФизическоеЛицо = КлючИЗначение.Ключ;
		БудущиеСобытия = КлючИЗначение.Значение;
		БудущиеСобытия.Свернуть("Приложение,Объект,Событие,ДатаСобытия");
		
		Если Не ОбновлениеИнформационнойБазыЗарплатаКадрыБазовый.ПодготовитьОбновлениеДанных(ПараметрыОбновления, "РегистрСведений.БудущиеСобытияУправлениеПерсоналом", "Объект", ФизическоеЛицо) Тогда
			ОбработкаЗавершена = Ложь;
			Продолжить;
		Иначе
			
			НаборЗаписей = РегистрыСведений.БудущиеСобытияУправлениеПерсоналом.СоздатьНаборЗаписей();
			НаборЗаписей.Отбор.Приложение.Установить(Приложение);
			НаборЗаписей.Отбор.Событие.Установить(Событие);
			НаборЗаписей.Отбор.Объект.Установить(ФизическоеЛицо);
			
			Для каждого СтрокаТЗ Из БудущиеСобытия Цикл
				ЗаполнитьЗначенияСвойств(НаборЗаписей.Добавить(), СтрокаТЗ);
			КонецЦикла;
			
			ОбновлениеИнформационнойБазы.ЗаписатьНаборЗаписей(НаборЗаписей);
			ОбновлениеИнформационнойБазыЗарплатаКадрыБазовый.ЗавершитьОбновлениеДанных(ПараметрыОбновления);
			
		КонецЕсли;
		
	КонецЦикла;
	
	ОбновлениеИнформационнойБазыЗарплатаКадрыБазовый.УстановитьПараметрОбновления(ПараметрыОбновления, "ОбработкаЗавершена", ОбработкаЗавершена);

КонецПроцедуры

Процедура ЗарегистрироватьОбъектыВыгружаемыеПоСсылкам(ПараметрыОбновления = Неопределено) Экспорт
	
	Если Не ПолучитьФункциональнуюОпцию("ИспользуетсяСервисКабинетСотрудника") Тогда
		ОбновлениеИнформационнойБазыЗарплатаКадрыБазовый.УстановитьПараметрОбновления(ПараметрыОбновления, "ОбработкаЗавершена", Истина);
		Возврат;
	КонецЕсли;
	
	Приложение = Перечисления.ПриложенияДляИнтеграции.КабинетСотрудника;
	Организации = ИнтеграцияУправлениеПерсоналом.ВыгружаемыеОрганизации(Приложение);
	Запрос = Новый Запрос;
	Запрос.УстановитьПараметр("Организации", Организации);
	Запрос.Текст = 
	"ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	ТекущиеРуководители.Руководитель КАК Ссылка
	|ПОМЕСТИТЬ ВТВыгружатьПоСсылкам
	|ИЗ
	|	РегистрСведений.ТекущиеРуководителиКабинетСотрудника КАК ТекущиеРуководители
	|ГДЕ
	|	ТекущиеРуководители.СтруктурнаяЕдиница В(&Организации)
	|	И ТекущиеРуководители.Руководитель <> ЗНАЧЕНИЕ(Справочник.ФизическиеЛица.ПустаяСсылка)
	|
	|ОБЪЕДИНИТЬ ВСЕ
	|
	|ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	ТекущиеРуководители.Должность
	|ИЗ
	|	РегистрСведений.ТекущиеРуководителиКабинетСотрудника КАК ТекущиеРуководители
	|ГДЕ
	|	ТекущиеРуководители.СтруктурнаяЕдиница В(&Организации)
	|	И ТекущиеРуководители.Должность <> ЗНАЧЕНИЕ(Справочник.Должности.ПустаяСсылка)
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ВыгружатьПоСсылкам.Ссылка КАК Ссылка
	|ИЗ
	|	ВТВыгружатьПоСсылкам КАК ВыгружатьПоСсылкам
	|		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ВыгружаемыеОбъектыКабинетСотрудника КАК ВыгружаемыеОбъекты
	|		ПО ВыгружатьПоСсылкам.Ссылка = ВыгружаемыеОбъекты.Ссылка
	|ГДЕ
	|	ВыгружаемыеОбъекты.УсловноВыгружается ЕСТЬ NULL";
	РезультатЗапроса = Запрос.Выполнить();
	
	Если РезультатЗапроса.Пустой() Тогда
		ОбновлениеИнформационнойБазыЗарплатаКадрыБазовый.УстановитьПараметрОбновления(ПараметрыОбновления, "ОбработкаЗавершена", Истина);
		Возврат;
	КонецЕсли;
	
	Выгрузка 	= НоваяТаблицаВыгружаемыеОбъекты();
	Изменения 	= НоваяТаблицаИзмененияДляОбмена();
	
	ТипЗначенияТипОбъекта = Новый Соответствие;
	ТипЗначенияТипОбъекта.Вставить(Тип("СправочникСсылка.ФизическиеЛица"), 	Перечисления.ТипыОбъектовИнтеграцияУправлениеПерсоналом.ФизическоеЛицо);
	ТипЗначенияТипОбъекта.Вставить(Тип("СправочникСсылка.Должности"), 		Перечисления.ТипыОбъектовИнтеграцияУправлениеПерсоналом.Должность);
	
	ОбработкаЗавершена = Истина;
	Выборка = РезультатЗапроса.Выбрать();
	Пока Выборка.Следующий() Цикл
		
		ТипОбъекта = ТипЗначенияТипОбъекта[ТипЗнч(Выборка.Ссылка)];
		
		НачатьТранзакцию();
		Попытка
			
			Если ПараметрыОбновления <> Неопределено Тогда
				Блокировка = Новый БлокировкаДанных;
				ЭлементБлокировки = Блокировка.Добавить("РегистрСведений.ВыгружаемыеОбъектыКабинетСотрудника");
				ЭлементБлокировки.УстановитьЗначение("Ссылка", Выборка.Ссылка);
				ЭлементБлокировки = Блокировка.Добавить("РегистрСведений.ИзмененияДляОбменаКабинетСотрудника");
				ЭлементБлокировки.УстановитьЗначение("Ссылка", Выборка.Ссылка);
				ЭлементБлокировки.УстановитьЗначение("ТипОбъекта", ТипОбъекта);
				Блокировка.Заблокировать();
			КонецЕсли;
			
			НаборЗаписей = РегистрыСведений.ВыгружаемыеОбъектыКабинетСотрудника.СоздатьНаборЗаписей();
			НаборЗаписей.Отбор.Ссылка.Установить(Выборка.Ссылка);
			ЗаписьНабора = НаборЗаписей.Добавить();
			ЗаписьНабора.Ссылка 				= Выборка.Ссылка;
			ЗаписьНабора.ВыгружаетсяПоСсылке 	= Истина;
			ОбновлениеИнформационнойБазы.ЗаписатьНаборЗаписей(НаборЗаписей);
			
			НаборЗаписей = РегистрыСведений.ИзмененияДляОбменаКабинетСотрудника.СоздатьНаборЗаписей();
			НаборЗаписей.Отбор.Ссылка.Установить(Выборка.Ссылка);
			ЗаписьНабора = НаборЗаписей.Добавить();
			ЗаписьНабора.Ссылка 		= Выборка.Ссылка;
			ЗаписьНабора.ТипОбъекта 	= ТипОбъекта;
			ЗаписьНабора.ВерсияДанных 	= Строка(Новый УникальныйИдентификатор);
			ОбновлениеИнформационнойБазы.ЗаписатьНаборЗаписей(НаборЗаписей);
			
			ЗафиксироватьТранзакцию();
			
		Исключение
			ОтменитьТранзакцию();
			ОбработкаЗавершена = Ложь;
		КонецПопытки;
		
	КонецЦикла;
	
	ОбновлениеИнформационнойБазыЗарплатаКадрыБазовый.УстановитьПараметрОбновления(ПараметрыОбновления, "ОбработкаЗавершена", ОбработкаЗавершена);

КонецПроцедуры

Процедура ОбновитьНастройкиФункциональности() Экспорт

	Если Не ПолучитьФункциональнуюОпцию("ИспользуетсяСервисКабинетСотрудника") Тогда
		Возврат;
	КонецЕсли;
	
	Запрос = Новый Запрос;
	Запрос.Текст = 
	"ВЫБРАТЬ
	|	НастройкиКабинетСотрудника.РазделЗарплата КАК РазделЗарплата
	|ИЗ
	|	РегистрСведений.НастройкиКабинетСотрудника КАК НастройкиКабинетСотрудника";
	РезультатЗапрос = Запрос.Выполнить();
	Если Не РезультатЗапрос.Пустой() Тогда
		Возврат;
	КонецЕсли;
	
	Запрос.Текст = 
	"ВЫБРАТЬ
	|	Настройки.ЗаявленияНаДСВ КАК ЗаявленияНаДСВ,
	|	Настройки.СпособРасчетаДСВПроцентом КАК СпособРасчетаДСВПроцентом,
	|	Настройки.СпособРасчетаДСВСуммой КАК СпособРасчетаДСВСуммой,
	|	Настройки.СпособРасчетаДСВПроцентомНеБолееСуммы КАК СпособРасчетаДСВПроцентомНеБолееСуммы,
	|	Настройки.СкрытьРазделКомпания КАК СкрытьРазделКомпания,
	|	Настройки.СкрытьРазделСправки КАК СкрытьРазделСправки,
	|	Настройки.СкрытьРазделДокументы КАК СкрытьРазделДокументы,
	|	Настройки.КомпенсацияОтпускаНедоступна КАК КомпенсацияОтпускаНедоступна,
	|	Настройки.ПолучениеДокументаСЭПНедоступно КАК ПолучениеДокументаСЭПНедоступно,
	|	Настройки.УровеньДоступаКИ КАК УровеньДоступаКИ,
	|	Настройки.ИнформацияОбОстаткахОтпусковНедоступна КАК ИнформацияОбОстаткахОтпусковНедоступна,
	|	Настройки.ДоступенОтказОтПодписания КАК ДоступенОтказОтПодписания,
	|	Настройки.ИспользоватьЗаявленияНаПереносОтпуска КАК ИспользоватьЗаявленияНаПереносОтпуска,
	|	Настройки.ИспользоватьЗаявленияНаОтменуОтпуска КАК ИспользоватьЗаявленияНаОтменуОтпуска,
	|	Настройки.ПоказыватьОхрануТруда КАК ПоказыватьОхрануТруда
	|ИЗ
	|	РегистрСведений.УдалитьИспользуемаяФункциональностьСервисаКабинетСотрудника КАК Настройки";
	Настройки = Запрос.Выполнить().Выгрузить();
	
	НаборЗаписей = РегистрыСведений.НастройкиКабинетСотрудника.СоздатьНаборЗаписей();
	ЗаписьНабора = НаборЗаписей.Добавить();
	ЗаполнитьЗначенияСвойств(ЗаписьНабора, НастройкиСервисаПоУмолчанию());
	
	Если Настройки.Количество() > 0 Тогда
		
		СтарыеНастройки = Настройки[0];
		
		ЗаписьНабора.ЗаявленияНаДСВ 						= СтарыеНастройки.ЗаявленияНаДСВ;
		ЗаписьНабора.СпособРасчетаДСВПроцентом 				= СтарыеНастройки.СпособРасчетаДСВПроцентом;
		ЗаписьНабора.СпособРасчетаДСВСуммой 				= СтарыеНастройки.СпособРасчетаДСВСуммой;
		ЗаписьНабора.СпособРасчетаДСВПроцентомНеБолееСуммы 	= СтарыеНастройки.СпособРасчетаДСВПроцентомНеБолееСуммы;
		ЗаписьНабора.РазделКомпания 						= Не СтарыеНастройки.СкрытьРазделКомпания;
		ЗаписьНабора.РазделСправки 							= Не СтарыеНастройки.СкрытьРазделСправки;
		ЗаписьНабора.РазделДокументы 						= Не СтарыеНастройки.СкрытьРазделДокументы;
		ЗаписьНабора.КомпенсацияОтпуска 					= Не СтарыеНастройки.КомпенсацияОтпускаНедоступна;
		ЗаписьНабора.ДоступноПолучениеДокументаСЭП 			= Не СтарыеНастройки.ПолучениеДокументаСЭПНедоступно;
		ЗаписьНабора.УровеньДоступаКИ 						= СтарыеНастройки.УровеньДоступаКИ;
		ЗаписьНабора.ПоказыватьОстаткиОтпусков 				= Не СтарыеНастройки.ИнформацияОбОстаткахОтпусковНедоступна;
		ЗаписьНабора.ДоступенОтказОтПодписания 				= СтарыеНастройки.ДоступенОтказОтПодписания;
		ЗаписьНабора.ИспользоватьЗаявленияНаПереносОтпуска 	= СтарыеНастройки.ИспользоватьЗаявленияНаПереносОтпуска;
		ЗаписьНабора.ИспользоватьЗаявленияНаОтменуОтпуска 	= СтарыеНастройки.ИспользоватьЗаявленияНаОтменуОтпуска;
		ЗаписьНабора.ПоказыватьОхрануТруда 					= СтарыеНастройки.ПоказыватьОхрануТруда;
		
	КонецЕсли;
	
	ОбновлениеИнформационнойБазы.ЗаписатьНаборЗаписей(НаборЗаписей);

КонецПроцедуры

#КонецОбласти

