	НастройкиФункциональности.Вставить("ДоступноИзменениеФото", 				Ложь);
	//
	НастройкиФункциональности.Вставить("ДоступноПолучениеФайлаДляПечати", 				Истина); // Всегда включено.
	НастройкиФункциональности.Вставить("ДоступноПолучениеСканаОригинала", 				Ложь);
	НастройкиФункциональности.Вставить("ДоступноПолучениеДокументаСЭП", 				Ложь);
	НастройкиФункциональности.Вставить("ДоступноПолучениеДокументаВБумажномВиде", 		Ложь);
	НастройкиФункциональности.Вставить("ДоступноПолучениеКопииНаЭлектроннуюПочту", 		Ложь);
	//
	НастройкиФункциональности.Вставить("ЗаявленияНаВычетыНДФЛ", 						Ложь);
	НастройкиФункциональности.Вставить("ЗаявленияНаДСВ", 								Ложь);
	НастройкиФункциональности.Вставить("СпособРасчетаДСВПроцентом", 					Ложь);
	НастройкиФункциональности.Вставить("СпособРасчетаДСВСуммой", 						Ложь);
	НастройкиФункциональности.Вставить("СпособРасчетаДСВПроцентомНеБолееСуммы", 		Ложь);
	//
	НастройкиФункциональности.Вставить("ПоказыватьОхрануТруда", 						Ложь);
	НастройкиФункциональности.Вставить("ПоказыватьРабочиеМестаОхраныТруда", 			Ложь);
	НастройкиФункциональности.Вставить("ПоказыватьМедицинскиеОсмотры", 					Ложь);
	НастройкиФункциональности.Вставить("ПоказыватьПсихиатрическиеОсвидетельствования", 	Ложь);
	//
	НастройкиФункциональности.Вставить("ИспользоватьПособияПриРожденииРебенка", 		Ложь);
	НастройкиФункциональности.Вставить("ИспользоватьПособияПоУходуЗаРебенком", 			Ложь);
	
	НастройкиФункциональности.Вставить("ДоступенОтказОтПодписания", 					Ложь);
	НастройкиФункциональности.Вставить("ПолучениеСогласияЭДОБЗКНаБумаге", 				Ложь);
	НастройкиФункциональности.Вставить("ПолучениеСогласияЭДОБЗКУНЭП", 					Ложь);
	НастройкиФункциональности.Вставить("ИспользоватьСборДанныхДляОформленияНаРаботу", 	Ложь);
	НастройкиФункциональности.Вставить("ИспользуетсяЭДОБЗК", 							Ложь);
	НастройкиФункциональности.Вставить("ИнтеграцияС1СПерсонал", 						Ложь);
	НастройкиФункциональности.Вставить("ИспользуютсяОбсуждения", 						Ложь);
	
	НастройкиФункциональности.Вставить("АдресСотрудника", 				Истина);
	НастройкиФункциональности.Вставить("ЛичнаяКонтактнаяИнформация", 	Истина);
	
	// Служебные настройки поддержки соответсвующей версии формата, устанавливаем значение Истина.
	// Выгружены будут только те настройки, которые соответсвуют формату DTO.
	НастройкиФункциональности.Вставить("ЗапросМестаРаботыВЗаявлении", 					Истина);
	НастройкиФункциональности.Вставить("РасширенныеРолиСогласования", 					Истина);
	НастройкиФункциональности.Вставить("РасширенныйЗапросНаИзменениеЛичнойИнформации", 	Истина);
	НастройкиФункциональности.Вставить("ОстаткиОтпусковВРазрезеСотрудников", 			Истина);
	НастройкиФункциональности.Вставить("ОформлятьУчебныеОтпускаОтдельнымЗаявлением", 	Истина);
	НастройкиФункциональности.Вставить("ОтпускПоУходуЗаРебенкомОтдельнымЗаявлением", 	Истина);
	НастройкиФункциональности.Вставить("ОтпускПоБеременностиИРодамОтдельнымЗаявлением", Истина);
	НастройкиФункциональности.Вставить("ПодтверждениеСогласиняНаЭДОСканом", 			Истина);
	
	// Заполнение настройками по умолчанию.
	НастройкиПоУмолчанию = НастройкиСервисаПоУмолчанию();
	ЗаполнитьЗначенияСвойств(НастройкиФункциональности, НастройкиПоУмолчанию);
	
	// Определение настроек доступных для изменения пользователем.
	ДоступныеНастройки = ДоступныеНастройкиСервиса();
	ИменаДоступныхНастроек = Новый Массив;
	Для каждого КлючИЗначение Из ДоступныеНастройки Цикл
		Если КлючиЗначение.Значение И НастройкиФункциональности.Свойство(КлючИЗначение.Ключ) Тогда
			ИменаДоступныхНастроек.Добавить(КлючИЗначение.Ключ);
		КонецЕсли;
	КонецЦикла;
	ДоступныеНастройкиСтрока = СтрСоединить(ИменаДоступныхНастроек,",");
	
	// Заполнение сохраненными настройками.
	Настройки = РегистрыСведений.НастройкиКабинетСотрудника.Настройки();
	ЗаполнитьЗначенияСвойств(НастройкиФункциональности, Настройки, ДоступныеНастройкиСтрока);
	
	ИспользуетсяЭДОБЗК = ИспользуетсяЭДОБЗК();
	
	Если Не Настройки.РазделОтпуск Тогда
		НастройкиФункциональности.ПоказыватьГрафикОтпусков = Ложь;
	КонецЕсли;
	
	Если ИспользуетсяЭДОБЗК Тогда
		// Раздел всегда показываем, если используется ЭДО.
		НастройкиФункциональности.РазделДокументы = Истина;
	КонецЕсли;
	
	// Заполнение настроек
	НастройкиФункциональности.ИспользуютсяОбсуждения = ПолучитьФункциональнуюОпцию("ИспользуютсяОбсужденияКабинетСотрудника");
	НастройкиФункциональности.ИспользуетсяЭДОБЗК = ИспользуетсяЭДОБЗК;
	НастройкиФункциональности.ИспользоватьСборДанныхДляОформленияНаРаботу = ИспользуетсяСборДанныхДляОформленияНаРаботу();
	
	НастройкиФункциональности.ДоступноПолучениеДокументаСЭП = ПолучитьФункциональнуюОпцию("ИспользоватьЭлектронныеПодписи")
		И НастройкиФункциональности.ДоступноПолучениеДокументаСЭП;
	НастройкиФункциональности.ДоступноПолучениеКопииНаЭлектроннуюПочту = СистемнаяУчетнаяЗаписьНастроена()
		И НастройкиФункциональности.ДоступноПолучениеКопииНаЭлектроннуюПочту;
		
	СпособПолученияСогласия = СпособПолученияСогласияНаПрисоединениеКЭДОБЗК();
	НастройкиФункциональности.ПолучениеСогласияЭДОБЗКНаБумаге = (СпособПолученияСогласия = Перечисления.СпособыПолученияСогласияНаПрисоединениеКЭДОБЗК.БумажныйДокумент);
	НастройкиФункциональности.ПолучениеСогласияЭДОБЗКУНЭП = (СпособПолученияСогласия = Перечисления.СпособыПолученияСогласияНаПрисоединениеКЭДОБЗК.УНЭП);	
	
	// Получаем значение константы, а не ФО.
	НастройкиФункциональности.ИнтеграцияС1СПерсонал = Константы.ИспользуетсяПриложение1СПерсонал.Получить();
	
	Если ИнтеграцияУправлениеПерсоналом.ДоступенРасширенныйФункционал() Тогда
		
		НастройкиРасчетаЗарплаты = ИнтеграцияКабинетСотрудникаВнутренний.НастройкиРасчетаЗарплаты();
		
		НастройкиФункциональности.Командировка 		= НастройкиРасчетаЗарплаты.ИспользоватьОплатуКомандировок И НастройкиФункциональности.Командировка;
		НастройкиФункциональности.Отгул 			= НастройкиРасчетаЗарплаты.ИспользоватьОтгулы И НастройкиФункциональности.Отгул;
		НастройкиФункциональности.ЗаявлениеУчебныйОтпуск 	= НастройкиРасчетаЗарплаты.ИспользоватьОтпускаУчебные И НастройкиФункциональности.ЗаявлениеУчебныйОтпуск;
		НастройкиФункциональности.ЗаявлениеОтпускБезОплаты 	= НастройкиРасчетаЗарплаты.ИспользоватьОтпускаБезОплаты И НастройкиФункциональности.ЗаявлениеОтпускБезОплаты;
		НастройкиФункциональности.ЕдиновременнаяВыплатаКОтпуску = НастройкиРасчетаЗарплаты.ИспользоватьЕдиновременнуюВыплатуКОтпуску И НастройкиФункциональности.ЕдиновременнаяВыплатаКОтпуску;
		НастройкиФункциональности.МатериальнаяПомощьКОтпуску 	= НастройкиРасчетаЗарплаты.ИспользоватьМатериальнуюПомощьПриОтпуске И НастройкиФункциональности.МатериальнаяПомощьКОтпуску;
		
		Если ИспользуетсяВерсияDTO("2.3") Тогда
			НастройкиФункциональности.ПоказыватьОстаткиОтпусков = Настройки.ПоказыватьОстаткиОтпусков;
		Иначе
			НастройкиФункциональности.РазделОтпуск = Настройки.ПоказыватьОстаткиОтпусков;
		КонецЕсли;
		
		НастройкиФункциональности.ПоказыватьОхрануТруда 			= ПоказыватьОхрануТруда() И НастройкиФункциональности.ПоказыватьОхрануТруда;
		НастройкиФункциональности.ПоказыватьРабочиеМестаОхраныТруда = ИспользоватьРабочиеМестаОхраныТруда() И НастройкиФункциональности.ПоказыватьРабочиеМестаОхраныТруда;
		НастройкиФункциональности.ПоказыватьМедицинскиеОсмотры 		= ИспользоватьМедицинскиеОсмотры() И НастройкиФункциональности.ПоказыватьМедицинскиеОсмотры;
		НастройкиФункциональности.ПоказыватьПсихиатрическиеОсвидетельствования 	= ИспользоватьПсихиатрическиеОсвидетельствования() И НастройкиФункциональности.ПоказыватьПсихиатрическиеОсвидетельствования;
		
	Иначе
		
		ДоступенРасчетЗарплаты = ИнтеграцияУправлениеПерсоналом.ДоступенРасчетЗарплатыДляНебольшихОрганизаций();
		НастройкиФункциональности.РазделОтсутствия 				= ДоступенРасчетЗарплаты И НастройкиФункциональности.РазделОтсутствия;
		НастройкиФункциональности.ЗаявлениеНаОтпуск 			= ДоступенРасчетЗарплаты И НастройкиФункциональности.ЗаявлениеНаОтпуск;
		НастройкиФункциональности.ЗаявлениеОтпускБезОплаты 		= ДоступенРасчетЗарплаты И НастройкиФункциональности.ЗаявлениеОтпускБезОплаты;
		НастройкиФункциональности.Болезнь 						= ДоступенРасчетЗарплаты И НастройкиФункциональности.Болезнь;
		НастройкиФункциональности.ОтпускПоБеременностиИРодам 	= ДоступенРасчетЗарплаты И НастройкиФункциональности.ОтпускПоБеременностиИРодам;
		
		НастройкиФункциональности.ИспользоватьЗаявленияНаОтменуОтпуска 	= ДоступенРасчетЗарплаты И НастройкиФункциональности.ИспользоватьЗаявленияНаОтменуОтпуска;
		НастройкиФункциональности.ИспользоватьЗаявленияНаПереносОтпуска = ДоступенРасчетЗарплаты И НастройкиФункциональности.ИспользоватьЗаявленияНаПереносОтпуска;
		
	КонецЕсли;
	
	Возврат НастройкиФункциональности;
	
КонецФункции

Функция ДоступныеНастройкиСервиса() Экспорт

	Настройки = ОбщегоНазначения.СтруктураПоМенеджеруЗаписи(
				РегистрыСведений.НастройкиКабинетСотрудника.СоздатьМенеджерЗаписи(),
				Метаданные.РегистрыСведений.НастройкиКабинетСотрудника);
	//
	ДоступенРасширенныйФункционал = ИнтеграцияУправлениеПерсоналом.ДоступенРасширенныйФункционал();
	ИспользуетсяЭДОБЗК = ИнтеграцияКабинетСотрудника.ИспользуетсяЭДОБЗК();
	
	ИспользоватьФормат503 = КабинетСотрудника.ИспользоватьФормат503();
	ИспользуетсяВерсияDTO_1_0 = ИнтеграцияКабинетСотрудника.ИспользуетсяВерсияDTO("1.0");
	ИспользуетсяВерсияDTO_2_0 = ИнтеграцияКабинетСотрудника.ИспользуетсяВерсияDTO("2.0");
	ИспользуетсяВерсияDTO_2_3 = ИнтеграцияКабинетСотрудника.ИспользуетсяВерсияDTO("2.3");
	ИспользуетсяВерсияDTO_3_0 = ИнтеграцияКабинетСотрудника.ИспользуетсяВерсияDTO("3.0");
	ИспользуетсяВерсияDTO_3_1 = ИнтеграцияКабинетСотрудника.ИспользуетсяВерсияDTO("3.1");
	
	Настройки.Справки2НДФЛ = Истина;
	Настройки.ДоступноПолучениеСканаОригинала 			= Истина;
	Настройки.ДоступноПолучениеДокументаВБумажномВиде 	= Истина;
	Настройки.ДоступноПолучениеДокументаСЭП 			= ПолучитьФункциональнуюОпцию("ИспользоватьЭлектронныеПодписи");
	Настройки.ДоступноПолучениеКопииНаЭлектроннуюПочту 	= ИнтеграцияКабинетСотрудника.СистемнаяУчетнаяЗаписьНастроена();
	
	Настройки.ЗаявленияНаВычетыНДФЛ = Истина;
	Настройки.ЗапросНаИзменениеЛичнойИнформации = Истина;
	Настройки.ОтсутствиеПоЛичнымОбстоятельствам = Истина;
	Настройки.Опоздание = Истина;
	
	Настройки.РазделЗарплата 	= ИспользоватьФормат503;
	Настройки.РазделСправки 	= ИспользоватьФормат503;
	Настройки.РазделКомпания 	= ИспользоватьФормат503;
	Настройки.РазделДокументы 	= ИспользоватьФормат503 И Не ИспользуетсяЭДОБЗК;
	
	Настройки.УровеньДоступаКИ = ИспользуетсяВерсияDTO_1_0;
	
	Настройки.ДоступенОтказОтПодписания = ИспользуетсяЭДОБЗК И ИспользуетсяВерсияDTO_2_3;
	
	Настройки.ИспользоватьПособияПриРожденииРебенка = ИспользуетсяВерсияDTO_3_1;
	Настройки.ИспользоватьПособияПоУходуЗаРебенком 	= ИспользуетсяВерсияDTO_3_1;
	
	Если ДоступенРасширенныйФункционал Тогда
		
		НастройкиРасчетаЗарплаты = ИнтеграцияКабинетСотрудникаВнутренний.НастройкиРасчетаЗарплаты();
		
		Настройки.СправкиСМестаРаботы = Истина;
		
		Настройки.ЗаявленияНаДСВ 			= НастройкиРасчетаЗарплаты.ИспользоватьДСВ;
		Настройки.СпособРасчетаДСВПроцентом = НастройкиРасчетаЗарплаты.ИспользоватьДСВ;
		Настройки.СпособРасчетаДСВСуммой 	= НастройкиРасчетаЗарплаты.ИспользоватьДСВ;
		Настройки.СпособРасчетаДСВПроцентомНеБолееСуммы = НастройкиРасчетаЗарплаты.ИспользоватьДСВ;
		
		Настройки.РазделОтпуск 				= ИспользуетсяВерсияDTO_2_3;
		Настройки.РазделОтсутствия 			= ИспользоватьФормат503;
		Настройки.КомпенсацияОтпуска 		= ИспользоватьФормат503;
		Настройки.ПоказыватьОстаткиОтпусков = ИспользоватьФормат503;
		
		Настройки.СправкиОстаткиОтпусков 		= ИспользуетсяВерсияDTO_1_0;
		
		Настройки.ЗаявлениеНаОтпуск 			= ИспользуетсяВерсияDTO_2_0;
		Настройки.ЗаявлениеОтпускБезОплаты 		= ИспользуетсяВерсияDTO_2_0;
		Настройки.ЗаявлениеУчебныйОтпуск 		= ИспользуетсяВерсияDTO_2_0 И НастройкиРасчетаЗарплаты.ИспользоватьОтпускаУчебные;
		
		Настройки.Болезнь 						= ИспользуетсяВерсияDTO_2_0;
		Настройки.ОтпускПоБеременностиИРодам 	= ИспользуетсяВерсияDTO_2_0;
		Настройки.ОтпускПоУходуЗаРебенком 		= ИспользуетсяВерсияDTO_2_0;
		Настройки.УходЗаРебенкомИнвалидом 		= ИспользуетсяВерсияDTO_2_0;
		Настройки.Командировка 					= ИспользуетсяВерсияDTO_2_0 И НастройкиРасчетаЗарплаты.ИспользоватьОплатуКомандировок;
		Настройки.Отгул 						= ИспользуетсяВерсияDTO_2_0 И НастройкиРасчетаЗарплаты.ИспользоватьОтгулы;
		
		Настройки.ЕдиновременнаяВыплатаКОтпуску = ИспользуетсяВерсияDTO_2_0 И НастройкиРасчетаЗарплаты.ИспользоватьЕдиновременнуюВыплатуКОтпуску;
		Настройки.МатериальнаяПомощьКОтпуску 	= ИспользуетсяВерсияDTO_2_0 И НастройкиРасчетаЗарплаты.ИспользоватьМатериальнуюПомощьПриОтпуске;
		
		Настройки.ДоступноИзменениеФото 				= ИспользуетсяВерсияDTO_3_0;
		Настройки.ИспользоватьЗаявленияНаОтменуОтпуска 	= ИспользуетсяВерсияDTO_3_0;
		Настройки.ИспользоватьЗаявленияНаПереносОтпуска = ИспользуетсяВерсияDTO_3_0;
		
		Настройки.ПоказыватьОхрануТруда 						= ДоступноИспользованиеОхраныТруда();
		Настройки.ПоказыватьРабочиеМестаОхраныТруда 			= ИспользуетсяВерсияDTO_3_1 И ДоступноИспользованиеРабочихМестОхраныТруда();
		Настройки.ПоказыватьМедицинскиеОсмотры 					= ДоступноИспользованиеМедицинскихОсмотров();
		Настройки.ПоказыватьПсихиатрическиеОсвидетельствования 	= ДоступноИспользованиеПсихиатрическихОсвидетельствований();
		
		Настройки.ПоказыватьГрафикОтпусков 			= ИспользуетсяВерсияDTO_3_1;
		Настройки.ВозвратИзОтпускаПоУходуЗаРебенком = ИспользуетсяВерсияDTO_3_1;
		
	Иначе
		
