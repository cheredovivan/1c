	Параметры.Вставить("ПунктПриемаГруза", НовыйПараметрыПунктаПриемаВыдачи());
	Параметры.Вставить("СпособОтгрузки", 0);
	Параметры.Вставить("ОтправительНаселенныйПункт", НовыеПараметрыНаселенногоПункта());
