		ПараметрыЗаказа.Вставить("МультизаказДата", ПолучитьЗначение(ТекущийЗаказ, "multiOrder.date", "Дата"));
		ПараметрыЗаказа.Вставить("МультизаказНомер", ПолучитьЗначение(ТекущийЗаказ, "multiOrder.number", , ""));
