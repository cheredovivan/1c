// Преобразовывает дату JSON без разделителя в дату.
//
// Параметры:
// 	ДатаСтрокой - Строка - Переданная строка с датой
//
// Возвращаемое значение:
//  Дата - Преобразованная  дата
//  
Функция ПрочитатьДатуJSONБезРазделителя(ДатаСтрокой) Экспорт
	
	Результат = Дата(1, 1, 1);
	
	Если ЗначениеЗаполнено(ДатаСтрокой)
		И СтрНайти(ДатаСтрокой, " ") > 0 Тогда
		
		Попытка
			Результат = ПрочитатьДатуJSON(СтрЗаменить(ДатаСтрокой, " ", "T"), ФорматДатыJSON.ISO);
		Исключение
			Результат = Дата(1, 1, 1);
		КонецПопытки;
		
	КонецЕсли;
	
	Возврат Результат;
	
КонецФункции

