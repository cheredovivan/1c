	ЗначениеВыбора.Вставить("МеждународноеСокращение",                   "GRM");
	ЗначениеВыбора.Вставить("Числитель",                                 1);
	ЗначениеВыбора.Вставить("Знаменатель",                               1000);
	ЗначениеВыбора.Вставить("ДляПересчетов",                             Истина);
