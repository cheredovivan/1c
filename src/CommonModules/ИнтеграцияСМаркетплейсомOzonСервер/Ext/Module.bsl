Функция ТоварНаходитсяВАрхивеПоДаннымСервиса(ДанныеСервиса)
	
	Результат = Ложь;
	
	ТипДанных = ТипЗнч(ДанныеСервиса);
	
	ИмяПоляАрхивированВручную       = ИмяПоляАрхивированВручную();
	ИмяПоляАрхивированАвтоматически = ИмяПоляАрхивированАвтоматически();
	
	Если ТипДанных = Тип("Структура") Тогда
		
		ДанныеЗаполнения = Новый Структура;
		ДанныеЗаполнения.Вставить(ИмяПоляАрхивированВручную, Ложь);
		ДанныеЗаполнения.Вставить(ИмяПоляАрхивированАвтоматически, Ложь);
		
		ЗаполнитьЗначенияСвойств(ДанныеЗаполнения, ДанныеСервиса);
		
		ЗначениеАрхивированВручную       = ДанныеЗаполнения[ИмяПоляАрхивированВручную];
		ЗначениеАрхивированАвтоматически = ДанныеЗаполнения[ИмяПоляАрхивированАвтоматически];
		
	ИначеЕсли ТипДанных = Тип("Соответствие") Тогда
		
		ЗначениеАрхивированВручную       = ДанныеСервиса[ИмяПоляАрхивированВручную];
		ЗначениеАрхивированАвтоматически = ДанныеСервиса[ИмяПоляАрхивированАвтоматически];
		
	Иначе
		Возврат Результат;
	КонецЕсли;
	
	Результат = (ЗначениеАрхивированВручную = Истина) Или (ЗначениеАрхивированАвтоматически = Истина);
	
	Возврат Результат;
	
КонецФункции

// Объединяет несколько ошибок в единое представление.
//
// Параметры:
//   ДанныеОбОшибках - Массив из см. НовыйРезультатИзСервиса - ошибки при загрузке товаров:
//     * attribute_id                  - Число - атрибут, в котором произошла ошибка.
//     * attribute_name                - Строка - название атрибута, в котором произошла ошибка (вариант 1).
//     * code                          - Строка - код ошибки;
//     * field                         - Строка - поле, в котором произошла ошибка.
//     * level                         - Строка - уровень ошибки.
//     * state                         - Строка - состояние товара, в котором произошла ошибка.
//     * description                   - Строка - описание ошибки (вариант 1).
//     * message                       - Строка - техническое описание ошибки (вариант 1).
//     * optional_description_elements - Соответствие из КлючИЗначение - дополнительные поля для описания ошибки (вариант 1).
//     * texts                         - Соответствие из КлючИЗначение - описание ошибок (вариант 2):
//       ** attribute_name             - Строка - название атрибута, в котором произошла ошибка.
//       ** description                - Строка - описание ошибки.
//       ** hint_code                  - Строка - код ошибки в системе Ozon.
//       ** message                    - Строка - текст ошибки.
//       ** params                     - Массив из НовыйРезультатИзСервиса - в каких параметрах допущена ошибка:
//         *** name                      - Строка - название параметра.
//         *** value                     - Строка - значение параметра.
//       ** short_description          - Строка - краткое описание ошибки.
//
// Возвращаемое значение:
//   см. ИнтеграцияСМаркетплейсамиСервер.НоваяОшибка.
//
Функция СобратьОписаниеОшибокСтатусаДобавления(ДанныеОбОшибках)
	
	ОписаниеОшибок = ИнтеграцияСМаркетплейсамиСервер.НоваяОшибка();
	
	Если ЗначениеЗаполнено(ДанныеОбОшибках) Тогда
		ШаблонОшибки1 = НСтр("ru = 'Для значения из атрибута <%1> отсутствует действующий сертификат бренда - необходимо добавить сертификат бренда в личном кабинете. Ошибка: %2 (Техническое описание ошибки: %3. Код ошибки: %4 (%5), состояние товара <%6>).';
							|en = 'There is no valid brand certificate for the value from the <%1> product option. Add a brand certificate in your personal account. Error: %2 (Technical error details: %3. Error code: %4 (%5). Condition of the <%6> item).'");
		ШаблонОшибки2 = НСтр("ru = 'Обнаружена ошибка в атрибуте <%1>: %2 (Техническое описание ошибки: %3. Код ошибки: %4 (%5), состояние товара <%6>).';
							|en = 'An error is found in the <%1> product option: %2 (Technical error details: %3. Error code: %4 (%5). Condition of the <%6> item).'");
		ШаблонОшибки3 = НСтр("ru = 'Обнаружена ошибка заполнения данных: %2 (Техническое описание ошибки: %3. Код ошибки: %4 (%5), состояние товара <%6>).';
							|en = 'A data population error is found: %2 (Technical error details: %3. Error code: %4 (%5). Condition of the <%6> item).'");
		ШаблонОшибки4 = НСтр("ru = 'Обнаружена ошибка в атрибуте <%1>: %2 (Код ошибки: %4 (%5), состояние товара <%6>).';
							|en = 'An error is found in the <%1> product option: %2 (Error code: %4 (%5). Condition of the <%6> item).'");
		ШаблонОшибки5 = НСтр("ru = 'Обнаружена ошибка заполнения данных: %2 (Код ошибки: %4 (%5), состояние товара <%6>).';
							|en = 'A data population error is found: %2 (Error code: %4 (%5). Condition of the <%6> item).'");
		
		МассивКодовОшибок = Новый Массив;
		МассивОписанийОшибок = Новый Массив;
		
		Для Каждого ДанныеОбОшибке Из ДанныеОбОшибках Цикл
			СтруктураДанныхОбОшибке = Новый Структура;
			СтруктураДанныхОбОшибке.Вставить("attribute_name", "");
			СтруктураДанныхОбОшибке.Вставить("code",           "");
			СтруктураДанныхОбОшибке.Вставить("description",    "");
			СтруктураДанныхОбОшибке.Вставить("level",          "");
			СтруктураДанныхОбОшибке.Вставить("message",        "");
			СтруктураДанныхОбОшибке.Вставить("state",          "");
			ЗаполнитьЗначенияСвойствИзСервиса(СтруктураДанныхОбОшибке, ДанныеОбОшибке);
			
			Если ЗначениеЗаполнено(ДанныеОбОшибке["texts"]) Тогда
				ЗаполнитьЗначенияСвойствИзСервиса(СтруктураДанныхОбОшибке, 
					ДанныеОбОшибке["texts"], 
					"attribute_name, description, message");
			КонецЕсли;
			
			Если Прав(СтруктураДанныхОбОшибке.description, 1) = "." Тогда
				СтруктураДанныхОбОшибке.description = Лев(СтруктураДанныхОбОшибке.description, СтрДлина(СтруктураДанныхОбОшибке.description) - 1);
			КонецЕсли;
			Если Прав(СтруктураДанныхОбОшибке.message, 1) = "." Тогда
				СтруктураДанныхОбОшибке.message = Лев(СтруктураДанныхОбОшибке.message, СтрДлина(СтруктураДанныхОбОшибке.message) - 1);
			КонецЕсли;
			Если СтруктураДанныхОбОшибке.message = СтруктураДанныхОбОшибке.description Тогда
				СтруктураДанныхОбОшибке.message = "";
			КонецЕсли;
			
			МассивКодовОшибок.Добавить(СтруктураДанныхОбОшибке.code);
			
			ТехническоеОписаниеОшибки = Неопределено;
			СтруктураДанныхОбОшибке.Свойство("message", ТехническоеОписаниеОшибки);
			
			Если СтрНайти(СтруктураДанныхОбОшибке.code, "brand_in_stop_list") Тогда
				ШаблонОшибки = ШаблонОшибки1;
			ИначеЕсли Не ЗначениеЗаполнено(ТехническоеОписаниеОшибки) Тогда
				ШаблонОшибки = ?(ПустаяСтрока(СтруктураДанныхОбОшибке.attribute_name), ШаблонОшибки5, ШаблонОшибки4);
			Иначе
				ШаблонОшибки = ?(ПустаяСтрока(СтруктураДанныхОбОшибке.attribute_name), ШаблонОшибки3, ШаблонОшибки2);
			КонецЕсли;
			
			ОписаниеОшибки = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(ШаблонОшибки,
				СтруктураДанныхОбОшибке.attribute_name,
				СтруктураДанныхОбОшибке.description,
				ТехническоеОписаниеОшибки,
				СтруктураДанныхОбОшибке.code,
				СтруктураДанныхОбОшибке.level,
				СтруктураДанныхОбОшибке.state);
			
			МассивОписанийОшибок.Добавить(ОписаниеОшибки);
		КонецЦикла;
		
		ОписаниеОшибок.КодОшибки      = СтрСоединить(МассивКодовОшибок, ", ");
		ОписаниеОшибок.ОписаниеОшибки = СтрСоединить(МассивОписанийОшибок, Символы.ПС);
	КонецЕсли;
	
	Возврат ОписаниеОшибок;
	
КонецФункции

