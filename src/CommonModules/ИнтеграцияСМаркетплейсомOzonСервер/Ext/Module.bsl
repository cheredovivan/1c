	Результат.Вставить("МодельАвтомобиля",           "");
	Результат.Вставить("НомерАвтомобиля",            "");
	Результат.Вставить("Активен",                    Истина);
	Результат.Вставить("ХешСумма",                   "");
	
	Возврат Результат;
	
КонецФункции

// Возвращает хеш-сумму основных показателей пропуска.
// 
// Параметры:
//   Пропуск - См. НовыйПропуск.
// 
// Возвращаемое значение:
//   Строка - хеш-сумма в виде строки.
//
Функция ХешСуммаПропуска(Знач Пропуск)
	
	Возврат ОбщегоНазначенияУТ.ХешСуммаСтроки(Пропуск.ЦельПриезда + "|" + Пропуск.ДатаВремяВъезда 
		+ "|" + Пропуск.Водитель + "|" + Пропуск.ТелефонВодителя
		+ "|" + Пропуск.МодельАвтомобиля + "|" + Пропуск.НомерАвтомобиля, ХешФункция.SHA256);
	
КонецФункции

