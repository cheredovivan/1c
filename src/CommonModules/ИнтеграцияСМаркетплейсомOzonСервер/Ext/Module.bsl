	ИначеЕсли ВладелецЗначения = "ОтправлениеВОтгрузке" Тогда
		Если ВРег(Значение) = ВРег("new") Тогда
			Результат = НСтр("ru = 'Добавляется в отгрузку';
							|en = 'Adding to shipment'");
		ИначеЕсли ВРег(Значение) = ВРег("success") Тогда
			Результат = НСтр("ru = 'Добавлен в отгрузку';
							|en = 'Added to shipment'");
		ИначеЕсли ВРег(Значение) = ВРег("excluded") Тогда
			Результат = НСтр("ru = 'Исключен из отгрузки';
							|en = 'Excluded from shipment'");
		КонецЕсли;
		
	// Статус формирования документов отгрузки.
	ИначеЕсли ВладелецЗначения = "ДокументыОтгрузки" Тогда
		Если ВРег(Значение) = ВРег("forming") Тогда
			Результат = НСтр("ru = 'Документы формируются, нужно подождать.';
							|en = 'Documents are being generated, please wait.'");
		ИначеЕсли ВРег(Значение) = ВРег("formed") Тогда
			Результат = НСтр("ru = 'Документы сформированы и готовы для скачивания.';
							|en = 'Documents are generated and ready for download.'");
		ИначеЕсли ВРег(Значение) = ВРег("confirmed") Тогда
			Результат = НСтр("ru = 'Документы подписаны Ozon.';
							|en = 'Documents are signed by Ozon.'");
		ИначеЕсли ВРег(Значение) = ВРег("confirmed_with_mismatch") Тогда
			Результат = НСтр("ru = 'Документы подписаны Ozon с расхождениями.';
							|en = 'Documents are signed by Ozon with discrepancies.'");
		ИначеЕсли ВРег(Значение) = ВРег("not_found") Тогда
			Результат = НСтр("ru = 'Документы не найдены.';
							|en = 'Documents are not found.'");
		ИначеЕсли ВРег(Значение) = ВРег("unknown_error") Тогда
			Результат = НСтр("ru = 'Произошла ошибка. Повторите запрос.';
							|en = 'An error occurred. Try again.'");
		КонецЕсли;
		
	// Тип тарификации отгрузки.
	ИначеЕсли ВладелецЗначения = "ТипТарифаОтгрузки" Тогда
		Если ВРег(Значение) = ВРег("discount") Тогда
			Результат = НСтр("ru = 'скидка';
							|en = 'discount'");
		ИначеЕсли ВРег(Значение) = ВРег("no_discount") Тогда
			Результат = НСтр("ru = 'без комиссии';
							|en = 'no fee'");
		ИначеЕсли ВРег(Значение) = ВРег("commission") Тогда
			Результат = НСтр("ru = 'комиссия';
							|en = 'fee'");
		КонецЕсли;
	КонецЕсли;
	
	Возврат Результат;
	
КонецФункции

// Получает хеш переданных двоичных данных.
//
// Параметры:
//   ДвоичныеДанныеФайла     - ДвоичныеДанные - двоичные данные для хеширования.
//   ПреобразоватьВHEX       - Булево - признак преобразования в формат Base 64 (Hex).
//   ПеревестиВНижнийРегистр - Булево - признак перевода строки в нижний регистр.
// 
// Возвращаемое значение:
//   Строка - значение хеш-суммы в формате Base 64.
//
Функция ХешСуммаДвоичныхДанных(ДвоичныеДанныеФайла, ПреобразоватьВHEX = Ложь, ПеревестиВНижнийРегистр = Ложь)

	ХешированиеДанных = Новый ХешированиеДанных(ХешФункция.MD5);
	ХешированиеДанных.Добавить(ДвоичныеДанныеФайла);

	ХешСумма = ХешированиеДанных.ХешСумма;
	
	Если ПреобразоватьВHEX Тогда
		ХешСумма = ПолучитьHexСтрокуИзДвоичныхДанных(ХешСумма);
	КонецЕсли;

	Если ПеревестиВНижнийРегистр Тогда
		ХешСумма = НРег(ХешСумма);
	КонецЕсли;

	Возврат ХешСумма;

КонецФункции

