// Определяет список складов FBS и rFBS торговой площадки Ozon.
// Описание метода см. https://docs.ozon.ru/api/seller/#operation/WarehouseAPI_WarehouseList (версия 1).
//
// Параметры:
//   УчетнаяЗаписьМаркетплейса - СправочникСсылка.УчетныеЗаписиМаркетплейсов - учетная запись подключения к сервису.
//   УчитыватьМетодДоставки    - Булево - признак получения информации по методу доставки склада.
//
// Возвращаемое значение:
//   - Неопределено - при выполнении запроса к внешнему ресурсу возникли ошибки.
//   - Массив Из Структура - результат выполнения запроса:
//       * ИдентификаторОбъектаМаркетплейса - Строка - идентификатор склада (warehouse_id);
//       * НаименованиеОбъектаМаркетплейса  - Строка - название склада (name);
//       * ЭтоRealFBS                       - Булево - признак работы склада по схеме rFBS (is_rfbs).
//       * ДоверительнаяПриемка             - Булево - признак доверительной приемки (has_entrusted_acceptance).
//       * Статус                           - Строка - представление статуса склада (status).
//       * ИдентификаторМетодаДоставки      - Строка - идентификатор метода склада площадки.
//       * НаименованиеМетодаДоставки       - Строка - наименование метода склада площадки.
//       * ВремяОтгрузки                    - Дата - время, до которого нужно отгрузить заказ по методу склада площадки.
//
Функция СкладыИзСервиса(УчетнаяЗаписьМаркетплейса, УчитыватьМетодДоставки = Ложь) Экспорт

	МассивСкладов = Новый Массив;

	Если Не ЗначениеЗаполнено(УчетнаяЗаписьМаркетплейса) Тогда
		Возврат МассивСкладов;
	КонецЕсли;
	
	ТекстЗапроса = "/v1/warehouse/list";
	ДанныеЗапроса = Новый Структура();
	РезультатЗапроса = ВыполнитьЗапросКСервису(ТекстЗапроса, ДанныеЗапроса, УчетнаяЗаписьМаркетплейса, Истина);

	Если РезультатЗапроса = Неопределено Тогда
		МассивСкладов = Неопределено;
	Иначе
		Для Каждого ЭлементРезультата Из РезультатЗапроса["result"] Цикл
			ОписаниеСклада = Новый Структура;
			ОписаниеСклада.Вставить("ИдентификаторОбъектаМаркетплейса", XMLСтрока(ЭлементРезультата["warehouse_id"]));
			ОписаниеСклада.Вставить("НаименованиеОбъектаМаркетплейса",  ЭлементРезультата["name"]);
			ОписаниеСклада.Вставить("ЭтоRealFBS",                       ЭлементРезультата["is_rfbs"]);
			ОписаниеСклада.Вставить("ДоверительнаяПриемка",             ЭлементРезультата["has_entrusted_acceptance"]);
			ОписаниеСклада.Вставить("Статус",                           ПредставлениеПоЗначению(ЭлементРезультата["status"], "Склад"));
			ОписаниеСклада.Вставить("АктивныйСклад",                    ЭтоАктивныйСклад(ОписаниеСклада.Статус));
			ОписаниеСклада.Вставить("ИдентификаторМетодаДоставки",      "");
			ОписаниеСклада.Вставить("НаименованиеМетодаДоставки",       "");
			ОписаниеСклада.Вставить("ВремяОтгрузки",                    Дата(1, 1, 1));
			
			Если УчитыватьМетодДоставки Тогда
				МетодДоставки = ПолучитьАктивныйМетодДоставкиСклада(
									УчетнаяЗаписьМаркетплейса,
									ОписаниеСклада.ИдентификаторОбъектаМаркетплейса);
				Если МетодДоставки <> Неопределено Тогда
					ЗаполнитьЗначенияСвойств(ОписаниеСклада, МетодДоставки);
				КонецЕсли;
			КонецЕсли;
			
			МассивСкладов.Добавить(ОписаниеСклада);
		КонецЦикла;
	КонецЕсли;

	Возврат МассивСкладов;

КонецФункции

// Определяет список активных складов Ozon с информацией об их средней загруженности на ближайшее время.
// Описание метода см. https://docs.ozon.ru/api/seller/#operation/SupplierAPI_SupplierAvailableWarehouses (версия 1).
//
// Параметры:
//   УчетнаяЗаписьМаркетплейса - СправочникСсылка.УчетныеЗаписиМаркетплейсов - учетная запись подключения к сервису.
//
// Возвращаемое значение:
//   Соответствие Из КлючИЗначение - результат выполнения запроса:
//     * Ключ     - Строка - название склада (name);
//     * Значение - Строка - идентификатор склада (id).
//
Функция ЗагруженностьСкладовИзСервиса(УчетнаяЗаписьМаркетплейса)
	
	Результат = Новый Соответствие;

	ТекстЗапроса     = "/v1/supplier/available_warehouses";
	ПараметрыЗапроса = Новый Структура;
	ОтветСервиса     = ВыполнитьЗапросКСервису(ТекстЗапроса, ПараметрыЗапроса, УчетнаяЗаписьМаркетплейса, Ложь);

	Если ОтветСервиса <> Неопределено Тогда
		Для Каждого ЭлементОтветаСервиса Из ОтветСервиса["result"] Цикл
			Результат.Вставить(ВРег(ЭлементОтветаСервиса["warehouse"]["name"]), ЧислоВСтроку(ЭлементОтветаСервиса["warehouse"]["id"]));
		КонецЦикла;
	КонецЕсли;

	Возврат Результат;

КонецФункции

// Определяет список методов доставки склада торговой площадки.
//
// Параметры:
//   УчетнаяЗаписьМаркетплейса - СправочникСсылка.УчетныеЗаписиМаркетплейсов - учетная запись подключения к сервису.
//   Параметры - Структура - см. НовыеПараметрыЗапросаОписанияМетодаДоставкиСклада.
//
// Возвращаемое значение:
//   МетодыДоставки - Массив из Структура - список информации по методам доставки склада площадки.
//
Функция МетодыДоставкиСклада(УчетнаяЗаписьМаркетплейса, Параметры)

	ТекстЗапроса = "/v1/delivery-method/list";
	
	Фильтр = Новый Структура;
	Фильтр.Вставить("warehouse_id", СтрокаВЧисло(Параметры.Фильтр.ИдентификаторСклада));
	
	Если Не ПустаяСтрока(Параметры.Фильтр.СтатусМетода) Тогда
		Фильтр.Вставить("status", Параметры.Фильтр.СтатусМетода);
	КонецЕсли;
	
	Если ЗначениеЗаполнено(Параметры.Фильтр.ИдентификаторСлужбыДоставки) Тогда
		Фильтр.Вставить("provider_id", СтрокаВЧисло(Параметры.Фильтр.ИдентификаторСлужбыДоставки));
	КонецЕсли;
	
	ПараметрыЗапроса = Новый Структура;
	ПараметрыЗапроса.Вставить("filter", Фильтр);
	ПараметрыЗапроса.Вставить("limit",  Параметры.МаксимальноеКоличествоВОтвете);
	
	АтрибутыОтвета = Новый Структура;
	АтрибутыОтвета.Вставить("Результат", "result");
	АтрибутыОтвета.Вставить("ПовторитьЗапрос", "has_next");
	
	МетодыДоставки = Новый Массив;
	
	Пока Истина Цикл
		ПараметрыЗапроса.Вставить("offset", Параметры.НачальноеСмещениеВОтвете);
		
		// POST-запрос
		РезультатЗапроса = ПолучитьРезультатЗапроса(ТекстЗапроса, ПараметрыЗапроса, УчетнаяЗаписьМаркетплейса, АтрибутыОтвета);
		
		Если РезультатЗапроса.Результат = Неопределено Тогда
			Прервать;
		Иначе
			Для Каждого ИнформацияОМетоде Из РезультатЗапроса.Результат Цикл
				ДатаВремяОтгрузки = ОбщегоНазначенияКлиентСервер.СтрокаВДату("00010101" + СтрЗаменить(ИнформацияОМетоде["cutoff"], ":", ""));
				
				МетодДоставки = Новый Структура;
				МетодДоставки.Вставить("Статус",                      ПредставлениеПоЗначению(ИнформацияОМетоде["status"], "Склад"));
				МетодДоставки.Вставить("ИдентификаторМетодаДоставки", XMLСтрока(ИнформацияОМетоде["id"]));
				МетодДоставки.Вставить("НаименованиеМетодаДоставки",  ИнформацияОМетоде["name"]);
				МетодДоставки.Вставить("ВремяОтгрузки",               ДатаВремяОтгрузки);
				
				МетодыДоставки.Добавить(МетодДоставки);
			КонецЦикла;
			
			Если Не РезультатЗапроса.ПовторитьЗапрос Или РезультатЗапроса.Результат.Количество() < Параметры.МаксимальноеКоличествоВОтвете Тогда
				Прервать;
			КонецЕсли;
			
			Параметры.НачальноеСмещениеВОтвете = Параметры.НачальноеСмещениеВОтвете + Параметры.МаксимальноеКоличествоВОтвете;
		КонецЕсли;
	КонецЦикла;
	
	Возврат МетодыДоставки;

КонецФункции

