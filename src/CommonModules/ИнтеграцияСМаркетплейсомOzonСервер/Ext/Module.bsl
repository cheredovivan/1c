Функция ПолучитьЭтикеткиОтправленийИзСервиса(УчетнаяЗапись, Параметры)
	
	ТекстЗапроса = "/v1/posting/fbs/package-label/get";
	
	ПараметрыЗапроса = Новый Структура;
	ПараметрыЗапроса.Вставить("task_id", СтрокаВЧисло(Параметры.ИдентификаторЗадания));
	
	АтрибутыОтвета = Новый Структура;
	АтрибутыОтвета.Вставить("Результат", "result");
	
	// POST-запрос
	РезультатЗапроса = ПолучитьРезультатЗапроса(ТекстЗапроса, ПараметрыЗапроса, УчетнаяЗапись, АтрибутыОтвета, Истина);
	
	Возврат РезультатЗапроса.Результат;
	
КонецФункции

// Добавляет информацию о стране-изготовителе товара для отправления в сервисе Ozon.
// Описание метода см. https://api-seller.ozon.ru/v2/posting/fbs/product/country/set (версия 2).
//
// Параметры:
//   УчетнаяЗапись - СправочникСсылка.УчетныеЗаписиМаркетплейсов - учетная запись подключения к сервису.
//   Параметры     - см. НовыеПараметрыЗаполненияСтраныПроизводителя.
// 
// Возвращаемое значение:
//   Структура - результат запроса:
//     * ИдентификаторТовара - Строка - при выполнении запроса к внешнему ресурсу возникли ошибки.
//     * ТребуетсяГТД - Булево- признак, что требуется заполнение номера ГТД.
//     * ЗапрещеноИзменениеСтраныПроисхождения - Булево- признак, что не удалось изменить страну происхождения.
//
Функция ЗаполнитьСтрануИзготовителяТовараДляОтправленияВСервисе(УчетнаяЗапись, Параметры)
	
	ТекстЗапроса = "/v2/posting/fbs/product/country/set";
	
	ПараметрыЗапроса = Новый Структура;
	ПараметрыЗапроса.Вставить("posting_number",   Параметры.НомерОтправления);
	ПараметрыЗапроса.Вставить("product_id",       СтрокаВЧисло(Параметры.ИдентификаторТовара));
	ПараметрыЗапроса.Вставить("country_iso_code", Параметры.КодСтраны);
	
	СлужебныеПараметры = СлужебныеПараметрыHTTPЗапроса();
	СлужебныеПараметры.ВернутьСтрокуОтвета = Истина;
	
	ПараметрыЗапроса.Вставить("СлужебныеПараметры", СлужебныеПараметры);
	
	// POST-запрос
	РезультатЗапроса = ПолучитьРезультатЗапроса(ТекстЗапроса, ПараметрыЗапроса, УчетнаяЗапись);
	
	АтрибутыОтвета = Новый Структура("ИдентификаторТовара, ТребуетсяГТД", "product_id", "is_gtd_needed");
	Результат = ОбработатьРезультатЗапроса(РезультатЗапроса, АтрибутыОтвета);
	
	Ответ = Новый Структура("ИдентификаторТовара, ТребуетсяГТД, ЗапрещеноИзменениеСтраныПроисхождения", "", Ложь, Ложь);
	ЗаполнитьЗначенияСвойств(Ответ, Результат);
	
	Ответ.ЗапрещеноИзменениеСтраныПроисхождения = Не ЗначениеЗаполнено(Ответ.ИдентификаторТовара);
	
	Возврат Ответ;
	
КонецФункции

// Проверяет и сохраняет данные экземпляров товаров из отправления в сервисе Ozon.
// Описание метода см. https://docs.ozon.ru/api/seller/#operation/PostingAPI_FbsPostingProductExemplarSetV6 (версия 6).
//
// Параметры:
//   УчетнаяЗапись - СправочникСсылка.УчетныеЗаписиМаркетплейсов - учетная запись подключения к сервису.
//   Параметры     - Структура Из КлючИЗначение - список параметров запроса, см. НовыеПараметрыЗапросаЭкземпляровТоваровПоОтправлению.
// 
// Возвращаемое значение:
//   - Неопределено - при выполнении запроса к внешнему ресурсу возникли ошибки.
//   - Булево       - результат выполнения метода.
//
Функция ПроверитьИСохранитьЭкземплярыТоваровПоОтправлениюВСервисе(УчетнаяЗапись, Параметры)
	
	ТекстЗапроса = "/v6/fbs/posting/product/exemplar/set";
	
	СтранаПроизводительВТоварах = Новый Соответствие;
	
	Товары = Новый Массив;
		
	Для Каждого СтрокаТаблицыЗначений Из Параметры.Товары Цикл
		Экземпляры = Новый Массив;
		
		Для Каждого ЭлементКоллекции Из СтрокаТаблицыЗначений.Экземпляры Цикл
			СведенияОМаркировке = Новый Массив;
			Если ЗначениеЗаполнено(ЭлементКоллекции.КодМаркировки) Тогда
				СведенияОМаркировке.Добавить(Новый Структура("mark, mark_type", ЭлементКоллекции.КодМаркировкиСтрока, "mandatory_mark"));
			КонецЕсли;
			Если ЗначениеЗаполнено(ЭлементКоллекции.УИН) Тогда
				СведенияОМаркировке.Добавить(Новый Структура("mark, mark_type", ЭлементКоллекции.УИН, "jw_uin"));
			КонецЕсли;
			
			Если ЭлементКоллекции.ТребуетсяСтранаПроисхождения Тогда
				ТребуетсяГТД = СтранаПроизводительВТоварах[СтрокаТаблицыЗначений.ИдентификаторТовара];
				Если ТребуетсяГТД = Неопределено Тогда
					ПараметрыЗапроса = НовыеПараметрыЗаполненияСтраныПроизводителя(
						Параметры.НомерОтправления,
						СтрокаТаблицыЗначений.ИдентификаторТовара,
						ЭлементКоллекции.СтранаПроисхожденияКод);
					
					Результат = ЗаполнитьСтрануИзготовителяТовараДляОтправленияВСервисе(УчетнаяЗапись, ПараметрыЗапроса);
					Если Результат <> Неопределено Тогда
						Если ЗначениеЗаполнено(Результат.ИдентификаторТовара) Тогда
							ТребуетсяГТД = Результат.ТребуетсяГТД;
						ИначеЕсли Результат.ЗапрещеноИзменениеСтраныПроисхождения Тогда
							ТребуетсяГТД = ЭлементКоллекции.ТребуетсяГТД;
						КонецЕсли;
						
						СтранаПроизводительВТоварах.Вставить(СтрокаТаблицыЗначений.ИдентификаторТовара, ТребуетсяГТД);
					КонецЕсли;
				КонецЕсли;
				
				СтрокаТаблицыЗначений.ЗаполненаСтранаПроисхождения = Истина;
				ЭлементКоллекции.ТребуетсяГТД = ТребуетсяГТД;
			КонецЕсли;
			
			Экземпляр = Новый Структура;
			Экземпляр.Вставить("exemplar_id",    ЭлементКоллекции.ИдентификаторЭкземпляра);
			Экземпляр.Вставить("is_gtd_absent",  ЭлементКоллекции.ТребуетсяГТД И Не ЗначениеЗаполнено(ЭлементКоллекции.НомерГТДКод));
			Экземпляр.Вставить("gtd",            СокрЛП(ЭлементКоллекции.НомерГТДКод));
			Экземпляр.Вставить("is_rnpt_absent", ЭлементКоллекции.ТребуетсяРНПТ И Не ЗначениеЗаполнено(ЭлементКоллекции.НомерГТДРегистрационныйНомер));
			Экземпляр.Вставить("rnpt",           СокрЛП(ЭлементКоллекции.НомерГТДРегистрационныйНомер));
			Экземпляр.Вставить("marks",          СведенияОМаркировке);
			
			Экземпляры.Добавить(Экземпляр);
		КонецЦикла;
		
		Товар = Новый Структура;
		Товар.Вставить("product_id", СтрокаВЧисло(СтрокаТаблицыЗначений.ИдентификаторТовара));
		Товар.Вставить("exemplars",  Экземпляры);
		
		Товары.Добавить(Товар);
	КонецЦикла;
	
	ПараметрыЗапроса = Новый Структура;
	ПараметрыЗапроса.Вставить("posting_number", Параметры.НомерОтправления);
	ПараметрыЗапроса.Вставить("products",       Товары);
	Если ЗначениеЗаполнено(Параметры.КоличествоКоробок) Тогда
		ПараметрыЗапроса.Вставить("multi_box_qty", Параметры.КоличествоКоробок);
	КонецЕсли;
	
	// POST-запрос
	РезультатЗапроса = ПолучитьРезультатЗапроса(ТекстЗапроса, ПараметрыЗапроса, УчетнаяЗапись,, Истина);
	
	Если РезультатЗапроса = Неопределено Тогда
		Возврат РезультатЗапроса;
	КонецЕсли;
	
	Возврат Истина;
	
КонецФункции

// Получает статусы добавления экземпляров товаров по отправлению из сервиса Ozon.
// Описание метода см. https://docs.ozon.ru/api/seller/#operation/PostingAPI_FbsPostingProductExemplarStatusV5 (версия 5).
//
// Параметры:
//   УчетнаяЗапись - СправочникСсылка.УчетныеЗаписиМаркетплейсов - учетная запись подключения к сервису.
//   Параметры     - Структура Из КлючИЗначение - список параметров запроса, см. НовыеПараметрыЗапросаЭкземпляровТоваровПоОтправлению.
// 
// Возвращаемое значение:
//   - Неопределено - при выполнении запроса к внешнему ресурсу возникли ошибки.
//   - Соответствие Из КлючИЗначение - результат выполнения метода:
//       * posting_number            - Строка - номер отправления.
//       * status                    - Строка - статус проверки всех экземпляров и доступности сборки.
//       * products                  - Массив Из См. НовыйРезультатИзСервиса - список товаров:
//         ** exemplars                - Массив Из См. НовыйРезультатИзСервиса - информация об экземплярах:
//           *** exemplar_id             - Число - идентификатор экземпляра;
//           *** gtd                     - Строка - номер грузовой таможенной декларации (ГТД).
//           *** gtd_check_status        - Строка - статус проверки грузовой таможенной декларации.
//           *** gtd_error_codes         - Массив Из Строка - коды ошибок при проверке грузовой таможенной декларации.
//           *** is_gtd_absent           - Булево - признак того, что не указан номер грузовой таможенной декларации (ГТД).
//           *** rnpt                    - Строка - регистрационный номер партии товара (РНПТ).
//           *** rnpt_check_status       - Строка - статус проверки регистрационного номера партии товара.
//           *** rnpt_error_codes        - Массив Из Строка - коды ошибок при проверке регистрационного номера партии.
//           *** is_rnpt_absent          - Булево - признак того, что не указан регистрационный номер партии товара (РНПТ).
//           *** marks                   - Массив Из См. НовыйРезультатИзСервиса - список контрольных идентификационных знаков (КИЗ) в одном экземпляре:
//             **** check_status           - Строка - статус проверки.
//             **** error_codes            - Массив Из Строка - ошибки при проверке контрольных идентификационных знаков (КИЗ).
//             **** mark                   - Строка - значение кода маркировки.
//             **** mark_type              - Строка - тип кода маркировки.
//         ** product_id               - Число - идентификатор товара.
//
Функция ПолучитьСтатусыЭкземпляровТоваровПоОтправлениюИзСервиса(УчетнаяЗапись, Параметры)
	
	ТекстЗапроса = "/v5/fbs/posting/product/exemplar/status";
	
	ПараметрыЗапроса = Новый Структура;
	ПараметрыЗапроса.Вставить("posting_number", Параметры.НомерОтправления);
	
	АтрибутыОтвета = Новый Структура;
	АтрибутыОтвета.Вставить("НомерОтправления", "posting_number");
	АтрибутыОтвета.Вставить("Статус",           "status");
	АтрибутыОтвета.Вставить("Товары",           "products");
	
	// POST-запрос
	РезультатЗапроса = ПолучитьРезультатЗапроса(ТекстЗапроса, ПараметрыЗапроса, УчетнаяЗапись, АтрибутыОтвета, Истина);
	
	Возврат РезультатЗапроса;
	
КонецФункции

// Получает информацию по экземплярам товаров по отправлению из сервиса Ozon.
// Описание метода см. https://docs.ozon.ru/api/seller/#operation/PostingAPI_FbsPostingProductExemplarCreateOrGetV6 (версия 6).
//
// Параметры:
//   УчетнаяЗапись - СправочникСсылка.УчетныеЗаписиМаркетплейсов - учетная запись подключения к сервису.
//   Параметры     - Структура Из КлючИЗначение - список параметров запроса, см. НовыеПараметрыЗапросаЭкземпляровТоваровПоОтправлению.
// 
// Возвращаемое значение:
//   - Неопределено - при выполнении запроса к внешнему ресурсу возникли ошибки.
//   - Соответствие Из КлючИЗначение   - результат выполнения метода:
//       * multi_box_qty               - Число - количество коробок, в которые упакован товар.
//       * posting_number              - Строка - номер отправления.
//       * products                    - Массив Из См. НовыйРезультатИзСервиса - список товаров:
//         ** exemplars                  - Массив Из См. НовыйРезультатИзСервиса - информация об экземплярах:
//           *** exemplar_id               - Число - идентификатор экземпляра.
//           *** gtd                       - Строка - номер грузовой таможенной декларации (ГТД).
//           *** is_gtd_absent             - Булево - признак того, что не указан номер грузовой таможенной декларации (ГТД).
//           *** rnpt                      - Строка - регистрационный номер партии товара (РНПТ).
//           *** is_rnpt_absent            - Булево - признак того, что не указан регистрационный номер партии товара (РНПТ).
//           *** marks                     - Массив Из См. НовыйРезультатИзСервиса - ошибки при проверке контрольных идентификационных знаков (КИЗ):
//             **** mark                     - Строка - значение кода маркировки.
//             **** mark_type                - Строка - тип кода маркировки.
//         ** is_gtd_needed              - Булево - признак того, что необходимо передать номер грузовой таможенной декларации (ГТД) для продукта и отправления.
//         ** is_jw_uin_needed           - Булево - признак того, что необходимо передать уникальный идентификационный номер ювелирного изделия (УИН).
//         ** is_mandatory_mark_needed   - Булево - признак того, что необходимо передать маркировку "Честный ЗНАК".
//         ** is_mandatory_mark_possible - Булево - признак того, что возможно заполнить маркировку "Честный ЗНАК".
//         ** is_rnpt_needed             - Булево - признак того, что необходимо передать номер партии товара (РНПТ).
//         ** product_id                 - Число - идентификатор товара.
//         ** quantity                   - Число - количество экземпляров.
//
Функция ПолучитьДанныеЭкземпляровТоваровПоОтправлениюИзСервиса(УчетнаяЗапись, Параметры)
	
	ТекстЗапроса = "/v6/fbs/posting/product/exemplar/create-or-get";
	
	ПараметрыЗапроса = Новый Структура;
	ПараметрыЗапроса.Вставить("posting_number", Параметры.НомерОтправления);
	
	АтрибутыОтвета = Новый Структура;
	АтрибутыОтвета.Вставить("НомерОтправления",  "posting_number");
	АтрибутыОтвета.Вставить("КоличествоКоробок", "multi_box_qty");
	АтрибутыОтвета.Вставить("Товары",            "products");
	
	// POST-запрос
	РезультатЗапроса = ПолучитьРезультатЗапроса(ТекстЗапроса, ПараметрыЗапроса, УчетнаяЗапись, АтрибутыОтвета, Истина);
	
	Возврат РезультатЗапроса;
	
КонецФункции

// Проверяет, что коды маркировки соответствуют требованиям системы "Честный ЗНАК" по составу и количеству символов.
// Описание метода см. https://docs.ozon.ru/api/seller/#operation/PostingAPI_FbsPostingProductExemplarValidateV5 (версия 5).
//
// Параметры:
//   УчетнаяЗапись - СправочникСсылка.УчетныеЗаписиМаркетплейсов - учетная запись подключения к сервису.
//   Параметры     - Структура Из КлючИЗначение - список параметров запроса, см. НовыеПараметрыЗапросаЭкземпляровТоваровПоОтправлению.
// 
// Возвращаемое значение:
//   - Неопределено - при выполнении запроса к внешнему ресурсу возникли ошибки.
//   - Массив Из См. НовыйРезультатИзСервиса - результат выполнения метода:
//       * error       - Строка - код ошибки.
//       * exemplars   - Массив Из См. НовыйРезультатИзСервиса - информация об экземплярах:
//         ** errors     - Массив Из Строка - ошибки валидации экземпляра.
//         ** gtd        - Строка - номер грузовой таможенной декларации (ГТД).
//         ** rnpt       - Строка - регистрационный номер партии товара (РНПТ).
//         ** marks      - Массив Из См. НовыйРезультатИзСервиса - список контрольных идентификационных знаков (КИЗ) в одном экземпляре:
//           *** errors    - Массив Из Строка - ошибки при проверке контрольных идентификационных знаков (КИЗ).
//           *** mark      - Строка - значение кода маркировки.
//           *** mark_type - Строка - тип кода маркировки.
//           *** valid     - Булево - результат прохождения проверки. Истина, если контрольный идентификационный знак (КИЗ) соответствует требованиям.
//         ** valid      - Булево - результат прохождения проверки. Истина, если код экземпляра соответствует требованиям.
//       * product_id  - Число - идентификатор товара на торговой площадке.
//       * valid       - Булево - результат прохождения проверки. Истина, если коды всех экземпляров соответствуют требованиям.
//
Функция ПроверитьКодыМаркировкиПоОтправлениюВСервисе(УчетнаяЗапись, Параметры)
	
	ТекстЗапроса = "/v5/fbs/posting/product/exemplar/validate";
	
	Товары = Новый Массив;
		
	Для Каждого СтрокаТаблицыЗначений Из Параметры.Товары Цикл
		Экземпляры = Новый Массив;
		
		Для Каждого ЭлементКоллекции Из СтрокаТаблицыЗначений.Экземпляры Цикл
			СведенияОМаркировке = Новый Массив;
			Если ЗначениеЗаполнено(ЭлементКоллекции.КодМаркировки) Тогда
				СведенияОМаркировке.Добавить(Новый Структура("mark, mark_type", ЭлементКоллекции.КодМаркировкиСтрока, "mandatory_mark"));
			КонецЕсли;
			Если ЗначениеЗаполнено(ЭлементКоллекции.УИН) Тогда
				СведенияОМаркировке.Добавить(Новый Структура("mark, mark_type", ЭлементКоллекции.УИН, "jw_uin"));
			КонецЕсли;
			
			Экземпляр = Новый Структура;
			Экземпляр.Вставить("gtd",   ЭлементКоллекции.НомерГТДКод);
			Экземпляр.Вставить("rnpt",  ЭлементКоллекции.НомерГТДРегистрационныйНомер);
			Экземпляр.Вставить("marks", СведенияОМаркировке);
			
			Экземпляры.Добавить(Экземпляр);
		КонецЦикла;
		
		Товар = Новый Структура;
		Товар.Вставить("product_id", СтрокаВЧисло(СтрокаТаблицыЗначений.ИдентификаторТовара));
		Товар.Вставить("exemplars",  Экземпляры);
		
		Товары.Добавить(Товар);
	КонецЦикла;
	
	ПараметрыЗапроса = Новый Структура;
	ПараметрыЗапроса.Вставить("posting_number", Параметры.НомерОтправления);
	ПараметрыЗапроса.Вставить("products",       Товары);
	
	АтрибутыОтвета = Новый Структура;
	АтрибутыОтвета.Вставить("Результат", "products");
	
	// POST-запрос
	РезультатЗапроса = ПолучитьРезультатЗапроса(ТекстЗапроса, ПараметрыЗапроса, УчетнаяЗапись, АтрибутыОтвета, Истина);
	
	Возврат РезультатЗапроса.Результат;
	
КонецФункции

