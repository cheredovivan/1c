// Изменяет состав отгрузки в сервисе Ozon.
// Описание метода см. https://api-seller.ozon.ru/v1/carriage/set-postings.
//
// Параметры:
//   УчетнаяЗапись         - СправочникСсылка.УчетныеЗаписиМаркетплейсов - учетная запись подключения к сервису.
//   ИдентификаторОтгрузки - Строка - идентификатор отгрузки.
//   ОтправленияВОтгрузке  - Массив из Строка - актуальный список отправлений в отгрузке.
//
// Возвращаемое значение:
//   Строка - описание ошибки.
//
Функция ИзменитьСоставОтгрузкиВСервисе(УчетнаяЗапись, ИдентификаторОтгрузки, ОтправленияВОтгрузке)

	ТекстЗапроса = "/v1/carriage/set-postings";
	
	ПараметрыЗапроса = Новый Структура;
	ПараметрыЗапроса.Вставить("carriage_id", СтрокаВЧисло(ИдентификаторОтгрузки));
	ПараметрыЗапроса.Вставить("posting_numbers", ОтправленияВОтгрузке);
	
	СлужебныеПараметры = СлужебныеПараметрыHTTPЗапроса();
	СлужебныеПараметры.ВернутьСтрокуОтвета = Истина;
	
	ПараметрыЗапроса.Вставить("СлужебныеПараметры", СлужебныеПараметры);
	
	АтрибутыОтвета = Новый Структура("Результат", "result");
	
	// POST-запрос
	РезультатЗапроса = ПолучитьРезультатЗапроса(ТекстЗапроса, ПараметрыЗапроса, УчетнаяЗапись, АтрибутыОтвета);
	
	Ошибки = Новый Соответствие;
	
	Если РезультатЗапроса <> Неопределено Тогда
		Результат = "";
	Иначе
		Для Каждого ДанныеОбОтправлении Из РезультатЗапроса.Результат Цикл
			Если ДанныеОбОтправлении["result"] = Истина
					Или Не ЗначениеЗаполнено(ДанныеОбОтправлении["error"]) Тогда
				Продолжить;
			КонецЕсли;
			
			СписокОтправлений = Ошибки.Получить(ДанныеОбОтправлении["error"]);
			Если СписокОтправлений = Неопределено Тогда
				СписокОтправлений = Новый Массив;
			КонецЕсли;
			
			СписокОтправлений.Добавить(ДанныеОбОтправлении["posting_number"]);
			
			Ошибки.Вставить(ДанныеОбОтправлении["error"], СписокОтправлений);
		КонецЦикла;
		
		Если Ошибки.Количество() > 0 Тогда
			ОписанияОшибок = Новый Массив;
			Для Каждого ДанныеОбОшибке Из Ошибки Цикл
				ОписанияОшибок.Добавить(СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(
					НСтр("ru = '""%1"" для отправлений: %2';
						|en = '%1 for shipments: %2'"),
					ДанныеОбОшибке.Ключ,
					СтрСоединить(ДанныеОбОшибке.Значение, ", ")));
			КонецЦикла;
			
			Результат = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(
				НСтр("ru = 'Не удалось подтвердить отгрузку по причине:
							|	%1.';
							|en = 'Cannot confirm the shipment due to:
							|	%1.'"),
				СтрСоединить(ОписанияОшибок, Символы.ПС + Символы.Таб));
		КонецЕсли;
	КонецЕсли;
	
	Возврат Результат;

КонецФункции

