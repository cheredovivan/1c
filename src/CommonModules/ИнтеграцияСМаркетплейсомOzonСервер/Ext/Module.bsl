// Возвращает список доступных отгрузок на торговой площадке.
// Описание метода см. https://api-seller.ozon.ru/v1/posting/carriage-available/list.
//
// Параметры:
//   УчетнаяЗапись               - СправочникСсылка.УчетныеЗаписиМаркетплейсов - учетная запись подключения к сервису.
//   ИдентификаторМетодаДоставки - Строка - идентификатор метода доставки склада площадки.
//   ДатаОтгрузки                - Дата - дата создания отгрузки.
//   СмещениеЧасовогоПояса       - Число - количество секунд смещения времени.
// 
// Возвращаемое значение:
//   Массив из см. НовыйСписокАтрибутовДоступныхОтгрузок - данные о доступных отгрузках.
//
Функция СписокДоступныхОтгрузокВСервисе(УчетнаяЗапись, ИдентификаторМетодаДоставки, ДатаОтгрузки,
			СмещениеЧасовогоПояса = 0)

	ТекстЗапроса = "/v1/posting/carriage-available/list";
	
	ДатаЗапроса = ИнтеграцияСМаркетплейсамиСервер.ДатаСоСмещениемЧасовогоПояса(
		ДатаОтгрузки,
		СмещениеЧасовогоПояса,
		Истина);
	
	ПараметрыЗапроса = Новый Структура;
	ПараметрыЗапроса.Вставить("delivery_method_id", СтрокаВЧисло(ИдентификаторМетодаДоставки));
	ПараметрыЗапроса.Вставить("departure_date",     ДатаUTC(ДатаЗапроса));
	
	СлужебныеПараметры = СлужебныеПараметрыHTTPЗапроса();
	
	МассивПолейТипаДата = Новый Массив;
	МассивПолейТипаДата.Добавить("cutoff_at");
	
	СлужебныеПараметры.ПоляТипаДата = МассивПолейТипаДата;
	
	ПараметрыЗапроса.Вставить("СлужебныеПараметры", СлужебныеПараметры);
	
	АтрибутыОтвета = Новый Структура("Результат", "result");
	
	// POST-запрос
	РезультатЗапроса = ПолучитьРезультатЗапроса(ТекстЗапроса, ПараметрыЗапроса, УчетнаяЗапись, АтрибутыОтвета);
	
	Результат = Новый Массив;
	Если РезультатЗапроса.Результат <> Неопределено Тогда
		Для Каждого ДанныеОбОтгрузкеИзСервиса Из РезультатЗапроса.Результат Цикл
			АтрибутыОтвета = НовыйСписокАтрибутовДоступныхОтгрузок();
			ДанныеОбОтгрузке = ОбработатьРезультатЗапроса(ДанныеОбОтгрузкеИзСервиса, АтрибутыОтвета);
			
			ОбработатьДанныеОбОтгрузке(ДанныеОбОтгрузке);
			
			Результат.Добавить(ДанныеОбОтгрузке);
		КонецЦикла;
	КонецЕсли;
	
	Возврат Результат;

КонецФункции

// Возвращает список доступных отгрузок на торговой площадке.
// Описание метода см. https://api-seller.ozon.ru/v1/carriage/delivery/list.
//
// Параметры:
//   УчетнаяЗапись               - СправочникСсылка.УчетныеЗаписиМаркетплейсов - учетная запись подключения к сервису.
//   ИдентификаторМетодаДоставки - Строка - идентификатор метода доставки склада площадки.
//   ДатаОтгрузки                - Дата - дата создания отгрузки.
//   СмещениеЧасовогоПояса       - Число - количество секунд смещения времени.
//
// Возвращаемое значение:
//   Массив из см. НовыйСписокАтрибутовДоступныхОтгрузок - данные о доступных отгрузках.
//
Функция СписокДоступныхПодтверждаемыхОтгрузокВСервисе(УчетнаяЗапись, ИдентификаторМетодаДоставки, ДатаОтгрузки,
			СмещениеЧасовогоПояса = 0)

	ТекстЗапроса = "/v1/carriage/delivery/list";
	
	ДатаЗапроса = ИнтеграцияСМаркетплейсамиСервер.ДатаСоСмещениемЧасовогоПояса(
		ДатаОтгрузки,
		СмещениеЧасовогоПояса,
		Истина);
	
	ПараметрыЗапроса = Новый Структура;
	ПараметрыЗапроса.Вставить("delivery_method_id", СтрокаВЧисло(ИдентификаторМетодаДоставки));
	ПараметрыЗапроса.Вставить("departure_date",     ДатаUTC(ДатаЗапроса));
	
	СлужебныеПараметры = СлужебныеПараметрыHTTPЗапроса();
	
	МассивПолейТипаДата = Новый Массив;
	МассивПолейТипаДата.Добавить("cutoff_at");
	
	СлужебныеПараметры.ПоляТипаДата = МассивПолейТипаДата;
	
	ПараметрыЗапроса.Вставить("СлужебныеПараметры", СлужебныеПараметры);
	
	АтрибутыОтвета = Новый Структура("Результат", "result");
	
	// POST-запрос
	РезультатЗапроса = ПолучитьРезультатЗапроса(ТекстЗапроса, ПараметрыЗапроса, УчетнаяЗапись, АтрибутыОтвета);
	
	Результат = Новый Массив;
	Если РезультатЗапроса.Результат <> Неопределено Тогда
		Для Каждого ДанныеОМетодеДоставкиОтгрузкиИзСервиса Из РезультатЗапроса.Результат Цикл
			АтрибутыОтвета = НовыйСписокАтрибутовДоступныхОтгрузок(, Истина);
			ДанныеОбОтгрузке = ОбработатьРезультатЗапроса(ДанныеОМетодеДоставкиОтгрузкиИзСервиса, АтрибутыОтвета);
			
			Для Каждого ДанныеОбОтгрузкеИзСервиса Из ДанныеОбОтгрузке.СписокОтгрузок Цикл
				ДанныеОбОтгрузкеДополненные = ОбработатьРезультатЗапроса(ДанныеОбОтгрузкеИзСервиса, АтрибутыОтвета);
				
				ПоляОтгрузки = "ИдентификаторОтгрузки, КоличествоОтправленийВОтгрузке, Статус";
				ЗаполнитьЗначенияСвойств(ДанныеОбОтгрузкеДополненные, ДанныеОбОтгрузке,, ПоляОтгрузки);
				ОбработатьДанныеОбОтгрузке(ДанныеОбОтгрузкеДополненные, Истина);
				ДанныеОбОтгрузкеДополненные.СписокОтгрузок = Новый Массив;
				
				Результат.Добавить(ДанныеОбОтгрузкеДополненные);
			КонецЦикла;
			
			Если Результат.Количество() = 0 Тогда
				ОбработатьДанныеОбОтгрузке(ДанныеОбОтгрузке, Истина);
				Результат.Добавить(ДанныеОбОтгрузке);
			КонецЕсли;
		КонецЦикла;
	КонецЕсли;
	
	Возврат Результат;

КонецФункции

// Получает информацию о результате задания на асинхронное формирование отгрузки в сервисе Ozon.
// Описание метода см. https://api-seller.ozon.ru/v2/posting/fbs/act/check-status.
//
// Параметры:
//   УчетнаяЗапись         - СправочникСсылка.УчетныеЗаписиМаркетплейсов - учетная запись подключения к сервису.
//   ИдентификаторОтгрузки - Строка - номер задания формирования отгрузки.
//
// Возвращаемое значение:
//   см. НовыйСписокАтрибутовПроверкиСтатусаОтгрузки.
//
Функция ПроверитьСтатусОтгрузкиВСервисе(УчетнаяЗапись, ИдентификаторОтгрузки)

	ТекстЗапроса = "/v2/posting/fbs/act/check-status";
	
	ПараметрыЗапроса = Новый Структура;
	ПараметрыЗапроса.Вставить("id", СтрокаВЧисло(ИдентификаторОтгрузки));
	
	АтрибутыОтвета = НовыйСписокАтрибутовПроверкиСтатусаОтгрузки();
	
	// POST-запрос
	РезультатЗапроса = ПолучитьРезультатЗапроса(ТекстЗапроса, ПараметрыЗапроса, УчетнаяЗапись, АтрибутыОтвета);
	
	РезультатПроверкиСтатусаОтгрузки = НовыйСписокАтрибутовПроверкиСтатусаОтгрузки(Ложь);
	Если РезультатЗапроса.Статус <> Неопределено Тогда
		ПривестиЗначенияРезультатаЗапроса(РезультатЗапроса, РезультатПроверкиСтатусаОтгрузки,
			"ОтправленияВОтгрузке, ОтправленияНеВОтгрузке, Статус, ОтгрузкаСформирована");
		
		СтатусыНесформированойОтгрузки = Новый Массив;
		СтатусыНесформированойОтгрузки.Добавить(ВРег("in_process"));
		
		Если СтатусыНесформированойОтгрузки.Найти(ВРег(РезультатЗапроса.Статус)) = Неопределено Тогда
			РезультатЗапроса.ОтгрузкаСформирована = Истина;
		КонецЕсли;
		
		РезультатЗапроса.Статус = ПредставлениеПоЗначению(РезультатЗапроса.Статус, "ФормированиеОтгрузки");
		
		ЗаполнитьЗначенияСвойств(РезультатПроверкиСтатусаОтгрузки, РезультатЗапроса);
	КонецЕсли;
	
	Возврат РезультатПроверкиСтатусаОтгрузки;

КонецФункции

// Возвращает информацию об отгрузке на торговой площадке.
// Описание метода см. https://api-seller.ozon.ru/v1/carriage/get.
//
// Параметры:
//   УчетнаяЗапись         - СправочникСсылка.УчетныеЗаписиМаркетплейсов - учетная запись подключения к сервису.
//   ИдентификаторОтгрузки - Строка - идентификатор отгрузки.
// 
// Возвращаемое значение:
//   см. РегистрыСведений.ЗаказыТорговыхПлощадок.НовыйСписокАтрибутовИнформацииОбОтгрузке.
//
Функция ИнформацияОбОтгрузкеВСервисе(УчетнаяЗапись, ИдентификаторОтгрузки)

	ТекстЗапроса = "/v1/carriage/get";
	
	ПараметрыЗапроса = Новый Структура;
	ПараметрыЗапроса.Вставить("carriage_id", СтрокаВЧисло(ИдентификаторОтгрузки));
	
	СлужебныеПараметры = СлужебныеПараметрыHTTPЗапроса();
	
	МассивПолейТипаДата = Новый Массив;
	МассивПолейТипаДата.Добавить("created_at");
	МассивПолейТипаДата.Добавить("updated_at");
	
	СлужебныеПараметры.ПоляТипаДата = МассивПолейТипаДата;
	
	ПараметрыЗапроса.Вставить("СлужебныеПараметры", СлужебныеПараметры);
	
	АтрибутыОтвета = НовыйСписокАтрибутовИнформацииОбОтгрузкеВСервисе();
	
	// POST-запрос
	РезультатЗапроса = ПолучитьРезультатЗапроса(ТекстЗапроса, ПараметрыЗапроса, УчетнаяЗапись, АтрибутыОтвета);
	
	ИнформацияОбОтгрузке = РегистрыСведений.ЗаказыТорговыхПлощадок.НовыйСписокАтрибутовИнформацииОбОтгрузке(УчетнаяЗапись);
	
	Если РезультатЗапроса.ИдентификаторОтгрузки <> Неопределено Тогда
		Если Не ПустаяСтрока(РезультатЗапроса.ДатаВыполнения) Тогда
			РезультатЗапроса.ДатаВыполнения =
				ПривестиЗначениеКТипу(СтрЗаменить(СокрЛП(РезультатЗапроса.ДатаВыполнения), "-", "") + "000000", Тип("Дата"));
		КонецЕсли;
		
		ПривестиЗначенияРезультатаЗапроса(РезультатЗапроса, ИнформацияОбОтгрузке,
			"Статус, ОтправленияВОтгрузке, ДоступныеДействия, Пропуска");
		
		РезультатЗапроса.МожноРедактировать = Ложь;
		
		СтатусыНесформированойОтгрузки = Новый Массив;
		СтатусыНесформированойОтгрузки.Добавить(ВРег("success"));
		СтатусыНесформированойОтгрузки.Добавить(ВРег("in_process"));
		СтатусыНесформированойОтгрузки.Добавить(ВРег("error"));
		
		Если СтатусыНесформированойОтгрузки.Найти(ВРег(РезультатЗапроса.Статус)) <> Неопределено Тогда
			РезультатЗапроса.ПовторитьЗапрос = Истина;
		КонецЕсли;
		
		СтатусыНесформированойОтгрузки.Добавить(ВРег("new"));
		
		Если СтатусыНесформированойОтгрузки.Найти(ВРег(РезультатЗапроса.Статус)) = Неопределено Тогда
			РезультатЗапроса.ОтгрузкаСформирована = Истина;
		КонецЕсли;
		
		СтатусыНеактуальнойОтгрузки = Новый Массив;
		СтатусыНеактуальнойОтгрузки.Добавить(ВРег("cancelled"));
		
		Если СтатусыНеактуальнойОтгрузки.Найти(ВРег(РезультатЗапроса.Статус)) <> Неопределено Тогда
			РезультатЗапроса.ОтгрузкаОтменена = Истина;
		КонецЕсли;
		
		СтатусыОбрабатываемойОтменыОтгрузки = Новый Массив;
		СтатусыОбрабатываемойОтменыОтгрузки.Добавить(ВРег("cancelation_processing"));
		СтатусыОбрабатываемойОтменыОтгрузки.Добавить(ВРег("cancelation_enqueued"));
		СтатусыОбрабатываемойОтменыОтгрузки.Добавить(ВРег("cancelation_failed"));
		СтатусыОбрабатываемойОтменыОтгрузки.Добавить(ВРег("error"));
		
		Если СтатусыОбрабатываемойОтменыОтгрузки.Найти(ВРег(РезультатЗапроса.Статус)) <> Неопределено Тогда
			РезультатЗапроса.ПовторитьЗапрос = Истина;
		КонецЕсли;
		
		ОбщегоНазначенияКлиентСервер.ДополнитьМассив(
			СтатусыНеактуальнойОтгрузки,
			СтатусыОбрабатываемойОтменыОтгрузки);
		
		СтатусыНеактуальнойОтгрузки.Добавить(ВРег("received"));
		СтатусыНеактуальнойОтгрузки.Добавить(ВРег("closed"));
		
		Если СтатусыНеактуальнойОтгрузки.Найти(ВРег(РезультатЗапроса.Статус)) = Неопределено Тогда
			РезультатЗапроса.ОтгрузкаАктуальна = Истина;
		КонецЕсли;
		
		Для Каждого ДоступноеДействие Из РезультатЗапроса.ДоступныеДействия Цикл
			Если ВРег(ДоступноеДействие) = ВРег("approve") Тогда
				РезультатЗапроса.МожноРедактировать = Истина;
			КонецЕсли;
			
			Если ВРег(ДоступноеДействие) = ВРег("cancel") Тогда
				РезультатЗапроса.МожноОтменить = Истина;
			КонецЕсли;
			
			Если ВРег(ДоступноеДействие) = ВРег("set_arrival_passes")
					Или ВРег(ДоступноеДействие) = ВРег("edit_arrival_passes")
					Или ВРег(ДоступноеДействие) = ВРег("mark_pass_for_returns") Тогда
				РезультатЗапроса.ТребуетсяПропуск = Истина;
			КонецЕсли;
			
			Если ВРег(ДоступноеДействие) = ВРег("get_label_containers") Тогда
				РезультатЗапроса.ДоступныеПечатныеФормы.Добавить("ПечатьЭтикетокГрузовыхМест");
			КонецЕсли;
			
			Если ВРег(ДоступноеДействие) = ВРег("get_waybill") Тогда
				РезультатЗапроса.ДоступныеПечатныеФормы.Добавить("ПечатьНакладнойАкта");
			КонецЕсли;
			
			Если ВРег(ДоступноеДействие) = ВРег("get_document")
					Или ВРег(ДоступноеДействие) = ВРег("get_digital_document") Тогда
				РезультатЗапроса.ДоступныеПечатныеФормы.Добавить("ПечатьЛистаОтгрузки");
				РезультатЗапроса.ДоступныеПечатныеФормы.Добавить("ПечатьШтрихкодаОтгрузки");
			КонецЕсли;
		КонецЦикла;
		
		Если РезультатЗапроса.ДоступныеПечатныеФормы.Количество() > 0 Тогда
			РезультатЗапроса.ДоступныеПечатныеФормы.Добавить("ПечатьКомплектаДокументов");
		КонецЕсли;
		
		РезультатЗапроса.Статус =
			ПредставлениеПоЗначению(
				РезультатЗапроса.Статус,
				?(ИнформацияОбОтгрузке.МожноРедактировать, "ОтгрузкаСПодтверждением", "Отгрузка"));
		
		ЗаполнитьЗначенияСвойств(ИнформацияОбОтгрузке, РезультатЗапроса);
	КонецЕсли;
	
	Возврат ИнформацияОбОтгрузке; // РегистрыСведений.ЗаказыТорговыхПлощадок.НовыйСписокАтрибутовИнформацииОбОтгрузке

КонецФункции

// Возвращает список отправлений в отгрузке на торговой площадке.
// Описание метода см. https://api-seller.ozon.ru/v2/posting/fbs/act/get-postings.
//
// Параметры:
//   УчетнаяЗапись         - СправочникСсылка.УчетныеЗаписиМаркетплейсов - учетная запись подключения к сервису.
//   ИдентификаторОтгрузки - Строка - идентификатор отгрузки.
// 
// Возвращаемое значение:
//   Массив из см. НовыйСписокАтрибутовОтправленийВОтгрузке - список отправлений в отгрузке.
//
Функция СписокОтправленийВОтгрузкеВСервисе(УчетнаяЗапись, ИдентификаторОтгрузки)

	ТекстЗапроса = "/v2/posting/fbs/act/get-postings";
	
	ПараметрыЗапроса = Новый Структура;
	ПараметрыЗапроса.Вставить("id", СтрокаВЧисло(ИдентификаторОтгрузки));
	
	АтрибутыОтвета = Новый Структура("Результат", "result");
	
	// POST-запрос
	РезультатЗапроса = ПолучитьРезультатЗапроса(ТекстЗапроса, ПараметрыЗапроса, УчетнаяЗапись, АтрибутыОтвета);
	
	Результат = Новый Массив;
	Если РезультатЗапроса.Результат <> Неопределено Тогда
		СтатусыВОтгрузке = Новый Массив;
		СтатусыВОтгрузке.Добавить(ВРег("new"));
		СтатусыВОтгрузке.Добавить(ВРег("success"));
		
		Для Каждого ДанныеОбОтправленииИзСервиса Из РезультатЗапроса.Результат Цикл
			АтрибутыОтвета = НовыйСписокАтрибутовОтправленийВОтгрузке();
			ДанныеОбОтправлении = ОбработатьРезультатЗапроса(ДанныеОбОтправленииИзСервиса, АтрибутыОтвета);
			
			АтрибутыОтвета = НовыйСписокАтрибутовОтправленийВОтгрузке(Ложь);
			ПривестиЗначенияРезультатаЗапроса(ДанныеОбОтправлении, АтрибутыОтвета, "Статус");
			
			Если СтатусыВОтгрузке.Найти(ВРег(ДанныеОбОтправлении.Статус)) <> Неопределено Тогда
				ДанныеОбОтправлении.ОтправлениеВОтгрузке = Истина;
			КонецЕсли;
			
			ДанныеОбОтправлении.Статус = ПредставлениеПоЗначению(ДанныеОбОтправлении.Статус, "ОтправлениеВОтгрузке");
			
			Результат.Добавить(ДанныеОбОтправлении);
		КонецЦикла;
	КонецЕсли;
	
	Возврат Результат;

КонецФункции

// Создает задание на формирование отгрузки в сервисе Ozon.
// Описание метода см. https://api-seller.ozon.ru/v2/posting/fbs/act/create.
//
// Параметры:
//   УчетнаяЗапись               - СправочникСсылка.УчетныеЗаписиМаркетплейсов - учетная запись подключения к сервису.
//   ИдентификаторМетодаДоставки - Строка - идентификатор метода доставки склада площадки.
//   ДатаОтгрузки                - Дата - дата создания отгрузки.
//   СмещениеЧасовогоПояса       - Число - количество секунд смещения времени.
//   КоличествоГрузовыхМест      - Число - количество грузовых мест в отгрузке.
// 
// Возвращаемое значение:
//   Структура из КлючИЗначение - результат формирования отгрузки:
//     * ИдентификаторОтгрузки - Строка - номер задания формирования отгрузки.
//     * ОписаниеОшибки        - Строка - описание ошибки с торговой площадки.
//
Функция СформироватьОтгрузкуВСервисе(УчетнаяЗапись, ИдентификаторМетодаДоставки, ДатаОтгрузки,
			СмещениеЧасовогоПояса = 0, КоличествоГрузовыхМест = 0)

	ТекстЗапроса = "/v2/posting/fbs/act/create";
	
	ДатаЗапроса = ИнтеграцияСМаркетплейсамиСервер.ДатаСоСмещениемЧасовогоПояса(
		ДатаОтгрузки,
		СмещениеЧасовогоПояса,
		Истина);
	
	ПараметрыЗапроса = Новый Структура;
	ПараметрыЗапроса.Вставить("delivery_method_id", СтрокаВЧисло(ИдентификаторМетодаДоставки));
	ПараметрыЗапроса.Вставить("departure_date",     ДатаUTC(ДатаЗапроса));
	
	Если ЗначениеЗаполнено(КоличествоГрузовыхМест) Тогда
		ПараметрыЗапроса.Вставить("containers_count", КоличествоГрузовыхМест);
	КонецЕсли;
	
	СлужебныеПараметры = СлужебныеПараметрыHTTPЗапроса();
	СлужебныеПараметры.ВернутьСтрокуОтвета = Истина;
	
	ПараметрыЗапроса.Вставить("СлужебныеПараметры", СлужебныеПараметры);
	
	АтрибутыОтвета = Новый Структура;
	АтрибутыОтвета.Вставить("ОписаниеОшибки",        "message");
	АтрибутыОтвета.Вставить("ИдентификаторОтгрузки", "result.id");
	
	// POST-запрос
	РезультатЗапроса = ПолучитьРезультатЗапроса(ТекстЗапроса, ПараметрыЗапроса, УчетнаяЗапись, АтрибутыОтвета);
	
	Результат = Новый Структура;
	Результат.Вставить("ОписаниеОшибки",        "");
	Результат.Вставить("ИдентификаторОтгрузки", "");
	
	Если ЗначениеЗаполнено(РезультатЗапроса.ОписаниеОшибки) Тогда
		Результат.ОписаниеОшибки = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(
			НСтр("ru = 'Не удалось сформировать отгрузку на дату %1 по причине: ""%2"".';
				|en = 'Cannot generate a shipment as of %1 due to: %2.'"),
			Формат(ДатаОтгрузки, "ДЛФ=D"),
			ПредставлениеПоЗначению(РезультатЗапроса.ОписаниеОшибки, "ОшибкаФормированияОтгрузки"));
	КонецЕсли;
	
	Если ЗначениеЗаполнено(РезультатЗапроса.ИдентификаторОтгрузки) Тогда
		Результат.ИдентификаторОтгрузки = ЧислоВСтроку(РезультатЗапроса.ИдентификаторОтгрузки);
	КонецЕсли;
	
	Возврат Результат;

КонецФункции

// Создает задание на формирование отгрузки в сервисе Ozon.
// Описание метода см. https://api-seller.ozon.ru/v1/carriage/create.
//
// Параметры:
//   УчетнаяЗапись               - СправочникСсылка.УчетныеЗаписиМаркетплейсов - учетная запись подключения к сервису.
//   ИдентификаторМетодаДоставки - Строка - идентификатор метода доставки склада площадки.
//   ДатаОтгрузки                - Дата - дата создания отгрузки.
//   СмещениеЧасовогоПояса       - Число - количество секунд смещения времени.
//   КоличествоГрузовыхМест      - Число - количество грузовых мест в отгрузке.
// 
// Возвращаемое значение:
//   Структура - результат формирования отгрузки:
//     * ИдентификаторОтгрузки - Строка - номер задания формирования отгрузки.
//     * ОписаниеОшибки        - Строка - описание ошибки с торговой площадки.
//
Функция СформироватьПодтверждаемуюОтгрузкуВСервисе(УчетнаяЗапись, ИдентификаторМетодаДоставки, ДатаОтгрузки,
			СмещениеЧасовогоПояса = 0, КоличествоГрузовыхМест = 0)

	ТекстЗапроса = "/v1/carriage/create";
	
	ДатаЗапроса = ИнтеграцияСМаркетплейсамиСервер.ДатаСоСмещениемЧасовогоПояса(
		ДатаОтгрузки,
		СмещениеЧасовогоПояса,
		Истина);
	
	ПараметрыЗапроса = Новый Структура;
	ПараметрыЗапроса.Вставить("delivery_method_id", СтрокаВЧисло(ИдентификаторМетодаДоставки));
	ПараметрыЗапроса.Вставить("departure_date",     ДатаUTC(ДатаЗапроса));
	
	СлужебныеПараметры = СлужебныеПараметрыHTTPЗапроса();
	СлужебныеПараметры.ВернутьСтрокуОтвета = Истина;
	
	ПараметрыЗапроса.Вставить("СлужебныеПараметры", СлужебныеПараметры);
	
	АтрибутыОтвета = Новый Структура;
	АтрибутыОтвета.Вставить("ОписаниеОшибки",        "message");
	АтрибутыОтвета.Вставить("ИдентификаторОтгрузки", "carriage_id");
	
	// POST-запрос
	РезультатЗапроса = ПолучитьРезультатЗапроса(ТекстЗапроса, ПараметрыЗапроса, УчетнаяЗапись, АтрибутыОтвета);
	
	Результат = Новый Структура;
