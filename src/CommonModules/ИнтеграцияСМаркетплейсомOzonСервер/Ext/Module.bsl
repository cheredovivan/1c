																	|en = '<Collection of values from the list>'");
		ИначеЕсли НоваяСтрока.ЯвляетсяКоллекциейЗначений Тогда
			НоваяСтрока.ПредставлениеТипаСоответствияЗначений = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(
				НСтр("ru = '<Коллекция значений типа %1>';
					|en = '<Collection of values of the %1 type>'"),
				ОпределитьПредставлениеТипа(НоваяСтрока.ТипЗначенияАтрибута1С));
		ИначеЕсли НоваяСтрока.НомерКартинкиКоллекцииТипов = 9 Тогда
			НоваяСтрока.ПредставлениеТипаСоответствияЗначений = НСтр("ru = '<Значение из списка>';
																	|en = '<Value from the list>'");
		Иначе
			НоваяСтрока.ПредставлениеТипаСоответствияЗначений = "<" + ОпределитьПредставлениеТипа(НоваяСтрока.ТипЗначенияАтрибута1С) + ">";
		КонецЕсли;

		РеквизитыЗаполнения = Неопределено;
		Если ПутьКРеквизитам1СПоУмолчанию.Свойство("Атрибут_" + ДанныеАтрибута.ИдентификаторАтрибутаМаркетплейса, РеквизитыЗаполнения) Тогда
			ПорядокСортировки = 0;
			РеквизитыЗаполнения.Свойство("ПорядокСортировки", ПорядокСортировки);
			НоваяСтрока.ПорядокСортировки = ПорядокСортировки;
		КонецЕсли;
	КонецЦикла;

	ДеревоРеквизитов.Строки.Сортировать("Уровень Возр, ОбязательныйАтрибут Убыв, ПорядокСортировки, НаименованиеАтрибутаМаркетплейса Возр", Истина);
	ДеревоРеквизитов.Колонки.Удалить("ПорядокСортировки");

	Возврат ДеревоРеквизитов;

КонецФункции

Функция ПолучитьОписаниеТовараМаркетплейса(ДанныеОбъекта, ПараметрыЗаполнения)

	Если ДанныеОбъекта = Неопределено
			Или Не ЗначениеЗаполнено(ПараметрыЗаполнения.ИдентификаторПубликации) 
					И ДанныеОбъекта.Количество() = 0 Тогда
		Возврат ПолучитьОписаниеНовогоТовараМаркетплейса();
	КонецЕсли;

	ТабличныйДокумент = Новый ТабличныйДокумент;
	ТабличныйДокументАтрибутов = Новый ТабличныйДокумент;

	Макет = РегистрыСведений.СтатусыПубликацииОбъектовМаркетплейсаOzon.ПолучитьМакет("ПФ_MXL_ОписаниеТовара");

	ОбластьЗаголовка = Макет.ПолучитьОбласть("ЗаголовокГруппыДанных");

	ОбластьМаркера = Макет.ПолучитьОбласть("Маркер|ОбластьМаркера");
	ОбластьПустогоМаркера = Макет.ПолучитьОбласть("СтрокаСвойства|ОбластьМаркера");

	ОбластьРазделитель = Макет.ПолучитьОбласть("Разделитель");

	ОбластьСвойствоЗаголовок         = Макет.ПолучитьОбласть("СтрокаСвойства|ОбластьЗаголовок");
	ОбластьСвойствоЗначениеЗаполнено = Макет.ПолучитьОбласть("СтрокаСвойства|ОбластьЗначение");
	ОбластьСвойствоПустоеЗначение    = Макет.ПолучитьОбласть("СтрокаСвойстваПустоеЗначение|ОбластьЗначение");
	ОбластьСвойствоПримечание        = Макет.ПолучитьОбласть("СтрокаСвойства|ОбластьПримечание");

	ОбластьГруппаАтрибутов = Макет.ПолучитьОбласть("СтрокаГруппыАтрибутов");
	
	ОбластьАтрибутГруппыОбязательныйЗаголовок   = Макет.ПолучитьОбласть("СтрокаАтрибутаГруппы|ОбластьЗаголовок");
	ОбластьАтрибутГруппыНеобязательныйЗаголовок = Макет.ПолучитьОбласть("СтрокаАтрибутаГруппыНеобязательного|ОбластьЗаголовок");
	ОбластьАтрибутОбязательныйЗаголовок         = Макет.ПолучитьОбласть("СтрокаАтрибута|ОбластьЗаголовок");
	ОбластьАтрибутНеобязательныйЗаголовок       = Макет.ПолучитьОбласть("СтрокаАтрибутаНеобязательного|ОбластьЗаголовок");
	
	ОбластьАтрибутЗначениеЗаполнено = Макет.ПолучитьОбласть("СтрокаАтрибутаГруппы|ОбластьЗначение");
	ОбластьАтрибутПустоеЗначение    = Макет.ПолучитьОбласть("СтрокаАтрибутаГруппыНеобязательного|ОбластьЗначение");
	ОбластьАтрибутПримечание        = Макет.ПолучитьОбласть("СтрокаАтрибутаГруппы|ОбластьПримечание");

	// Основные данные.
	ОбластьЗаголовка.Параметры.Заголовок = НСтр("ru = 'Основные данные';
												|en = 'Main data'");
	ТабличныйДокумент.Вывести(ОбластьЗаголовка);

	// Представление товара.
	ТабличныйДокумент.Вывести(ОбластьПустогоМаркера);
	ОбластьСвойствоЗаголовок.Параметры.Заголовок = НСтр("ru = 'Представление товара на площадке маркетплейса';
														|en = 'Item presentation on the marketplace'");
	ТабличныйДокумент.Присоединить(ОбластьСвойствоЗаголовок);
	Значение = Строка(ПараметрыЗаполнения.ПредставлениеОбъектаМаркетплейса);
	Если ПустаяСтрока(Значение) Тогда
		Значение = НСтр("ru = 'Не задано представление товара';
						|en = 'Item presentation is not set'");
		Область = ОбластьСвойствоПустоеЗначение;
	Иначе
		Область = ОбластьСвойствоЗначениеЗаполнено;
	КонецЕсли;
	Область.Параметры.Значение = Значение;
	ТабличныйДокумент.Присоединить(Область);
	
	// Штрихкод.
	ТабличныйДокумент.Вывести(ОбластьПустогоМаркера);
	ОбластьСвойствоЗаголовок.Параметры.Заголовок = НСтр("ru = 'Штрихкод';
														|en = 'Barcode'");
	ТабличныйДокумент.Присоединить(ОбластьСвойствоЗаголовок);
	Значение = Строка(ПараметрыЗаполнения.Штрихкод);
	Если ПустаяСтрока(Значение) Тогда
		Значение = НСтр("ru = 'Не задан штрихкод';
						|en = 'Barcode is not set'");
		Область = ОбластьСвойствоПустоеЗначение;
	Иначе
		Область = ОбластьСвойствоЗначениеЗаполнено ;
	КонецЕсли;
	Область.Параметры.Значение = Значение;
	ТабличныйДокумент.Присоединить(Область);
	
	// Ставка НДС.
	ТабличныйДокумент.Вывести(ОбластьПустогоМаркера);
	ОбластьСвойствоЗаголовок.Параметры.Заголовок = НСтр("ru = 'Ставка НДС (из номенклатуры)';
														|en = 'VAT rate (from the items)'");
	ТабличныйДокумент.Присоединить(ОбластьСвойствоЗаголовок);
	Значение = ЧислоВСтроку(ПараметрыЗаполнения.СтавкаНДС, Истина);
	Если ПустаяСтрока(Значение) Тогда
		Значение = НСтр("ru = 'Не задана ставка НДС';
						|en = 'VAT rate is not specified'");
		Область = ОбластьСвойствоПустоеЗначение;
	Иначе
		Значение = Значение + " %";
		Область = ОбластьСвойствоЗначениеЗаполнено ;
	КонецЕсли;
	Область.Параметры.Значение = Значение;
	ТабличныйДокумент.Присоединить(Область);

	// Категория 1С.
	ТипКатегории = ТипЗнч(ПараметрыЗаполнения.Категория1С);
	Если ТипКатегории = Тип("СправочникСсылка.Номенклатура") Тогда
		ТипКатегорииСтрокой = НРег(Перечисления.ИсточникиКатегорийДляМаркетплейса.ИерархияНоменклатуры);
	ИначеЕсли ТипКатегории = Тип("СправочникСсылка.ВидыНоменклатуры") Тогда
		ТипКатегорииСтрокой = НРег(Перечисления.ИсточникиКатегорийДляМаркетплейса.ВидНоменклатуры);
	ИначеЕсли ТипКатегории = Тип("СправочникСсылка.ТоварныеКатегории") Тогда
		ТипКатегорииСтрокой = НРег(Перечисления.ИсточникиКатегорийДляМаркетплейса.ТоварнаяКатегория);
	Иначе
		ТипКатегорииСтрокой = "";
	КонецЕсли;
	
	ТабличныйДокумент.Вывести(ОбластьПустогоМаркера);
	ОбластьСвойствоЗаголовок.Параметры.Заголовок = НСтр("ru = 'Категория 1С';
														|en = '1С category'")
		+ ?(ПустаяСтрока(ТипКатегорииСтрокой), "", " (" + ТипКатегорииСтрокой + ")");
	ТабличныйДокумент.Присоединить(ОбластьСвойствоЗаголовок);
	Значение = Строка(ПараметрыЗаполнения.Категория1С);
	Если ПустаяСтрока(Значение) Тогда
		Значение = НСтр("ru = 'Не определена категория 1С';
						|en = '1C category is undefined'");
		Область = ОбластьСвойствоПустоеЗначение;
	Иначе
		Область = ОбластьСвойствоЗначениеЗаполнено;
	КонецЕсли;
	Область.Параметры.Значение = Значение;
	ТабличныйДокумент.Присоединить(Область);

	// Источник категории маркетплейса.
	ТабличныйДокумент.Вывести(ОбластьПустогоМаркера);
	ОбластьСвойствоЗаголовок.Параметры.Заголовок = НСтр("ru = 'Категория 1С, являющаяся источником категории маркетплейса (по настройкам соответствия)';
														|en = '1C category that is a marketplace category source (according to the mapping settings)'");
	ТабличныйДокумент.Присоединить(ОбластьСвойствоЗаголовок);
	Значение = Строка(ПараметрыЗаполнения.ИсточникКатегорииМаркетплейса);
	Если ПустаяСтрока(Значение) Тогда
		Значение = НСтр("ru = 'Не определен источник категории маркетплейса';
						|en = 'Marketplace category source is undefined'");
		Область = ОбластьСвойствоПустоеЗначение;
	Иначе
		Область = ОбластьСвойствоЗначениеЗаполнено;
	КонецЕсли;
	Область.Параметры.Значение = Значение;
	ТабличныйДокумент.Присоединить(Область);

	// Категория маркетплейса.
	ТабличныйДокумент.Вывести(ОбластьПустогоМаркера);
	ОбластьСвойствоЗаголовок.Параметры.Заголовок = НСтр("ru = 'Категория маркетплейса';
														|en = 'Marketplace category'");
	ТабличныйДокумент.Присоединить(ОбластьСвойствоЗаголовок);
	Значение = СтрЗаменить(ПараметрыЗаполнения.НаименованиеКатегорииМаркетплейса, НСтр("ru = '<не определена>';
																						|en = '<not defined>'"), "");
	Если ПустаяСтрока(Значение) Тогда
		Значение = НСтр("ru = 'Не определена категория маркетплейса';
						|en = 'Marketplace category is undefined'");
		Область = ОбластьСвойствоПустоеЗначение;
	Иначе
		Область = ОбластьСвойствоЗначениеЗаполнено;
	КонецЕсли;
	Если Не ПустаяСтрока(ПараметрыЗаполнения.ИдентификаторКатегорииМаркетплейса) Тогда
		Значение = Значение + " (id " + ПараметрыЗаполнения.ИдентификаторКатегорииМаркетплейса + ")";
	КонецЕсли;
	Область.Параметры.Значение = Значение;
	ТабличныйДокумент.Присоединить(Область);

	// Идентификатор публикации - идентификатор товара в 1С.
	ТабличныйДокумент.Вывести(ОбластьПустогоМаркера);
	ОбластьСвойствоЗаголовок.Параметры.Заголовок = НСтр("ru = 'Идентификатор публикации (артикул на площадке маркетплейса)';
														|en = 'Publication ID (item ID on the marketplace)'");
	ТабличныйДокумент.Присоединить(ОбластьСвойствоЗаголовок);
	ОбластьСвойствоЗначениеЗаполнено.Параметры.Значение = ПараметрыЗаполнения.ИдентификаторПубликации;
	ТабличныйДокумент.Присоединить(ОбластьСвойствоЗначениеЗаполнено);

	// Идентификатор товара на маркетплейсе.
	ТабличныйДокумент.Вывести(ОбластьПустогоМаркера);
	ОбластьСвойствоЗаголовок.Параметры.Заголовок = НСтр("ru = 'Идентификатор товара на маркетплейсе';
														|en = 'Item ID on the marketplace'");
	ТабличныйДокумент.Присоединить(ОбластьСвойствоЗаголовок);
	Значение = ПараметрыЗаполнения.ИдентификаторОбъектаМаркетплейса;
	Если ПустаяСтрока(Значение) Тогда
		Значение = НСтр("ru = 'Товар не прошел модерацию';
						|en = 'The item has not passed moderation'");
		Область = ОбластьСвойствоПустоеЗначение;
	Иначе
		Область = ОбластьСвойствоЗначениеЗаполнено;
	КонецЕсли;
	Область.Параметры.Значение = Значение;
	ТабличныйДокумент.Присоединить(Область);

	Если ДанныеОбъекта.Количество() > 0 Тогда
		СлужебныеАтрибуты = НовыеСлужебныеАтрибуты();
		ПредставленияПризнакаОбъединенияКарточекТоваров = ДанныеДляПроверкиПризнакаОбъединенияКарточекТоваров();

		ТабличныйДокументАтрибутов.Вывести(ОбластьРазделитель);
		ОбластьЗаголовка.Параметры.Заголовок = НСтр("ru = 'Атрибуты товара';
													|en = 'Product options'");
		ТабличныйДокументАтрибутов.Вывести(ОбластьЗаголовка);

		ТекущийРодитель = "";

		Для Каждого ДанныеАтрибута Из ДанныеОбъекта Цикл
			
			// Вставка признака Объединения карточек.
			ЗначениеСравнения = СтрЗаменить(ДанныеАтрибута.НаименованиеАтрибутаМаркетплейса, " ", "_");
			Если ПредставленияПризнакаОбъединенияКарточекТоваров.Найти(ЗначениеСравнения) <> Неопределено Тогда
				Значение = ДанныеАтрибута.ЗначениеАтрибутаМаркетплейса;
				Примечание = "";

				МассивПримечаний = Новый Массив;
				Если ЗначениеЗаполнено(Значение) Тогда
					Область = ОбластьСвойствоЗначениеЗаполнено;
				Иначе
					Значение = НСтр("ru = 'Не заполнено значение';
									|en = 'Value is not filled'");
					Область = ОбластьСвойствоПустоеЗначение;

					Примечание = НСтр("ru = 'Причина: %1';
										|en = 'Reason: %1'");
					Если ДанныеАтрибута.НеЗаполненПутьКРеквизиту1С Тогда
						ЗначениеПримечания = НСтр("ru = 'не указан путь к реквизиту 1С';
													|en = 'path to 1C attribute is not specified'");
					ИначеЕсли ДанныеАтрибута.НеЗаполненРеквизит1С Тогда
						ШаблонДополнения = НСтр("ru = 'не заполнен реквизит 1С <%1>';
												|en = '<%1> 1C attribute is not filled'");
						ЗначениеПримечания = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(ШаблонДополнения, ДанныеАтрибута.ПредставлениеРеквизита1С);
					КонецЕсли;
					МассивПримечаний.Добавить(СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(Примечание, ЗначениеПримечания));
				КонецЕсли;

				Если ДанныеАтрибута.ИзменилсяРеквизит1С Тогда
					ТабличныйДокумент.Вывести(ОбластьМаркера);

					ШаблонДополнения = НСтр("ru = 'Изменено значение реквизита 1С - новое значение [ %1 ]';
											|en = '1C attribute value is changed. The new value is [ %1 ]'");
					МассивПримечаний.Добавить(СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(ШаблонДополнения, ДанныеАтрибута.НовоеЗначениеРеквизита1С));
				Иначе
					ТабличныйДокумент.Вывести(ОбластьПустогоМаркера);
				КонецЕсли;

				ОбластьСвойствоЗаголовок.Параметры.Заголовок = НСтр("ru = 'Товар объединяется в одну карточку по значению';
																	|en = 'Goods are combined into one card by value'");
				ТабличныйДокумент.Присоединить(ОбластьСвойствоЗаголовок);

				Область.Параметры.Значение = Значение;
				ТабличныйДокумент.Присоединить(Область);

				ОбластьСвойствоПримечание.Параметры.Примечание = СтрСоединить(МассивПримечаний, Символы.ПС);
				ТабличныйДокумент.Присоединить(ОбластьСвойствоПримечание);

				Продолжить;
			КонецЕсли;

			Если ТекущийРодитель <> ДанныеАтрибута.ИдентификаторРодителяАтрибутаМаркетплейса Тогда
				ТекущийРодитель = ДанныеАтрибута.ИдентификаторРодителяАтрибутаМаркетплейса;

				// Вывод группы атрибутов.
				Если ЗначениеЗаполнено(ТекущийРодитель) Тогда
					ЧастиИдентификатора = СтрРазделить(ТекущийРодитель, "_", Истина);
					Если ЧастиИдентификатора.Количество() > 1 Тогда
						ИдентификаторРодителяАтрибута = ЧастиИдентификатора[1];
					Иначе
						ИдентификаторРодителяАтрибута = ЧастиИдентификатора[0];
					КонецЕсли;

					ЧастиНаименования = СтрРазделить(СтрЗаменить(ДанныеАтрибута.НаименованиеРодителяАтрибутаМаркетплейса, ">>", Символы.ПС), Символы.ПС, Истина);
					Если ЧастиНаименования.Количество() > 1 Тогда
						НаименованиеРодителяАтрибута = ЧастиНаименования[1];
					Иначе
						НаименованиеРодителяАтрибута = ЧастиНаименования[0];
					КонецЕсли;

					Если ПустаяСтрока(НаименованиеРодителяАтрибута) Тогда
						НаименованиеРодителяАтрибута = ДанныеАтрибута.НаименованиеАтрибутаМаркетплейса;
					КонецЕсли;

					ОбластьГруппаАтрибутов.Параметры.Заголовок = НаименованиеРодителяАтрибута;
					Если СлужебныеАтрибуты.Найти(ДанныеАтрибута.ИдентификаторАтрибутаМаркетплейса) = Неопределено Тогда
						ОбластьГруппаАтрибутов.Параметры.Заголовок =  ОбластьГруппаАтрибутов.Параметры.Заголовок
								+ " (id " + ИдентификаторРодителяАтрибута + ")";
					КонецЕсли;
					ТабличныйДокументАтрибутов.Вывести(ОбластьГруппаАтрибутов);
				КонецЕсли;
			КонецЕсли;

			// Вывод атрибута.
			Если ЗначениеЗаполнено(ТекущийРодитель) Тогда
				Если ДанныеАтрибута.ОбязательныйАтрибут Тогда
					ИспользуемаяОбластьЗаголовок = ОбластьАтрибутГруппыОбязательныйЗаголовок;
				Иначе
					ИспользуемаяОбластьЗаголовок = ОбластьАтрибутГруппыНеобязательныйЗаголовок;
				КонецЕсли;
			Иначе
				Если ДанныеАтрибута.ОбязательныйАтрибут Тогда
					ИспользуемаяОбластьЗаголовок = ОбластьАтрибутОбязательныйЗаголовок;
				Иначе
					ИспользуемаяОбластьЗаголовок = ОбластьАтрибутНеобязательныйЗаголовок;
				КонецЕсли;
			КонецЕсли;

			Если ДанныеАтрибута.ИзменилсяРеквизит1С Тогда
				ТабличныйДокументАтрибутов.Вывести(ОбластьМаркера);
			Иначе
				ТабличныйДокументАтрибутов.Вывести(ОбластьПустогоМаркера);
			КонецЕсли;

			ИспользуемаяОбластьЗаголовок.Параметры.Заголовок = ДанныеАтрибута.НаименованиеАтрибутаМаркетплейса;
			Если СлужебныеАтрибуты.Найти(ДанныеАтрибута.ИдентификаторАтрибутаМаркетплейса) = Неопределено Тогда
				ИспользуемаяОбластьЗаголовок.Параметры.Заголовок = ИспользуемаяОбластьЗаголовок.Параметры.Заголовок
						+ " (id " + ДанныеАтрибута.ИдентификаторАтрибутаМаркетплейса + ")";
			КонецЕсли;
			ТабличныйДокументАтрибутов.Присоединить(ИспользуемаяОбластьЗаголовок);

			МассивПримечаний = Новый Массив;
			Если ЗначениеЗаполнено(ДанныеАтрибута.ЗначениеАтрибутаМаркетплейса) Тогда
				ОбластьАтрибутЗначениеЗаполнено.Параметры.Значение = ДанныеАтрибута.ЗначениеАтрибутаМаркетплейса;
				Если Не ПустаяСтрока(ДанныеАтрибута.ИдентификаторЗначенияАтрибутаМаркетплейса) Тогда
					ОбластьАтрибутЗначениеЗаполнено.Параметры.Значение = ОбластьАтрибутЗначениеЗаполнено.Параметры.Значение
							+ " (id " + ДанныеАтрибута.ИдентификаторЗначенияАтрибутаМаркетплейса + ")";
				КонецЕсли;
				ТабличныйДокументАтрибутов.Присоединить(ОбластьАтрибутЗначениеЗаполнено);

				Если ДанныеАтрибута.ЗначениеАтрибутаЗаданоПользователем Тогда
					МассивПримечаний.Добавить(НСтр("ru = 'Значение заполнено / изменено пользователем';
													|en = 'Value is filled / changed by the user'"));
				ИначеЕсли ДанныеАтрибута.УстановленоЗначениеПоУмолчанию Тогда
					МассивПримечаний.Добавить(НСтр("ru = 'Установлено значение по умолчанию';
													|en = 'Default value is set'"));
				КонецЕсли;
			Иначе
				Если ДанныеАтрибута.ОбязательныйАтрибут Тогда
					ОбластьАтрибутПустоеЗначение.Параметры.Значение = НСтр("ru = 'Не заполнен обязательный атрибут';
																			|en = 'Required product option is not filled'");
				Иначе
					ОбластьАтрибутПустоеЗначение.Параметры.Значение = НСтр("ru = 'Не заполнен атрибут';
																			|en = 'Product option is not filled'");
				КонецЕсли;
				ТабличныйДокументАтрибутов.Присоединить(ОбластьАтрибутПустоеЗначение);

				Значение = "";
				Если ДанныеАтрибута.НеЗаполненПутьКРеквизиту1С Тогда
					Значение = НСтр("ru = 'не указан путь к реквизиту 1С';
									|en = 'path to 1C attribute is not specified'");
				ИначеЕсли ДанныеАтрибута.НеЗаполненРеквизит1С Тогда
					Шаблон = НСтр("ru = 'не заполнен реквизит 1С <%1>';
									|en = 'the <%1> 1C attribute is not filled'");
					Значение = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(Шаблон, ДанныеАтрибута.ПредставлениеРеквизита1С);
				ИначеЕсли ДанныеАтрибута.НеЗаполненоСоответствиеАтрибута Тогда
					Шаблон = НСтр("ru = 'не заполнено соответствие для значения <%1>';
									|en = 'the <%1> value is not mapped'");
					Значение = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(Шаблон, ДанныеАтрибута.ЗначениеРеквизита1С);
				КонецЕсли;

				Если Не ПустаяСтрока(Значение) Тогда
					МассивПримечаний.Добавить(СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(НСтр("ru = 'Причина: %1';
																											|en = 'Reason: %1'"), Значение));
				КонецЕсли;
			КонецЕсли;

			Если ДанныеАтрибута.ИзменилсяРеквизит1С Тогда
				Шаблон = НСтр("ru = 'Изменено значение реквизита 1С - новое значение <%1>';
								|en = '1C attribute value is changed. The new value is <%1>'");
				МассивПримечаний.Добавить(СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(Шаблон, ДанныеАтрибута.НовоеЗначениеРеквизита1С));
			КонецЕсли;

			ОбластьАтрибутПримечание.Параметры.Примечание = СтрСоединить(МассивПримечаний, Символы.ПС);
			ТабличныйДокументАтрибутов.Присоединить(ОбластьАтрибутПримечание);

		КонецЦикла;
	Иначе
		ТабличныйДокументАтрибутов.Вывести(ОбластьРазделитель);
		ОбластьЗаголовка.Параметры.Заголовок = НСтр("ru = 'Не обнаружены сохраненные данные по атрибутам.';
													|en = 'No saved product option data is found.'") + Символы.ПС
				+ НСтр("ru = 'Необходимо заполнить атрибуты товара.';
						|en = 'Fill product options.'");
		ТабличныйДокументАтрибутов.Вывести(ОбластьЗаголовка);
	КонецЕсли;

	ТабличныйДокумент.Вывести(ТабличныйДокументАтрибутов);
	
	Если ПараметрыЗаполнения.Цены.Количество() > 0 Тогда
		ТабличныйДокумент.Вывести(ОбластьРазделитель);
		ОбластьЗаголовка.Параметры.Заголовок = НСтр("ru = 'Цены товара';
													|en = 'Item prices'");
		ТабличныйДокумент.Вывести(ОбластьЗаголовка);

		Для Каждого ДанныеЦены Из ПараметрыЗаполнения.Цены Цикл
			ТабличныйДокумент.Вывести(ОбластьПустогоМаркера);

			ДанныеЦены = ДанныеЦены.Значение;

			Если ДанныеЦены.ПроверкаЗаполнения Тогда
				ИспользуемаяОбластьЗаголовок = ОбластьАтрибутОбязательныйЗаголовок;
			Иначе
				ИспользуемаяОбластьЗаголовок = ОбластьАтрибутНеобязательныйЗаголовок;
			КонецЕсли;

			Значение = ДанныеЦены.ВидЦены;
			Если Не ЗначениеЗаполнено(Значение) Тогда
				Значение = НСтр("ru = '<Не задан вид цены>';
								|en = '<Price type is not specified>'");
			КонецЕсли;
			ИспользуемаяОбластьЗаголовок.Параметры.Заголовок = Значение;
			ТабличныйДокумент.Присоединить(ИспользуемаяОбластьЗаголовок);

			Если ЗначениеЗаполнено(ДанныеЦены.Цена) Тогда
				ОбластьАтрибутЗначениеЗаполнено.Параметры.Значение = Формат(ДанныеЦены.Цена, "ЧДЦ=2; ЧН=; ЧГ=") + " " + ПараметрыЗаполнения.Валюта;
				ТабличныйДокумент.Присоединить(ОбластьАтрибутЗначениеЗаполнено);
			ИначеЕсли ДанныеЦены.ПроверкаЗаполнения Тогда
				ОбластьАтрибутПустоеЗначение.Параметры.Значение = НСтр("ru = 'Не задана цена';
																		|en = 'Price is not specified'");
				ТабличныйДокумент.Присоединить(ОбластьАтрибутПустоеЗначение);
			Иначе
				ОбластьАтрибутЗначениеЗаполнено.Параметры.Значение = НСтр("ru = 'Не задана цена';
																			|en = 'Price is not specified'");
				ТабличныйДокумент.Присоединить(ОбластьАтрибутЗначениеЗаполнено);
			КонецЕсли;

			ОбластьАтрибутПримечание.Параметры.Примечание = 
					СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(НСтр("ru = 'Вид цены маркетплейса: <%1>';
																				|en = 'Marketplace price type: <%1>'"),
							ДанныеЦены.ПредставлениеВидаЦены);
			ТабличныйДокумент.Присоединить(ОбластьАтрибутПримечание);
		КонецЦикла;
	КонецЕсли;

	Возврат ТабличныйДокумент;

КонецФункции

// Возвращает сообщение об отсутствии атрибутов.
// 
// Возвращаемое значение:
//  Строка - форматированная строка.
//
Функция ПолучитьОписаниеНовогоТовараМаркетплейса() Экспорт

	ТабличныйДокумент = Новый ТабличныйДокумент;
	Макет = РегистрыСведений.СтатусыПубликацииОбъектовМаркетплейсаOzon.ПолучитьМакет("ПФ_MXL_ОписаниеТовара");

	// Основные данные.
	ОбластьЗаголовка = Макет.ПолучитьОбласть("ЗаголовокГруппыДанных");
	ОбластьЗаголовка.Параметры.Заголовок = НСтр("ru = 'Основные данные';
												|en = 'Main data'");
	ТабличныйДокумент.Вывести(ОбластьЗаголовка);

	// Информация о товаре.
	ОбластьНезаполненныхАтрибутов = Макет.ПолучитьОбласть("СтрокаНезаполненныхАтрибутов");
	ТабличныйДокумент.Вывести(ОбластьНезаполненныхАтрибутов);
	
	Возврат ТабличныйДокумент;

КонецФункции

// Возвращает результат обработки выбора или подбора.
//
// Параметры:
//   ТаблицаТоваров            - см. НоваяТаблицаПубликацииТоваров.
//   УчетнаяЗаписьМаркетплейса - СправочникСсылка.УчетныеЗаписиМаркетплейсов - учетная запись подключения к сервису.
//
// Возвращаемое значение:
//   Булево - результат обработки.
//
Функция ОбработкаВыбораПодбор(ТаблицаТоваров, УчетнаяЗаписьМаркетплейса) Экспорт

	Результат = Новый Структура("ДобавлениеЗаписейВыполнено", Истина);

	МенеджерВременныхТаблиц = Новый МенеджерВременныхТаблиц;
	Запрос = Новый Запрос;
	Запрос.МенеджерВременныхТаблиц = МенеджерВременныхТаблиц;
	Запрос.УстановитьПараметр("УчетнаяЗаписьМаркетплейса", УчетнаяЗаписьМаркетплейса);
	Запрос.УстановитьПараметр("ТаблицаТоваров", ТаблицаТоваров);
	Запрос.Текст =
	"ВЫБРАТЬ
	|	&УчетнаяЗаписьМаркетплейса КАК УчетнаяЗаписьМаркетплейса,
	|	ТаблицаТоваров.Номенклатура КАК Номенклатура,
	|	ТаблицаТоваров.Характеристика КАК Характеристика,
	|	ТаблицаТоваров.Упаковка КАК Упаковка
	|ПОМЕСТИТЬ ТаблицаТоваров
	|ИЗ
	|	&ТаблицаТоваров КАК ТаблицаТоваров
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ТаблицаТоваров.УчетнаяЗаписьМаркетплейса КАК УчетнаяЗаписьМаркетплейса,
	|	ТаблицаТоваров.Номенклатура КАК Номенклатура,
	|	ТаблицаТоваров.Характеристика КАК Характеристика,
	|	ТаблицаТоваров.Упаковка КАК Упаковка,
	|	СтатусыПубликацииОбъектовМаркетплейсаOzon.НоменклатураПартнера КАК НоменклатураПартнера,
	|	ЕСТЬNULL(СтатусыПубликацииОбъектовМаркетплейсаOzon.ИдентификаторКатегорииМаркетплейса, """") КАК ИдентификаторКатегорииМаркетплейса,
	|	ВЫБОР
	|		КОГДА СтатусыПубликацииОбъектовМаркетплейсаOzon.ИсточникКатегорииМаркетплейса ЕСТЬ NULL
	|			ТОГДА СправУчетныеЗаписи.ИсточникКатегории
	|		КОГДА ТИПЗНАЧЕНИЯ(СтатусыПубликацииОбъектовМаркетплейсаOzon.ИсточникКатегорииМаркетплейса) = ТИП(Справочник.Номенклатура)
	|			ТОГДА ЗНАЧЕНИЕ(Перечисление.ИсточникиКатегорийДляМаркетплейса.ИерархияНоменклатуры)
	|		КОГДА ТИПЗНАЧЕНИЯ(СтатусыПубликацииОбъектовМаркетплейсаOzon.ИсточникКатегорииМаркетплейса) = ТИП(Справочник.ВидыНоменклатуры)
	|			ТОГДА ЗНАЧЕНИЕ(Перечисление.ИсточникиКатегорийДляМаркетплейса.ВидНоменклатуры)
	|		КОГДА ТИПЗНАЧЕНИЯ(СтатусыПубликацииОбъектовМаркетплейсаOzon.ИсточникКатегорииМаркетплейса) = ТИП(Справочник.ТоварныеКатегории)
	|			ТОГДА ЗНАЧЕНИЕ(Перечисление.ИсточникиКатегорийДляМаркетплейса.ТоварнаяКатегория)
	|		ИНАЧЕ СправУчетныеЗаписи.ИсточникКатегории
	|	КОНЕЦ КАК ИсточникКатегории
	|ПОМЕСТИТЬ ДанныеПубликации
	|ИЗ
	|	ТаблицаТоваров КАК ТаблицаТоваров
	|		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.УчетныеЗаписиМаркетплейсов КАК СправУчетныеЗаписи
	|		ПО ТаблицаТоваров.УчетнаяЗаписьМаркетплейса = СправУчетныеЗаписи.Ссылка
	|		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СтатусыПубликацииОбъектовМаркетплейсаOzon КАК СтатусыПубликацииОбъектовМаркетплейсаOzon
	|		ПО ТаблицаТоваров.Номенклатура = СтатусыПубликацииОбъектовМаркетплейсаOzon.Номенклатура
	|			И ТаблицаТоваров.Характеристика = СтатусыПубликацииОбъектовМаркетплейсаOzon.Характеристика
	|			И ТаблицаТоваров.Упаковка = СтатусыПубликацииОбъектовМаркетплейсаOzon.Упаковка
	|			И ТаблицаТоваров.УчетнаяЗаписьМаркетплейса = СтатусыПубликацииОбъектовМаркетплейсаOzon.УчетнаяЗаписьМаркетплейса
	|			И (СтатусыПубликацииОбъектовМаркетплейсаOzon.ВидОбъектаМаркетплейса = ЗНАЧЕНИЕ(Перечисление.ВидыОбъектовМаркетплейсов.Товар))
	|ГДЕ
	|	СтатусыПубликацииОбъектовМаркетплейсаOzon.Номенклатура ЕСТЬ NULL
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|УНИЧТОЖИТЬ ТаблицаТоваров";

	Запрос.Выполнить();

	ПараметрыЗаполненияТоваров = НовыеПараметрыПодготовкиДанныхПубликуемыхТоваров();
	ПараметрыЗаполненияТоваров.ПолучитьАтрибуты = Истина;
	ПараметрыЗаполненияТоваров.ПолучитьИзображения = Истина;
	ПараметрыЗаполненияТоваров.ПолучитьСертификаты = Истина;
	ПараметрыЗаполненияТоваров.ОбновитьДанные = Истина;
	ПараметрыЗаполненияТоваров.ПроверитьОшибкиЗаполнения = Истина;

	ДанныеПубликации = ПодготовитьДанныеПубликуемыхТоваров(МенеджерВременныхТаблиц, ПараметрыЗаполненияТоваров);

	// Заполнение признаков продаж по схемам работы.
	РесурсыРегистраСтатусов = Метаданные.РегистрыСведений.СтатусыПубликацииОбъектовМаркетплейсаOzon.Ресурсы;

	ДанныеПубликации.Колонки.Добавить("ПродаетсяПоСхемеРаботыFBO", Новый ОписаниеТипов("Булево"));
	ПродаетсяПоСхемеРаботыFBO = РесурсыРегистраСтатусов.ПродаетсяПоСхемеРаботыFBO.ЗначениеЗаполнения;
	ДанныеПубликации.ЗаполнитьЗначения(ПродаетсяПоСхемеРаботыFBO, "ПродаетсяПоСхемеРаботыFBO");

	ДанныеПубликации.Колонки.Добавить("ПродаетсяПоСхемеРаботыFBS", Новый ОписаниеТипов("Булево"));
	ПродаетсяПоСхемеРаботыFBS = РесурсыРегистраСтатусов.ПродаетсяПоСхемеРаботыFBS.ЗначениеЗаполнения;
	ДанныеПубликации.ЗаполнитьЗначения(ПродаетсяПоСхемеРаботыFBS, "ПродаетсяПоСхемеРаботыFBS");

	ДанныеПубликации.Колонки.Добавить("ПродаетсяПоСхемеРаботыDBS", Новый ОписаниеТипов("Булево"));
	ПродаетсяПоСхемеРаботыDBS = РесурсыРегистраСтатусов.ПродаетсяПоСхемеРаботыDBS.ЗначениеЗаполнения;
	ДанныеПубликации.ЗаполнитьЗначения(ПродаетсяПоСхемеРаботыDBS, "ПродаетсяПоСхемеРаботыDBS");

	// Создание позиций товаров.
	Если Не ПустаяСтрока(ПараметрыЗаполненияТоваров.СообщениеПользователю) Тогда
		Результат.Вставить("СообщениеПользователю", ПараметрыЗаполненияТоваров.СообщениеПользователю);
	КонецЕсли;

	Если ДанныеПубликации.Количество() Тогда
		СоздатьЗаписиВРегистреСтатусовПубликацииОбъектовМаркетплейсаOzon(ДанныеПубликации);
	Иначе
		Результат.ДобавлениеЗаписейВыполнено = Ложь;
	КонецЕсли;

	Возврат Результат;

КонецФункции

#КонецОбласти

#Область СозданиеКарточкиНоменклатуры

// Возвращает текст запроса поиска идентификаторов категорий товаров.
//
// Возвращаемое значение:
//   Строка - Текст запроса.
//
Функция ТекстЗапросаИдентификаторовКатегорийТоваров() Экспорт

	ТекстЗапроса =
		"ВЫБРАТЬ РАЗРЕШЕННЫЕ РАЗЛИЧНЫЕ
		|	СтатусыПубликации.ИдентификаторКатегорииМаркетплейса КАК ИдентификаторКатегории
		|ПОМЕСТИТЬ ИдентификаторыКатегорий
		|ИЗ
		|	РегистрСведений.СтатусыПубликацииОбъектовМаркетплейсаOzon КАК СтатусыПубликации
		|ГДЕ
		|	СтатусыПубликации.УчетнаяЗаписьМаркетплейса = &УчетнаяЗапись
		|	И СтатусыПубликации.ВидОбъектаМаркетплейса = ЗНАЧЕНИЕ(Перечисление.ВидыОбъектовМаркетплейсов.Товар)
		|	И СтатусыПубликации.ИдентификаторПубликации В(&ИдентификаторыТоваров)
		|
		|ИНДЕКСИРОВАТЬ ПО
		|	ИдентификаторКатегории";
	
	Возврат ТекстЗапроса;

КонецФункции

Функция ИдентификаторНоменклатурыКонтрагента(НоменклатураКонтрагента)
	Возврат НоменклатураКонтрагента.Артикул
КонецФункции

// Параметры:
//  УчетнаяЗапись - СправочникСсылка.УчетныеЗаписиМаркетплейсов -
//  Категория1С - СправочникСсылка.ВидыНоменклатуры, СправочникСсылка.Номенклатура, СправочникСсылка.ТоварныеКатегории -
//  ДанныеКонтрагента - см. ИнтеграцияСМаркетплейсамиСервер.НоваяТаблицаДанныхКонтрагентаДляСозданияНоменклатуры
//  ДанныеСоздаваемыхЭлементов - см. ИнтеграцияСМаркетплейсамиСервер.НовыеДанныеДляСозданияНоменклатурыПоДаннымКонтрагента
//
Процедура ПриЗаполненииДанныхДляСозданияНоменклатурыПоДаннымКонтрагента(УчетнаяЗапись, Знач Категория1С,
			ДанныеКонтрагента, ДанныеСоздаваемыхЭлементов) Экспорт
	
	ПереопределитьКатегорию1СПриНеобходимости(УчетнаяЗапись, Категория1С);
	
	Если Не ЗначениеЗаполнено(Категория1С) Тогда
		Возврат;
	КонецЕсли;
	
	Замер = ОценкаПроизводительности.НачатьЗамерДлительнойОперации(
		"ОбщийМодуль.ИнтеграцияСМаркетплейсомOzonСервер.ПриЗаполненииДанныхДляСозданияНоменклатурыПоДаннымКонтрагента");
	
	ИдентификаторыКатегорий = Новый Массив; // Массив из Строка
	
	ВсеЗначенияАтрибутов = ПолучитьЗначенияАтрибутовИЗаполнитьИдентификаторыКатегорий(
		УчетнаяЗапись, ДанныеКонтрагента, ИдентификаторыКатегорий);
	
	Если ИдентификаторыКатегорий.Количество() = 0 Тогда
		Возврат;
	КонецЕсли;
	
	СоответствияАтрибутовКатегорий = ЗаполненныеСоответствияАтрибутовКатегорий(
		УчетнаяЗапись, ИдентификаторыКатегорий, Категория1С);
	
	Если СоответствияАтрибутовКатегорий.Количество() = 0 Тогда
		Возврат;
	КонецЕсли;
	
	ЗаполнитьЗначенияАтрибутовСопоставленнымиЗначениями(
		УчетнаяЗапись, ВсеЗначенияАтрибутов, СоответствияАтрибутовКатегорий);
	
	ЗначенияАтрибутовДляСозданияНоменклатуры = ЗначенияАтрибутовДляСозданияНоменклатуры(
		УчетнаяЗапись, ВсеЗначенияАтрибутов, СоответствияАтрибутовКатегорий);
	
	ЗаполнитьДанныеДляСозданияНоменклатуры(
		ДанныеКонтрагента, ДанныеСоздаваемыхЭлементов, ЗначенияАтрибутовДляСозданияНоменклатуры);
	
	ОценкаПроизводительности.ЗакончитьЗамерДлительнойОперации(
		Замер,
		ДанныеКонтрагента.Количество() / 100);
	
КонецПроцедуры

Процедура ПереопределитьКатегорию1СПриНеобходимости(УчетнаяЗапись, Категория1С)
	
	Если Не ЗначениеЗаполнено(Категория1С) Тогда
		Возврат;
	КонецЕсли;
	
	ИсточникКатегории = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(УчетнаяЗапись, "ИсточникКатегории");
	
	ВыборкаДанных = ВыборкаИсточникаКатегорииМаркетплейса(
		ОбщегоНазначенияКлиентСервер.ЗначениеВМассиве(Категория1С), ИсточникКатегории, УчетнаяЗапись);
		
	Если ВыборкаДанных.Следующий() Тогда
		Категория1С = ВыборкаДанных.РодительКатегории1С; // СправочникСсылка.ВидыНоменклатуры, СправочникСсылка.Номенклатура, СправочникСсылка.ТоварныеКатегории
	КонецЕсли;
	
КонецПроцедуры

Функция ПолучитьЗначенияАтрибутовИЗаполнитьИдентификаторыКатегорий(УчетнаяЗапись, ДанныеДляСозданияНоменклатуры,
			ИдентификаторыКатегорий)
	
	Результат = НоваяТаблицаЗначенийАтрибутов();
	
	ВыборкаЗначенийАтрибутов = ВыборкаЗначенийАтрибутовДляСозданияНоменклатуры(
		УчетнаяЗапись, ДанныеДляСозданияНоменклатуры);
	
	Пока ВыборкаЗначенийАтрибутов.СледующийПоЗначениюПоля("ИдентификаторКатегории") Цикл
		
		ИдентификаторыКатегорий.Добавить(ВыборкаЗначенийАтрибутов.ИдентификаторКатегории);
		
		Пока ВыборкаЗначенийАтрибутов.Следующий() Цикл
			
			ЗначенияАтрибутовТовара = ЗначенияАтрибутовТовараИзВыборкиДанныхОбъектов(ВыборкаЗначенийАтрибутов);
			
			Для Каждого СтрокаЗначенияАтрибута Из ЗначенияАтрибутовТовара Цикл
				
				НоваяСтрока = Результат.Добавить();
				
				НоваяСтрока.Идентификатор          = ВыборкаЗначенийАтрибутов.Идентификатор;
				НоваяСтрока.ИдентификаторКатегории = СтрокаЗначенияАтрибута.ИдентификаторКатегорииМаркетплейса;
				НоваяСтрока.ИдентификаторАтрибута  = СтрокаЗначенияАтрибута.ИдентификаторАтрибутаМаркетплейса;
				НоваяСтрока.ИдентификаторЗначенияАтрибута =
					СтрокаЗначенияАтрибута.ИдентификаторЗначенияАтрибутаМаркетплейса;
				
				НоваяСтрока.ЗначениеАтрибута  = СтрокаЗначенияАтрибута.ЗначениеАтрибутаМаркетплейса;
				НоваяСтрока.ЗначениеРеквизита = СтрокаЗначенияАтрибута.ЗначениеРеквизита1С;
				
			КонецЦикла;
			
		КонецЦикла;
		
	КонецЦикла;
	
	Если Результат.Количество() > КоличествоСтрокДляИндексации() Тогда
		Результат.Индексы.Добавить("ИдентификаторКатегории, ИдентификаторАтрибута");
	КонецЕсли;
	
	Возврат Результат;
	
КонецФункции

// Возвращаемое значение:
//  ТаблицаЗначений:
//   * Идентификатор - Строка -
//   * ИдентификаторКатегории - Строка -
//   * ИдентификаторАтрибута - Строка -
//   * ИдентификаторЗначенияАтрибута - Строка -
//   * ИдентификаторВладельцаОбъекта - Строка -
//   * ЗначениеАтрибута - Произвольный -
//   * ЗначениеРеквизита - Произвольный -
//   * НеобходимоЗаписатьСопоставление - Булево -
//
Функция НоваяТаблицаЗначенийАтрибутов()
	
	Результат = Новый ТаблицаЗначений;
	
	Результат.Колонки.Добавить("Идентификатор",                 ОбщегоНазначения.ОписаниеТипаСтрока(50));
	Результат.Колонки.Добавить("ИдентификаторКатегории",        ОбщегоНазначения.ОписаниеТипаСтрока(50));
	Результат.Колонки.Добавить("ИдентификаторАтрибута",         ОбщегоНазначения.ОписаниеТипаСтрока(50));
	Результат.Колонки.Добавить("ИдентификаторЗначенияАтрибута", ОбщегоНазначения.ОписаниеТипаСтрока(50));
	Результат.Колонки.Добавить("ИдентификаторВладельцаОбъекта", ОбщегоНазначения.ОписаниеТипаСтрока(110));
	
	Результат.Колонки.Добавить("ЗначениеАтрибута");               // Произвольный
	Результат.Колонки.Добавить("ЗначениеРеквизита");              // Произвольный
	Результат.Колонки.Добавить("НеобходимоЗаписатьСопоставление", Новый ОписаниеТипов("Булево"));
	
	Возврат Результат;
	
КонецФункции

// Параметры:
//  УчетнаяЗапись - СправочникСсылка.УчетныеЗаписиМаркетплейсов -
//  ДанныеДляСозданияНоменклатуры - см. ИнтеграцияСМаркетплейсамиСервер.НоваяТаблицаДанныхКонтрагентаДляСозданияНоменклатуры
// 
// Возвращаемое значение:
//  ВыборкаИзРезультатаЗапроса:
//   * ИдентификаторКатегории - Строка -
//   * Идентификатор - Строка -
//   * ДанныеОбъекта - ХранилищеЗначения -
//
Функция ВыборкаЗначенийАтрибутовДляСозданияНоменклатуры(УчетнаяЗапись, ДанныеДляСозданияНоменклатуры)
	
	ИдентификаторыТоваров = Новый Массив; // Массив из Строка
	
	Для Каждого Строка Из ДанныеДляСозданияНоменклатуры Цикл
		ИдентификаторыТоваров.Добавить(ИдентификаторНоменклатурыКонтрагента(Строка.НоменклатураКонтрагента));
	КонецЦикла;
	
	Запрос = Новый Запрос;
	Запрос.Текст =
		"ВЫБРАТЬ
		|	СтатусыПубликации.ИдентификаторКатегорииМаркетплейса КАК ИдентификаторКатегории,
		|	СтатусыПубликации.ИдентификаторПубликации КАК Идентификатор,
		|	СтатусыПубликации.ДанныеОбъекта КАК ДанныеОбъекта
		|ИЗ
		|	РегистрСведений.СтатусыПубликацииОбъектовМаркетплейсаOzon КАК СтатусыПубликации
		|ГДЕ
		|	СтатусыПубликации.УчетнаяЗаписьМаркетплейса = &УчетнаяЗапись
		|	И СтатусыПубликации.ВидОбъектаМаркетплейса = ЗНАЧЕНИЕ(Перечисление.ВидыОбъектовМаркетплейсов.Товар)
		|	И СтатусыПубликации.Номенклатура = ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка)
		|	И СтатусыПубликации.Характеристика = ЗНАЧЕНИЕ(Справочник.ХарактеристикиНоменклатуры.ПустаяСсылка)
		|	И СтатусыПубликации.Упаковка = ЗНАЧЕНИЕ(Справочник.УпаковкиЕдиницыИзмерения.ПустаяСсылка)
		|	И СтатусыПубликации.ОбъектПубликации = НЕОПРЕДЕЛЕНО
		|	И СтатусыПубликации.ИдентификаторПубликации В(&ИдентификаторыТоваров)
		|
		|УПОРЯДОЧИТЬ ПО
		|	ИдентификаторКатегории,
		|	Идентификатор";
	
	Запрос.УстановитьПараметр("УчетнаяЗапись",         УчетнаяЗапись);
	Запрос.УстановитьПараметр("ИдентификаторыТоваров", ИдентификаторыТоваров);
	
	Выборка = Запрос.Выполнить().Выбрать();
	
	Возврат Выборка;
	
КонецФункции

// Возвращаемое значение:
//  см. НоваяТаблицаЗначенийРеквизитов
//
Функция ЗначенияАтрибутовТовараИзВыборкиДанныхОбъектов(Выборка)
	Возврат Выборка.ДанныеОбъекта.Получить();
КонецФункции

// Параметры:
//  УчетнаяЗапись - СправочникСсылка.УчетныеЗаписиМаркетплейсов -
//  ИдентификаторыКатегорий - Массив из Строка -
//  Категория1С - СправочникСсылка.ВидыНоменклатуры, СправочникСсылка.Номенклатура, СправочникСсылка.ТоварныеКатегории -
// 
// Возвращаемое значение:
//  см. НоваяТаблицаСоответствияАтрибутовКатегорий
//
Функция ЗаполненныеСоответствияАтрибутовКатегорий(УчетнаяЗапись, ИдентификаторыКатегорий, Категория1С)
	
	Результат = НоваяТаблицаСоответствияАтрибутовКатегорий();
	
	ВсеСоответствияАтрибутовКатегорий = ПолучитьДанныеСоответствияАтрибутовКатегории(
		УчетнаяЗапись, ИдентификаторыКатегорий, Категория1С);
	
	ВсеДополнительныеСвойства = ТаблицаДополнительныхСвойств();
	
	Если ВсеДополнительныеСвойства.Количество() > КоличествоСтрокДляИндексации() Тогда
		ВсеДополнительныеСвойства.Индексы.Добавить("Представление, ИмяНабора");
	КонецЕсли;
	
	Поля = ПолучитьПоляСхемыДляПолученияРеквизитовАтрибутов();
	
	РезультатСозданияСхемы = СоздатьСхемуКомпоновкиДанныхДляВыбораРеквизита(Поля, Новый УникальныйИдентификатор());
	
	АдресСхемыАтрибутов               = РезультатСозданияСхемы.АдресСхемы;
	СоответствиеПсевдонимовМетаданных = РезультатСозданияСхемы.СоответствиеПсевдонимовМетаданных;
	
	КомпоновщикНастроек = Новый КомпоновщикНастроекКомпоновкиДанных;
	КомпоновщикНастроек.Инициализировать(Новый ИсточникДоступныхНастроекКомпоновкиДанных(АдресСхемыАтрибутов));
	
	ДоступныеПоляВыбора = КомпоновщикНастроек.Настройки.ДоступныеПоляВыбора;
	
	УдалитьИзВременногоХранилища(АдресСхемыАтрибутов);
	
	Для Каждого Строка Из ВсеСоответствияАтрибутовКатегорий Цикл
		
		Если ПустаяСтрока(Строка.Реквизит1С) Тогда
			Продолжить; // Нет соответствия
		КонецЕсли;
		
		ЧастиПути = ИнтеграцияСМаркетплейсомOzonКлиентСервер.ЧастиПутиКПолю(Строка.Реквизит1С);
		
		Если ЧастиПути.Количество() < 2 Тогда
			Продолжить; // В соответствии нет обращения через точку
		КонецЕсли;
		
		ИмяВладельцаРеквизита = ЧастиПути[ЧастиПути.ВГраница() - 1];
		
		Если Не ИнтеграцияСМаркетплейсамиКлиентСервер.ИмяСоответствуетТребованиямИменованияСвойств(ИмяВладельцаРеквизита) Тогда
			Продолжить;
		КонецЕсли;
		
		ДанныеЗаполнения = Новый Структура(ИмяВладельцаРеквизита, "");
		ЗаполнитьЗначенияСвойств(ДанныеЗаполнения, СоответствиеПсевдонимовМетаданных);
		
		ИмяМетаданныхВладельцаРеквизита = ДанныеЗаполнения[ИмяВладельцаРеквизита];
		
		ЭтоРеквизитНоменклатуры   = (ИмяМетаданныхВладельцаРеквизита = Поля.Номенклатура.ПолноеИмяМетаданных);
		ЭтоРеквизитХарактеристики = (ИмяМетаданныхВладельцаРеквизита = Поля.Характеристика.ПолноеИмяМетаданных);
		ЭтоРеквизитУпаковки       = (ИмяМетаданныхВладельцаРеквизита = Поля.Упаковка.ПолноеИмяМетаданных);
			
		Если Не ЭтоРеквизитНоменклатуры И Не ЭтоРеквизитХарактеристики И Не ЭтоРеквизитУпаковки Тогда
			Продолжить;
		КонецЕсли;
		
		ПолеПоРеквизиту = ДоступныеПоляВыбора.НайтиПоле(Новый ПолеКомпоновкиДанных(Строка.Реквизит1С));
		
		Если ПолеПоРеквизиту = Неопределено Тогда
			Продолжить;
		КонецЕсли;
		
		НоваяСтрока = Результат.Добавить();
		
		НоваяСтрока.ИдентификаторКатегории1С = Строка.ИдентификаторКатегории1С;
		НоваяСтрока.ИдентификаторКатегории   = Строка.ИдентификаторКатегорииМаркетплейса;
		НоваяСтрока.ИдентификаторАтрибута    = Строка.ИдентификаторАтрибутаМаркетплейса;
		НоваяСтрока.ПолныйПутьКРеквизиту1С   = Строка.Реквизит1С;
		
		НоваяСтрока.ЭтоРеквизитНоменклатуры   = ЭтоРеквизитНоменклатуры;
		НоваяСтрока.ЭтоРеквизитХарактеристики = ЭтоРеквизитХарактеристики;
		НоваяСтрока.ЭтоРеквизитУпаковки       = ЭтоРеквизитУпаковки;
		
		НоваяСтрока.ТипРеквизита = ПолеПоРеквизиту.ТипЗначения;
		
		НоваяСтрока.НужноСопоставитьЗначения = ЭтоСсылочныйТипДанных(НоваяСтрока.ТипРеквизита);
		
		НоваяСтрока.ЭтоДопРеквизит = СтрЗаканчиваетсяНа(НоваяСтрока.ПолныйПутьКРеквизиту1С, "]");
		
		Если НоваяСтрока.ЭтоДопРеквизит Тогда
			
			ИмяРеквизита = ЧастиПути[ЧастиПути.ВГраница()];
			ИмяРеквизита = СтрЗаменить(ИмяРеквизита, "[", "");
			ИмяРеквизита = СтрЗаменить(ИмяРеквизита, "]", "");
			
			ИмяМетаданныхВладельцаРеквизита = СтрЗаменить(ИмяМетаданныхВладельцаРеквизита, ".", "_");
			
			Отбор = Новый Структура;
			Отбор.Вставить("Представление", ИмяРеквизита);
			Отбор.Вставить("ИмяНабора",     ИмяМетаданныхВладельцаРеквизита);
			
			ДополнительныеСвойства = ВсеДополнительныеСвойства.НайтиСтроки(Отбор);
			
			Если ДополнительныеСвойства.Количество() = 0 Тогда
				Продолжить;
			КонецЕсли;
			
			ОписаниеСвойства = ДополнительныеСвойства[0];
			
			НоваяСтрока.Свойство = ОписаниеСвойства.Ссылка;
			
		Иначе
			НоваяСтрока.ИмяРеквизита = ЧастиПути[ЧастиПути.ВГраница()];
		КонецЕсли;
		
	КонецЦикла;
	
	Возврат Результат;
	
КонецФункции

// Возвращаемое значение:
//  ТаблицаЗначений:
//   * ИдентификаторКатегории1С - Строка -
//   * ИдентификаторКатегории - Строка -
//   * ИдентификаторАтрибута - Строка -
//   * ПолныйПутьКРеквизиту1С - Строка -
//   * ЭтоРеквизитНоменклатуры - Булево -
//   * ЭтоРеквизитХарактеристики - Булево -
//   * ЭтоРеквизитУпаковки - Булево -
//   * НужноСопоставитьЗначения - Булево -
//   * ИмяРеквизита - Строка -
//   * ТипРеквизита - ОписаниеТипов -
//   * ЭтоДопРеквизит - Булево -
//   * Свойство - ПланВидовХарактеристикСсылка.ДополнительныеРеквизитыИСведения -
//
Функция НоваяТаблицаСоответствияАтрибутовКатегорий()
	
	Результат = Новый ТаблицаЗначений;
	
	Результат.Колонки.Добавить("ИдентификаторКатегории1С", ОбщегоНазначения.ОписаниеТипаСтрока(36));
	Результат.Колонки.Добавить("ИдентификаторКатегории",   ОбщегоНазначения.ОписаниеТипаСтрока(50));
	Результат.Колонки.Добавить("ИдентификаторАтрибута",    ОбщегоНазначения.ОписаниеТипаСтрока(50));
	
	Результат.Колонки.Добавить("ПолныйПутьКРеквизиту1С", ОбщегоНазначения.ОписаниеТипаСтрока(500));
	
	Результат.Колонки.Добавить("ЭтоРеквизитНоменклатуры",   Новый ОписаниеТипов("Булево"));
	Результат.Колонки.Добавить("ЭтоРеквизитХарактеристики", Новый ОписаниеТипов("Булево"));
	Результат.Колонки.Добавить("ЭтоРеквизитУпаковки",       Новый ОписаниеТипов("Булево"));
	Результат.Колонки.Добавить("НужноСопоставитьЗначения",  Новый ОписаниеТипов("Булево"));
	
	Результат.Колонки.Добавить("ИмяРеквизита", ОбщегоНазначения.ОписаниеТипаСтрока(150));
	Результат.Колонки.Добавить("ТипРеквизита", Новый ОписаниеТипов("ОписаниеТипов"));
	
	Результат.Колонки.Добавить("ЭтоДопРеквизит", Новый ОписаниеТипов("Булево"));
	Результат.Колонки.Добавить(
		"Свойство", Новый ОписаниеТипов("ПланВидовХарактеристикСсылка.ДополнительныеРеквизитыИСведения"));
	
	Возврат Результат;
	
КонецФункции

Функция ЭтоСсылочныйТипДанных(ОписаниеТипов)
	
	Результат = Ложь;
	
	Для Каждого Тип Из ОписаниеТипов.Типы() Цикл
		
		Результат = ОбщегоНазначения.ЭтоСсылка(Тип);
		
		Если Результат Тогда
			Прервать;
		КонецЕсли;
		
	КонецЦикла;
	
	Возврат Результат;
	
КонецФункции

// Параметры:
//  УчетнаяЗапись - СправочникСсылка.УчетныеЗаписиМаркетплейсов -
//  ВсеЗначенияАтрибутов - см. НоваяТаблицаЗначенийАтрибутов
//  СоответствияАтрибутовКатегорий - см. НоваяТаблицаСоответствияАтрибутовКатегорий
//
Процедура ЗаполнитьЗначенияАтрибутовСопоставленнымиЗначениями(УчетнаяЗапись, ВсеЗначенияАтрибутов,
			СоответствияАтрибутовКатегорий)
	
	ТипыЗначенияАтрибута = Новый ОписаниеТипов(
		НовыеПростыеТипы(), Новый КвалификаторыЧисла(15, 0), Новый КвалификаторыСтроки(500));
	
	ТаблицаПоиска = НоваяТаблицаПоискаЗначенийАтрибутов(ТипыЗначенияАтрибута);
	
	ОтборСоответствий = Новый Структура("НужноСопоставитьЗначения", Истина);
	
	СтрокиДляСопоставления = СоответствияАтрибутовКатегорий.НайтиСтроки(ОтборСоответствий);
	
	Для Каждого СтрокаСоответствия Из СтрокиДляСопоставления Цикл
		
		Отбор = Новый Структура;
		
		Отбор.Вставить("ИдентификаторКатегории", СтрокаСоответствия.ИдентификаторКатегории);
		Отбор.Вставить("ИдентификаторАтрибута",  СтрокаСоответствия.ИдентификаторАтрибута);
		
		НайденныеСтроки = ВсеЗначенияАтрибутов.НайтиСтроки(Отбор);
		
		ИдентификаторВладельцаОбъекта = ИдентификаторВладельцаОбъектаМаркетплейса(
			СтрокаСоответствия.ИдентификаторКатегории1С,
			СтрокаСоответствия.ИдентификаторКатегории,
			СтрокаСоответствия.ИдентификаторАтрибута);
		
		Для Каждого СтрокаСоЗначением Из НайденныеСтроки Цикл
			
			СтрокаСоЗначением.ИдентификаторВладельцаОбъекта = ИдентификаторВладельцаОбъекта;
			
			НоваяСтрока = ТаблицаПоиска.Добавить();
			
			НоваяСтрока.Индекс = ВсеЗначенияАтрибутов.Индекс(СтрокаСоЗначением);
			
			НоваяСтрока.ИдентификаторВладельцаОбъекта = ИдентификаторВладельцаОбъекта;
			НоваяСтрока.ИдентификаторЗначенияАтрибута = СтрокаСоЗначением.ИдентификаторЗначенияАтрибута;
			
			//@skip-check statement-type-change
			НоваяСтрока.ЗначениеАтрибута = ТипыЗначенияАтрибута.ПривестиЗначение(СтрокаСоЗначением.ЗначениеАтрибута);
			
			НоваяСтрока.ЭтоДопРеквизит = СтрокаСоответствия.ЭтоДопРеквизит;
			НоваяСтрока.Свойство       = СтрокаСоответствия.Свойство;
			
		КонецЦикла;
		
	КонецЦикла;
	
	Если ТаблицаПоиска.Количество() = 0 Тогда
		Возврат;
	КонецЕсли;
	
	Запрос = Новый Запрос;
	Запрос.Текст = ТекстЗапросаПоискСопоставленныхЗначений();
	
	Запрос.УстановитьПараметр("УчетнаяЗапись", УчетнаяЗапись);
	Запрос.УстановитьПараметр("ТаблицаПоиска", ТаблицаПоиска);
	
	ПакетЗапросов = Запрос.ВыполнитьПакет();
	
	КоличествоЗапросов = ПакетЗапросов.Количество();
	
	ЗаполнитьСопоставлениеВТаблицеПоВыборке(ВсеЗначенияАтрибутов, ПакетЗапросов[КоличествоЗапросов - 2], Ложь);
	ЗаполнитьСопоставлениеВТаблицеПоВыборке(ВсеЗначенияАтрибутов, ПакетЗапросов[КоличествоЗапросов - 1], Истина);
	
КонецПроцедуры

// Параметры:
//  ТипыЗначенияАтрибута - ОписаниеТипов - Описание типов по простым типам, см. НовыеПростыеТипы
//
// Возвращаемое значение:
//  ТаблицаЗначений:
//   * Индекс - Число -
//   * ИдентификаторВладельцаОбъекта - Строка -
//   * ИдентификаторЗначенияАтрибута- Строка -
//   * ЗначениеАтрибута - Строка, Число, Дата, Булево -
//   * ЭтоДопРеквизит - Булево -
//   * Свойство - ПланВидовХарактеристикСсылка.ДополнительныеРеквизитыИСведения -
//
Функция НоваяТаблицаПоискаЗначенийАтрибутов(ТипыЗначенияАтрибута)
	
	Результат = Новый ТаблицаЗначений;
	
	Результат.Колонки.Добавить("Индекс", ОбщегоНазначения.ОписаниеТипаЧисло(10, 0, ДопустимыйЗнак.Неотрицательный));
	
	Результат.Колонки.Добавить("ИдентификаторВладельцаОбъекта", ОбщегоНазначения.ОписаниеТипаСтрока(110));
	Результат.Колонки.Добавить("ИдентификаторЗначенияАтрибута", ОбщегоНазначения.ОписаниеТипаСтрока(36));
	
	Результат.Колонки.Добавить("ЗначениеАтрибута", ТипыЗначенияАтрибута);
	
	Результат.Колонки.Добавить("ЭтоДопРеквизит", Новый ОписаниеТипов("Булево"));
	Результат.Колонки.Добавить(
		"Свойство", Новый ОписаниеТипов("ПланВидовХарактеристикСсылка.ДополнительныеРеквизитыИСведения"));
	
	Возврат Результат;
	
КонецФункции

Функция ТекстЗапросаПоискСопоставленныхЗначений()
	
	ТекстЗапроса =
		"ВЫБРАТЬ
		|	ТаблицаПоиска.Индекс КАК Индекс,
		|	ТаблицаПоиска.ИдентификаторВладельцаОбъекта КАК ИдентификаторВладельцаОбъекта,
		|	ТаблицаПоиска.ИдентификаторЗначенияАтрибута КАК ИдентификаторЗначенияАтрибута,
		|	ТаблицаПоиска.ЗначениеАтрибута КАК ЗначениеАтрибута,
		|	ТаблицаПоиска.ЭтоДопРеквизит КАК ЭтоДопРеквизит,
		|	ТаблицаПоиска.Свойство КАК Свойство
		|ПОМЕСТИТЬ ВТТаблицаПоиска
		|ИЗ
		|	&ТаблицаПоиска КАК ТаблицаПоиска
		|
		|ИНДЕКСИРОВАТЬ ПО
		|	ИдентификаторЗначенияАтрибута,
		|	ИдентификаторВладельцаОбъекта
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ РАЗЛИЧНЫЕ
		|	ТаблицаПоиска.ИдентификаторВладельцаОбъекта КАК ИдентификаторВладельцаОбъекта,
		|	ТаблицаПоиска.ИдентификаторЗначенияАтрибута КАК ИдентификаторЗначенияАтрибута
		|ПОМЕСТИТЬ ВТИдентификаторыЗначенийАтрибутов
		|ИЗ
		|	ВТТаблицаПоиска КАК ТаблицаПоиска
		|
		|ИНДЕКСИРОВАТЬ ПО
		|	ИдентификаторЗначенияАтрибута,
		|	ИдентификаторВладельцаОбъекта
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ
		|	ИдентификаторыЗначенийАтрибутов.ИдентификаторВладельцаОбъекта КАК ИдентификаторВладельцаОбъекта,
		|	ИдентификаторыЗначенийАтрибутов.ИдентификаторЗначенияАтрибута КАК ИдентификаторЗначенияАтрибута,
		|	СоответствияОбъектов.Объект1С КАК ЗначениеРеквизита
		|ПОМЕСТИТЬ ВТСопоставленныеЗначения
		|ИЗ
		|	ВТИдентификаторыЗначенийАтрибутов КАК ИдентификаторыЗначенийАтрибутов
		|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.СоответствияОбъектовМаркетплейсов КАК СоответствияОбъектов
		|		ПО (СоответствияОбъектов.УчетнаяЗаписьМаркетплейса = &УчетнаяЗапись)
		|			И (СоответствияОбъектов.ВидОбъектаМаркетплейса = ЗНАЧЕНИЕ(Перечисление.ВидыОбъектовМаркетплейсов.ЗначениеАтрибутаКатегорииТоваров))
		|			И ИдентификаторыЗначенийАтрибутов.ИдентификаторЗначенияАтрибута = СоответствияОбъектов.ИдентификаторОбъектаМаркетплейса
		|			И ИдентификаторыЗначенийАтрибутов.ИдентификаторВладельцаОбъекта = СоответствияОбъектов.ИдентификаторВладельцаОбъектаМаркетплейса
		|
		|ИНДЕКСИРОВАТЬ ПО
		|	ИдентификаторЗначенияАтрибута,
		|	ИдентификаторВладельцаОбъекта
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|УНИЧТОЖИТЬ ВТИдентификаторыЗначенийАтрибутов
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ РАЗЛИЧНЫЕ
		|	ТаблицаПоиска.ИдентификаторВладельцаОбъекта КАК ИдентификаторВладельцаОбъекта,
		|	ТаблицаПоиска.ИдентификаторЗначенияАтрибута КАК ИдентификаторЗначенияАтрибута,
		|	ТаблицаПоиска.Свойство КАК Свойство,
		|	ТаблицаПоиска.ЗначениеАтрибута КАК ЗначениеАтрибута
		|ПОМЕСТИТЬ ВТДополнительныеРеквизиты
		|ИЗ
		|	ВТТаблицаПоиска КАК ТаблицаПоиска
		|		ЛЕВОЕ СОЕДИНЕНИЕ ВТСопоставленныеЗначения КАК СопоставленныеЗначения
		|		ПО ТаблицаПоиска.ИдентификаторВладельцаОбъекта = СопоставленныеЗначения.ИдентификаторВладельцаОбъекта
		|			И ТаблицаПоиска.ИдентификаторЗначенияАтрибута = СопоставленныеЗначения.ИдентификаторЗначенияАтрибута
		|ГДЕ
		|	ТаблицаПоиска.ЭтоДопРеквизит
		|	И СопоставленныеЗначения.ИдентификаторЗначенияАтрибута ЕСТЬ NULL
		|
		|ИНДЕКСИРОВАТЬ ПО
		|	Свойство
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ
		|	ДополнительныеРеквизиты.ИдентификаторВладельцаОбъекта КАК ИдентификаторВладельцаОбъекта,
		|	ДополнительныеРеквизиты.ИдентификаторЗначенияАтрибута КАК ИдентификаторЗначенияАтрибута,
		|	МАКСИМУМ(ЗначенияСвойств.Ссылка) КАК ЗначениеРеквизита
		|ПОМЕСТИТЬ ВТНайденныеЗначения
		|ИЗ
		|	ВТДополнительныеРеквизиты КАК ДополнительныеРеквизиты
		|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.ЗначенияСвойствОбъектов КАК ЗначенияСвойств
		|		ПО ДополнительныеРеквизиты.Свойство = ЗначенияСвойств.Владелец
		|			И (ЗначенияСвойств.Наименование = ДополнительныеРеквизиты.ЗначениеАтрибута)
		|			И (НЕ ЗначенияСвойств.ПометкаУдаления)
		|
		|СГРУППИРОВАТЬ ПО
		|	ДополнительныеРеквизиты.ИдентификаторВладельцаОбъекта,
		|	ДополнительныеРеквизиты.ИдентификаторЗначенияАтрибута
		|
		|ОБЪЕДИНИТЬ ВСЕ
		|
		|ВЫБРАТЬ
		|	ДополнительныеРеквизиты.ИдентификаторВладельцаОбъекта,
		|	ДополнительныеРеквизиты.ИдентификаторЗначенияАтрибута,
		|	МАКСИМУМ(ЗначенияСвойств.Ссылка)
		|ИЗ
		|	ВТДополнительныеРеквизиты КАК ДополнительныеРеквизиты
		|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.ЗначенияСвойствОбъектовИерархия КАК ЗначенияСвойств
		|		ПО ДополнительныеРеквизиты.Свойство = ЗначенияСвойств.Владелец
		|			И (ЗначенияСвойств.Наименование = ДополнительныеРеквизиты.ЗначениеАтрибута)
		|			И (НЕ ЗначенияСвойств.ПометкаУдаления)
		|
		|СГРУППИРОВАТЬ ПО
		|	ДополнительныеРеквизиты.ИдентификаторВладельцаОбъекта,
		|	ДополнительныеРеквизиты.ИдентификаторЗначенияАтрибута
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|УНИЧТОЖИТЬ ВТДополнительныеРеквизиты
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ
		|	ТаблицаПоиска.Индекс КАК Индекс,
		|	СопоставленныеЗначения.ЗначениеРеквизита КАК ЗначениеРеквизита
		|ИЗ
		|	ВТТаблицаПоиска КАК ТаблицаПоиска
		|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТСопоставленныеЗначения КАК СопоставленныеЗначения
		|		ПО ТаблицаПоиска.ИдентификаторВладельцаОбъекта = СопоставленныеЗначения.ИдентификаторВладельцаОбъекта
		|			И ТаблицаПоиска.ИдентификаторЗначенияАтрибута = СопоставленныеЗначения.ИдентификаторЗначенияАтрибута
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ
		|	ТаблицаПоиска.Индекс КАК Индекс,
		|	НайденныеЗначения.ЗначениеРеквизита КАК ЗначениеРеквизита
		|ИЗ
		|	ВТТаблицаПоиска КАК ТаблицаПоиска
		|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТНайденныеЗначения КАК НайденныеЗначения
		|		ПО ТаблицаПоиска.ИдентификаторВладельцаОбъекта = НайденныеЗначения.ИдентификаторВладельцаОбъекта
		|			И ТаблицаПоиска.ИдентификаторЗначенияАтрибута = НайденныеЗначения.ИдентификаторЗначенияАтрибута
		|			И (ТаблицаПоиска.ЭтоДопРеквизит)";
	
	Возврат ТекстЗапроса;
	
КонецФункции

Процедура ЗаполнитьСопоставлениеВТаблицеПоВыборке(ВсеЗначенияАтрибутов, РезультатЗапроса, ЗаписатьСопоставление)
	
	Выборка = ВыборкаЗапросаПоискаСопоставленныхЗначений(РезультатЗапроса);
	
	Пока Выборка.Следующий() Цикл
		
		Строка = ВсеЗначенияАтрибутов.Получить(Выборка.Индекс);
		
		Строка.ЗначениеРеквизита = Выборка.ЗначениеРеквизита;
		Строка.НеобходимоЗаписатьСопоставление = ЗаписатьСопоставление;
		
	КонецЦикла;
	
КонецПроцедуры

// Параметры:
//  РезультатЗапроса - РезультатЗапроса -
// 
// Возвращаемое значение:
//  ВыборкаИзРезультатаЗапроса:
//   * Индекс - Число -
//   * ЗначениеРеквизита - Произвольный -
//
Функция ВыборкаЗапросаПоискаСопоставленныхЗначений(РезультатЗапроса)
	Возврат РезультатЗапроса.Выбрать();
КонецФункции

// Параметры:
//  УчетнаяЗапись - СправочникСсылка.УчетныеЗаписиМаркетплейсов -
//  ВсеЗначенияАтрибутов - см. НоваяТаблицаЗначенийАтрибутов
//  СоответствияАтрибутовКатегорий - см. НоваяТаблицаСоответствияАтрибутовКатегорий
// 
// Возвращаемое значение:
//  см. НовыеЗначенияАтрибутовДляСозданияНоменклатуры
//
Функция ЗначенияАтрибутовДляСозданияНоменклатуры(УчетнаяЗапись, ВсеЗначенияАтрибутов, СоответствияАтрибутовКатегорий)
	
	Результат = НовыеЗначенияАтрибутовДляСозданияНоменклатуры();
	
	КэшСозданныхЗначений = НовыйКэшСозданныхЗначений();
	
	Для Каждого СтрокаСоответствия Из СоответствияАтрибутовКатегорий Цикл
		
		ЭтоЕдиницаИзмеренияВеса      = Ложь;
		ЭтоЕдиницаИзмеренияГабаритов = Ложь;
		
		Если СтрокаСоответствия.ИдентификаторАтрибута = "weight_unit" Тогда
			ЭтоЕдиницаИзмеренияВеса = Истина;
			
		ИначеЕсли СтрокаСоответствия.ИдентификаторАтрибута = "dimension_unit" Тогда
			ЭтоЕдиницаИзмеренияГабаритов = Истина;
		КонецЕсли;
		
		Отбор = Новый Структура;
		
		Отбор.Вставить("ИдентификаторКатегории", СтрокаСоответствия.ИдентификаторКатегории);
		Отбор.Вставить("ИдентификаторАтрибута",  СтрокаСоответствия.ИдентификаторАтрибута);
		
		НайденныеСтроки = ВсеЗначенияАтрибутов.НайтиСтроки(Отбор);
		
		Для Каждого СтрокаСоЗначением Из НайденныеСтроки Цикл
			
			НоваяСтрока = Результат.Добавить();
			
			НоваяСтрока.ИмяРеквизита = СтрокаСоответствия.ИмяРеквизита;
			
			НоваяСтрока.ЭтоДопРеквизит = СтрокаСоответствия.ЭтоДопРеквизит;
			НоваяСтрока.Свойство       = СтрокаСоответствия.Свойство;
			
			НоваяСтрока.ЭтоРеквизитНоменклатуры   = СтрокаСоответствия.ЭтоРеквизитНоменклатуры;
			НоваяСтрока.ЭтоРеквизитХарактеристики = СтрокаСоответствия.ЭтоРеквизитХарактеристики;
			НоваяСтрока.ЭтоРеквизитУпаковки       = СтрокаСоответствия.ЭтоРеквизитУпаковки;
			
			НоваяСтрока.Идентификатор = СтрокаСоЗначением.Идентификатор;
			
			Если СтрокаСоЗначением.ЗначениеРеквизита <> Неопределено Тогда
				НоваяСтрока.ЗначениеРеквизита = СтрокаСоЗначением.ЗначениеРеквизита;
				
			ИначеЕсли НоваяСтрока.ЭтоДопРеквизит И СтрокаСоответствия.НужноСопоставитьЗначения Тогда
				
				ЗначениеРеквизита = СоздатьЗначениеДополнительногоРеквизита(
					СтрокаСоответствия.Свойство,
					СтрокаСоответствия.ТипРеквизита,
					СтрокаСоЗначением.ЗначениеАтрибута,
					КэшСозданныхЗначений);
				
				СтрокаСоЗначением.ЗначениеРеквизита               = ЗначениеРеквизита;
				СтрокаСоЗначением.НеобходимоЗаписатьСопоставление = (ЗначениеРеквизита <> Неопределено);
				
				НоваяСтрока.ЗначениеРеквизита = ЗначениеРеквизита;
				
			Иначе
				
				НоваяСтрока.ЗначениеРеквизита = СтрокаСоответствия.ТипРеквизита.ПривестиЗначение(
					СтрокаСоЗначением.ЗначениеАтрибута);
				
			КонецЕсли;
			
			Если ЭтоЕдиницаИзмеренияВеса И Не ЗначениеЗаполнено(НоваяСтрока.ЗначениеРеквизита) Тогда
				
				ДобавитьКоэффициентУпаковкиДляПересчета(
					Результат,
					ЕдиницыИзмеренияВесаУпаковки(),
					ИнтеграцияСМаркетплейсамиСервер.ИмяСвойстваУпаковкиКоэффициентВесаВКг(),
					СтрокаСоЗначением.Идентификатор,
					СтрокаСоЗначением.ИдентификаторЗначенияАтрибута);
				
			ИначеЕсли ЭтоЕдиницаИзмеренияГабаритов И Не ЗначениеЗаполнено(НоваяСтрока.ЗначениеРеквизита) Тогда
				
				ДобавитьКоэффициентУпаковкиДляПересчета(
					Результат,
					ЕдиницыИзмеренияГабаритовУпаковки(),
					ИнтеграцияСМаркетплейсамиСервер.ИмяСвойстваУпаковкиКоэффициентГабаритовВМ(),
					СтрокаСоЗначением.Идентификатор,
					СтрокаСоЗначением.ИдентификаторЗначенияАтрибута);
				
			КонецЕсли;
			
		КонецЦикла;
		
	КонецЦикла;
	
	ЗаписатьСопоставленныеЗначения(УчетнаяЗапись, ВсеЗначенияАтрибутов);
	
	Если Результат.Количество() > КоличествоСтрокДляИндексации() Тогда
		Результат.Индексы.Добавить("Идентификатор");
	КонецЕсли;
	
	Возврат Результат;
	
КонецФункции

// Возвращаемое значение:
//  ТаблицаЗначений:
//   * Идентификатор - Строка -
//   * ИмяРеквизита - Строка -
//   * ЗначениеРеквизита - Произвольный -
//   * ЭтоДопРеквизит - Булево -
//   * Свойство - ПланВидовХарактеристикСсылка.ДополнительныеРеквизитыИСведения -
//   * ЭтоРеквизитНоменклатуры - Булево -
//   * ЭтоРеквизитХарактеристики - Булево -
//   * ЭтоРеквизитУпаковки - Булево -
//
Функция НовыеЗначенияАтрибутовДляСозданияНоменклатуры()
	
	Результат = Новый ТаблицаЗначений;
	
	Результат.Колонки.Добавить("Идентификатор", ОбщегоНазначения.ОписаниеТипаСтрока(50));
	
	Результат.Колонки.Добавить("ИмяРеквизита", ОбщегоНазначения.ОписаниеТипаСтрока(150));
	Результат.Колонки.Добавить("ЗначениеРеквизита"); // Произвольный
	
	Результат.Колонки.Добавить("ЭтоДопРеквизит", Новый ОписаниеТипов("Булево"));
	Результат.Колонки.Добавить(
		"Свойство", Новый ОписаниеТипов("ПланВидовХарактеристикСсылка.ДополнительныеРеквизитыИСведения"));
	
	Результат.Колонки.Добавить("ЭтоРеквизитНоменклатуры",   Новый ОписаниеТипов("Булево"));
	Результат.Колонки.Добавить("ЭтоРеквизитХарактеристики", Новый ОписаниеТипов("Булево"));
	Результат.Колонки.Добавить("ЭтоРеквизитУпаковки",       Новый ОписаниеТипов("Булево"));
	
	Возврат Результат;
	
КонецФункции

// Возвращаемое значение:
//  Соответствие из КлючИЗначение:
//   * Ключ - ПланВидовХарактеристикСсылка.ДополнительныеРеквизитыИСведения -
//   * Значение - см. НовыеСозданныеЗначенияСвойства
//
Функция НовыйКэшСозданныхЗначений()
	Возврат Новый Соответствие;
КонецФункции

// Возвращаемое значение:
//  Соответствие из КлючИЗначение:
//   * Ключ - Строка -
//   * Значение - СправочникСсылка.ЗначенияСвойствОбъектов, СправочникСсылка.ЗначенияСвойствОбъектовИерархия -
//
Функция НовыеСозданныеЗначенияСвойства()
	Возврат Новый Соответствие;
КонецФункции

// Параметры:
//  Свойство - ПланВидовХарактеристикСсылка.ДополнительныеРеквизитыИСведения -
//  ТипЗначения - ОписаниеТипов -
//  ЗначениеСвойства - Произвольный -
//  КэшСозданныхЗначений - см. НовыйКэшСозданныхЗначений
// 
// Возвращаемое значение:
//  - Неопределено
//  - СправочникСсылка.ЗначенияСвойствОбъектовИерархия
//  - СправочникСсылка.ЗначенияСвойствОбъектов
//
Функция СоздатьЗначениеДополнительногоРеквизита(Свойство, ТипЗначения, ЗначениеСвойства, КэшСозданныхЗначений)
	
	Результат = Неопределено;
	
	СозданныеЗначенияСвойства = КэшСозданныхЗначений.Получить(Свойство);
	
	Если СозданныеЗначенияСвойства = Неопределено Тогда
		
		КэшСозданныхЗначений.Вставить(Свойство, НовыеСозданныеЗначенияСвойства());
		СозданныеЗначенияСвойства = КэшСозданныхЗначений.Получить(Свойство);
		
	Иначе
		Результат = СозданныеЗначенияСвойства.Получить(ЗначениеСвойства);
	КонецЕсли;
	
	Если Результат <> Неопределено Тогда
		Возврат Результат;
	КонецЕсли;
	
	ЗначениеСвойстваСтрокой = ПривестиЗначениеКТипу(ЗначениеСвойства, Тип("Строка")); // Строка
	
	ДанныеЗаполнения = Новый Структура;
	
	ДанныеЗаполнения.Вставить("Владелец", Свойство);
	ДанныеЗаполнения.Вставить("Наименование", ЗначениеСвойстваСтрокой);
	
	Попытка
		
		СправочникОбъект = ОбъектСправочникаПоТипуЗначения(ТипЗначения);
		
		ЗаполнитьЗначенияСвойств(СправочникОбъект, ДанныеЗаполнения);
		
		СправочникОбъект.Заполнить(Неопределено);
		СправочникОбъект.Записать();
		
		СозданныеЗначенияСвойства.Вставить(ЗначениеСвойства, СправочникОбъект.Ссылка);
		
		Результат = СправочникОбъект.Ссылка;
		
	Исключение
		
		Результат = Неопределено;
		
		Тексты = Новый Массив; // Массив из Строка
		
		ШаблонОшибки = НСтр("ru = 'Не удалось создать значение дополнительного свойства ""%1"".';
							|en = 'Cannot create the value of the ""%1"" additional property.'",
			ОбщегоНазначения.КодОсновногоЯзыка());
		
		ТекстОшибки = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(ШаблонОшибки, ЗначениеСвойстваСтрокой);
		
		Тексты.Добавить(ТекстОшибки);
		
		Тексты.Добавить(ОбработкаОшибок.ПодробноеПредставлениеОшибки(ИнформацияОбОшибке()));
		
		ТекстОшибки = СтрСоединить(Тексты, Символы.ПС);
		
		ЗаписьЖурналаРегистрации(
			СобытиеЖурналаРегистрации(), УровеньЖурналаРегистрации.Ошибка, , Свойство, ТекстОшибки);
		
	КонецПопытки;
	
	Возврат Результат;
	
КонецФункции

// Параметры:
//  ТипЗначения - ОписаниеТипов -
// 
// Возвращаемое значение:
//  - СправочникОбъект.ЗначенияСвойствОбъектовИерархия
//  - СправочникОбъект.ЗначенияСвойствОбъектов
//
Функция ОбъектСправочникаПоТипуЗначения(ТипЗначения)
	
	Если ТипЗначения.СодержитТип(Тип("СправочникСсылка.ЗначенияСвойствОбъектовИерархия")) Тогда
		Возврат Справочники.ЗначенияСвойствОбъектовИерархия.СоздатьЭлемент();
	Иначе
		Возврат Справочники.ЗначенияСвойствОбъектов.СоздатьЭлемент();
	КонецЕсли;
	
КонецФункции

Процедура ДобавитьКоэффициентУпаковкиДляПересчета(Результат, ЕдиницыИзмерения, ИмяРеквизита, Идентификатор,
			ИдентификаторЗначенияАтрибута)
	
	Коэффициент = 0;
	
	Для Каждого ЕдиницаИзмерения Из ЕдиницыИзмерения Цикл
		
		Если ИдентификаторЗначенияАтрибута = ЕдиницаИзмерения.ИдентификаторЗначенияАтрибутаМаркетплейса Тогда
			
			Коэффициент = Окр(ЕдиницаИзмерения.Числитель / ЕдиницаИзмерения.Знаменатель, 11);
			Прервать;
			
		КонецЕсли;
		
	КонецЦикла;
	
	Если Коэффициент = 0 Тогда
		Возврат;
	КонецЕсли;
	
	НоваяСтрока = Результат.Добавить();
	
	НоваяСтрока.ИмяРеквизита        = ИмяРеквизита;
	НоваяСтрока.ЭтоРеквизитУпаковки = Истина;
	НоваяСтрока.Идентификатор       = Идентификатор;
	НоваяСтрока.ЗначениеРеквизита   = Коэффициент
	
КонецПроцедуры

Процедура ЗаписатьСопоставленныеЗначения(УчетнаяЗапись, ВсеЗначенияАтрибутов)
	
	Отбор = Новый Структура;
	Отбор.Вставить("НеобходимоЗаписатьСопоставление", Истина);
	
	ТаблицаДляЗаписи = ВсеЗначенияАтрибутов.Скопировать(Отбор);
	
	Если ТаблицаДляЗаписи.Количество() = 0 Тогда
		Возврат;
	КонецЕсли;
	
	КолонкиГруппировки =
		"ИдентификаторВладельцаОбъекта, ИдентификаторЗначенияАтрибута, ЗначениеРеквизита, ЗначениеАтрибута";
	
	ТаблицаДляЗаписи.Свернуть(КолонкиГруппировки);
	
	ДатаАктуальности = ТекущаяДатаСеанса();
	
	НаборЗаписей = РегистрыСведений.СоответствияОбъектовМаркетплейсов.СоздатьНаборЗаписей();
	
	Для Каждого Строка Из ТаблицаДляЗаписи Цикл
		
		ЗначениеАтрибутаСтрокой = ПривестиЗначениеКТипу(Строка.ЗначениеАтрибута, Тип("Строка")); // Строка
		
		Запись = НаборЗаписей.Добавить();
		
		Запись.УчетнаяЗаписьМаркетплейса = УчетнаяЗапись;
		Запись.ВидОбъектаМаркетплейса    = Перечисления.ВидыОбъектовМаркетплейсов.ЗначениеАтрибутаКатегорииТоваров;
		
		Запись.ИдентификаторОбъектаМаркетплейса          = Строка.ИдентификаторЗначенияАтрибута;
		Запись.ИдентификаторВладельцаОбъектаМаркетплейса = Строка.ИдентификаторВладельцаОбъекта;
		//@skip-check statement-type-change
		Запись.Объект1С                                  = Строка.ЗначениеРеквизита;
		Запись.НаименованиеОбъектаМаркетплейса           = ЗначениеАтрибутаСтрокой;
		
		Запись.ДатаАктуальности = ДатаАктуальности;
		
	КонецЦикла;
	
	НаборЗаписей.Записать(РежимЗамещения.Добавление);
	
КонецПроцедуры

Процедура ЗаполнитьДанныеДляСозданияНоменклатуры(ДанныеКонтрагента, ДанныеСоздаваемыхЭлементов,
			ЗначенияАтрибутовДляСозданияНоменклатуры)
	
	Для Каждого Строка Из ДанныеКонтрагента Цикл
		
		Идентификатор = ИдентификаторНоменклатурыКонтрагента(Строка.НоменклатураКонтрагента);
		
		Отбор = Новый Структура;
		Отбор.Вставить("Идентификатор", Идентификатор);
		
		ЗначенияАтрибутов = ЗначенияАтрибутовДляСозданияНоменклатуры.НайтиСтроки(Отбор);
		
		Для Каждого ЗначениеАтрибута Из ЗначенияАтрибутов Цикл
			
			Ключ     = КлючЭлементаКоллекцииПоЗначениюАтрибута(ЗначениеАтрибута);
			Значение = ЗначениеАтрибута.ЗначениеРеквизита;
			
			ИмяКоллекции = "Реквизиты";
			
			ОписаниеДанных = ОписаниеСоздаваемыхДанных(
				ДанныеСоздаваемыхЭлементов, Строка, ЗначениеАтрибута, ИмяКоллекции);
			
			Если ОписаниеДанных = Неопределено Тогда
				Продолжить;
			КонецЕсли;
			
			КоллекцияРеквизитов = ОписаниеДанных[ИмяКоллекции]; // Структура, Соответствие
			
			ДобавитьЭлементВКоллекцию(КоллекцияРеквизитов, Ключ, Значение);
			
		КонецЦикла;
		
	КонецЦикла;
	
КонецПроцедуры

// Параметры:
//  ДанныеСоздаваемыхЭлементов - см. ИнтеграцияСМаркетплейсамиСервер.НовыеДанныеДляСозданияНоменклатурыПоДаннымКонтрагента
//  Строка - СтрокаТаблицыЗначений из см. ИнтеграцияСМаркетплейсамиСервер.НоваяТаблицаДанныхКонтрагентаДляСозданияНоменклатуры
//  ЗначениеАтрибута - СтрокаТаблицыЗначений из см. НовыеЗначенияАтрибутовДляСозданияНоменклатуры
//  ИмяКоллекции - Строка - 
// 
// Возвращаемое значение:
//  - Структура
//  - Неопределено
//
Функция ОписаниеСоздаваемыхДанных(ДанныеСоздаваемыхЭлементов, Строка, ЗначениеАтрибута, ИмяКоллекции)
	
	Если ЗначениеАтрибута.ЭтоРеквизитНоменклатуры Тогда
		
		ОписаниеДанных = ДанныеСоздаваемыхЭлементов.СоздаваемаяНоменклатура.Получить(Строка.Номенклатура);
		
		Если ЗначениеАтрибута.ЭтоДопРеквизит Тогда
			ИмяКоллекции = "ДополнительныеРеквизиты";
		КонецЕсли;
		
	ИначеЕсли ЗначениеАтрибута.ЭтоРеквизитХарактеристики Тогда
		
		ОписаниеДанных = ДанныеСоздаваемыхЭлементов.СоздаваемыеХарактеристики.Получить(Строка.Характеристика);
		
		Если ЗначениеАтрибута.ЭтоДопРеквизит Тогда
			ИмяКоллекции = "ДополнительныеРеквизиты";
		КонецЕсли;
		
	ИначеЕсли ЗначениеАтрибута.ЭтоРеквизитУпаковки Тогда
		ОписаниеДанных = ДанныеСоздаваемыхЭлементов.СоздаваемыеУпаковки.Получить(Строка.Упаковка);
		
	Иначе
		ОписаниеДанных = Неопределено;
	КонецЕсли;
	
	Возврат ОписаниеДанных;
	
КонецФункции

// Параметры:
//  ЗначениеАтрибута - СтрокаТаблицыЗначений из см. ИнтеграцияСМаркетплейсамиСервер.ДанныеДляСозданияНоменклатурыПоДаннымКонтрагента
// 
// Возвращаемое значение:
//  - Строка
//  - ПланВидовХарактеристикСсылка.ДополнительныеРеквизитыИСведения
//
Функция КлючЭлементаКоллекцииПоЗначениюАтрибута(ЗначениеАтрибута)
	
	Если ЗначениеАтрибута.ЭтоДопРеквизит Тогда
		Возврат ЗначениеАтрибута.Свойство;
	Иначе
		Возврат ЗначениеАтрибута.ИмяРеквизита;
	КонецЕсли;
	
КонецФункции

// Параметры:
//  Коллекция - Структура, Соответствие из КлючИЗначение -
//  Ключ - Строка, ПланВидовХарактеристикСсылка.ДополнительныеРеквизитыИСведения -
//  Значение - Произвольный -
//
Процедура ДобавитьЭлементВКоллекцию(Коллекция, Ключ, Значение)
	
	Если ТипЗнч(Коллекция) = Тип("Структура") Тогда
		
		ИсточникСтруктура = Новый Структура;
		ИсточникСтруктура.Вставить(Ключ, Значение);
		
		ОбщегоНазначенияКлиентСервер.ДополнитьСтруктуру(Коллекция, ИсточникСтруктура, Ложь);
		
	ИначеЕсли ТипЗнч(Коллекция) = Тип("Соответствие") Тогда
		
		ИсточникСоответствие = Новый Соответствие;
		ИсточникСоответствие.Вставить(Ключ, Значение);
		
		ОбщегоНазначенияКлиентСервер.ДополнитьСоответствие(Коллекция, ИсточникСоответствие, Ложь);
		
	КонецЕсли;
	
КонецПроцедуры

Функция КоличествоСтрокДляИндексации()
	Возврат 1000;
КонецФункции

#КонецОбласти

#Область ИмпортДанныхСлужебный

Процедура ИмпортироватьДанныеТоварногоКаталогаСТорговойПлощадки(УчетнаяЗапись, ДополнительныеПараметры,
			Идентификаторы = Неопределено)
	
	Параметры = ИнтеграцияСМаркетплейсамиКлиентСервер.ПараметрыПолученияТоваровИзСервиса(
		Перечисления.ВидыМаркетплейсов.МаркетплейсOzon);
	Параметры.КлючЗадания = Параметры.КлючЗадания + "_" + УчетнаяЗапись.УникальныйИдентификатор();
	
	// Настройки получения порции вызываемого метода - параметры выполнения вызываемого метода.
	Параметры.ПараметрыМетода.УчетнаяЗапись = УчетнаяЗапись;
	
	Если ТипЗнч(Идентификаторы) = Тип("Структура") Тогда
		ЗаполнитьЗначенияСвойств(Параметры.ПараметрыМетода.Идентификаторы, Идентификаторы);
		
		ЕстьИдентификаторы = ИнтеграцияСМаркетплейсамиКлиентСервер.ЕстьПереданныеИдентификаторы(
			Параметры.ПараметрыМетода.Идентификаторы);
		
		Если Не ЕстьИдентификаторы Тогда
			Возврат;
		КонецЕсли;
	КонецЕсли;
	
	Параметры.ПараметрыМетода.НастройкиИмпорта.ЗаписыватьНовые           = Истина;
	Параметры.ПараметрыМетода.НастройкиИмпорта.ОбновлятьНесопоставленные = Истина;
	
	Параметры.ПараметрыМетода.НастройкиИмпорта.ОбновлятьСопоставленные = 
		ДополнительныеПараметры.ОбновлятьСопоставленные;
	
	Параметры.ПараметрыМетода.ЭтоЗапускВручную = ДополнительныеПараметры.ЭтоЗапускВручную;
	
	Результат = ИнтеграцияСМаркетплейсамиВызовСервера.ПолучитьТоварыИзСервиса(Параметры, Ложь, Ложь);
	
	Если Результат.Статус = "Ошибка" Тогда
		ЗаписьЖурналаРегистрации(
			СобытиеЖурналаРегистрации(),
			УровеньЖурналаРегистрации.Ошибка,,,
			Результат.ПодробноеПредставлениеОшибки);
	КонецЕсли;
	
КонецПроцедуры

// Текст запроса для поиска записей в регистре статусов публикации по данным из описания товаров.
//
// Параметры:
//   РеквизитНомерСтроки - Строка - имя реквизита номера строки в таблице описаний товаров.
//   ВыбратьТовары       - Булево - признак выборки данных по товарам.
//   ВыбратьИзображения  - Булево - признак выборки данных по изображениям товаров.
//
// Возвращаемое значение:
//   Строка - текст запроса.
//
Функция ТекстЗапросаПоискаЗаписейСтатусовПубликацииПриИмпорте(РеквизитНомерСтроки = "",
			ВыбратьТовары = Истина, ВыбратьИзображения = Истина)

	ТекстЗапроса =
		"ВЫБРАТЬ
		|	&НомерСтроки КАК НомерСтроки,
		|	ОписаниеТоваров.УчетнаяЗаписьМаркетплейса КАК УчетнаяЗаписьМаркетплейса,
		|	ОписаниеТоваров.ИдентификаторПубликации КАК ИдентификаторПубликации,
		|	ОписаниеТоваров.ИдентификаторОбъектаМаркетплейса КАК ИдентификаторОбъектаМаркетплейса,
		|	ОписаниеТоваров.Статус КАК Статус
		|ПОМЕСТИТЬ ВТОписаниеТоваров
		|ИЗ
		|	&ОписаниеТоваров КАК ОписаниеТоваров
		|
		|ИНДЕКСИРОВАТЬ ПО НАБОРАМ
		|(
		|	(УчетнаяЗаписьМаркетплейса,
		|	ИдентификаторОбъектаМаркетплейса),
		|	(ИдентификаторОбъектаМаркетплейса),
		|	(ИдентификаторПубликации)
		|)
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ
		|	НоменклатураПоИдентификаторуПубликации.Артикул КАК ИдентификаторПубликации,
		|	МАКСИМУМ(НоменклатураПоИдентификаторуПубликации.Ссылка) КАК Ссылка
		|ПОМЕСТИТЬ ВТНоменклатураПоИдентификаторуПубликации
		|ИЗ
		|	Справочник.НоменклатураКонтрагентов КАК НоменклатураПоИдентификаторуПубликации
		|		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СтатусыПубликацииОбъектовМаркетплейсаOzon КАК СтатусыПубликацииОбъектов
		|		ПО (СтатусыПубликацииОбъектов.ВидОбъектаМаркетплейса = ЗНАЧЕНИЕ(Перечисление.ВидыОбъектовМаркетплейсов.Товар))
		|			И (СтатусыПубликацииОбъектов.ОбъектПубликации = НЕОПРЕДЕЛЕНО)
		|			И НоменклатураПоИдентификаторуПубликации.Ссылка = СтатусыПубликацииОбъектов.НоменклатураПартнера
		|ГДЕ
		|	НоменклатураПоИдентификаторуПубликации.Владелец = &Партнер
		|	И НЕ НоменклатураПоИдентификаторуПубликации.ПометкаУдаления
		|	И ИСТИНА В
		|			(ВЫБРАТЬ ПЕРВЫЕ 1
		|				ИСТИНА
		|			ИЗ
		|				ВТОписаниеТоваров КАК ОписаниеТоваров
		|			ГДЕ
		|				ОписаниеТоваров.ИдентификаторПубликации = НоменклатураПоИдентификаторуПубликации.Артикул)
		|	И СтатусыПубликацииОбъектов.НоменклатураПартнера ЕСТЬ NULL
		|
		|СГРУППИРОВАТЬ ПО
		|	НоменклатураПоИдентификаторуПубликации.Артикул
		|
		|ИНДЕКСИРОВАТЬ ПО
		|	ИдентификаторПубликации
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ
		|	ОписаниеТоваров.НомерСтроки КАК НомерСтроки,
		|	ОписаниеТоваров.УчетнаяЗаписьМаркетплейса КАК УчетнаяЗаписьМаркетплейса,
		|	ОписаниеТоваров.ИдентификаторПубликации КАК ИдентификаторПубликации,
		|	ОписаниеТоваров.ИдентификаторОбъектаМаркетплейса КАК ИдентификаторОбъектаМаркетплейса,
		|	ЛОЖЬ КАК ЭтоНоваяЗапись,
		|	ЕСТЬNULL(СтатусыПубликации.ВидОбъектаМаркетплейса, ЗНАЧЕНИЕ(Перечисление.ВидыОбъектовМаркетплейсов.Товар)) КАК ВидОбъектаМаркетплейса,
		|	ЕСТЬNULL(СтатусыПубликации.Номенклатура, ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка)) КАК Номенклатура,
		|	ЕСТЬNULL(СтатусыПубликации.Характеристика, ЗНАЧЕНИЕ(Справочник.ХарактеристикиНоменклатуры.ПустаяСсылка)) КАК Характеристика,
		|	ЕСТЬNULL(СтатусыПубликации.Упаковка, ЗНАЧЕНИЕ(Справочник.УпаковкиЕдиницыИзмерения.ПустаяСсылка)) КАК Упаковка,
		|	ЕСТЬNULL(СтатусыПубликации.ОбъектПубликации, НЕОПРЕДЕЛЕНО) КАК ОбъектПубликации,
		|	ЕСТЬNULL(СтатусыПубликации.Статус, ЗНАЧЕНИЕ(Перечисление.СтатусыОбъектовМаркетплейсаOzon.ПустаяСсылка)) КАК СтарыйСтатус,
		|	ОписаниеТоваров.Статус КАК НовыйСтатус,
		|	ЕСТЬNULL(СтатусыПубликации.НоменклатураПартнера, &ПустаяСсылка) КАК НоменклатураПартнера,
		|	100 + ВЫБОР
		|		КОГДА СтатусыПубликации.Номенклатура <> ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка)
		|			ТОГДА 1
		|		ИНАЧЕ 0
		|	КОНЕЦ + ВЫБОР
		|		КОГДА СтатусыПубликации.Характеристика <> ЗНАЧЕНИЕ(Справочник.ХарактеристикиНоменклатуры.ПустаяСсылка)
		|			ТОГДА 1
		|		ИНАЧЕ 0
		|	КОНЕЦ + ВЫБОР
		|		КОГДА СтатусыПубликации.Упаковка <> ЗНАЧЕНИЕ(Справочник.УпаковкиЕдиницыИзмерения.ПустаяСсылка)
		|			ТОГДА 1
		|		ИНАЧЕ 0
		|	КОНЕЦ КАК Приоритет,
		|	&ДополнительныеПоляВыборки
		|ПОМЕСТИТЬ ВТДанные
		|ИЗ
		|	ВТОписаниеТоваров КАК ОписаниеТоваров
		|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.СтатусыПубликацииОбъектовМаркетплейсаOzon КАК СтатусыПубликации
		|		ПО (СтатусыПубликации.УчетнаяЗаписьМаркетплейса = ОписаниеТоваров.УчетнаяЗаписьМаркетплейса)
		|			И (СтатусыПубликации.ВидОбъектаМаркетплейса = ЗНАЧЕНИЕ(Перечисление.ВидыОбъектовМаркетплейсов.Товар))
		|			И (СтатусыПубликации.ИдентификаторПубликации = ОписаниеТоваров.ИдентификаторПубликации)
		|			И (СтатусыПубликации.ИдентификаторОбъектаМаркетплейса = ОписаниеТоваров.ИдентификаторОбъектаМаркетплейса)
		|
		|ОБЪЕДИНИТЬ ВСЕ
		|
		|ВЫБРАТЬ
		|	ОписаниеТоваров.НомерСтроки,
		|	ОписаниеТоваров.УчетнаяЗаписьМаркетплейса,
		|	ОписаниеТоваров.ИдентификаторПубликации,
		|	ОписаниеТоваров.ИдентификаторОбъектаМаркетплейса,
		|	ЛОЖЬ,
		|	ЕСТЬNULL(СтатусыПубликации.ВидОбъектаМаркетплейса, ЗНАЧЕНИЕ(Перечисление.ВидыОбъектовМаркетплейсов.Товар)),
		|	ЕСТЬNULL(СтатусыПубликации.Номенклатура, ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка)),
		|	ЕСТЬNULL(СтатусыПубликации.Характеристика, ЗНАЧЕНИЕ(Справочник.ХарактеристикиНоменклатуры.ПустаяСсылка)),
		|	ЕСТЬNULL(СтатусыПубликации.Упаковка, ЗНАЧЕНИЕ(Справочник.УпаковкиЕдиницыИзмерения.ПустаяСсылка)),
		|	ЕСТЬNULL(СтатусыПубликации.ОбъектПубликации, НЕОПРЕДЕЛЕНО),
		|	ЕСТЬNULL(СтатусыПубликации.Статус, ЗНАЧЕНИЕ(Перечисление.СтатусыОбъектовМаркетплейсаOzon.ПустаяСсылка)),
		|	ОписаниеТоваров.Статус,
		|	ЕСТЬNULL(СтатусыПубликации.НоменклатураПартнера, &ПустаяСсылка),
		|	50 + ВЫБОР
		|		КОГДА СтатусыПубликации.Номенклатура <> ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка)
		|			ТОГДА 1
		|		ИНАЧЕ 0
		|	КОНЕЦ + ВЫБОР
		|		КОГДА СтатусыПубликации.Характеристика <> ЗНАЧЕНИЕ(Справочник.ХарактеристикиНоменклатуры.ПустаяСсылка)
		|			ТОГДА 1
		|		ИНАЧЕ 0
		|	КОНЕЦ + ВЫБОР
		|		КОГДА СтатусыПубликации.Упаковка <> ЗНАЧЕНИЕ(Справочник.УпаковкиЕдиницыИзмерения.ПустаяСсылка)
		|			ТОГДА 1
		|		ИНАЧЕ 0
		|	КОНЕЦ,
		|	&ДополнительныеПоляВыборки
		|ИЗ
		|	ВТОписаниеТоваров КАК ОписаниеТоваров
		|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.СтатусыПубликацииОбъектовМаркетплейсаOzon КАК СтатусыПубликации
		|		ПО (СтатусыПубликации.УчетнаяЗаписьМаркетплейса = ОписаниеТоваров.УчетнаяЗаписьМаркетплейса)
		|			И (СтатусыПубликации.ВидОбъектаМаркетплейса = ЗНАЧЕНИЕ(Перечисление.ВидыОбъектовМаркетплейсов.Товар))
		|			И (СтатусыПубликации.ИдентификаторОбъектаМаркетплейса = ОписаниеТоваров.ИдентификаторОбъектаМаркетплейса)
		|			И (СтатусыПубликации.ИдентификаторПубликации <> ОписаниеТоваров.ИдентификаторПубликации)
		|
		|ОБЪЕДИНИТЬ ВСЕ
		|
		|ВЫБРАТЬ
		|	ОписаниеТоваров.НомерСтроки,
		|	ОписаниеТоваров.УчетнаяЗаписьМаркетплейса,
		|	ОписаниеТоваров.ИдентификаторПубликации,
		|	ОписаниеТоваров.ИдентификаторОбъектаМаркетплейса,
		|	ЛОЖЬ,
		|	ЕСТЬNULL(СтатусыПубликации.ВидОбъектаМаркетплейса, ЗНАЧЕНИЕ(Перечисление.ВидыОбъектовМаркетплейсов.Товар)),
		|	ЕСТЬNULL(СтатусыПубликации.Номенклатура, ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка)),
		|	ЕСТЬNULL(СтатусыПубликации.Характеристика, ЗНАЧЕНИЕ(Справочник.ХарактеристикиНоменклатуры.ПустаяСсылка)),
		|	ЕСТЬNULL(СтатусыПубликации.Упаковка, ЗНАЧЕНИЕ(Справочник.УпаковкиЕдиницыИзмерения.ПустаяСсылка)),
		|	ЕСТЬNULL(СтатусыПубликации.ОбъектПубликации, НЕОПРЕДЕЛЕНО),
		|	ЕСТЬNULL(СтатусыПубликации.Статус, ЗНАЧЕНИЕ(Перечисление.СтатусыОбъектовМаркетплейсаOzon.ПустаяСсылка)),
		|	ОписаниеТоваров.Статус,
		|	ЕСТЬNULL(СтатусыПубликации.НоменклатураПартнера, &ПустаяСсылка),
		|	10 + ВЫБОР
		|		КОГДА СтатусыПубликации.Номенклатура <> ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка)
		|			ТОГДА 1
		|		ИНАЧЕ 0
		|	КОНЕЦ + ВЫБОР
		|		КОГДА СтатусыПубликации.Характеристика <> ЗНАЧЕНИЕ(Справочник.ХарактеристикиНоменклатуры.ПустаяСсылка)
		|			ТОГДА 1
		|		ИНАЧЕ 0
		|	КОНЕЦ + ВЫБОР
		|		КОГДА СтатусыПубликации.Упаковка <> ЗНАЧЕНИЕ(Справочник.УпаковкиЕдиницыИзмерения.ПустаяСсылка)
		|			ТОГДА 1
		|		ИНАЧЕ 0
		|	КОНЕЦ,
		|	&ДополнительныеПоляВыборки
		|ИЗ
		|	ВТОписаниеТоваров КАК ОписаниеТоваров
		|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.СтатусыПубликацииОбъектовМаркетплейсаOzon КАК СтатусыПубликации
		|		ПО (СтатусыПубликации.УчетнаяЗаписьМаркетплейса = ОписаниеТоваров.УчетнаяЗаписьМаркетплейса)
		|			И (СтатусыПубликации.ВидОбъектаМаркетплейса = ЗНАЧЕНИЕ(Перечисление.ВидыОбъектовМаркетплейсов.Товар))
		|			И (СтатусыПубликации.ИдентификаторПубликации = ОписаниеТоваров.ИдентификаторПубликации)
		|			И (СтатусыПубликации.ИдентификаторОбъектаМаркетплейса = """")
		|
		|ОБЪЕДИНИТЬ ВСЕ
		|
		|ВЫБРАТЬ
		|	ОписаниеТоваров.НомерСтроки,
		|	ОписаниеТоваров.УчетнаяЗаписьМаркетплейса,
		|	ОписаниеТоваров.ИдентификаторПубликации,
		|	ОписаниеТоваров.ИдентификаторОбъектаМаркетплейса,
		|	ИСТИНА,
		|	ЗНАЧЕНИЕ(Перечисление.ВидыОбъектовМаркетплейсов.Товар),
		|	ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка),
		|	ЗНАЧЕНИЕ(Справочник.ХарактеристикиНоменклатуры.ПустаяСсылка),
		|	ЗНАЧЕНИЕ(Справочник.УпаковкиЕдиницыИзмерения.ПустаяСсылка),
		|	НЕОПРЕДЕЛЕНО,
		|	ЗНАЧЕНИЕ(Перечисление.СтатусыОбъектовМаркетплейсаOzon.ПустаяСсылка),
		|	ОписаниеТоваров.Статус,
		|	ЕСТЬNULL(НоменклатураПоИдентификаторуМаркетплейса.Ссылка, ЕСТЬNULL(НоменклатураПоИдентификаторуПубликации.Ссылка, &ПустаяСсылка)),
		|	0,
		|	&ПустыеПоляВыборки
		|ИЗ
		|	ВТОписаниеТоваров КАК ОписаниеТоваров
		|		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.НоменклатураКонтрагентов КАК НоменклатураПоИдентификаторуМаркетплейса
		|		ПО (НоменклатураПоИдентификаторуМаркетплейса.Владелец = &Партнер)
		|			И (НоменклатураПоИдентификаторуМаркетплейса.Идентификатор = ОписаниеТоваров.ИдентификаторОбъектаМаркетплейса)
		|			И (НЕ НоменклатураПоИдентификаторуМаркетплейса.ПометкаУдаления)
		|		ЛЕВОЕ СОЕДИНЕНИЕ ВТНоменклатураПоИдентификаторуПубликации КАК НоменклатураПоИдентификаторуПубликации
		|		ПО (НоменклатураПоИдентификаторуПубликации.ИдентификаторПубликации = ОписаниеТоваров.ИдентификаторПубликации)
		|
		|ИНДЕКСИРОВАТЬ ПО НАБОРАМ
		|(
		|	(НоменклатураПартнера),
		|	(Номенклатура)
		|)";
	
	Если ПустаяСтрока(РеквизитНомерСтроки) Тогда
		ТекстНомераСтроки = "АВТОНОМЕРЗАПИСИ()";
	Иначе
		ТекстНомераСтроки = "ОписаниеТоваров." + РеквизитНомерСтроки;
	КонецЕсли;
	
	ТекстЗапроса = СтрЗаменить(ТекстЗапроса,
		"&НомерСтроки",
		ТекстНомераСтроки);
	
	ДобавляемыеПоляВыборки = Новый Массив;
	ПустыеПоляВыборки      = Новый Массив;
	
	Если ВыбратьТовары Тогда
		ТекстЗапроса = ТекстЗапроса + ОбщегоНазначения.РазделительПакетаЗапросов()
			+ "ВЫБРАТЬ
			  |	Данные.НомерСтроки КАК НомерСтроки,
			  |	Данные.УчетнаяЗаписьМаркетплейса КАК УчетнаяЗаписьМаркетплейса,
			  |	Данные.ИдентификаторПубликации КАК ИдентификаторПубликации,
			  |	Данные.ИдентификаторОбъектаМаркетплейса КАК ИдентификаторОбъектаМаркетплейса,
			  |	Данные.ЭтоНоваяЗапись КАК ЭтоНоваяЗапись,
			  |	Данные.ВидОбъектаМаркетплейса КАК ВидОбъектаМаркетплейса,
			  |	Данные.Номенклатура КАК Номенклатура,
			  |	Данные.Характеристика КАК Характеристика,
			  |	Данные.Упаковка КАК Упаковка,
			  |	Данные.НоменклатураПартнера КАК НоменклатураПартнера,
			  |	ЕСТЬNULL(СправочникНоменклатура.ЕдиницаИзмерения, ЗНАЧЕНИЕ(Справочник.УпаковкиЕдиницыИзмерения.ПустаяСсылка)) КАК ЕдиницаИзмерения,
			  |	ЕСТЬNULL(ДанныеЕдиницыНоменклатуры.Наименование, ЕСТЬNULL(ДанныеЕдиницыПоУмолчанию.Наименование, """")) КАК ЕдиницаИзмеренияНаименование,
			  |	ЕСТЬNULL(ДанныеЕдиницыНоменклатуры.Код, ЕСТЬNULL(ДанныеЕдиницыПоУмолчанию.Код, """")) КАК ЕдиницаИзмеренияКод,
			  |	ПРЕДСТАВЛЕНИЕ(Данные.Упаковка) КАК НаименованиеУпаковки,
			  |	ЕСТЬNULL(&ТекстЗапросаКоэффициентУпаковки, 1) КАК УпаковкаКоэффициент,
			  |	Данные.ОбъектПубликации КАК ОбъектПубликации,
			  |	Данные.СтарыйСтатус КАК СтарыйСтатус,
			  |	Данные.НовыйСтатус КАК НовыйСтатус,
			  |	Данные.Приоритет КАК Приоритет,
			  |	&ДополнительныеПоляВыборки
			  |ИЗ
			  |	ВТДанные КАК Данные
			  |		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Номенклатура КАК СправочникНоменклатура
			  |		ПО Данные.Номенклатура = СправочникНоменклатура.Ссылка
			  |		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.УпаковкиЕдиницыИзмерения КАК ДанныеЕдиницыНоменклатуры
			  |		ПО (СправочникНоменклатура.ЕдиницаИзмерения = ДанныеЕдиницыНоменклатуры.Ссылка)
			  |		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.УпаковкиЕдиницыИзмерения КАК ДанныеЕдиницыПоУмолчанию
			  |		ПО (ДанныеЕдиницыПоУмолчанию.Ссылка = &ЕдиницаИзмеренияКоличестваШтук)
			  |
			  |УПОРЯДОЧИТЬ ПО
			  |	НомерСтроки,
			  |	Приоритет УБЫВ
			  |ИТОГИ ПО
			  |	НомерСтроки
			  |;
			  |
			  |////////////////////////////////////////////////////////////////////////////////
			  |ВЫБРАТЬ РАЗЛИЧНЫЕ
			  |	Данные.УчетнаяЗаписьМаркетплейса КАК УчетнаяЗаписьМаркетплейса,
			  |	Данные.ВидОбъектаМаркетплейса КАК ВидОбъектаМаркетплейса,
			  |	Данные.НоменклатураПартнера КАК НоменклатураПартнера
			  |ИЗ
			  |	ВТДанные КАК Данные
			  |ГДЕ
			  |	НЕ Данные.ЭтоНоваяЗапись";
		
		ТекстЗапроса = СтрЗаменить(ТекстЗапроса,
			"&ТекстЗапросаКоэффициентУпаковки",
			Справочники.УпаковкиЕдиницыИзмерения.ТекстЗапросаКоэффициентаУпаковки(
				"(ВЫБОР
				|	КОГДА Данные.Упаковка = ЗНАЧЕНИЕ(Справочник.УпаковкиЕдиницыИзмерения.ПустаяСсылка)
				|		ТОГДА СправочникНоменклатура.ЕдиницаИзмерения
				|	ИНАЧЕ Данные.Упаковка
				|КОНЕЦ)",
				"Данные.Номенклатура"));
		
		ИсключаемыеПоля =
			"ИдентификаторПубликации,
			|ИдентификаторОбъектаМаркетплейса,
			|Статус";
		
		ОбъектМетаданных = Метаданные.РегистрыСведений.СтатусыПубликацииОбъектовМаркетплейсаOzon;
		
		Для Каждого Ресурс Из ОбъектМетаданных.Ресурсы Цикл
			Если СтрНайти(ИсключаемыеПоля, Ресурс.Имя) > 0 Тогда
				Продолжить;
			КонецЕсли;
			
			ДобавляемыеПоляВыборки.Добавить(Ресурс.Имя + " " + "КАК" + " " + Ресурс.Имя);
			ПустыеПоляВыборки.Добавить("NULL");
		КонецЦикла;
		
		Для Каждого Реквизит Из ОбъектМетаданных.Реквизиты Цикл
			Если СтрНайти(ИсключаемыеПоля, Реквизит.Имя) > 0 Тогда
				Продолжить;
			КонецЕсли;
			
			ДобавляемыеПоляВыборки.Добавить(Реквизит.Имя + " " + "КАК" + " " + Реквизит.Имя);
			ПустыеПоляВыборки.Добавить("NULL");
		КонецЦикла;
	Иначе
		ДобавляемыеПоляВыборки.Добавить("NULL");
		ПустыеПоляВыборки.Добавить("NULL");
	КонецЕсли;
	
	ТекстЗапроса = СтрЗаменить(ТекстЗапроса,
		"&ДополнительныеПоляВыборки",
		СтрСоединить(ДобавляемыеПоляВыборки, ", "));
	
	ТекстЗапроса = СтрЗаменить(ТекстЗапроса,
		"&ПустыеПоляВыборки",
		СтрСоединить(ПустыеПоляВыборки, ", "));
	
	Если ВыбратьИзображения Тогда
		ТекстЗапроса = ТекстЗапроса + ОбщегоНазначения.РазделительПакетаЗапросов()
			+ "ВЫБРАТЬ РАЗЛИЧНЫЕ
			  |	Данные.НомерСтроки КАК НомерСтроки,
			  |	Данные.УчетнаяЗаписьМаркетплейса КАК УчетнаяЗапись,
			  |	Данные.Номенклатура КАК Номенклатура,
			  |	Данные.Характеристика КАК Характеристика,
			  |	Данные.Упаковка КАК Упаковка,
			  |	Данные.НоменклатураПартнера КАК НоменклатураПартнера,
			  |	НоменклатураКонтрагентов.Номенклатура КАК НоменклатураИзНоменклатурыПартнера,
			  |	НоменклатураКонтрагентов.Характеристика КАК ХарактеристикаИзНоменклатурыПартнера,
			  |	НоменклатураКонтрагентов.Упаковка КАК УпаковкаИзНоменклатурыПартнера
			  |ПОМЕСТИТЬ ВТВладельцыФайлов
			  |ИЗ
			  |	ВТДанные КАК Данные
			  |		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.НоменклатураКонтрагентов КАК НоменклатураКонтрагентов
			  |		ПО Данные.НоменклатураПартнера = НоменклатураКонтрагентов.Ссылка
			  |;
			  |
			  |////////////////////////////////////////////////////////////////////////////////
			  |ВЫБРАТЬ
			  |	Данные.НомерСтроки КАК НомерСтроки,
			  |	Данные.УчетнаяЗапись КАК УчетнаяЗапись,
			  |	Данные.Номенклатура КАК Номенклатура,
			  |	Данные.Характеристика КАК Характеристика,
			  |	Данные.Упаковка КАК Упаковка,
			  |	Данные.НоменклатураПартнера КАК НоменклатураПартнера,
			  |	Данные.НоменклатураИзНоменклатурыПартнера КАК НоменклатураЗаписи,
			  |	Данные.ХарактеристикаИзНоменклатурыПартнера КАК ХарактеристикаЗаписи,
			  |	ВЫБОР
			  |		КОГДА Данные.УпаковкаИзНоменклатурыПартнера = СправНоменклатура.ЕдиницаИзмерения
			  |			ТОГДА ЗНАЧЕНИЕ(Справочник.УпаковкиЕдиницыИзмерения.ПустаяСсылка)
			  |		ИНАЧЕ Данные.УпаковкаИзНоменклатурыПартнера
			  |	КОНЕЦ КАК УпаковкаЗаписи,
			  |	ПрисоединенныеФайлы.Ссылка КАК СсылкаФайла,
			  |	ПрисоединенныеФайлы.Описание КАК Описание
			  |ПОМЕСТИТЬ ВТИзображенияНоменклатуры
			  |ИЗ
			  |	ВТВладельцыФайлов КАК Данные
			  |		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.Номенклатура КАК СправНоменклатура
			  |		ПО Данные.НоменклатураИзНоменклатурыПартнера = СправНоменклатура.Ссылка
			  |		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.НоменклатураПрисоединенныеФайлы КАК ПрисоединенныеФайлы
			  |		ПО Данные.НоменклатураИзНоменклатурыПартнера = ПрисоединенныеФайлы.ВладелецФайла
			  |			И (ПрисоединенныеФайлы.ПубликуетсяВСервисах)
			  |			И (НЕ ПрисоединенныеФайлы.ПометкаУдаления)
			  |ГДЕ
			  |	Данные.НоменклатураИзНоменклатурыПартнера <> ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка)
			  |;
			  |
			  |////////////////////////////////////////////////////////////////////////////////
			  |ВЫБРАТЬ
			  |	Данные.НомерСтроки КАК НомерСтроки,
			  |	Данные.СсылкаФайла КАК СсылкаФайла,
			  |	Данные.НоменклатураПартнера КАК НоменклатураПартнера,
			  |	ЕСТЬNULL(СтатусыПубликации.ИдентификаторПубликации, """") КАК ИдентификаторПубликации,
			  |	ВЫБОР
			  |		КОГДА СтатусыПубликации.ПредставлениеОбъектаМаркетплейса ЕСТЬ NULL
			  |				ИЛИ СтатусыПубликации.ПредставлениеОбъектаМаркетплейса = """"
			  |			ТОГДА Данные.Описание
			  |		ИНАЧЕ СтатусыПубликации.ПредставлениеОбъектаМаркетплейса
			  |	КОНЕЦ КАК ПутьКПубличномуФайлу,
			  |	НЕ СтатусыПубликации.ОбъектПубликации ЕСТЬ NULL КАК ЭтоИзображениеТовараOzon,
			  |	Данные.УчетнаяЗапись КАК УчетнаяЗапись,
			  |	Данные.НоменклатураЗаписи КАК Номенклатура,
			  |	Данные.ХарактеристикаЗаписи КАК Характеристика,
			  |	Данные.УпаковкаЗаписи КАК Упаковка,
			  |	СтатусыПубликации.*
			  |ИЗ
			  |	ВТИзображенияНоменклатуры КАК Данные
			  |		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СтатусыПубликацииОбъектовМаркетплейсаOzon КАК СтатусыПубликации
			  |		ПО Данные.УчетнаяЗапись = СтатусыПубликации.УчетнаяЗаписьМаркетплейса
			  |			И (СтатусыПубликации.ВидОбъектаМаркетплейса = ЗНАЧЕНИЕ(Перечисление.ВидыОбъектовМаркетплейсов.Изображение))
			  |			И Данные.Номенклатура = СтатусыПубликации.Номенклатура
			  |			И Данные.Характеристика = СтатусыПубликации.Характеристика
			  |			И Данные.Упаковка = СтатусыПубликации.Упаковка
			  |			И Данные.НоменклатураПартнера = СтатусыПубликации.НоменклатураПартнера
			  |			И Данные.СсылкаФайла = СтатусыПубликации.ОбъектПубликации";
	КонецЕсли;
	
	ТекстЗапроса = СтрЗаменить(ТекстЗапроса,
		"&ПустаяСсылка",
		"ЗНАЧЕНИЕ(Справочник.НоменклатураКонтрагентов.ПустаяСсылка)");
	
	Возврат ТекстЗапроса;

КонецФункции

// Выполняет поиск номенклатуры, характеристик, упаковок, категорий для указанных товарных позиций из сервиса Ozon одним запросом.
//
// Параметры:                                                                   
//   Идентификаторы               - Массив Из Строка - список идентификаторов (внутренних публикации или внешних Ozon) 
//                                    товарных позиций, для которых выполняется поиск.
//                                - Строка - идентификатор (внутренний или внешний) публикации товарной позиции, для 
//                                    которой выполняется поиск.
//   ИмяИдентификатора            - Строка - имя поля идентификатора в анализируемых данных.
//   ОписаниеТоваров              - ТаблицаЗначений - описания товаров из сервиса, см. НоваяТаблицаОписанияТоваров.
//   ОписаниеХарактеристикТоваров - ТаблицаЗначений - значения реквизитов товара из сервиса, 
//                                    см. НоваяТаблицаОписанияХарактеристикТоваров.
//   ПараметрыРасчетаСтавкиНДС    - Структура - параметры определения ставок НДС по номенклатуре:
//     * НалогообложениеНДС         - ПеречислениеСсылка.ТипыНалогообложенияНДС - тип налогообложения НДС;
//     * Организация                - СправочникСсылка.Организации - организация;
//     * Дата                       - Дата - период определения ставок НДС.
//   РазделителиСловПоиска        - Строка - перечень символов-разделителей для поиска по частям наименований.
//   ПоискПоХарактеристикам       - Булево - Истина, если выполнять поиск характеристик номенклатуры по частям наименования.
//
// Возвращаемое значение:
//   Структура - результат выполнения функции:
//     * ОсновныеСправочники               - Неопределено,
//                                         - ВыборкаИзРезультатаЗапроса - результат поиска номенклатуры, характеристик, упаковки:
//       ** УчетнаяЗаписьМаркетплейса        - СправочникСсылка.УчетныеЗаписиМаркетплейсов - учетная запись подключения к сервису.
//       ** ИдентификаторПубликации          - Строка - внутренний идентификатор товара в системе продавца;
//       ** ИдентификаторОбъектаМаркетплейса - Строка - идентификатор товара на маркетплейсе;
//       ** ВариантПоиска                    - Число - вариант поиска: 
//                                               (-1) - не найдено; 
//                                               0 - неоднозначный поиск; 
//                                               1, 2 - по штрихкоду; 
//                                               3, 4 - по артикулу; 
//                                               5 - по идентификаторам из сервиса Ozon;
//                                               6 - по наименованию; 
//                                               10 и более - по словам наименований; 
//                                               для поиска по номенклатуре контрагентов добавляется 0.1.
//       ** ОднозначныйПоиск                 - Булево - признак однозначных требований к результатам поиска;
//       ** Номенклатура                     - СправочникСсылка.Номенклатура - найденная номенклатура;
//       ** ХарактеристикиИспользуются       - Булево - признак использования характеристик номенклатуры;
//       ** Характеристика                   - СправочникСсылка.ХарактеристикиНоменклатуры - найденная характеристика;
//       ** ИспользоватьУпаковки             - Булево - признак использования упаковок номенклатуры;
//       ** Упаковка                         - СправочникСсылка.УпаковкиЕдиницыИзмерения - найденная упаковка;
//       ** ЕдиницаИзмерения                 - СправочникСсылка.УпаковкиЕдиницыИзмерения - единица хранения номенклатуры;
//                                           - Неопределено - категория не найдена;
//       ** СтавкаНДС                        - СправочникСсылка.СтавкиНДС - ставка НДС номенклатуры;
//       ** Ставка                           - Число - ставка НДС номенклатуры.
//       ** Категория                        - СправочникСсылка.ВидыНоменклатуры, СправочникСсылка.Номенклатура, СправочникСсылка.ТоварныеКатегории -
//                                               начальная категория 1С для определения.
//     * Категории                         - Неопределено,
//                                         - Массив Из СправочникСсылка.ВидыНоменклатуры,
//                                         - Массив Из СправочникСсылка.Номенклатура,
//                                         - Массив Из СправочникСсылка.ТоварныеКатегории - категории, для которых нужно 
//                                             получить категорию-источник.
//
Функция НайтиНоменклатурыХарактеристикиУпаковки(Знач Идентификаторы, Знач ИмяИдентификатора, 
			ОписаниеТоваров, ОписаниеХарактеристикТоваров, ПараметрыРасчетаСтавкиНДС,
			РазделителиСловПоиска = Неопределено, ПоискПоХарактеристикам = Ложь) Экспорт
			
	Результат = Новый Структура;
	Результат.Вставить("ОсновныеСправочники", Неопределено);
	Результат.Вставить("Категории",           Неопределено);
		
	Если ТипЗнч(Идентификаторы) = Тип("Строка") Тогда
		Идентификаторы = СтрРазделить(Идентификаторы, Символы.ПС, Ложь);
	КонецЕсли;
	Если Идентификаторы.Количество() = 0 Тогда
		Возврат Результат;
	КонецЕсли;
	
	Замер = ОценкаПроизводительности.НачатьЗамерДлительнойОперации(
		"ОбщийМодуль.ИнтеграцияСМаркетплейсомOzonСервер.НайтиНоменклатурыХарактеристикиУпаковки");
	
	ИспользоватьХарактеристикиНоменклатуры = ПолучитьФункциональнуюОпцию("ИспользоватьХарактеристикиНоменклатуры");
	Если Не ИспользоватьХарактеристикиНоменклатуры Тогда
		ПоискПоХарактеристикам = Ложь;
	КонецЕсли;
	
	ТаблицаПоиска = НоваяТаблицаПоискаНоменклатурыХарактеристикиУпаковки();
	
	Для Каждого СтрокаТаблицыЗначений Из ОписаниеТоваров Цикл
		Если Не ЗначениеЗаполнено(СтрокаТаблицыЗначений[ИмяИдентификатора]) Тогда
			Продолжить;
		КонецЕсли;
		Если Идентификаторы.Найти(СтрокаТаблицыЗначений[ИмяИдентификатора]) = Неопределено Тогда
			Продолжить;
		КонецЕсли;
		
		// Поиск по штрихкоду
		Для Каждого ЭлементКоллекции Из СтрРазделить(СтрокаТаблицыЗначений.Штрихкод, ";", Ложь) Цикл
			НоваяСтрока                                  = ТаблицаПоиска.Добавить();
			НоваяСтрока.УчетнаяЗаписьМаркетплейса        = СтрокаТаблицыЗначений.УчетнаяЗаписьМаркетплейса;
			НоваяСтрока.ИдентификаторПубликации          = СтрокаТаблицыЗначений.ИдентификаторПубликации;
			НоваяСтрока.ИдентификаторОбъектаМаркетплейса = СтрокаТаблицыЗначений.ИдентификаторОбъектаМаркетплейса;
			НоваяСтрока.ВариантПоиска                    = 1;
			НоваяСтрока.ЗначениеПоиска                   = СокрЛП(ЭлементКоллекции);
			НоваяСтрока.ПоискХарактеристики              = Ложь;
		КонецЦикла;
		
		// Поиск по артикулу как штрихкоду
		НоваяСтрока                                  = ТаблицаПоиска.Добавить();
		НоваяСтрока.УчетнаяЗаписьМаркетплейса        = СтрокаТаблицыЗначений.УчетнаяЗаписьМаркетплейса;
		НоваяСтрока.ИдентификаторПубликации          = СтрокаТаблицыЗначений.ИдентификаторПубликации;
		НоваяСтрока.ИдентификаторОбъектаМаркетплейса = СтрокаТаблицыЗначений.ИдентификаторОбъектаМаркетплейса;
		НоваяСтрока.ВариантПоиска                    = 2;
		НоваяСтрока.ЗначениеПоиска                   = СтрокаТаблицыЗначений.ИдентификаторПубликации;
		НоваяСтрока.ПоискХарактеристики              = Ложь;
		
		// Поиск по артикулу
		НоваяСтрока                                  = ТаблицаПоиска.Добавить();
		НоваяСтрока.УчетнаяЗаписьМаркетплейса        = СтрокаТаблицыЗначений.УчетнаяЗаписьМаркетплейса;
		НоваяСтрока.ИдентификаторПубликации          = СтрокаТаблицыЗначений.ИдентификаторПубликации;
		НоваяСтрока.ИдентификаторОбъектаМаркетплейса = СтрокаТаблицыЗначений.ИдентификаторОбъектаМаркетплейса;
		НоваяСтрока.ВариантПоиска                    = 4;
		НоваяСтрока.ЗначениеПоиска                   = СтрокаТаблицыЗначений.ИдентификаторПубликации;
		НоваяСтрока.ПоискХарактеристики              = Ложь;
		
		// Поиск по идентификатору контрагента
		Если ЗначениеЗаполнено(СтрокаТаблицыЗначений.ИдентификаторОбъектаМаркетплейса) Тогда
			НоваяСтрока                                  = ТаблицаПоиска.Добавить();
			НоваяСтрока.УчетнаяЗаписьМаркетплейса        = СтрокаТаблицыЗначений.УчетнаяЗаписьМаркетплейса;
			НоваяСтрока.ИдентификаторПубликации          = СтрокаТаблицыЗначений.ИдентификаторПубликации;
			НоваяСтрока.ИдентификаторОбъектаМаркетплейса = СтрокаТаблицыЗначений.ИдентификаторОбъектаМаркетплейса;
			НоваяСтрока.ВариантПоиска                    = 5;
			НоваяСтрока.ЗначениеПоиска                   = СтрокаТаблицыЗначений.ИдентификаторОбъектаМаркетплейса;
			НоваяСтрока.ПоискХарактеристики              = Ложь;
		КонецЕсли;
		
		Если ЗначениеЗаполнено(СтрокаТаблицыЗначений.ИдентификаторFBOSKU) Тогда
			НоваяСтрока                                  = ТаблицаПоиска.Добавить();
			НоваяСтрока.УчетнаяЗаписьМаркетплейса        = СтрокаТаблицыЗначений.УчетнаяЗаписьМаркетплейса;
			НоваяСтрока.ИдентификаторПубликации          = СтрокаТаблицыЗначений.ИдентификаторПубликации;
			НоваяСтрока.ИдентификаторОбъектаМаркетплейса = СтрокаТаблицыЗначений.ИдентификаторОбъектаМаркетплейса;
			НоваяСтрока.ВариантПоиска                    = 5;
			НоваяСтрока.ЗначениеПоиска                   = СтрокаТаблицыЗначений.ИдентификаторFBOSKU;
			НоваяСтрока.ПоискХарактеристики              = Ложь;
		КонецЕсли;
		
		Если ЗначениеЗаполнено(СтрокаТаблицыЗначений.ИдентификаторFBSSKU) Тогда
			НоваяСтрока                                  = ТаблицаПоиска.Добавить();
			НоваяСтрока.УчетнаяЗаписьМаркетплейса        = СтрокаТаблицыЗначений.УчетнаяЗаписьМаркетплейса;
			НоваяСтрока.ИдентификаторПубликации          = СтрокаТаблицыЗначений.ИдентификаторПубликации;
			НоваяСтрока.ИдентификаторОбъектаМаркетплейса = СтрокаТаблицыЗначений.ИдентификаторОбъектаМаркетплейса;
			НоваяСтрока.ВариантПоиска                    = 5;
			НоваяСтрока.ЗначениеПоиска                   = СтрокаТаблицыЗначений.ИдентификаторFBSSKU;
			НоваяСтрока.ПоискХарактеристики              = Ложь;
		КонецЕсли;
		
		// Поиск по наименованию
		Если ЗначениеЗаполнено(СтрокаТаблицыЗначений.ПредставлениеОбъектаМаркетплейса) Тогда
			НоваяСтрока                                  = ТаблицаПоиска.Добавить();
			НоваяСтрока.УчетнаяЗаписьМаркетплейса        = СтрокаТаблицыЗначений.УчетнаяЗаписьМаркетплейса;
			НоваяСтрока.ИдентификаторПубликации          = СтрокаТаблицыЗначений.ИдентификаторПубликации;
			НоваяСтрока.ИдентификаторОбъектаМаркетплейса = СтрокаТаблицыЗначений.ИдентификаторОбъектаМаркетплейса;
			НоваяСтрока.ВариантПоиска                    = 6;
			НоваяСтрока.ЗначениеПоиска                   = СтрокаТаблицыЗначений.ПредставлениеОбъектаМаркетплейса;
			НоваяСтрока.ПоискХарактеристики              = Ложь;
			
			Наименование         = СтрокаТаблицыЗначений.ПредставлениеОбъектаМаркетплейса;
			НаименованияПоСловам = Новый Массив;
			НомераВхождений      = Новый Соответствие;
			КоллекцияЭлементов   = СтроковыеФункцииКлиентСервер.РазложитьСтрокуВМассивСлов(Наименование, РазделителиСловПоиска);
			
			Для Каждого ЭлементКоллекции Из КоллекцияЭлементов Цикл
				НомерВхождения = НомераВхождений.Получить(ЭлементКоллекции);
				Если НомерВхождения = Неопределено Тогда
					НомерВхождения = 0;
				КонецЕсли;
				НомерВхождения = НомерВхождения + 1;
				
				НаименованияПоСловам.Вставить(0, Сред(Наименование, 1, СтрНайти(Наименование, ЭлементКоллекции,,, НомерВхождения) - 1) + ЭлементКоллекции);
				НомераВхождений.Вставить(ЭлементКоллекции, НомерВхождения);
			КонецЦикла;
			
			Индекс = 0;
			Пока Индекс < НаименованияПоСловам.Количество() Цикл
				НоваяСтрока                                  = ТаблицаПоиска.Добавить();
				НоваяСтрока.УчетнаяЗаписьМаркетплейса        = СтрокаТаблицыЗначений.УчетнаяЗаписьМаркетплейса;
				НоваяСтрока.ИдентификаторПубликации          = СтрокаТаблицыЗначений.ИдентификаторПубликации;
				НоваяСтрока.ИдентификаторОбъектаМаркетплейса = СтрокаТаблицыЗначений.ИдентификаторОбъектаМаркетплейса;
				НоваяСтрока.ВариантПоиска                    = 10 + Индекс;
				НоваяСтрока.ЗначениеПоиска                   = НаименованияПоСловам[Индекс];
				НоваяСтрока.ПоискХарактеристики              = Ложь;
				
				Если ЗначениеЗаполнено(НаименованияПоСловам[Индекс]) 
					 И ПоискПоХарактеристикам Тогда
					Для Каждого ЭлементКоллекции Из НаименованияПоСловам Цикл
						Если СтрНайти(ЭлементКоллекции, НаименованияПоСловам[Индекс]) = 1 Тогда
							Наименование = СтрЗаменить(ЭлементКоллекции, НаименованияПоСловам[Индекс], "");
							
							Если ЗначениеЗаполнено(Наименование) Тогда
								НоваяСтрока                                  = ТаблицаПоиска.Добавить();
								НоваяСтрока.УчетнаяЗаписьМаркетплейса        = СтрокаТаблицыЗначений.УчетнаяЗаписьМаркетплейса;
								НоваяСтрока.ИдентификаторПубликации          = СтрокаТаблицыЗначений.ИдентификаторПубликации;
								НоваяСтрока.ИдентификаторОбъектаМаркетплейса = СтрокаТаблицыЗначений.ИдентификаторОбъектаМаркетплейса;
								НоваяСтрока.ВариантПоиска                    = 10 + Индекс;
								НоваяСтрока.ЗначениеПоиска                   = Наименование;
								НоваяСтрока.ПоискХарактеристики              = Истина;
								
								Если СтроковыеФункцииКлиентСервер.ЭтоРазделительСлов(КодСимвола(Наименование, 1), РазделителиСловПоиска) Тогда
									НоваяСтрока                                  = ТаблицаПоиска.Добавить();
									НоваяСтрока.УчетнаяЗаписьМаркетплейса        = СтрокаТаблицыЗначений.УчетнаяЗаписьМаркетплейса;
									НоваяСтрока.ИдентификаторПубликации          = СтрокаТаблицыЗначений.ИдентификаторПубликации;
									НоваяСтрока.ИдентификаторОбъектаМаркетплейса = СтрокаТаблицыЗначений.ИдентификаторОбъектаМаркетплейса;
									НоваяСтрока.ВариантПоиска                    = 10 + Индекс;
									НоваяСтрока.ЗначениеПоиска                   = Сред(Наименование, 2);
									НоваяСтрока.ПоискХарактеристики              = Истина;
									
									НоваяСтрока                                  = ТаблицаПоиска.Добавить();
									НоваяСтрока.УчетнаяЗаписьМаркетплейса        = СтрокаТаблицыЗначений.УчетнаяЗаписьМаркетплейса;
									НоваяСтрока.ИдентификаторПубликации          = СтрокаТаблицыЗначений.ИдентификаторПубликации;
									НоваяСтрока.ИдентификаторОбъектаМаркетплейса = СтрокаТаблицыЗначений.ИдентификаторОбъектаМаркетплейса;
									НоваяСтрока.ВариантПоиска                    = 10 + Индекс;
									НоваяСтрока.ЗначениеПоиска                   = СокрЛП(Сред(Наименование, 2));
									НоваяСтрока.ПоискХарактеристики              = Истина;
								КонецЕсли;
							КонецЕсли;
						КонецЕсли;
					КонецЦикла;
				КонецЕсли;
				
				Индекс = Индекс + 1;
			КонецЦикла;
		КонецЕсли;
	КонецЦикла;
	
	ИдентификаторАтрибута_Артикул = ИнтеграцияСМаркетплейсомOzonКлиентСервер.КонстантыСервиса()["ИдентификаторАтрибута_Артикул"];
	ИдентификаторАтрибута_Наименование = ИнтеграцияСМаркетплейсомOzonКлиентСервер.КонстантыСервиса()["ИдентификаторАтрибута_Наименование"];

	Для Каждого СтрокаТаблицыЗначений Из ОписаниеХарактеристикТоваров Цикл
		Если Не ЗначениеЗаполнено(СтрокаТаблицыЗначений[ИмяИдентификатора]) Тогда
			Продолжить;
		КонецЕсли;
		Если Идентификаторы.Найти(СтрокаТаблицыЗначений[ИмяИдентификатора]) = Неопределено Тогда
			Продолжить;
		КонецЕсли;
		
		// Поиск по артикулу
		НайденныеСтроки = СтрокаТаблицыЗначений.ЗначенияАтрибутов.НайтиСтроки(Новый Структура("ИдентификаторАтрибутаМаркетплейса", ИдентификаторАтрибута_Артикул));
		Если НайденныеСтроки.Количество() > 0 
			 И ЗначениеЗаполнено(НайденныеСтроки[0].ЗначениеАтрибутаМаркетплейса) Тогда
