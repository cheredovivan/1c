	МассивПолейТипаДата.Добавить("delivery_date_begin");
	МассивПолейТипаДата.Добавить("delivery_date_end");
	МассивПолейТипаДата.Добавить("delivering_date");
	МассивПолейТипаДата.Добавить("in_process_at");
