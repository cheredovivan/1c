		ИначеЕсли ВРег(Значение) = ВРег("has_postings_with_registration_error") Тогда
			Результат = НСтр("ru = 'Часть отправлений не может попасть в акт из-за ошибки регистрации в сервисе доставки.';
							|en = 'Cannot include some shipments into the certificate due to the registration error in the delivery service.'");
		ИначеЕсли ВРег(Значение) = ВРег("has_seller_returns_in_stock") Тогда
