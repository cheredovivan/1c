			ЧастиИдентификатора = СтрРазделить(ДанныеАтрибута.ИдентификаторРодителяАтрибутаМаркетплейса, "_", Истина);

			ИдентификаторРодителяАтрибута     = ЧастиИдентификатора[0];
			ИдентификаторКомплексногоАтрибута = "";

			Если ЧастиИдентификатора.Количество() > 1 Тогда
				ИдентификаторКомплексногоАтрибута = ЧастиИдентификатора[1];
			КонецЕсли;

			ЧастиНаименования = СтрРазделить(СтрЗаменить(ДанныеАтрибута.НаименованиеРодителяАтрибутаМаркетплейса, ">>", Символы.ПС), Символы.ПС, Истина);

			НаименованиеРодителяАтрибута     = ЧастиНаименования[0];
			НаименованиеКомплексногоАтрибута = "";

