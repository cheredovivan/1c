// Конструктор списка атрибутов доступных перевозок.
//
// Параметры:
//   ЭтоЧтениеИзСервиса - Булево - признак получения списка атрибутов для чтения из сервиса.
//   ЭтоНовыйМетод      - Булево - Признак использования нового метода получения сведений об отгрузке.
//
// Возвращаемое значение:
//   Структура - список атрибутов:
//     * ИдентификаторСклада            - Строка - идентификатор склада.
//     * ИдентификаторМетодаДоставки    - Строка - идентификатор метода доставки.
//     * ИдентификаторОтгрузки          - Строка - идентификатор отгрузки.
//     * НомерОтгрузки                  - Строка - номер отгрузки.
//     * Статус                         - Строка - статус отгрузки.
//     * СрокОтгрузки                   - Строка, Дата - дата и время отгрузки.
//     * ДоверительнаяПриемка           - Строка, Булево - признак доверительной приемки.
//     * ВремяОтгрузкиРекомендуемое     - Строка, Дата - дата и время, рекомендуемые для отгрузки.
//     * СмещениеЧасовогоПояса          - Строка, Число - смещение часового пояса.
//     * КоличествоОтправленийВСборке   - Строка, Число - количество отправлений, которые нужно собрать.
//     * КоличествоСобранныхОтправлений - Строка, Число - количество отправлений, которые уже собраны.
//     * КоличествоОтправленийВОтгрузке - Строка, Число - количество отправлений в отгрузке.
//     * КоличествоОтправленийДоступно  - Строка, Число - количество отправлений в плане отгрузки.
//     * СписокОтгрузок                 - Строка, Массив из Соответствие - список отгрузок.
//     * СпособОтгрузки                 - Строка - способ отгрузки.
//     * АдресПунктаПрием               - Строка - адрес пункта приема.
//     * ВремяПриемаНачало              - Строка - время приема начало.
//     * ВремяПриемаОкончание           - Строка - время приема окончание.
//     * Ошибки                         - Строка - список ошибок.
//
Функция НовыйСписокАтрибутовДоступныхОтгрузок(ЭтоЧтениеИзСервиса = Истина, ЭтоНовыйМетод = Ложь)

	АтрибутыОтвета = Новый Структура;
	
	Если ЭтоНовыйМетод Тогда
		АтрибутыОтвета.Вставить("ИдентификаторОтгрузки",
								?(ЭтоЧтениеИзСервиса, "id", ""));
		АтрибутыОтвета.Вставить("Статус",
								?(ЭтоЧтениеИзСервиса, "status", ""));
		АтрибутыОтвета.Вставить("КоличествоОтправленийВОтгрузке",
								?(ЭтоЧтениеИзСервиса, "postings_count", 0));
	Иначе
		АтрибутыОтвета.Вставить("ИдентификаторОтгрузки",
								?(ЭтоЧтениеИзСервиса, "carriage_id", ""));
		АтрибутыОтвета.Вставить("Статус",
								?(ЭтоЧтениеИзСервиса, "carriage_status", ""));
		АтрибутыОтвета.Вставить("КоличествоОтправленийВОтгрузке",
								?(ЭтоЧтениеИзСервиса, "carriage_postings_count", 0));
	КонецЕсли;
	
	АтрибутыОтвета.Вставить("ИдентификаторСклада",
							?(ЭтоЧтениеИзСервиса, "warehouse_id", ""));
	АтрибутыОтвета.Вставить("ИдентификаторМетодаДоставки",
							?(ЭтоЧтениеИзСервиса, "delivery_method_id", ""));
	АтрибутыОтвета.Вставить("СрокОтгрузки",
							?(ЭтоЧтениеИзСервиса, "cutoff_at", Дата(1,1,1)));
	АтрибутыОтвета.Вставить("ДоверительнаяПриемка",
							?(ЭтоЧтениеИзСервиса, "has_entrusted_acceptance", Ложь)); // Строка, Булево.
	АтрибутыОтвета.Вставить("ВремяОтгрузкиРекомендуемое",
							?(ЭтоЧтениеИзСервиса, "recommended_time_local", Дата(1,1,1)));
	АтрибутыОтвета.Вставить("СмещениеЧасовогоПояса",
							?(ЭтоЧтениеИзСервиса, "recommended_time_utc_offset_in_minutes", 0));
	АтрибутыОтвета.Вставить("КоличествоОтправленийВСборке",
							?(ЭтоЧтениеИзСервиса, "mandatory_postings_count", 0));
	АтрибутыОтвета.Вставить("КоличествоСобранныхОтправлений",
							?(ЭтоЧтениеИзСервиса, "mandatory_packaged_count", 0));
	АтрибутыОтвета.Вставить("КоличествоОтправленийДоступно",
							?(ЭтоЧтениеИзСервиса, "postings_for_another_carriage_count", 0));
	АтрибутыОтвета.Вставить("СписокОтгрузок",
							?(ЭтоЧтениеИзСервиса, "carriages", Новый Массив));
	АтрибутыОтвета.Вставить("СпособОтгрузки",
							?(ЭтоЧтениеИзСервиса, "dropoff_point_type", ""));
	АтрибутыОтвета.Вставить("АдресПунктаПриема",
							?(ЭтоЧтениеИзСервиса, "dropoff_address", ""));
	АтрибутыОтвета.Вставить("ВремяПриемаНачало",
							?(ЭтоЧтениеИзСервиса, "", "")); // Пока возвращает неверное время - timeslot_from.
	АтрибутыОтвета.Вставить("ВремяПриемаОкончание",
							?(ЭтоЧтениеИзСервиса, "timeslot_to", ""));
	АтрибутыОтвета.Вставить("ОписаниеОшибки",
							?(ЭтоЧтениеИзСервиса, "errors", ""));
	
	Возврат АтрибутыОтвета;

КонецФункции

// Конструктор списка атрибутов проверки статуса отгрузки.
//
// Параметры:
//   ЭтоЧтениеИзСервиса - Булево - признак получения списка атрибутов для чтения из сервиса.
//
// Возвращаемое значение:
//   Структура - список атрибутов:
//     * ТипДокументов            - Строка - тип документов.
//     * ОтправленияВОтгрузке     - Строка, Массив из Строка - Список отправлений в отгрузке.
//     * ЕстьОтправленияКОтгрузке - Строка, Булево - признак наличия доступных, но не включенных в отгрузку отправлений.
//     * ЧастичнаяПеревозка       - Строка, Булево - признак частичной перевозки.
//     * НомерЧастичнойПеревозки  - Строка, Число - номер частичной перевозки.
//     * ОтгрузкаСформирована     -Булево - признак сформированной отгрузки.
//
Функция НовыйСписокАтрибутовПроверкиСтатусаОтгрузки(ЭтоЧтениеИзСервиса = Истина)

	АтрибутыОтвета = Новый Структура;
	
	АтрибутыОтвета.Вставить("Статус",
							?(ЭтоЧтениеИзСервиса, "result.status", ""));
	АтрибутыОтвета.Вставить("ТипДокументов",
							?(ЭтоЧтениеИзСервиса, "result.act_type", ""));
	АтрибутыОтвета.Вставить("ОтправленияВОтгрузке",
							?(ЭтоЧтениеИзСервиса, "result.added_to_act", Новый Массив));
	АтрибутыОтвета.Вставить("ЕстьОтправленияКОтгрузке",
							?(ЭтоЧтениеИзСервиса, "result.has_postings_for_next_carriage", Ложь));
	АтрибутыОтвета.Вставить("ОтправленияНеВОтгрузке",
							?(ЭтоЧтениеИзСервиса, "result.removed_from_act", Новый Массив));
	АтрибутыОтвета.Вставить("ЧастичнаяПеревозка",
							?(ЭтоЧтениеИзСервиса, "result.is_partial", Ложь));
	АтрибутыОтвета.Вставить("НомерЧастичнойПеревозки",
							?(ЭтоЧтениеИзСервиса, "result.partial_num", 0));
	
	Если Не ЭтоЧтениеИзСервиса Тогда
		АтрибутыОтвета.Вставить("ОтгрузкаСформирована", Ложь);
	КонецЕсли;
	
	Возврат АтрибутыОтвета;

КонецФункции

// Конструктор списка атрибутов информации об отгрузке.
//
// Возвращаемое значение:
//   Структура - список атрибутов для чтения из сервиса:
//     * ИдентификаторОтгрузки           - Строка - поле идентификатора отгрузки.
//     * Статус                          - Строка - поле статуса отгрузки.
//     * ИдентификаторСклада             - Строка - идентификатор склада.
//     * ИдентификаторМетодаДоставки     - Строка - идентификатор метода доставки.
//     * ТипОтгрузки                     - Строка - поле типа отгрузки.
//     * ДатаВыполнения                  - Строка - поле даты и времени выполнения отгрузки.
//     * ДатаСоздания                    - Строка - поле даты и времени создания отгрузки.
//     * ДатаОбновления                  - Строка - поле даты и времени обновления отгрузки.
//     * КоличествоОбновлений            - Строка - поле количества обновлений отгрузки.
//     * ЧастичнаяПеревозка              - Строка - поле признака частичной перевозки.
//     * НомерЧастичнойПеревозки         - Строка - поле номера частичной перевозки.
//     * ТипДокументов                   - Строка - поле типа документов.
//     * ДоступныеДействия               - Строка - поле списка доступных действий с перевозкой.
//     * МожноОтменить                   - Строка - поле признака доступности отмены отгрузки.
//     * ПричинаНедоступностиОтмены      - Строка - поле описания причины недоступности отмены отгрузки.
//     * КоличествоГрузовыхМест          - Строка - поле количества грузовых мест.
//     * РаспечатаныЭтикеткиГрузовыхМест - Строка - поле признака того, что этикетки грузовых мест уже печатались.
//     * ЕстьОтправленияКОтгрузке        - Строка - поле признака наличия доступных, но не включенных в отгрузку отправлений.
//     * Пропуска                        - Строка - поле списка пропусков для отгрузки.
//
Функция НовыйСписокАтрибутовИнформацииОбОтгрузкеВСервисе()

	АтрибутыОтвета = Новый Структура;
	АтрибутыОтвета.Вставить("ИдентификаторОтгрузки",           "carriage_id");
	АтрибутыОтвета.Вставить("Статус",                          "status");
	АтрибутыОтвета.Вставить("ИдентификаторСклада",             "warehouse_id");
	АтрибутыОтвета.Вставить("ИдентификаторМетодаДоставки",     "delivery_method_id");
	АтрибутыОтвета.Вставить("ТипОтгрузки",                     "integration_type");
	АтрибутыОтвета.Вставить("ДатаВыполнения",                  "departure_date");
	АтрибутыОтвета.Вставить("ДатаСоздания",                    "created_at");
	АтрибутыОтвета.Вставить("ДатаОбновления",                  "updated_at");
	АтрибутыОтвета.Вставить("КоличествоОбновлений",            "retry_count");
	АтрибутыОтвета.Вставить("ЧастичнаяПеревозка",              "is_partial");
	АтрибутыОтвета.Вставить("НомерЧастичнойПеревозки",         "partial_num");
	АтрибутыОтвета.Вставить("ТипДокументов",                   "act_type");
	АтрибутыОтвета.Вставить("ДоступныеДействия",               "available_actions");
	АтрибутыОтвета.Вставить("МожноОтменить",                   "cancel_availability.is_cancel_available");
	АтрибутыОтвета.Вставить("ПричинаНедоступностиОтмены",      "cancel_availability.reason");
	АтрибутыОтвета.Вставить("КоличествоГрузовыхМест",          "containers_count");
	АтрибутыОтвета.Вставить("РаспечатаныЭтикеткиГрузовыхМест", "is_container_label_printed");
	АтрибутыОтвета.Вставить("ЕстьОтправленияКОтгрузке",        "has_postings_for_next_carriage");
	АтрибутыОтвета.Вставить("Пропуска",                        "arrival_pass_ids");
	
	Возврат АтрибутыОтвета;

КонецФункции

// Конструктор списка атрибутов отправлений в отгрузке.
//
// Параметры:
//   ЭтоЧтениеИзСервиса - Булево - признак получения списка атрибутов для чтения из сервиса.
//
// Возвращаемое значение:
//   Структура - список атрибутов:
//     * НомерОтправления     - Строка - номер отправления.
//     * Статус               - Строка - статус отгрузки.
//     * ДатаОбновления       - Строка, Дата - дата и время последнего обновления отгрузки.
//     * ОтправлениеВОтгрузке - Булево - признак включения отправления в отгрузку.
//
Функция НовыйСписокАтрибутовОтправленийВОтгрузке(ЭтоЧтениеИзСервиса = Истина)

	АтрибутыОтвета = Новый Структура;
	
	АтрибутыОтвета.Вставить("НомерОтправления",
							?(ЭтоЧтениеИзСервиса, "posting_number", ""));
	АтрибутыОтвета.Вставить("Статус",
							?(ЭтоЧтениеИзСервиса, "status", ""));
	АтрибутыОтвета.Вставить("ДатаОбновления",
							?(ЭтоЧтениеИзСервиса, "updated_at", Дата(1,1,1)));
	
	Если Не ЭтоЧтениеИзСервиса Тогда
		АтрибутыОтвета.Вставить("ОтправлениеВОтгрузке", Ложь);
	КонецЕсли;
	
	Возврат АтрибутыОтвета;

КонецФункции

