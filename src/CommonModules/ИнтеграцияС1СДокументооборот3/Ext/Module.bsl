// Обновляет представление быстрого поиска. Возвращает представление фильтров.
//
// Параметры:
//   БыстрыйПоиск - ДанныеФормыКоллекция - таблица "Быстрый поиск" на форме списка задач.
//   КнопкаСброситьОтбор - КнопкаФормы - кнопка "Сбросить отбор".
//
// Возвращаемое значение:
//   Строка - представление фильтров
//
Функция ОбновитьПредставлениеБыстрогоПоиска(БыстрыйПоиск, КнопкаСброситьОтбор) Экспорт
	
	ЕстьБыстрыйПоиск = Ложь;
	ПредставлениеФильтров = Новый ФорматированнаяСтрока("");
	
	Для Каждого Строка Из БыстрыйПоиск Цикл
		
		Строка.ПредставлениеЗначения = ПредставлениеЗначения(Строка.Значение);
		
		Если ЗначениеЗаполнено(Строка.Значение) И Не Строка.Фиксированный Тогда
			
			ЕстьБыстрыйПоиск = Истина;
			
			Если Строка(ПредставлениеФильтров) <> "" Тогда
				ПредставлениеФильтров = Новый ФорматированнаяСтрока(ПредставлениеФильтров, ", ");
			КонецЕсли;
			
			ПредставлениеФильтров = Новый ФорматированнаяСтрока(
				ПредставлениеФильтров,
				Новый ФорматированнаяСтрока(Строка.ПредставлениеПараметра,, ЦветаСтиля.ТекстИнформационнойНадписи),
				Новый ФорматированнаяСтрока(" " + Строка.ПредставлениеЗначения));
			
		КонецЕсли;
		
	КонецЦикла;
	
	КнопкаСброситьОтбор.Видимость = ЕстьБыстрыйПоиск;
	
	Возврат ПредставлениеФильтров;
	
КонецФункции

// Возвращает описание веб-сервиса 1С:Документооборота.
//
// Возвращаемое значение:
//   Строка
//
Функция ОписаниеВебСервисов() Экспорт
	
	Макет = Справочники.ПравилаИнтеграцииС1СДокументооборотом3.ПолучитьМакет("ОписаниеВебСервисов");
	
	Возврат Макет.ПолучитьТекст();
	
КонецФункции

// Возвращает основного сотрудника текущего пользователя 1С:Документооборота.
//
// Возвращаемое значение:
//   ОбъектXDTO - объект XDTO типа DMEmployee.
//   Неопределено
//
Функция ОсновнойСотрудникТекущегоПользователя() Экспорт
	
	Прокси = ИнтеграцияС1СДокументооборотБазоваяФункциональностьПовтИсп.ПолучитьПрокси();
	
	Если ПараметрыСеанса.ИнтеграцияС1СДокументооборотТекущийПользователь = "" Тогда
		
		Запрос = ИнтеграцияС1СДокументооборотБазоваяФункциональность.ТекущийПользовательДокументооборотаЗапрос(Прокси);
		Ответ = ИнтеграцияС1СДокументооборотБазоваяФункциональность.ВыполнитьЗапрос(Прокси, Запрос);
		
	Иначе
		
		Ответ = ИнтеграцияС1СДокументооборотБазоваяФункциональность.СтрокаВОбъектXDTO(
			Прокси,
			ПараметрыСеанса.ИнтеграцияС1СДокументооборотТекущийПользователь);
		
	КонецЕсли;
	
	Если ИнтеграцияС1СДокументооборотБазоваяФункциональность.ПроверитьТип(Прокси, Ответ, "DMError") Тогда
		Возврат Неопределено;
	Иначе
		СотрудникиПользователя = Ответ.employees; // СписокXDTO
		Для Каждого Сотрудник Из СотрудникиПользователя Цикл
			Если Сотрудник.mainEmployee = Истина Тогда
				Возврат Сотрудник;
			КонецЕсли;
		КонецЦикла;
	КонецЕсли;
	
КонецФункции

// Возвращает подходящие объекты для создания на основании задачи.
//
// Параметры:
//   ЗадачаXDTO - ОбъектXDTO - объект XDTO типа DMTaskAction.
//   ДеревоПриложений - КоллекцияСтрокДереваЗначений - дерево приложений задачи.
//   ОсновнойПредметID - Строка - неявно возвращаемое значение, идентификатор основного объекта.
//   ОсновнойПредметТип - Строка - неявно возвращаемое значение, тип основного объекта.
//
// Возвращаемое значение:
//   Массив из см. ИнтеграцияС1СДокументооборот3КлиентСервер.ОбъектДляСозданияНаОснованииЗадачи
//
Функция ПодходящиеОбъектыДляСозданияНаОснованииЗадачи(ЗадачаXDTO, ДеревоПриложений,
		ОсновнойПредметID = "", ОсновнойПредметТип = "") Экспорт
	
	ОсновнойПредмет = ОсновнойПредметЗадачи(ЗадачаXDTO);
	ОсновнойПредметID = ОсновнойПредмет.ID;
	ОсновнойПредметТип = ОсновнойПредмет.Тип;
	
	ПодходящиеОбъекты = Новый Массив;
	
	ИмяПодсистемыИнтерфейсДокументовЭДО = "ЭлектронноеВзаимодействие.ОбменСКонтрагентами.ИнтерфейсДокументовЭДО";
	ЕстьИнтерфейсДокументовЭДО = ОбщегоНазначения.ПодсистемаСуществует(ИмяПодсистемыИнтерфейсДокументовЭДО);
	Если ЕстьИнтерфейсДокументовЭДО И ДоступенФункционалПолученияОсновногоФайлаДокументаЭДО() Тогда
		Для Каждого СтрокаПриложение Из ДеревоПриложений Цикл
			Для Каждого СтрокаФайл Из СтрокаПриложение.Строки Цикл
				Если СтрокаФайл.ПриложениеТип = "DMIncomingElectronicDocument"
						Или СтрокаФайл.ПриложениеТип = "DMOutgoingElectronicDocument" Тогда
					ПодходящийОбъект = ИнтеграцияС1СДокументооборот3КлиентСервер.ОбъектДляСозданияНаОснованииЗадачи();
					ПодходящийОбъект.ДокументЭДО.Наименование = СтрокаФайл.Приложение;
					ПодходящийОбъект.ДокументЭДО.ID = СтрокаФайл.ПриложениеID;
					ПодходящийОбъект.ДокументЭДО.Тип = СтрокаФайл.ПриложениеТип;
					ПодходящийОбъект.ПредставлениеОбъектаИС = СтрШаблон(
						Нстр("ru = 'Отразить электронный документ ""%1"" в учете';
							|en = 'Record the ""%1"" electronic document in accounting'"),
						СтрокаФайл.Приложение);
					ПодходящиеОбъекты.Добавить(ПодходящийОбъект);
				КонецЕсли;
			КонецЦикла;
		КонецЦикла;
	КонецЕсли;
	
	Если ОсновнойПредмет.ТипОбъектаДО = "" Тогда
		Возврат ПодходящиеОбъекты;
	КонецЕсли;
	
	ВыборкаПравил = ИнтеграцияС1СДокументооборот3ПовтИсп.ОбщийНаборПравилИнтеграции(
		ОсновнойПредмет.ТипОбъектаДО,
		ОсновнойПредмет.ВидДокументаДОID);
	Пока ВыборкаПравил.Следующий() Цикл
		
		КлючевыеРеквизитыДО = ВыборкаПравил.КлючевыеРеквизитыДО; // ВыборкаИзРезультатаЗапроса
		КлючевойРеквизит = КлючевыеРеквизитыДО.Выбрать();
		ПравилоПодходит = Истина;
		Пока КлючевойРеквизит.Следующий() Цикл
			Если КлючевойРеквизит.ЭтоДополнительныйРеквизитДО Тогда
				
				ДопРеквизитПодходит = Ложь;
				Для Каждого ДопРеквизитXDTO Из ОсновнойПредмет.mainApplication.additionalProperties Цикл
					Если ДопРеквизитXDTO.objectID.id = КлючевойРеквизит.Имя Тогда
						Если ИнтеграцияС1СДокументооборотБазоваяФункциональность.СвойствоУстановлено(
								ДопРеквизитXDTO, "propertySimpleValue") Тогда
							ДопРеквизитПодходит =
								(ДопРеквизитXDTO.propertySimpleValue = КлючевойРеквизит.ЗначениеРеквизита);
						ИначеЕсли ИнтеграцияС1СДокументооборотБазоваяФункциональность.СвойствоУстановлено(
								ДопРеквизитXDTO, "propertyObjectValue") Тогда
							ДопРеквизитПодходит =
								(ДопРеквизитXDTO.propertyObjectValue.objectID.id = КлючевойРеквизит.ЗначениеРеквизитаID)
								И (ДопРеквизитXDTO.propertyObjectValue.objectID.type = КлючевойРеквизит.ЗначениеРеквизитаТип);
						КонецЕсли;
						Прервать;
					КонецЕсли;
				КонецЦикла;
				Если Не ДопРеквизитПодходит Тогда
					ПравилоПодходит = Ложь;
					Прервать;
				КонецЕсли;
				
			ИначеЕсли ИнтеграцияС1СДокументооборотБазоваяФункциональность.СвойствоУстановлено(
					ОсновнойПредмет.mainApplication, КлючевойРеквизит.Имя) Тогда
				
				Если ТипЗнч(ОсновнойПредмет.mainApplication[КлючевойРеквизит.Имя]) = Тип("ОбъектXDTO")
						И ИнтеграцияС1СДокументооборотБазоваяФункциональность.СвойствоУстановлено(
							ОсновнойПредмет.mainApplication[КлючевойРеквизит.Имя], "objectID") Тогда
					Если ОсновнойПредмет.mainApplication[КлючевойРеквизит.Имя].objectID.ID <> КлючевойРеквизит.ЗначениеРеквизитаID
							Или ОсновнойПредмет.mainApplication[КлючевойРеквизит.Имя].objectID.type
								<> КлючевойРеквизит.ЗначениеРеквизитаТип Тогда
						ПравилоПодходит = Ложь;
						Прервать;
					КонецЕсли;
				Иначе
					Если ОсновнойПредмет.mainApplication[КлючевойРеквизит.Имя] <> КлючевойРеквизит.ЗначениеРеквизита Тогда
						ПравилоПодходит = Ложь;
						Прервать;
					КонецЕсли;
				КонецЕсли;
				
			Иначе
				
				ПравилоПодходит = Ложь;
				Прервать;
				
			КонецЕсли;
		КонецЦикла;
		
		Если ПравилоПодходит Тогда
			ПодходящийОбъект = ИнтеграцияС1СДокументооборот3КлиентСервер.ОбъектДляСозданияНаОснованииЗадачи();
			ПодходящийОбъект.ПравилоИнтеграции = ВыборкаПравил.Ссылка;
			МассивПредставлениеОбъектаИС = Новый Массив;
			МассивПредставлениеОбъектаИС.Добавить(ВыборкаПравил.ПредставлениеОбъектаИССКлючевымиПолями);
			Если Не ПустаяСтрока(ВыборкаПравил.Комментарий) Тогда
				МассивПредставлениеОбъектаИС.Добавить(ВыборкаПравил.Комментарий);
			КонецЕсли;
			ПодходящийОбъект.ПредставлениеОбъектаИС = СтрСоединить(МассивПредставлениеОбъектаИС, ", ");
			ПодходящиеОбъекты.Добавить(ПодходящийОбъект);
		КонецЕсли;
		
	КонецЦикла;
	ВыборкаПравил.Сбросить();
	
	Возврат ПодходящиеОбъекты;
	
КонецФункции

// Возвращает из предварительных данных правило загрузки в ДО, соответствующее объекту ИС.
//
// Параметры:
//   Прокси - WSПрокси - объект для подключения к web-сервисам Документооборота.
//   ПредварительныеДанные - см. ИнтеграцияС1СДокументооборот3.ПредварительныеДанныеДляВыгрузкиВДО
//   ОбъектИС - ЛюбаяСсылка - объект ИС, для которого необходимо получить правило.
//
// Возвращаемое значение:
//   ОбъектXDTO - объект XDTO типа DMDataLoadingRule.
//   Неопределено - если подходящее правило загрузки не было найдено.
//
Функция ПравилоЗагрузкиВДОИзПредварительныхДанных(Прокси, ПредварительныеДанные, ОбъектИС) Экспорт
	
	ПравилоЗагрузкиВДО = Неопределено;
	
	ПравилоЗагрузкиВДОСтрока = ПредварительныеДанные.СоответствиеПравилЗагрузкиВДООбъектамИС[ОбъектИС];
	Если ПравилоЗагрузкиВДОСтрока <> Неопределено Тогда
		ПравилоЗагрузкиВДО = ИнтеграцияС1СДокументооборотБазоваяФункциональность.СтрокаВОбъектXDTO(
			Прокси,
			ПравилоЗагрузкиВДОСтрока);
	КонецЕсли;
	
	Возврат ПравилоЗагрузкиВДО;
	
КонецФункции

// Возвращает данные, необходимые для выгрузки объекта ИС в ДО: список табличных частей объекта ИС, которые требуются
// для заполнения объекта ДО, список выражений на встроенном языке, которые требуется выполнить на стороне ИС для
// последующей передачи результатов на сторону ДО, список печатных форм, которые требуется сформировать на стороне ИС,
// список связей с создаваемым объектом ДО, которые автоматически формируются по данным объекта ИС,
// список обязательных связей, которые требуется выбрать интерактивно.
//
// Параметры:
//   Прокси - WSПрокси - объект для подключения к web-сервисам Документооборота.
//   СписокОбъектовИС - Массив из ЛюбаяСсылка - объекты ИС, для которых требуется найти соответствующие правила.
//   ПодходящиеПравилаИнтеграции - Соответствие из КлючИЗначение - соответствующие правила интеграции:
//     * Ключ - ОпределяемыйТип.ИнтеграцияС1СДокументооборотВсеСсылкиПереопределяемый - объект ИС.
//     * Значение - Массив из СправочникСсылка.ПравилаИнтеграцииС1СДокументооборотом3 - список подходящих правил.
//   ТребуетсяСписокВыражений - Булево - требуется получение списка выражений на встроенном языке.
//   ТребуетсяСписокОбязательныхСвязей - Булево - требуется получение списка обязательных связей,
//     которые пользователь должен выбрать интерактивно.
//
// Возвращаемое значение:
//   Структура:
//     * ДоступенФункционалФайлов - Булево - поддерживает ли веб-сервис Документооборота работу с файлами.
//     * ДоступенФункционалСозданияСвязей - Булево - поддерживает ли веб-сервис Документооборота функционал
//         создания связей.
//     * СоответствиеПравилЗагрузкиВДООбъектамИС - Соответствие из КлючИЗначение - соответствующие правила загрузки в ДО:
//       ** Ключ - ОпределяемыйТип.ИнтеграцияС1СДокументооборотВсеСсылкиПереопределяемый - объект ИС.
//       ** Значение - Строка - строковое представление объекта XDTO типа DMDataLoadingRule.
//                   - Неопределено - если подходящее правило загрузки не было найдено.
//     * СоответствиеСпискаВыгружаемыхТЧОбъектамИС - Соответствие из КлючИЗначение:
//       ** Ключ - ОпределяемыйТип.ИнтеграцияС1СДокументооборотВсеСсылкиПереопределяемый - объект ИС.
//       ** Значение - Массив из Строка - список имен табличных частей, которые требуется выгрузить в ДО.
//     * СоответствиеСпискаВыраженийОбъектамИС - Соответствие из КлючИЗначение:
//       ** Ключ - ОпределяемыйТип.ИнтеграцияС1СДокументооборотВсеСсылкиПереопределяемый - объект ИС.
//       ** Значение - Массив из Строка - список выражений на встроенном языке.
//     * СоответствиеПечатныхФормОбъектамИС - Соответствие из КлючИЗначение:
//       ** Ключ - ОпределяемыйТип.ИнтеграцияС1СДокументооборотВсеСсылкиПереопределяемый - объект ИС.
//       ** Значение - СписокXDTO - таблица печатных форм к передаче в ДО.
//     * ТипыФайловПечатныхФорм - Соответствие из КлючИЗначение:
//       ** Ключ - ОпределяемыйТип.ИнтеграцияС1СДокументооборотВсеСсылкиПереопределяемый - объект ИС.
//       ** Значение - Строка - тип файлов сохранения печатных форм.
//     * СоответствиеСоздаваемыхСвязейОбъектамИС - Соответствие из КлючИЗначение:
//       ** Ключ - ОпределяемыйТип.ИнтеграцияС1СДокументооборотВсеСсылкиПереопределяемый - объект ИС.
//       ** Значение - Массив из см. ИнтеграцияС1СДокументооборотБазоваяФункциональностьКлиентСервер.СвязьКСозданию
//     * СоответствиеОбязательныхСвязейОбъектамИС - Соответствие из КлючИЗначение:
//       ** Ключ - ОпределяемыйТип.ИнтеграцияС1СДокументооборотВсеСсылкиПереопределяемый - объект ИС.
//       ** Значение - Массив из см. ИнтеграцияС1СДокументооборотБазоваяФункциональностьКлиентСервер.НастройкаСвязиОбъектовДО
//
Функция ПредварительныеДанныеДляВыгрузкиВДО(Прокси, СписокОбъектовИС, ПодходящиеПравилаИнтеграции,
		ТребуетсяСписокВыражений = Истина, ТребуетсяСписокОбязательныхСвязей = Истина) Экспорт
	
	Данные = Новый Структура;
	Данные.Вставить("ДоступенФункционалФайлов", ДоступенФункционалФайлов());
	Данные.Вставить("ДоступенФункционалСозданияСвязей", ДоступенФункционалСозданияСвязей());
	Данные.Вставить("СоответствиеПравилЗагрузкиВДООбъектамИС", Новый Соответствие);
	Данные.Вставить("СоответствиеСпискаВыгружаемыхТЧОбъектамИС", Новый Соответствие);
	Данные.Вставить("СоответствиеСпискаВыраженийОбъектамИС", Новый Соответствие);
	Данные.Вставить("СоответствиеПечатныхФормОбъектамИС", Новый Соответствие);
	Данные.Вставить("ТипыФайловПечатныхФорм", Новый Соответствие);
	Данные.Вставить("СоответствиеСоздаваемыхСвязейОбъектамИС", Новый Соответствие);
	Данные.Вставить("СоответствиеОбязательныхСвязейОбъектамИС", Новый Соответствие);
	
	Данные.СоответствиеПравилЗагрузкиВДООбъектамИС = СоответствиеПравилЗагрузкиВДООбъектамИС(
		Прокси,
		СписокОбъектовИС,
		ПодходящиеПравилаИнтеграции);
	
	ТребуетсяСписокВыгружаемыхТЧ = ТребуетсяСписокВыражений
		И ИнтеграцияС1СДокументооборотБазоваяФункциональностьПовтИсп.ДоступенФункционалВерсииСервиса("3.0.16.30");
	
	Если СписокОбъектовИС.Количество() = 0
			Или (Не ТребуетсяСписокВыгружаемыхТЧ
				И Не ТребуетсяСписокВыражений
				И Не Данные.ДоступенФункционалФайлов
				И Не Данные.ДоступенФункционалСозданияСвязей) Тогда
		Возврат Данные;
	КонецЕсли;
	
	Если ТребуетсяСписокВыгружаемыхТЧ Тогда
		ЗапросСпискаВыгружаемыхТЧ = ИнтеграцияС1СДокументооборотБазоваяФункциональность.СоздатьОбъект(
			Прокси,
			"DMGetRequiredTabularSectionListRequest");
		ПравилаДляСпискаСпискаВыгружаемыхТЧ = ЗапросСпискаВыгружаемыхТЧ.allObjectsTabularSections; // СписокXDTO
	КонецЕсли;
	Если ТребуетсяСписокВыражений Тогда
		ЗапросСпискаВыражений = ИнтеграцияС1СДокументооборотБазоваяФункциональность.СоздатьОбъект(
			Прокси,
			"DMGetExpressionListRequest");
		ПравилаДляСпискаВыражений = ЗапросСпискаВыражений.objects; // СписокXDTO
	КонецЕсли;
	Если Данные.ДоступенФункционалФайлов Тогда
		ЗапросПечатныхФорм = ИнтеграцияС1СДокументооборотБазоваяФункциональность.СоздатьОбъект(
			Прокси,
			"DMGetPrintedFormListRequest");
		ПравилаДляПечатныхФорм = ЗапросПечатныхФорм.objects; // СписокXDTO
	КонецЕсли;
	Если Данные.ДоступенФункционалСозданияСвязей Тогда
		ЗапросСоздаваемыхСвязей = ИнтеграцияС1СДокументооборотБазоваяФункциональность.СоздатьОбъект(
			Прокси,
			"DMGetAddedRelationsRequest");
		ПравилаДляСоздаваемыхСвязей = ЗапросСоздаваемыхСвязей.objects; // СписокXDTO
	КонецЕсли;
	
	ДобавленныеПравила = Новый Массив;
	Для Каждого ОбъектИС Из СписокОбъектовИС Цикл
		ПравилоЗагрузкиВДО = ПравилоЗагрузкиВДОИзПредварительныхДанных(Прокси, Данные, ОбъектИС);
		Если ПравилоЗагрузкиВДО = Неопределено
				Или ДобавленныеПравила.Найти(ПравилоЗагрузкиВДО.ID) <> Неопределено Тогда
			Продолжить;
		КонецЕсли;
		
		Если ТребуетсяСписокВыгружаемыхТЧ Тогда
			ПравилоИСписокВсехТЧ = ИнтеграцияС1СДокументооборотБазоваяФункциональность.СоздатьОбъект(
				Прокси,
				"DMObjectTabularSections");
			СписокВсехТЧ = ПравилоИСписокВсехТЧ.tabularSections; // СписокXDTO
			ПравилоИСписокВсехТЧ.object = КопияОбъектаXDTO(ПравилоЗагрузкиВДО, Прокси);
			Для Каждого ТабличнаяЧасть Из ОбъектИС.Метаданные().ТабличныеЧасти Цикл
				СписокВсехТЧ.Добавить(ТабличнаяЧасть.Имя);
			КонецЦикла;
			ПравилаДляСпискаСпискаВыгружаемыхТЧ.Добавить(ПравилоИСписокВсехТЧ);
		КонецЕсли;
		Если ТребуетсяСписокВыражений Тогда
			ПравилаДляСпискаВыражений.Добавить(КопияОбъектаXDTO(ПравилоЗагрузкиВДО, Прокси));
		КонецЕсли;
		Если Данные.ДоступенФункционалФайлов Тогда
			ПравилаДляПечатныхФорм.Добавить(КопияОбъектаXDTO(ПравилоЗагрузкиВДО, Прокси));
		КонецЕсли;
		Если Данные.ДоступенФункционалСозданияСвязей Тогда
			ПравилаДляСоздаваемыхСвязей.Добавить(КопияОбъектаXDTO(ПравилоЗагрузкиВДО, Прокси));
		КонецЕсли;
		ДобавленныеПравила.Добавить(ПравилоЗагрузкиВДО.ID);
	КонецЦикла;
	
	МассивВсехВыгружаемыхТЧ = Новый Массив;
	МассивВсехВыражений = Новый Массив;
	МассивВсехПечатныхФорм = Новый Массив;
	МассивВсехСоздаваемыхСвязей = Новый Массив;
	МассивВсехНастроекСвязей = Новый Массив;
	
	ЗапросыПакета = Новый Массив;
	
	Если ТребуетсяСписокВыгружаемыхТЧ И ПравилаДляСпискаСпискаВыгружаемыхТЧ.Количество() > 0 Тогда
		ЗапросыПакета.Добавить(ЗапросСпискаВыгружаемыхТЧ);
	КонецЕсли;
	Если ТребуетсяСписокВыражений И ПравилаДляСпискаВыражений.Количество() > 0 Тогда
		ЗапросыПакета.Добавить(ЗапросСпискаВыражений);
	КонецЕсли;
	Если Данные.ДоступенФункционалФайлов И ПравилаДляПечатныхФорм.Количество() > 0 Тогда
		ЗапросыПакета.Добавить(ЗапросПечатныхФорм);
	КонецЕсли;
	Если Данные.ДоступенФункционалСозданияСвязей Тогда
		Если ПравилаДляСоздаваемыхСвязей.Количество() > 0 Тогда
			ЗапросыПакета.Добавить(ЗапросСоздаваемыхСвязей);
		КонецЕсли;
		Если ТребуетсяСписокОбязательныхСвязей Тогда
			СоответствиеСсылкаИзОбъектамИС = Новый Соответствие;
			Для Каждого ЭлементСоответствияПодходящиеПравила Из ПодходящиеПравилаИнтеграции Цикл
				Если ЭлементСоответствияПодходящиеПравила.Значение.Количество() <> 1 Тогда
					Продолжить;
				КонецЕсли;
				ПравилоИнтеграции = ЭлементСоответствияПодходящиеПравила.Значение[0];
				РеквизитыПравила = ОбщегоНазначения.ЗначенияРеквизитовОбъекта(ПравилоИнтеграции, "ТипОбъектаДО, ВидДокументаДОID");
				Если РеквизитыПравила.ТипОбъектаДО = "DMDocument" Тогда
					СсылкаИзТип = "DMDocumentType";
					СсылкаИзID = РеквизитыПравила.ВидДокументаДОID;
				Иначе
					СсылкаИзТип = РеквизитыПравила.ТипОбъектаДО;
					СсылкаИзID = "";
				КонецЕсли;
				Если СсылкаИзТип = "DMCorrespondent" Тогда
					Продолжить;
				КонецЕсли;
				СсылкаИз = ИнтеграцияС1СДокументооборотБазоваяФункциональностьКлиентСервер.ДанныеСсылочногоОбъектаДО(
					СсылкаИзID,
					СсылкаИзТип);
				ЗапросУжеСоздан = Ложь;
				Для Каждого ЭлементСоответствияСсылкаИз Из СоответствиеСсылкаИзОбъектамИС Цикл
					Если ОбщегоНазначения.ДанныеСовпадают(ЭлементСоответствияСсылкаИз.Значение, СсылкаИз) Тогда
						ЗапросУжеСоздан = Истина;
						Прервать;
					КонецЕсли;
				КонецЦикла;
				Если Не ЗапросУжеСоздан Тогда
					ЗапросыПакета.Добавить(НастройкиСвязейЗапрос(Прокси, СсылкаИз));
				КонецЕсли;
				СоответствиеСсылкаИзОбъектамИС.Вставить(ЭлементСоответствияПодходящиеПравила.Ключ, СсылкаИз);
			КонецЦикла;
		КонецЕсли;
	КонецЕсли;
	
	Если ЗапросыПакета.Количество() > 0 Тогда
		
		ОтветНаЗапросыПакета = ИнтеграцияС1СДокументооборотБазоваяФункциональность.ВыполнитьПакетныйЗапрос(
			Прокси,
			ЗапросыПакета,
			Истина);
		
		Для Каждого Ответ Из ОтветНаЗапросыПакета.responses Цикл
			
			Если ИнтеграцияС1СДокументооборотБазоваяФункциональность.ПроверитьТип(
					Прокси, Ответ, "DMGetRequiredTabularSectionListResponse") Тогда
				МассивВсехВыгружаемыхТЧ = Ответ.requiredObjectsTabularSections; // СписокXDTO
				
			ИначеЕсли ИнтеграцияС1СДокументооборотБазоваяФункциональность.ПроверитьТип(
					Прокси, Ответ, "DMGetExpressionListResponse") Тогда
				МассивВсехВыражений = Ответ.objectsExpressionRows; // СписокXDTO
				
			ИначеЕсли ИнтеграцияС1СДокументооборотБазоваяФункциональность.ПроверитьТип(
					Прокси, Ответ, "DMGetPrintedFormListResponse") Тогда
				МассивВсехПечатныхФорм = Ответ.objectsPrintedForms; // СписокXDTO
				
			ИначеЕсли ИнтеграцияС1СДокументооборотБазоваяФункциональность.ПроверитьТип(
					Прокси, Ответ, "DMGetAddedRelationsResponse") Тогда
				МассивВсехСоздаваемыхСвязей = Ответ.objectsAddedRelations; // СписокXDTO
				
			ИначеЕсли ИнтеграцияС1СДокументооборотБазоваяФункциональность.ПроверитьТип(
					Прокси, Ответ, "DMGetRelationsSettingsResponse") Тогда
				ОбщегоНазначенияКлиентСервер.ДополнитьМассив(
					МассивВсехНастроекСвязей,
					Ответ.relationsSettings);
				
			КонецЕсли;
			
		КонецЦикла;
		
	КонецЕсли;
	
	Для Каждого ОбъектИС Из СписокОбъектовИС Цикл
		ПравилоЗагрузкиВДО = ПравилоЗагрузкиВДОИзПредварительныхДанных(Прокси, Данные, ОбъектИС);
		СписокВыгружаемыхТЧ = Новый Массив;
		СписокВыражений = Новый Массив;
		СписокПечатныхФорм = Новый Массив;
		СписокАвтоматическиДобавляемыхСвязей = Новый Массив;
		ТипФайловПечатныхФорм = "";
		
		Если ПравилоЗагрузкиВДО <> Неопределено Тогда
			Если ТребуетсяСписокВыгружаемыхТЧ Тогда
				Для Каждого Элемент Из МассивВсехВыгружаемыхТЧ Цикл
					Если Элемент.object.ID = ПравилоЗагрузкиВДО.ID Тогда
						СписокВыгружаемыхТЧ = Элемент.tabularSections;
						Прервать;
					КонецЕсли;
				КонецЦикла;
			КонецЕсли;
			
			Если ТребуетсяСписокВыражений Тогда
				Для Каждого Элемент Из МассивВсехВыражений Цикл
					Если Элемент.object.ID = ПравилоЗагрузкиВДО.ID Тогда
						СписокВыражений = Элемент.expressionRows;
						Прервать;
					КонецЕсли;
				КонецЦикла;
			КонецЕсли;
			
			Если Данные.ДоступенФункционалФайлов Тогда
				Для Каждого Элемент Из МассивВсехПечатныхФорм Цикл
					Если Элемент.object.ID = ПравилоЗагрузкиВДО.ID Тогда
						СписокПечатныхФорм = Элемент.printedForms;
						ТипФайловПечатныхФорм = Элемент.spreadsheetDocumentFileType;
						Прервать;
					КонецЕсли;
				КонецЦикла;
			КонецЕсли;
			
			Если Данные.ДоступенФункционалСозданияСвязей Тогда
				Для Каждого Элемент Из МассивВсехСоздаваемыхСвязей Цикл
					Если Элемент.object.ID = ПравилоЗагрузкиВДО.ID Тогда
						СписокАвтоматическиДобавляемыхСвязей = Элемент.addedRelations;
						Прервать;
					КонецЕсли;
				КонецЦикла;
			КонецЕсли;
		КонецЕсли;
		
		Если ТребуетсяСписокВыгружаемыхТЧ Тогда
			Данные.СоответствиеСпискаВыгружаемыхТЧОбъектамИС[ОбъектИС] = Новый Массив;
			Для Каждого ИмяТаблицыИС Из СписокВыгружаемыхТЧ Цикл
				Данные.СоответствиеСпискаВыгружаемыхТЧОбъектамИС[ОбъектИС].Добавить(ИмяТаблицыИС);
			КонецЦикла;
		КонецЕсли;
		Если ТребуетсяСписокВыражений Тогда
			Данные.СоответствиеСпискаВыраженийОбъектамИС.Вставить(ОбъектИС, СписокВыражений);
		КонецЕсли;
		Если Данные.ДоступенФункционалФайлов Тогда
			Данные.СоответствиеПечатныхФормОбъектамИС.Вставить(ОбъектИС, СписокПечатныхФорм);
			Данные.ТипыФайловПечатныхФорм.Вставить(ОбъектИС, ТипФайловПечатныхФорм);
		КонецЕсли;
		Если Данные.ДоступенФункционалСозданияСвязей Тогда
			// Заполним связи, которые будут созданы автоматически.
			СвязиКСозданию = Новый Массив;
			ЗаполнитьСписокСвязиКСозданию(
				Прокси,
				СвязиКСозданию,
				ОбъектИС,
				СписокАвтоматическиДобавляемыхСвязей);
			Данные.СоответствиеСоздаваемыхСвязейОбъектамИС.Вставить(ОбъектИС, СвязиКСозданию);
			Если ТребуетсяСписокОбязательныхСвязей Тогда
				// Заполним обязательные связи, которые требуется выбрать интерактивно.
				ОбязательныеСвязи = Новый Массив;
				СсылкаИз = СоответствиеСсылкаИзОбъектамИС[ОбъектИС];
				Если СсылкаИз <> Неопределено Тогда
					Для Каждого НастройкаСвязи Из МассивВсехНастроекСвязей Цикл
						Если Не НастройкаСвязи.required Или НастройкаСвязи.relationFrom.objectID.type <> СсылкаИз.Тип Тогда
							Продолжить;
						КонецЕсли;
						Если ЗначениеЗаполнено(НастройкаСвязи.relationFrom.objectID.ID)
								И НастройкаСвязи.relationFrom.objectID.ID <> СсылкаИз.ID Тогда
							Продолжить;
						КонецЕсли;
						СвязьБудетСозданаАвтоматически = Ложь;
						СсылкаНаТип = НастройкаСвязи.relationTo.objectID.type;
						Если СсылкаНаТип = "DMDocumentType" Тогда
							СсылкаНаТип = "DMDocument";
						КонецЕсли;
						Для Каждого СвязьКСозданию Из СвязиКСозданию Цикл
							Если СвязьКСозданию.ТипСвязиID = НастройкаСвязи.relationType.objectID.ID
									И СвязьКСозданию.СсылкаНаТип = СсылкаНаТип Тогда
								СвязьБудетСозданаАвтоматически = Истина;
								Прервать;
							КонецЕсли;
						КонецЦикла;
						Если Не СвязьБудетСозданаАвтоматически Тогда
							СсылкаНа = ИнтеграцияС1СДокументооборотБазоваяФункциональностьКлиентСервер.ДанныеСсылочногоОбъектаДО(
								НастройкаСвязи.relationTo.objectID.ID,
								НастройкаСвязи.relationTo.objectID.type,
								НастройкаСвязи.relationTo.name,
								НастройкаСвязи.relationTo.objectID.presentation,
								НастройкаСвязи.relationTo.objectID.navigationRef);
							ТипСвязи = ИнтеграцияС1СДокументооборотБазоваяФункциональностьКлиентСервер.ДанныеСсылочногоОбъектаДО(
								НастройкаСвязи.relationType.objectID.ID,
								НастройкаСвязи.relationType.objectID.type,
								НастройкаСвязи.relationType.name,
								НастройкаСвязи.relationType.objectID.presentation,
								НастройкаСвязи.relationType.objectID.navigationRef);
							ОбязательнаяСвязь = ИнтеграцияС1СДокументооборотБазоваяФункциональностьКлиентСервер.НастройкаСвязиОбъектовДО(
								СсылкаИз,
								СсылкаНа,
								ТипСвязи);
							ОбязательнаяСвязь.ТипСвязиКомментарий = НастройкаСвязи.relationType.comment;
							ОбязательныеСвязи.Добавить(ОбязательнаяСвязь);
						КонецЕсли;
					КонецЦикла;
				КонецЕсли;
			КонецЕсли;
			Данные.СоответствиеОбязательныхСвязейОбъектамИС.Вставить(ОбъектИС, ОбязательныеСвязи);
		КонецЕсли;
	КонецЦикла;
	
	Возврат Данные;
	
КонецФункции

// Формирует универсальную таблицу присоединяемых печатных форм для последующего создания.
//
// Параметры:
//   ПечатныеФормы - Массив из ОбъектXDTO - список объектов XDTO типа DMPrintedForm.
//
// Возвращаемое значение:
//   см. ИнтеграцияС1СДокументооборотБазоваяФункциональность.ПрисоединяемыеПечатныеФормы
//
Функция ПрисоединяемыеПечатныеФормы(ПечатныеФормы) Экспорт
	
	ПрисоединяемыеПечатныеФормы = ИнтеграцияС1СДокументооборотБазоваяФункциональность.ПрисоединяемыеПечатныеФормы();
	Для Каждого Строка Из ПечатныеФормы Цикл
		НоваяСтрока = ПрисоединяемыеПечатныеФормы.Добавить();
		НоваяСтрока.ИмяКоманды = Строка.commandName;
		НоваяСтрока.МенеджерПечати = Строка.printManager;
		НоваяСтрока.Обновлять = Строка.update;
		Если ИнтеграцияС1СДокументооборотБазоваяФункциональность.СвойствоУстановлено(Строка, "role") Тогда
			НоваяСтрока.РольФайлаID = Строка.role.objectID.id;
		КонецЕсли;
		Если ИнтеграцияС1СДокументооборотБазоваяФункциональность.СвойствоУстановлено(Строка, "additionalParameters") Тогда
			НоваяСтрока.ДополнительныеПараметры = Строка.additionalParameters;
		КонецЕсли;
	КонецЦикла;
	
	Возврат ПрисоединяемыеПечатныеФормы;
	
КонецФункции

// Добавляет команды Документооборота на форму объекта при создании на сервере.
//
// Параметры:
//   Форма - ФормаКлиентскогоПриложения - форма, из которой вызвана процедура.
//   МестоРазмещенияКоманд - ГруппаФормы - место для размещения, по умолчанию - командная панель формы.
//   ПараметрыОповещения - Структура - параметры, где может быть установлен источник команды.
//
Процедура ПриСозданииНаСервере(Форма, МестоРазмещенияКоманд = Неопределено, ПараметрыОповещения = Неопределено) Экспорт
	
	Если Не ИспользоватьИнтеграцию() Тогда
		Возврат;
	КонецЕсли;
	
	// Переместим команды создания на основании вниз.
	Если Форма.Элементы.Найти("ФормаСоздатьНаОсновании") <> Неопределено Тогда
		Если Форма.Элементы.Найти("ФормаОбщаяКомандаИнтеграцияС1СДокументооборотНачатьОбработку")
				<> Неопределено Тогда
			Форма.Элементы.Переместить(
				Форма.Элементы.ФормаОбщаяКомандаИнтеграцияС1СДокументооборотНачатьОбработку,
				Форма.Элементы.ФормаСоздатьНаОсновании);
		КонецЕсли;
	КонецЕсли;
	
КонецПроцедуры

// Обрабатывает результат действия над задачей или с обработкой.
//
// Параметры:
//   РезультатДействияСтрока - Строка - результат действия, преобразованный в строку.
//   ДанныеДляОбновления - ОбъектXDTO - неявно возвращаемое значение, в которое будут переданы данные,
//     необходимые для обновления формы.
//   ВыполняетсяОбновлениеОбъекта - Булево - признак наличия в пакетном запросе запроса на обновление объекта.
//   КонтрольОтправкиФайлов - см. ИнтеграцияС1СДокументооборотБазоваяФункциональность.КонтрольОтправкиФайлов
//
// Возвращаемое значение:
//   Структура:
//     * Успешно - Булево
//     * ТекстПредупреждения - Строка
//     * ТребуемоеИнтерактивноеДействие - см. ИнтеграцияС1СДокументооборот3КлиентСервер.ТребуемоеИнтерактивноеДействие
//
Функция РезультатВыполненияКомандыПоОбъекту(РезультатДействияСтрока, ДанныеДляОбновления,
		ВыполняетсяОбновлениеОбъекта = Ложь, КонтрольОтправкиФайлов = Неопределено) Экспорт
	
	Результат = Новый Структура("Успешно, ТекстПредупреждения, ТребуемоеИнтерактивноеДействие",
		Истина, "", ИнтеграцияС1СДокументооборот3КлиентСервер.ТребуемоеИнтерактивноеДействие());
	
	Прокси = ИнтеграцияС1СДокументооборотБазоваяФункциональностьПовтИсп.ПолучитьПрокси();
	ОтветНаЗапросыПакета = ИнтеграцияС1СДокументооборотБазоваяФункциональность.СтрокаВОбъектXDTO(
		Прокси,
		РезультатДействияСтрока);
	
	Если ВыполняетсяОбновлениеОбъекта Тогда
		ОтветОбновитьОбъект = ОтветНаЗапросыПакета.responses[0];
		РезультатВыполнения = ОтветНаЗапросыПакета.responses[1];
		
		Если ИнтеграцияС1СДокументооборотБазоваяФункциональность.ПроверитьТип(Прокси, ОтветОбновитьОбъект, "DMError") Тогда
			ИнтеграцияС1СДокументооборотБазоваяФункциональность.ЗаписатьОшибкуXDTO(ОтветОбновитьОбъект);
		Иначе
			// Объект ДО был успешно обновлен.
			Для Каждого Строка Из КонтрольОтправкиФайлов Цикл
				РегистрыСведений.КонтрольОтправкиФайловВ1СДокументооборот.СохранитьХешСуммуВерсииФайла(
					Строка.Источник,
					Строка.ИмяФайла,
					Строка.ТабличныйДокумент)
			КонецЦикла;
		КонецЕсли;
	Иначе
		РезультатВыполнения = ОтветНаЗапросыПакета.responses[0];
	КонецЕсли;
	
	Если ИнтеграцияС1СДокументооборотБазоваяФункциональность.ПроверитьТип(Прокси, РезультатВыполнения, "DMError") Тогда
		Результат.Успешно = Ложь;
		Результат.ТекстПредупреждения = СокрЛП(РезультатВыполнения.description);
	ИначеЕсли ИнтеграцияС1СДокументооборотБазоваяФункциональность.СвойствоУстановлено(
			РезультатВыполнения, "requiredInteractiveAction") Тогда
		Результат.Успешно = Ложь;
		ЗаполнитьИнтерактивноеДействие(
			Прокси,
			Результат.ТребуемоеИнтерактивноеДействие,
			РезультатВыполнения.requiredInteractiveAction);
		Если ИнтеграцияС1СДокументооборотБазоваяФункциональность.СвойствоУстановлено(
				РезультатВыполнения, "interactiveActionResultCache") Тогда
			Результат.ТребуемоеИнтерактивноеДействие.КешПредыдущихРезультатов =
				ИнтеграцияС1СДокументооборотБазоваяФункциональность.ОбъектXDTOВСтроку(Прокси, РезультатВыполнения);
		КонецЕсли;
	КонецЕсли;
	
	Если ОтветНаЗапросыПакета.responses.Количество() = 2 Тогда
		ДанныеДляОбновления = ОтветНаЗапросыПакета.responses[1];
		Если ИнтеграцияС1СДокументооборотБазоваяФункциональность.ПроверитьТип(Прокси, ДанныеДляОбновления, "DMError") Тогда
			ИнтеграцияС1СДокументооборотБазоваяФункциональность.ЗаписатьОшибку(
				СокрЛП(ДанныеДляОбновления.description),
				ДанныеДляОбновления.subject);
		КонецЕсли;
	КонецЕсли;
	
	Возврат Результат;
	
КонецФункции

// Возвращает результат выполнения выражения на встроенном языке. Выражение может использоваться только для вычисления
// какого-либо значения, которое в итоге вернет функция. Никакие изменения в базу данных при этом вноситься не
// должны, поэтому выражение выполняется в транзакции, которая откатывается после завершения выполнения.
//
// Параметры:
//   ВычисляемоеВыражение - Строка - выражение на встроенном языке.
//   Источник - Произвольный - источник данных.
//   Приемник - Произвольный - приемник данных.
//   ТекущаяСтрока - Произвольный - передается в случае, если выражение используется для заполнения реквизита
//     строки табличной части.
//
// Возвращаемое значение:
//   Произвольный - результат выполнения выражения на встроенном языке.
//
Функция РезультатВыраженияНаВстроенномЯзыке(ВычисляемоеВыражение, Знач Источник, Приемник = Неопределено,
		ТекущаяСтрока = Неопределено) Экспорт
	
	Параметры = Новый Структура("Источник", Источник);
	
	Если ТипЗнч(Источник) = Тип("ОбъектXDTO") Тогда
		// Преобразуем объект XDTO в структуру, и дополним полученную структуру свойствами, которых нет в
		// исходном объекте XDTO, но к которым могут идти обращения из выражения на встроенном языке.
		Параметры.Источник = Новый Структура;
		Для Каждого СвойствоXDTO Из Источник.Тип().Свойства Цикл
			Параметры.Источник.Вставить(СвойствоXDTO.Имя, Источник[СвойствоXDTO.Имя]);
		КонецЦикла;
		Если ИнтеграцияС1СДокументооборотБазоваяФункциональность.СвойствоУстановлено(Источник, "statusByActionTypeRows") Тогда
			Для Каждого СтрокаСтатус Из Источник.statusByActionTypeRows.rows Цикл
				Параметры.Источник.Вставить(
					СтрШаблон("status_%1", СтрЗаменить(СтрокаСтатус.actionType.objectID.id, "-", "_")),
					СтрокаСтатус.status);
			КонецЦикла;
		КонецЕсли;
	КонецЕсли;
	Параметры.Вставить("Приемник", Приемник);
	Параметры.Вставить("ТекущаяСтрока", ТекущаяСтрока);
	Параметры.Вставить("Результат", Неопределено);
	
	Ошибка = "";
	Попытка
		ИнтеграцияС1СДокументооборотБазоваяФункциональность.ВыполнитьВБезопасномРежиме(
			ВычисляемоеВыражение,
			Параметры);
	Исключение
		Ошибка = СтрШаблон(
			НСтр("ru = 'Найдена ошибка в выражении на встроенном языке:
				|%1';
				|en = 'An error is found in the expression in 1C:Enterprise language:
				|%1'"),
			ОбработкаОшибок.КраткоеПредставлениеОшибки(ИнформацияОбОшибке()));
	КонецПопытки;
	
	Если Ошибка = "" Тогда
		Возврат Параметры.Результат;
	Иначе
		ВызватьИсключение Ошибка;
	КонецЕсли;
	
КонецФункции

// Сбрасывает быстрый поиск (отбор объектов).
//
// Параметры:
//   БыстрыйПоиск - ДанныеФормыКоллекция - таблица "Быстрый поиск" на форме списка задач.
//   КнопкаСброситьОтбор - КнопкаФормы - кнопка "Сбросить отбор".
//
Процедура СброситьБыстрыйПоиск(БыстрыйПоиск, КнопкаСброситьОтбор) Экспорт
	
	Для Каждого Строка Из БыстрыйПоиск Цикл
		
		Если Строка.Фиксированный Тогда
			Продолжить;
		КонецЕсли;
		
		Строка.Значение = Строка.ЗначениеПоУмолчанию;
		Строка.ЗначениеID = Строка.ЗначениеПоУмолчаниюID;
		Строка.ЗначениеТип = Строка.ЗначениеПоУмолчаниюТип;
		
	КонецЦикла;
	
	ОбновитьПредставлениеБыстрогоПоиска(БыстрыйПоиск, КнопкаСброситьОтбор);
	
КонецПроцедуры

// Копирует объект XDTO.
//
// Параметры:
//   Приемник - ОбъектXDTO - куда копировать.
//   Источник - ОбъектXDTO - от куда копировать.
//   ПроксиДО - WSПрокси - объект для подключения к web-сервисам Документооборота. Если указан - копируется объект
//     DMService, если не указан - копируется объект DMILService.
//
Процедура СкопироватьОбъектXDTO(Приемник, Источник, ПроксиДО = Неопределено) Экспорт
	
	Если ПроксиДО = Неопределено Тогда
		Приемник = СоздатьОбъектБИД(Источник.Тип().Имя);
	Иначе
		Приемник = ИнтеграцияС1СДокументооборотБазоваяФункциональность.СоздатьОбъект(ПроксиДО, Источник.Тип().Имя);
	КонецЕсли;
	Для Каждого Свойство Из Источник.Тип().Свойства Цикл
		
		Если Не ИнтеграцияС1СДокументооборотБазоваяФункциональность.СвойствоУстановлено(Источник, Свойство.Имя) Тогда
			Продолжить;
		КонецЕсли;
		
		Если ТипЗнч(Источник[Свойство.Имя]) = Тип("ОбъектXDTO") Тогда
			СкопироватьОбъектXDTO(Приемник[Свойство.Имя], Источник[Свойство.Имя], ПроксиДО);
			
		ИначеЕсли ТипЗнч(Источник[Свойство.Имя]) = Тип("СписокXDTO") Тогда
			ТаблицаПриемника = Приемник[Свойство.Имя]; // СписокXDTO
			Для Каждого СтрокаСпискаИсточника Из Источник[Свойство.Имя] Цикл
				СтрокаСпискаПриемника = Неопределено;
				СкопироватьОбъектXDTO(СтрокаСпискаПриемника, СтрокаСпискаИсточника, ПроксиДО);
				ТаблицаПриемника.Добавить(СтрокаСпискаПриемника);
			КонецЦикла;
			
		Иначе
			Приемник[Свойство.Имя] = Источник[Свойство.Имя];
			
		КонецЕсли;
		
	КонецЦикла;
	
КонецПроцедуры

// Создает новый объект Документооборота по объекту ИС и указанному правилу.
//
// Параметры:
//   Прокси - WSПрокси - объект для подключения к web-сервисам Документооборота.
//   ОбъектИС - ЛюбаяСсылка - объект ИС, источник данных заполнения.
//   Правило - СправочникСсылка.ПравилаИнтеграцииС1СДокументооборотом3 - правило заполнения.
//   СвязиКСозданию - Массив из см. ИнтеграцияС1СДокументооборотБазоваяФункциональностьКлиентСервер.СвязьКСозданию
//   ПредварительныеДанныеВХранилище - Строка - адрес во временном хранилище, содержащий
//     структуру см. ИнтеграцияС1СДокументооборот3.ПредварительныеДанныеДляВыгрузкиВДО
//
// Возвращаемое значение:
//   ОбъектXDTO - созданный объект:
//     * name - Строка
//     * objectID - ОбъектXDTO:
//       ** ID - Строка
//       ** type - Строка
//   Строка - сообщение об ошибке.
//
Функция СоздатьОбъектДОПоПравилу(Прокси, ОбъектИС, Правило, СвязиКСозданию = Неопределено,
		ПредварительныеДанныеВХранилище = Неопределено) Экспорт
	
	СписокОбъектовИС = ОбщегоНазначенияКлиентСервер.ЗначениеВМассиве(ОбъектИС);
	
	ПодходящиеПравилаИнтеграции = Новый Соответствие;
	ПодходящиеПравилаИнтеграции.Вставить(ОбъектИС, ОбщегоНазначенияКлиентСервер.ЗначениеВМассиве(Правило));
	
	ПредварительныеДанные = Неопределено;
	Если ПредварительныеДанныеВХранилище <> Неопределено Тогда
		ПредварительныеДанные = ПолучитьИзВременногоХранилища(ПредварительныеДанныеВХранилище);
		УдалитьИзВременногоХранилища(ПредварительныеДанныеВХранилище);
	КонецЕсли;
	
	Если ПредварительныеДанные = Неопределено Тогда
		ПредварительныеДанные = ПредварительныеДанныеДляВыгрузкиВДО(
			Прокси,
			СписокОбъектовИС,
			ПодходящиеПравилаИнтеграции);
	КонецЕсли;
	
	Если СвязиКСозданию <> Неопределено Тогда
		Для Каждого СвязьКСозданию Из СвязиКСозданию Цикл
			ПредварительныеДанные.СоответствиеСоздаваемыхСвязейОбъектамИС[ОбъектИС].Добавить(СвязьКСозданию);
		КонецЦикла;
	КонецЕсли;
	
	Результат = СоздатьОбъектыДОПоПравилам(
		Прокси,
		СписокОбъектовИС,
		ПредварительныеДанные);
	
	Если Результат[ОбъектИС].Успешно Тогда
		Возврат Результат[ОбъектИС].ОбъектXDTO;
	Иначе
		Возврат Результат[ОбъектИС].ТекстОшибки;
	КонецЕсли;
	
КонецФункции

// Создает новые объекты Документооборота для списка объектов ИС по соответствующим им правилам.
//
// Параметры:
//   Прокси - WSПрокси - объект для подключения к web-сервисам Документооборота.
//   СписокОбъектовИС - Массив из ЛюбаяСсылка - объекты ИС, источник данных заполнения.
//   ПредварительныеДанные - см. ИнтеграцияС1СДокументооборот3.ПредварительныеДанныеДляВыгрузкиВДО
//
// Возвращаемое значение:
//   Соответствие из КлючИЗначение:
//     * Ключ - ОпределяемыйТип.ИнтеграцияС1СДокументооборотВсеСсылкиПереопределяемый - объект ИС.
//     * Значение - Структура - результат по объекту ИС:
//       ** Успешно - Булево - Истина, если связанный объект в ДО был создан успешно, Ложь, если возникли ошибки.
//       ** ТекстОшибки - Строка - сообщение об ошибке.
//       ** ОбъектXDTO - ОбъектXDTO - созданный в ДО объект.
//
Функция СоздатьОбъектыДОПоПравилам(Прокси, СписокОбъектовИС, ПредварительныеДанные) Экспорт
	
	Результат = Новый Соответствие;
	
	Запрос = ИнтеграцияС1СДокументооборотБазоваяФункциональность.СоздатьОбъект(Прокси, "DMCreateFromIncomingDataRequest");
	
	КонтрольОтправкиФайлов = ИнтеграцияС1СДокументооборотБазоваяФункциональность.КонтрольОтправкиФайлов();
	
	// Добавим в запрос те объекты, для которых есть единственное подходящее dataLoadingRule.
	Для Каждого ОбъектИС Из СписокОбъектовИС Цикл
		
		РезультатПоОбъекту = Новый Структура("Успешно, ТекстОшибки, ОбъектXDTO", Истина, "", Неопределено);
		
		ПравилоЗагрузкиВДО = ПравилоЗагрузкиВДОИзПредварительныхДанных(Прокси, ПредварительныеДанные, ОбъектИС);
		Если ПравилоЗагрузкиВДО = Неопределено Тогда
			РезультатПоОбъекту.Успешно = Ложь;
			РезультатПоОбъекту.ТекстОшибки = НСтр("ru = 'Не найдено подходящее правило загрузки данных в 1С:Документооборот';
													|en = 'Suitable 1C:Document Management data import rule is not found'");
			Результат.Вставить(ОбъектИС, РезультатПоОбъекту);
			Продолжить;
		КонецЕсли;
		
		СтруктураСозданияОбъекта = ИнтеграцияС1СДокументооборотБазоваяФункциональность.СоздатьОбъект(
			Прокси,
			"DMIncomingDataRequestStructure",
			ОбъектИС);
		
		Попытка
			СтруктураСозданияОбъекта.incomingData = ВходящиеДанныеОбъектаИС(
				Прокси,
				ОбъектИС,
				ПредварительныеДанные.СоответствиеСпискаВыгружаемыхТЧОбъектамИС[ОбъектИС],
				ПредварительныеДанные.СоответствиеСпискаВыраженийОбъектамИС[ОбъектИС]);
		Исключение
			РезультатПоОбъекту.Успешно = Ложь;
			РезультатПоОбъекту.ТекстОшибки = ОбработкаОшибок.ПодробноеПредставлениеОшибки(ИнформацияОбОшибке());
			Результат.Вставить(ОбъектИС, РезультатПоОбъекту);
			Продолжить;
		КонецПопытки;
		
		СтруктураСозданияОбъекта.dataLoadingRule = ПравилоЗагрузкиВДО;
		
		Если ПредварительныеДанные.ДоступенФункционалФайлов Тогда
			
			ПечатныеФормы = СтруктураСозданияОбъекта.files; // СписокXDTO
			
			ПечатныеФормыКСозданию = ИнтеграцияС1СДокументооборотБазоваяФункциональность.ПечатныеФормыКСозданию(
				ОбъектИС,
				ПрисоединяемыеПечатныеФормы(ПредварительныеДанные.СоответствиеПечатныхФормОбъектамИС[ОбъектИС]),
				Перечисления.ТипыФайловСохраненияПечатныхФормОбъектов[ПредварительныеДанные.ТипыФайловПечатныхФорм[ОбъектИС]],
				КонтрольОтправкиФайлов);
			
			Для Каждого ПараметрыСоздания Из ПечатныеФормыКСозданию Цикл
				ФайлXDTO = ИнтеграцияС1СДокументооборотБазоваяФункциональность.ФайлXDTOИзПараметровСоздания(
					Прокси,
					ПараметрыСоздания);
				ПечатныеФормы.Добавить(ФайлXDTO);
			КонецЦикла;
			
		КонецЕсли;
		
		Если ПредварительныеДанные.ДоступенФункционалСозданияСвязей Тогда
			ПеренестиСвязиКСозданиюВСтруктуруВходящихДанных(
				Прокси,
				ПредварительныеДанные.СоответствиеСоздаваемыхСвязейОбъектамИС[ОбъектИС],
				СтруктураСозданияОбъекта);
		КонецЕсли;
		
		Запрос.objects.Добавить(СтруктураСозданияОбъекта);
		
		Результат.Вставить(ОбъектИС, РезультатПоОбъекту);
		
	КонецЦикла;
	
	Если Запрос.objects.Количество() = 0 Тогда
		Возврат Результат;
	КонецЕсли;
	
	Ответ = ИнтеграцияС1СДокументооборотБазоваяФункциональность.ВыполнитьЗапрос(Прокси, Запрос);
	ИнтеграцияС1СДокументооборотБазоваяФункциональность.ПроверитьВозвратВебСервиса(Прокси, Ответ);
	
	Для Каждого Строка Из КонтрольОтправкиФайлов Цикл
		РегистрыСведений.КонтрольОтправкиФайловВ1СДокументооборот.СохранитьХешСуммуВерсииФайла(
			Строка.Источник,
			Строка.ИмяФайла,
			Строка.ТабличныйДокумент)
	КонецЦикла;
	
	ШтрихкодУстановленПриСоздании =
		ИнтеграцияС1СДокументооборотБазоваяФункциональностьПовтИсп.ДоступенФункционалВерсииСервиса("3.0.19.29");
	
	Для Каждого СтруктураОтвета Из Ответ.objects Цикл
		ОбъектИС = ИнтеграцияС1СДокументооборотБазоваяФункциональность.СсылкаИзUUID(
			СтруктураОтвета.key.type,
			СтруктураОтвета.key.ID);
		Если СтруктураОтвета.success Тогда
			
			ОбъектXDTO = СтруктураОтвета.object;
			Результат[ОбъектИС].ОбъектXDTO = ОбъектXDTO;
			
			НаличиеПрисоединенныхФайлов = Ложь; // Доп. запрос к ДО делать не требуется.
			Если ИнтеграцияС1СДокументооборотБазоваяФункциональность.СвойствоУстановлено(ОбъектXDTO, "files") Тогда
				НаличиеПрисоединенныхФайлов = (ОбъектXDTO.files.Количество() > 0);
			КонецЕсли;
			ИнтеграцияС1СДокументооборотБазоваяФункциональностьВызовСервера.ДобавитьСвязь(
				ОбъектXDTO.objectID.ID,
				ОбъектXDTO.objectID.type,
				ОбъектИС,
				НаличиеПрисоединенныхФайлов,
				Ложь);
			
			Если Не ШтрихкодУстановленПриСоздании Тогда
				ИнтеграцияС1СДокументооборотБазоваяФункциональность.УстановитьШтрихкод(
					ОбъектИС,
					ОбъектXDTO.objectID.ID,
					ОбъектXDTO.objectID.type);
			КонецЕсли;
			
		Иначе
			Результат[ОбъектИС].Успешно = Ложь;
			Результат[ОбъектИС].ТекстОшибки =
				ИнтеграцияС1СДокументооборотБазоваяФункциональностьКлиентСервер.УдалитьТехИнформациюИзТекстаОшибки(
					СтруктураОтвета.error.description);
			ИнтеграцияС1СДокументооборотБазоваяФункциональность.ЗаписатьОшибку(СтруктураОтвета.error.description);
		КонецЕсли;
	КонецЦикла;
	
	Возврат Результат;
	
КонецФункции

// Находит на стороне 1С:Документооборот правила загрузки данных в 1С:Документооборот, соответствующие
// переданным объектам интегрированной системы.
//
// Параметры:
//   Прокси - WSПрокси - объект для подключения к web-сервисам Документооборота.
//   СписокОбъектовИС - Массив из ЛюбаяСсылка - объекты ИС, для которых требуется найти соответствующие правила.
//   ПодходящиеПравилаИнтеграции - Соответствие из КлючИЗначение - соответствующие правила интеграции:
//     * Ключ - ОпределяемыйТип.ИнтеграцияС1СДокументооборотВсеСсылкиПереопределяемый - объект ИС.
//     * Значение - Массив из СправочникСсылка.ПравилаИнтеграцииС1СДокументооборотом3 - список подходящих правил.
//
// Возвращаемое значение:
//   Соответствие из КлючИЗначение:
//     * Ключ - ОпределяемыйТип.ИнтеграцияС1СДокументооборотВсеСсылкиПереопределяемый - объект ИС.
//     * Значение - Строка - строковое представление объекта XDTO типа DMDataLoadingRule.
//                - Неопределено - если подходящее правило загрузки не было найдено.
//
Функция СоответствиеПравилЗагрузкиВДООбъектамИС(Прокси, СписокОбъектовИС, ПодходящиеПравилаИнтеграции) Экспорт
	
	Результат = Новый Соответствие;
	
	Если СписокОбъектовИС.Количество() = 0 Тогда
		Возврат Результат;
	КонецЕсли;
	
	ВсеПравила = Новый Массив;
	Для Каждого СоответствиеПравил Из ПодходящиеПравилаИнтеграции Цикл
		Для Каждого Правило Из СоответствиеПравил.Значение Цикл
			Если ВсеПравила.Найти(Правило) = Неопределено Тогда
				ВсеПравила.Добавить(Правило);
			КонецЕсли;
		КонецЦикла;
	КонецЦикла;
	СсылкиНаПравилаЗагрузкиВДО = ОбщегоНазначения.ЗначениеРеквизитаОбъектов(ВсеПравила, "ПравилоЗагрузкиДанныхВДО");
	
	Для Каждого ОбъектИС Из СписокОбъектовИС Цикл
		ПравилоЗагрузкиВДО = Неопределено;
		Если ПодходящиеПравилаИнтеграции[ОбъектИС] <> Неопределено
				И ПодходящиеПравилаИнтеграции[ОбъектИС].Количество() = 1 Тогда
			ПравилоИнтеграции = ПодходящиеПравилаИнтеграции[ОбъектИС][0];
			ПравилоЗагрузкиВДОXDTO = ИнтеграцияС1СДокументооборотБазоваяФункциональность.СоздатьObjectID(
				Прокси,
				СсылкиНаПравилаЗагрузкиВДО[ПравилоИнтеграции],
				"DMDataLoadingRule");
			ПравилоЗагрузкиВДО = ИнтеграцияС1СДокументооборотБазоваяФункциональность.ОбъектXDTOВСтроку(
				Прокси,
				ПравилоЗагрузкиВДОXDTO);
		КонецЕсли;
		Результат.Вставить(ОбъектИС, ПравилоЗагрузкиВДО);
	КонецЦикла;
	
	Возврат Результат;
	
КонецФункции

// Устанавливает условное оформление таблицы "Быстрый поиск".
//
// Параметры:
//   УсловноеОформление - УсловноеОформлениеКомпоновкиДанных.
//   ТаблицаБыстрыйПоиск - ТаблицаФормы - таблица "Быстрый поиск".
//   ЯчейкаЗначение - ПолеФормы - ячейка "Значение" таблицы "Быстрый поиск".
//
Процедура УстановитьУсловноеОформлениеТаблицыБыстрыйПоиск(УсловноеОформление, ТаблицаБыстрыйПоиск, ЯчейкаЗначение) Экспорт
	
	// Пустой текст значения.
	Элемент = УсловноеОформление.Элементы.Добавить();
	
	Поля = Элемент.Поля.Элементы;
	Поля.Добавить().Поле = Новый ПолеКомпоновкиДанных(ЯчейкаЗначение.Имя);
	
	ОтборЭлемента = Элемент.Отбор.Элементы.Добавить(Тип("ЭлементОтбораКомпоновкиДанных"));
	ОтборЭлемента.ЛевоеЗначение = Новый ПолеКомпоновкиДанных(ТаблицаБыстрыйПоиск.Имя + ".Значение");
	ОтборЭлемента.ВидСравнения = ВидСравненияКомпоновкиДанных.Заполнено;
	
	Элемент.Оформление.УстановитьЗначениеПараметра("Текст", "");
	
	// Фиксированная ячейка.
	Элемент = УсловноеОформление.Элементы.Добавить();
	
	Поля = Элемент.Поля.Элементы;
	Поля.Добавить().Поле = Новый ПолеКомпоновкиДанных(ТаблицаБыстрыйПоиск.Имя);
	
	ОтборЭлемента = Элемент.Отбор.Элементы.Добавить(Тип("ЭлементОтбораКомпоновкиДанных"));
	ОтборЭлемента.ЛевоеЗначение = Новый ПолеКомпоновкиДанных(ТаблицаБыстрыйПоиск.Имя + ".Фиксированный");
	ОтборЭлемента.ВидСравнения = ВидСравненияКомпоновкиДанных.Равно;
	ОтборЭлемента.ПравоеЗначение = Истина;
	
	Элемент.Оформление.УстановитьЗначениеПараметра("ЦветФона", ЦветаСтиля.ЦветФонаВсеПоля);
	Элемент.Оформление.УстановитьЗначениеПараметра("ТолькоПросмотр", Истина);
	
КонецПроцедуры

