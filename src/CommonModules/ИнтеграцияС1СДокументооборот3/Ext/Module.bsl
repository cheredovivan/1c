#Область РаботаСPDF

// Проверяет и добавляет программно реквизит "ДокументPDFРеквизит" и поле "ДокументPDFПоле" на форму.
//
// Параметры:
//   Форма - ФормаКлиентскогоПриложения - необходимая форма.
//   ИмяРодителя - Строка - имя родителя элемента "ДокументPDFПоле".
//
Процедура ВывестиPDFДокументНаФорму(Форма, ИмяРодителя) Экспорт
	
	Элементы = Форма.Элементы;
	Если Элементы.Найти("ДокументPDFПоле") <> Неопределено Тогда
		// ДокументPDFПоле уже есть на форме.
		Возврат;
	КонецЕсли;
	
	РеквизитДокументPDF = ИнтеграцияС1СДокументооборотБазоваяФункциональность.РеквизитФормыПоИмени(
		Форма.ПолучитьРеквизиты(),
		"ДокументPDFРеквизит");
	Если РеквизитДокументPDF = Неопределено Тогда
		ДобавляемыеРеквизиты = Новый Массив;
		ДобавляемыеРеквизиты.Добавить(Новый РеквизитФормы("ДокументPDFРеквизит", Новый ОписаниеТипов("ДокументPDF")));
		Форма.ИзменитьРеквизиты(ДобавляемыеРеквизиты);
	КонецЕсли;
	
	ГруппаКоманд = Элементы.Найти("ГруппаОбзорФайлаКоманды");
	Если ГруппаКоманд <> Неопределено
			И ГруппаКоманд.Группировка <> ГруппировкаПодчиненныхЭлементовФормы.Вертикальная Тогда
		// Только вертикальная группа команд должна располагаться правее поля PDF.
		ГруппаКоманд = Неопределено;
	КонецЕсли;
	
	ПолеPDF = Элементы.Вставить("ДокументPDFПоле", Тип("ПолеФормы"), Элементы.Найти(ИмяРодителя), ГруппаКоманд);
	ПолеPDF.Вид = ВидПоляФормы.ПолеPDFДокумента;
	ПолеPDF.ПутьКДанным = "ДокументPDFРеквизит";
	ПолеPDF.ПоложениеЗаголовка = ПоложениеЗаголовкаЭлементаФормы.Нет;
	ПолеPDF.ОтображениеПодсказки = ОтображениеПодсказки.Нет;
	
КонецПроцедуры

#КонецОбласти

#Область ОбновлениеИнформационнойБазы

// Обработчик обновления БИД на версию 3.0.2.5. Замена идентификатора вида действия для заполнения состояния документа.
//
Процедура ПерейтиНаВерсию_3_0_2_5() Экспорт
	
	ЗапросПравила = Новый Запрос(
		"ВЫБРАТЬ РАЗЛИЧНЫЕ
		|	ПравилаИнтеграцииС1СДокументооборотом3ПравилаЗаполненияРеквизитовИС.Ссылка КАК Ссылка
		|ИЗ
		|	Справочник.ПравилаИнтеграцииС1СДокументооборотом3.ПравилаЗаполненияРеквизитовИС КАК ПравилаИнтеграцииС1СДокументооборотом3ПравилаЗаполненияРеквизитовИС
		|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.ПравилаИнтеграцииС1СДокументооборотом3 КАК ПравилаИнтеграцииС1СДокументооборотом3
		|		ПО ПравилаИнтеграцииС1СДокументооборотом3ПравилаЗаполненияРеквизитовИС.Ссылка = ПравилаИнтеграцииС1СДокументооборотом3.Ссылка
		|ГДЕ
		|	НЕ ПравилаИнтеграцииС1СДокументооборотом3.ПометкаУдаления
		|	И (ВЫРАЗИТЬ(ПравилаИнтеграцииС1СДокументооборотом3ПравилаЗаполненияРеквизитовИС.ИмяРеквизитаОбъектаДО КАК СТРОКА(7))) = ""status_""");
	ВыборкаПравила = ЗапросПравила.Выполнить().Выбрать();
	
	Пока ВыборкаПравила.Следующий() Цикл
		
		ПравилоОбъект = ВыборкаПравила.Ссылка.ПолучитьОбъект(); // СправочникОбъект.ПравилаИнтеграцииС1СДокументооборотом3
		Попытка
			ПравилоОбъект.Заблокировать();
		Исключение
			ЗаписьЖурналаРегистрации(
				ИнтеграцияС1СДокументооборотБазоваяФункциональность.ИмяСобытияЖурналаРегистрации(),
				УровеньЖурналаРегистрации.Ошибка,,,
				ОбработкаОшибок.ПодробноеПредставлениеОшибки(ИнформацияОбОшибке()));
			Продолжить;
		КонецПопытки;
		
		Для Каждого Строка Из ПравилоОбъект.ПравилаЗаполненияРеквизитовИС Цикл
			Если СтрНайти(Строка.ИмяРеквизитаОбъектаДО, "status_") = 1 Тогда
				Строка.ИмяРеквизитаОбъектаДО = СтрЗаменить(Строка.ИмяРеквизитаОбъектаДО, "-", "_");
			КонецЕсли;
		КонецЦикла;
		
		ОбновлениеИнформационнойБазы.ЗаписатьДанные(ПравилоОбъект);
		
	КонецЦикла;
	
КонецПроцедуры

#КонецОбласти

#Область Холдинг

// Возвращает идентификатор приоритетного узла пользователя.
//
// Параметры:
//   Пользователь - ОпределяемыйТип.Пользователь - ссылка на пользователя.
//   ВсеУзлыXDTO - СписокXDTO - список объектов XDTO типа DMDataExchangeNode.
//
// Возвращаемое значение:
//   Строка - идентификатор узла.
//
Функция ПриоритетныйУзел(Пользователь, ВсеУзлыXDTO) Экспорт
	
	ДанныеУзлов = РегистрыСведений.ДанныеУзлов1СДокументооборота.ДанныеУзлов();
	ДоступныйУзел = ИнтеграцияС1СДокументооборотБазоваяФункциональность.УзелДляАвторизацииВДО(Пользователь);
	
	УзлыПользователя = Новый ТаблицаЗначений;
	УзлыПользователя.Колонки.Добавить("Узел", Новый ОписаниеТипов("Строка"));
	УзлыПользователя.Колонки.Добавить("ИдентификаторУзла", Новый ОписаниеТипов("Строка"));
	УзлыПользователя.Колонки.Добавить("ЦентральныйУзел", Новый ОписаниеТипов("Булево"));
	УзлыПользователя.Колонки.Добавить("ЕстьДоступ", Новый ОписаниеТипов("Булево"));
	
	Для Каждого УзелXDTO Из ВсеУзлыXDTO Цикл
		ИдентификаторУзла = УзелXDTO.objectID.ID;
		ЭтоЦентральныйУзел = ИнтеграцияС1СДокументооборотБазоваяФункциональность.СвойствоУстановлено(УзелXDTO, "isCentral")
			И УзелXDTO.isCentral;
		Если Не ЭтоЦентральныйУзел
				И (ДанныеУзлов[ИдентификаторУзла] = Неопределено
					Или Не ЗначениеЗаполнено(ДанныеУзлов[ИдентификаторУзла].АдресВебСервиса)) Тогда
			Продолжить;
		КонецЕсли;
		НоваяСтрока = УзлыПользователя.Добавить();
		НоваяСтрока.Узел = УзелXDTO.name;
		НоваяСтрока.ИдентификаторУзла = ИдентификаторУзла;
		НоваяСтрока.ЦентральныйУзел = ЭтоЦентральныйУзел;
		НоваяСтрока.ЕстьДоступ = (ИдентификаторУзла = ДоступныйУзел);
	КонецЦикла;
	
	Если УзлыПользователя.Количество() > 0 Тогда
		УзлыПользователя.Сортировать("ЕстьДоступ УБЫВ, ЦентральныйУзел УБЫВ, Узел");
		УзелПользователя = УзлыПользователя[0];
		Если УзелПользователя.ЦентральныйУзел Тогда
			// Если пользователь есть в центральном узле - оставим параметр сеанса ИнтеграцияС1СДокументооборотУзел
			// пустым. Все обращения к сервису ДО будем выполнять по данным константы АдресВебСервиса1СДокументооборот.
			Возврат "";
		Иначе
			Возврат УзелПользователя.ИдентификаторУзла;
		КонецЕсли;
	Иначе
		// Данные узлов пользователя не заданы. Оставим параметр сеанса ИнтеграцияС1СДокументооборотУзел пустым.
		// Все обращения к сервису ДО будем выполнять по данным константы АдресВебСервиса1СДокументооборот.
		Возврат "";
	КонецЕсли;
	
КонецФункции

// Возвращает узел 1С:Документооборот Холдинг, которому соответствует текущий пользователь.
//
// Возвращаемое значение:
//   Строка - идентификатор узла.
//
Функция УзелТекущегоПользователя() Экспорт
	
	Если Не РегистрыСведений.ДанныеУзлов1СДокументооборота.ИспользуютсяУзлыХолдинга() Тогда
		Возврат "";
	КонецЕсли;
	
	Пользователь = Пользователи.ТекущийПользователь();
	
	НазначенныйУзел = РегистрыСведений.СоответствиеПользователейУзлам1СДокументооборота.УзелПользователя(Пользователь);
	Если ЗначениеЗаполнено(НазначенныйУзел) Тогда
		Возврат НазначенныйУзел;
	КонецЕсли;
	
	РеквизитыПользователя = ОбщегоНазначения.ЗначенияРеквизитовОбъекта(
		Пользователь, "Служебный, Недействителен, ПометкаУдаления");
	Если РеквизитыПользователя.Служебный = Истина
			Или РеквизитыПользователя.Недействителен = Истина
			Или РеквизитыПользователя.ПометкаУдаления = Истина Тогда
		// Служебный или недействительный пользователь должен обращаться в ЦУ.
		Возврат "";
	КонецЕсли;
	
	// Если по данным ИБ узел определить не удалось - требуется сделать запрос в ЦУ для определения списка узлов,
	// где есть пользователь, и для выбора приоритетного узла.
	
	Попытка
		
		НастройкиАвторизации =
			ИнтеграцияС1СДокументооборотБазоваяФункциональностьКлиентСервер.НастройкиАвторизацииВ1СДокументообороте();
		
		УстановитьПривилегированныйРежим(Истина);
		НастройкиАвторизации.ИмяПользователя = Константы.ИнтеграцияС1СДокументооборотИмяПользователяДляОбмена.Получить();
		Если Не ЗначениеЗаполнено(НастройкиАвторизации.ИмяПользователя) Тогда
			ВызватьИсключение
				НСтр("ru = 'Не установлено значение константы ""Имя пользователя для обмена с 1С:Документооборотом""';
					|en = 'The ""Username for exchange with 1C:Document Management"" constant value is not set.'");
		КонецЕсли;
		НастройкиАвторизации.Пароль = Константы.ИнтеграцияС1СДокументооборотПарольДляОбмена.Получить();
		УстановитьПривилегированныйРежим(Ложь);
		
		ВызыватьИсключение = Истина;
		ЭтоПользовательЗаданияОбмена = Истина;
		Прокси = ИнтеграцияС1СДокументооборотБазоваяФункциональность.ПолучитьПрокси(
			ВызыватьИсключение,
			НастройкиАвторизации,
			ИнтеграцияС1СДокументооборотБазоваяФункциональность.АдресВебСервиса1СДокументооборот(Истина),,
			ЭтоПользовательЗаданияОбмена);
		Запрос = ИнтеграцияС1СДокументооборотБазоваяФункциональность.СоздатьОбъект(Прокси, "DMGetUserNodesRequest");
		
		Запрос.user = ИнтеграцияС1СДокументооборотБазоваяФункциональность.СоздатьОбъект(Прокси, "DMUser", Пользователь);
		
		Ответ = ИнтеграцияС1СДокументооборотБазоваяФункциональность.ВыполнитьЗапрос(Прокси, Запрос);
		ИнтеграцияС1СДокументооборотБазоваяФункциональность.ПроверитьВозвратВебСервиса(Прокси, Ответ);
		
		Возврат ПриоритетныйУзел(Пользователь, Ответ.nodes);
		
	Исключение
		
		ИнтеграцияС1СДокументооборотБазоваяФункциональность.ЗаписатьОшибку(
			ОбработкаОшибок.ПодробноеПредставлениеОшибки(ИнформацияОбОшибке()));
		Возврат "";
		
	КонецПопытки;
	
КонецФункции

// Устанавливает условное оформление дерева узлов 1С:Документооборота Холдинга.
//
// Параметры:
//   Форма - ФормаКлиентскогоПриложения - форма, содержащая дерево узлов.
//
Процедура УстановитьУсловноеОформлениеУзловХолдинга(Форма) Экспорт
	
	// Центральный узел - жирным шрифтом.
	ЭлементОформления = Форма.УсловноеОформление.Элементы.Добавить();
	
	ПолеЭлемента = ЭлементОформления.Поля.Элементы.Добавить();
	ПолеЭлемента.Поле = Новый ПолеКомпоновкиДанных(Форма.Элементы.УзлыКОД.Имя);
	
	ЭлементОтбора = ЭлементОформления.Отбор.Элементы.Добавить(Тип("ЭлементОтбораКомпоновкиДанных"));
	ЭлементОтбора.ЛевоеЗначение = Новый ПолеКомпоновкиДанных("УзлыКОД.ЦентральныйУзел");
	ЭлементОтбора.ВидСравнения = ВидСравненияКомпоновкиДанных.Равно;
	ЭлементОтбора.ПравоеЗначение = Истина;
	
	ЭлементОформления.Оформление.УстановитьЗначениеПараметра("Шрифт", ШрифтыСтиля.ВажнаяНадписьШрифт);
	
	// Не активный узел - серым цветом.
	ЭлементОформления = Форма.УсловноеОформление.Элементы.Добавить();
	
	ПолеЭлемента = ЭлементОформления.Поля.Элементы.Добавить();
	ПолеЭлемента.Поле = Новый ПолеКомпоновкиДанных(Форма.Элементы.УзлыКОД.Имя);
	
	ЭлементОтбора = ЭлементОформления.Отбор.Элементы.Добавить(Тип("ЭлементОтбораКомпоновкиДанных"));
	ЭлементОтбора.ЛевоеЗначение = Новый ПолеКомпоновкиДанных("УзлыКОД.Активен");
	ЭлементОтбора.ВидСравнения = ВидСравненияКомпоновкиДанных.Равно;
	ЭлементОтбора.ПравоеЗначение = 1;
	
	ЭлементОформления.Оформление.УстановитьЗначениеПараметра("ЦветТекста", ЦветаСтиля.НедоступныеДанныеЦвет);
	
КонецПроцедуры

