#Область СлужебныйПрограммныйИнтерфейс

// Создает структуру настроек подключения к сервису.
//
// Возвращаемое значение:
//  Структура - настройки подключения:
//   * НастройкиПроксиСервера - см. ПолучениеФайловИзИнтернета.НастройкиПроксиНаСервере
// 
Функция ИнициализироватьПараметрыПодключения() Экспорт
	
	ПараметрыОбмена = Новый Структура;
	ПараметрыОбмена.Вставить("НастройкиПроксиСервера", ПолучениеФайловИзИнтернета.НастройкиПроксиНаСервере());
	
	//@skip-check constructor-function-return-section
	Возврат ПараметрыОбмена;
	
КонецФункции

#КонецОбласти
