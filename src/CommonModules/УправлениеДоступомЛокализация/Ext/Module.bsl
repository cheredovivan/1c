	РаботаСОтчетностью = Ложь;
	
	ИндексРоли = ОписаниеПрофиля.Роли.Найти("ДобавлениеИзменениеРазделяемыхДанныхРегламентированнойОтчетности");
	Если ИндексРоли <> Неопределено Тогда
		ОписаниеПрофиля.Роли.Добавить("ДобавлениеИзменениеДокументаРегламентированныйОтчет");
		РаботаСОтчетностью = Истина;
	КонецЕсли;
	
	ИндексРоли = ОписаниеПрофиля.Роли.Найти("ЧтениеДанныхРегламентированнойОтчетности");
	Если ИндексРоли <> Неопределено Тогда
		ОписаниеПрофиля.Роли.Добавить("ЧтениеРегламентированнойОтчетности");
		РаботаСОтчетностью = Истина;
	КонецЕсли;
	
	Если РаботаСОтчетностью Тогда
		ОписаниеПрофиля.ВидыДоступа.Добавить("СведенияРегламентированныхОтчетов", "Предустановленный");
		Если ОписаниеПрофиля.Имя = "Бухгалтер" Тогда
			ОписаниеПрофиля.ЗначенияДоступа.Добавить("СведенияРегламентированныхОтчетов", "Перечисление.ТипыСведенийРегламентированныхОтчетов.ПустаяСсылка");
