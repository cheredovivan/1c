			|ГДЕ
			|	ЗначениеРазрешено(Организация)
			|	И ЗначениеРазрешено(Субъект)";
		ИначеЕсли Список = Метаданные.РегистрыСведений["СубъектыДляСкрытияПерсональныхДанных"] Тогда
			Ограничение.Текст =
			"РазрешитьЧтениеИзменение
			|ГДЕ
			|	ЗначениеРазрешено(Субъект)";
		КонецЕсли;
	КонецЕсли;
	//++ НЕ УТ
	ЗарплатаКадры.ПриЗаполненииОграниченияДоступа(Список, Ограничение);
	Если Список = Метаданные.Справочники.ФизическиеЛица Тогда
		Ограничение.Текст =
		"РазрешитьЧтениеИзменение
		|ГДЕ
		|	ЭтоГруппа
		|	ИЛИ ЗначениеРазрешено(Ссылка)";
	ИначеЕсли Список = Метаданные.Документы.РегламентированныйОтчет Тогда
		Ограничение.Текст =
		"ПрисоединитьДополнительныеТаблицы
		|ЭтотСписок КАК Т
		|ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СведенияРегламентированныхОтчетов КАК Т1
		|	ПО Т.ИсточникОтчета = Т1.ИмяОтчета
		|;
		|
		|РазрешитьЧтениеИзменение
		|ГДЕ
		|	ЗначениеРазрешено(Т.Организация)
		|	И ДляВсехСтрок(ЗначениеРазрешено(
		|		ЕСТЬNULL(Т1.ТипСведений, ЗНАЧЕНИЕ(Перечисление.ТипыСведенийРегламентированныхОтчетов.ПустаяСсылка)),
		|		NULL КАК ИСТИНА))";
	ИначеЕсли Список = Метаданные.Справочники.РегламентированныеОтчеты Тогда
		Ограничение.Текст =
		"ПрисоединитьДополнительныеТаблицы
		|ЭтотСписок КАК Т
		|ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СведенияРегламентированныхОтчетов КАК Т1
		|	ПО Т.Ссылка = Т1.РегламентированныйОтчет
		|;
		|
		|РазрешитьЧтениеИзменение
		|ГДЕ
		|	ЭтоГруппа ИЛИ
		|	ДляВсехСтрок(ЗначениеРазрешено(
		|		ЕСТЬNULL(Т1.ТипСведений, ЗНАЧЕНИЕ(Перечисление.ТипыСведенийРегламентированныхОтчетов.ПустаяСсылка)),
		|		NULL КАК ИСТИНА))";
	КонецЕсли;
	//-- НЕ УТ

	//-- Локализация
	
КонецПроцедуры

