#Область СлужебныйПрограммныйИнтерфейс

// Получает составные имена для полей выборки из регистра РаспоряженияНаОтгрузкуИВозврат
// по имени показателя, имени ресурса и признаку таблицы оборотов.
//
// Параметры:
// 	ИмяПоказателя - Строка
// 	ИмяРесурса - Строка
// 	Оборот - Булево.
//
// Возвращаемое значение:
// 	Структура:
// 		* ИмяПоляТаблицы - Строка
// 		* ИмяПоляВыборки - Строка.
//
Функция ИменаПолейВыборки(ИмяПоказателя, ИмяРесурса, Оборот) Экспорт
	
	Результат = Новый Структура();
	Результат.Вставить("ИмяПоляТаблицы", "");
	Результат.Вставить("ИмяПоляВыборки", "");
	
	ТаблицаСоответствия = РаботаСРаспоряжениямиПовтИсп.ТаблицаСоответствияИменПолейВыборки();
	
	ПараметрыПоиска = Новый Структура();
	ПараметрыПоиска.Вставить("ИмяПоказателя", ИмяПоказателя);
	ПараметрыПоиска.Вставить("ИмяРесурса", ИмяРесурса);
	ПараметрыПоиска.Вставить("Оборот", Оборот);
	
	ПоискСтрок = ТаблицаСоответствия.НайтиСтроки(ПараметрыПоиска);
	Если ПоискСтрок.Количество() > 0 Тогда
		
		СтрокаИменПолей = ПоискСтрок[0];
		
		Результат.ИмяПоляТаблицы = СтрокаИменПолей.ИмяПоляТаблицы;
		Результат.ИмяПоляВыборки = СтрокаИменПолей.ИмяПоляВыборки;
		
	Иначе
		
		ТекстИсключения = СтрШаблон(НСтр("ru = 'Ошибка вычисления имен полей при попытке получения данных по распоряжениям.
			|Показатель: ""%1""
			|Ресурс: ""%2""
			|Оборот %3';
			|en = 'Error calculating field names when trying to receive data on references.
			|Indicator: ""%1""
			|Resource: ""%2""
			|%3 turnover'"),
			ИмяПоказателя,
			ИмяРесурса,
			Оборот);
		
		ВызватьИсключение ТекстИсключения;
		
	КонецЕсли;
	
	Возврат Результат;
	
КонецФункции

