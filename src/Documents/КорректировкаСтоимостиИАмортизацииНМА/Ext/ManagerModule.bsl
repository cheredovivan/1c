			ДанныеСтроки.АмортизацияНаНачалоГодаБУ = 0;
			ДанныеСтроки.АмортизацияНаНачалоГодаНУ = 0;
			ДанныеСтроки.АмортизацияНаНачалоГодаПР = 0;
