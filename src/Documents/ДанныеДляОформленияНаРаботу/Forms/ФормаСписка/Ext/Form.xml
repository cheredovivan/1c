					<dcssch:field>Ссылка</dcssch:field>
				</Field>
				<Field xsi:type="dcssch:DataSetFieldField">
					<dcssch:dataPath>АдресМестаПроживанияПредставление</dcssch:dataPath>
					<dcssch:field>АдресМестаПроживанияПредставление</dcssch:field>
