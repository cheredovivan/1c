	КонецЕсли;	
	
	Подпись = ОбменСГИСЭПД.ПредставлениеПодписи(ДанныеЗаполнения, КлючПодписи);	
	Если ЗначениеЗаполнено(Подпись) Тогда
		Результат.Вставить(ИмяКлючаРезультата, Подпись);
	КонецЕсли;
	
