Процедура ЗначениеДобавитьВМассив(ДанныеЗаполнения, МассивДляЗаполнения, ИмяКлюча, ТекстЗначения = "", ФорматЗначения = Неопределено)
	
	ЗначениеПоиска = ОбменСГИСЭПД.ПолучитьЗначениеСтруктуры(ДанныеЗаполнения, ИмяКлюча);
	
	Если Не ЗначениеЗаполнено(ЗначениеПоиска) Тогда
		Возврат;
	КонецЕсли;
	
	Если ЗначениеЗаполнено(ФорматЗначения) Тогда
		ЗначениеПоиска = Формат(ЗначениеПоиска, ФорматЗначения);	
	КонецЕсли;
	
	ЗначениеПоискаСтрокой = ТекстЗначения + ЗначениеПоиска;
	МассивДляЗаполнения.Добавить(ЗначениеПоискаСтрокой);	
	
