		ЗначениеПоиска = Формат(ЗначениеПоиска, ФорматЗначения);	
	КонецЕсли;
	Результат.Вставить(ИмяКлючаРезультата, ЗначениеПоиска); 
	
КонецПроцедуры

Процедура ПодписьВРезультат(ДанныеЗаполнения, Результат, ИмяКлючаРезультата, КлючИДФайла, КлючПодписи)
	
	ИДФайл = ОбменСГИСЭПД.ПолучитьЗначениеСтруктуры(ДанныеЗаполнения, КлючИДФайла);
	Если Не ЗначениеЗаполнено(ИДФайл) Тогда
