			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>false</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	Таблица.Этап КАК ЭтапПотребитель
ИЗ
	(
		ВЫБРАТЬ
			Таблица.Этап КАК Этап
		ИЗ
			РегистрНакопления.ОбеспечениеПроизводственныхПроцессов КАК Таблица
		ГДЕ
			Распоряжение = &amp;Распоряжение
			
			И Номенклатура = &amp;Номенклатура
			И Характеристика = &amp;Характеристика
			
			И Назначение = &amp;Назначение
			И Этап &lt;&gt; &amp;ПустаяСсылкаЭтап
			
			И (Склад = &amp;Склад И Подразделение = &amp;Подразделение ИЛИ НЕ &amp;ОтборПоПолучателю)
			
			И Регистратор &lt;&gt; &amp;Регистратор
				И Активность
			
		СГРУППИРОВАТЬ ПО
			
			Таблица.Этап,
			Таблица.Назначение
		
		ИМЕЮЩИЕ
			СУММА(Таблица.Требуется) &gt; СУММА(Таблица.Запланировано)
											 + СУММА(Таблица.ПереданоКПроизводству)
											 + СУММА(Таблица.Производится)
											 + СУММА(Таблица.Произведено)
		
		ОБЪЕДИНИТЬ ВСЕ
		
		ВЫБРАТЬ
			Таблица.Этап КАК Этап
		ИЗ
			РегистрНакопления.ОбеспечениеПроизводственныхПроцессов КАК Таблица
		ГДЕ
			Распоряжение = &amp;Распоряжение
			
			И Номенклатура = &amp;Номенклатура
			И Характеристика = &amp;Характеристика
			
			И Назначение = &amp;Назначение
			
			И Этап &lt;&gt; &amp;ПустаяСсылкаЭтап
			
			И (Склад = &amp;Склад ИЛИ НЕ &amp;ОтборПоПолучателю)
			
			И Регистратор &lt;&gt; &amp;Регистратор
				И Активность
			
		СГРУППИРОВАТЬ ПО
			
			Таблица.Этап
		
		ИМЕЮЩИЕ
			СУММА(Таблица.ЗапланированаРазборка) &gt; ВЫБОР
														КОГДА СУММА(Таблица.ГотовКРазборке) &gt; СУММА(Таблица.ТребуетсяРазборка)
															ТОГДА
																СУММА(Таблица.ГотовКРазборке)
														ИНАЧЕ
															СУММА(Таблица.ТребуетсяРазборка)
														КОНЕЦ
		
		ОБЪЕДИНИТЬ ВСЕ
		
		ВЫБРАТЬ
			Таблица.Ссылка
		ИЗ
			Документ.ЭтапПроизводства2_2 КАК Таблица
		ГДЕ
			Таблица.Назначение = &amp;Назначение
		
	) КАК Таблица

	СГРУППИРОВАТЬ ПО
		
		Таблица.Этап</QueryText>
