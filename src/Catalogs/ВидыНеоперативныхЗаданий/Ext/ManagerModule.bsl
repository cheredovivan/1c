// Добавляет в список процедуры-обработчики обновления данных ИБ
// для всех поддерживаемых версий библиотеки или конфигурации.
// Вызывается перед началом обновления данных ИБ для построения плана обновления.
//
//  Обработчики - ТаблицаЗначений - описание полей, см. в процедуре
//                ОбновлениеИнформационнойБазы.НоваяТаблицаОбработчиковОбновления.
//
// Параметры:
// 	Обработчики - см. ОбновлениеИнформационнойБазы.НоваяТаблицаОбработчиковОбновления
//
Процедура ПриДобавленииОбработчиковОбновления(Обработчики) Экспорт

	Обработчик = Обработчики.Добавить();
	Обработчик.Процедура = "Справочники.ВидыНеоперативныхЗаданий.ЗаполнитьПредопределенныеНастройкиВидовНеоперативныхЗаданий";
	Обработчик.Версия = "*";
	Обработчик.РежимВыполнения = "Оперативно";
	Обработчик.Идентификатор = Новый УникальныйИдентификатор("cd45555d-f1f6-4a32-a02a-c00bf89351c1");
	Обработчик.Комментарий = НСтр("ru = 'Обновляет реквизиты предопределенных элементов справочника  ""Виды неоперативных заданий""';
									|en = 'Updates attributes of predefined items of the ""Backdate job types"" catalog'");
	Обработчик.ОбщиеДанные = Истина;
	
	Изменяемые = Новый Массив;
	Изменяемые.Добавить(Метаданные.Справочники.ВидыНеоперативныхЗаданий.ПолноеИмя());
	Обработчик.ИзменяемыеОбъекты = СтрСоединить(Изменяемые, ",");
	
КонецПроцедуры

Процедура ЗаполнитьПредопределенныеНастройкиВидовНеоперативныхЗаданий(Параметры = Неопределено) Экспорт
	
	// 1. Удаление элементов справочника, не являющихся предопределенными.
	Запрос = Новый Запрос;
	Запрос.Текст = "
	|ВЫБРАТЬ
	|	ВидыНеоперативныхЗаданий.Ссылка КАК Ссылка
	|ИЗ
	|	Справочник.ВидыНеоперативныхЗаданий КАК ВидыНеоперативныхЗаданий
	|ГДЕ
	|	НЕ ВидыНеоперативныхЗаданий.Предопределенный";
	Выборка = Запрос.Выполнить().Выбрать();
	Пока Выборка.Следующий() Цикл
		ВидНеоперативныхЗаданийОбъект = Выборка.Ссылка.ПолучитьОбъект();
		ВидНеоперативныхЗаданийОбъект.ОбменДанными.Загрузка = Истина;
		ВидНеоперативныхЗаданийОбъект.Удалить();
	КонецЦикла;
	
	// 2. Обработка предопределенных видов неоперативных заданий.
	МетаданныеОбъекта = Метаданные.Справочники.ВидыНеоперативныхЗаданий;
	НастройкиОбновления = МультиязычностьСервер.НастройкиОбновлениеПредопределенныхДанных(МетаданныеОбъекта);
	
	ТабличныеЧасти = Новый Массив;
	ТабличныеЧасти.Добавить("ЧитаемыеРегистры");
	ТабличныеЧасти.Добавить("ИзменяемыеРегистры");
	
	Для Каждого ДанныеЭлемента Из НастройкиОбновления.ПредопределенныеДанные Цикл
		
		ЭлементСсылка = Справочники.ВидыНеоперативныхЗаданий[ДанныеЭлемента.ИмяПредопределенныхДанных];
		ЭлементОбъект = ЭлементСсылка.ПолучитьОбъект();
		
		ЗаполнитьЗначенияСвойств(ЭлементОбъект, ДанныеЭлемента, ,
			"Ссылка, ПометкаУдаления, МаксимальнаяПорция, НомерОчереди");
		
		Если Не ЗначениеЗаполнено(ЭлементОбъект.МаксимальнаяПорция) Тогда
			ЭлементОбъект.МаксимальнаяПорция = ДанныеЭлемента.МаксимальнаяПорция;
		КонецЕсли;
		
		Для Каждого ИмяТабличнойЧасти Из ТабличныеЧасти Цикл
			ЭлементОбъект[ИмяТабличнойЧасти].Загрузить(ДанныеЭлемента[ИмяТабличнойЧасти]);
		КонецЦикла;
		
		Если ОбщегоНазначения.РазделениеВключено() Тогда
			
			Для Каждого ЛокализуемыйРеквизитОбъекта Из НастройкиОбновления.ЛокализуемыеРеквизитыОбъекта Цикл
				Имя = ЛокализуемыйРеквизитОбъекта.Ключ;
				ЭлементОбъект[Имя] = ДанныеЭлемента[Имя + "_" + НастройкиОбновления.ОсновнойЯзык];

				СведенияОбЯзыках = МультиязычностьСервер.СведенияОЯзыках();
				Для Каждого ЯзыкИспользуется Из СведенияОбЯзыках.Используется Цикл
					Если ЯзыкИспользуется.Значение Тогда
						КодЯзыка = СведенияОбЯзыках[ЯзыкИспользуется.Ключ];
						ЭлементОбъект[Имя + ЯзыкИспользуется.Ключ] = ДанныеЭлемента[Имя + "_" + КодЯзыка];
					КонецЕсли;
				КонецЦикла;
			КонецЦикла;
			
		КонецЕсли;
		
		ЭлементОбъект.ОбменДанными.Загрузка = Истина;
		ЭлементОбъект.Записать();
		
	КонецЦикла;
	
	ЗаполнитьНомерОчередиВидовНеоперативныхЗаданий();

КонецПроцедуры

Процедура ЗаполнитьНомерОчередиВидовНеоперативныхЗаданий()
	
	Запрос = Новый Запрос;
	Запрос.Текст = "
	|ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	ЧитаемыеРегистры.Ссылка КАК ВидЗадания,
	|	ЧитаемыеРегистры.Регистр КАК РегистрЧтение
	|ПОМЕСТИТЬ ВТ_РегистрыЧтение
	|ИЗ
	|	Справочник.ВидыНеоперативныхЗаданий.ЧитаемыеРегистры КАК ЧитаемыеРегистры
	|		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.ВидыНеоперативныхЗаданий.ИзменяемыеРегистры КАК ИзменяемыеРегистры
	|		ПО ЧитаемыеРегистры.Ссылка = ИзменяемыеРегистры.Ссылка
	|		И ИзменяемыеРегистры.Регистр = ЧитаемыеРегистры.Регистр
	|ГДЕ
	|	ИзменяемыеРегистры.Ссылка ЕСТЬ NULL
	|
	|ИНДЕКСИРОВАТЬ ПО
	|	РегистрЧтение,
	|	ВидЗадания
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	ВидыНеоперативныхЗаданийИзменяемыеРегистры.Ссылка КАК ВидЗадания,
	|	ВидыНеоперативныхЗаданийИзменяемыеРегистры.Регистр КАК РегистрЗапись
	|ПОМЕСТИТЬ ВТ_РегистрыЗапись
	|ИЗ
	|	Справочник.ВидыНеоперативныхЗаданий.ИзменяемыеРегистры КАК ВидыНеоперативныхЗаданийИзменяемыеРегистры
	|
	|ИНДЕКСИРОВАТЬ ПО
	|	РегистрЗапись,
	|	ВидЗадания
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ВТ_РегистрыЧтение.РегистрЧтение КАК РегистрЧтениеЗависит,
	|	ВТ_РегистрыЧтение.ВидЗадания КАК ВидЗаданияЧтение,
	|	ВТ_РегистрыЗапись.ВидЗадания КАК ВидЗаданияЗапись
	|ПОМЕСТИТЬ ВТ_ЗависимостьРегистров
	|ИЗ
	|	ВТ_РегистрыЧтение КАК ВТ_РегистрыЧтение
	|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТ_РегистрыЗапись КАК ВТ_РегистрыЗапись
	|		ПО ВТ_РегистрыЧтение.РегистрЧтение = ВТ_РегистрыЗапись.РегистрЗапись
	|		И ВТ_РегистрыЗапись.ВидЗадания <> ВТ_РегистрыЧтение.ВидЗадания
	|
	|ИНДЕКСИРОВАТЬ ПО
	|	ВидЗаданияЧтение,
	|	ВидЗаданияЗапись
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	ВТ_ЗависимостьРегистров.ВидЗаданияЧтение КАК ВидЗаданияЧтение,
	|	ВТ_ЗависимостьРегистров.ВидЗаданияЗапись КАК ВидЗаданияЗапись
	|ПОМЕСТИТЬ ВТ_ЗависимостьВидовЗаданий
	|ИЗ
	|	ВТ_ЗависимостьРегистров КАК ВТ_ЗависимостьРегистров
	|
	|ИНДЕКСИРОВАТЬ ПО
	|	ВидЗаданияЧтение,
	|	ВидЗаданияЗапись
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ЗависимостьЧтениеЗапись.ВидЗаданияЧтение.Наименование КАК ВидЗаданияЧтение,
	|	ЗависимостьЧтениеЗапись.ВидЗаданияЗапись.Наименование КАК ВидЗаданияЗапись,
	|	ЗависимостьЗаписьЧтение.ВидЗаданияЧтение КАК Конфликт
	|ИЗ
	|	ВТ_ЗависимостьВидовЗаданий КАК ЗависимостьЗаписьЧтение
	|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТ_ЗависимостьВидовЗаданий КАК ЗависимостьЧтениеЗапись
	|		ПО ЗависимостьЧтениеЗапись.ВидЗаданияЗапись = ЗависимостьЗаписьЧтение.ВидЗаданияЧтение
	|		И ЗависимостьЧтениеЗапись.ВидЗаданияЧтение = ЗависимостьЗаписьЧтение.ВидЗаданияЗапись
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ВТ_ЗависимостьВидовЗаданий.ВидЗаданияЧтение КАК ВидЗаданияЧтение,
	|	ВТ_ЗависимостьВидовЗаданий.ВидЗаданияЗапись КАК ВидЗаданияЗапись
	|ИЗ
	|	ВТ_ЗависимостьВидовЗаданий КАК ВТ_ЗависимостьВидовЗаданий
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ВидыНеоперативныхЗаданий.Ссылка КАК Ссылка,
	|	ВидыНеоперативныхЗаданий.НомерОчереди КАК НомерОчереди
	|ИЗ
	|	Справочник.ВидыНеоперативныхЗаданий КАК ВидыНеоперативныхЗаданий
	|ГДЕ
	|	НЕ ВидыНеоперативныхЗаданий.Ссылка В (&ИсключаемыеВидыЗаданий)";
	
	// Исключаемые виды заданий указаны в справочнике справочно.
	// В механизме неоперативной очереди по ним не должны регистрироваться и обрабатываться задания.
	ИсключаемыеВидыЗаданий = Новый Массив;
	
	//++ НЕ УТ
	
	//++ Локализация
	ИсключаемыеВидыЗаданий.Добавить(Справочники.ВидыНеоперативныхЗаданий.ОтражениеВРегламентированномУчете);
	//-- Локализация
	
	//-- НЕ УТ
	
	//++ НЕ УТКА
	ИсключаемыеВидыЗаданий.Добавить(Справочники.ВидыНеоперативныхЗаданий.ОтражениеВМеждународномУчете);
	//-- НЕ УТКА
	
	Запрос.УстановитьПараметр("ИсключаемыеВидыЗаданий", ИсключаемыеВидыЗаданий);
	
	ПакетыЗапросов = Запрос.ВыполнитьПакет();
	
	ЦиклическаяЗависимость = ПакетыЗапросов[4].Выгрузить();
	
	Если ЦиклическаяЗависимость.Количество() > 0 Тогда
		
		СписокОписаний = Новый Массив;
		СписокОписаний.Добавить(НСтр("ru = 'При определении номера очереди видов заданий обнаружены циклические зависимости:';
									|en = 'Circular dependencies are found when determining the job type queue number:'"));
		
		Для каждого Зависимость Из ЦиклическаяЗависимость Цикл
			
			СписокОписаний.Добавить(СтрШаблон(НСтр("ru = '- вид задания ""%1"" зависит от ""%2""';
													|en = '- the ""%1"" job type depends on ""%2""'"),
				Зависимость.ВидЗаданияЧтение, Зависимость.ВидЗаданияЗапись));
			
		КонецЦикла;
		
		ТекстИсключения = СтрСоединить(СписокОписаний, Символы.ПС);
		ВызватьИсключение ТекстИсключения;
		
	КонецЕсли;
	
	ЗависимостьВидовЗаданий = ПакетыЗапросов[5].Выгрузить();
	ЗависимостьВидовЗаданий.Индексы.Добавить("ВидЗаданияЧтение");

	СписокВидовЗаданий = ПакетыЗапросов[6].Выгрузить();
	
	ПорядокЗаданийОчереди = Новый Соответствие;
	
	// Первоначальное заполнение номеров очереди для последующего расчета
	Для каждого ВидЗадания Из СписокВидовЗаданий Цикл
		ПорядокЗаданийОчереди.Вставить(ВидЗадания.Ссылка, 1);
	КонецЦикла;
	
	// Определение номера очереди с учетом зависимости видов заданий
	Для каждого ВидЗадания Из СписокВидовЗаданий Цикл
		ОпределитьНомерЗаданияПоЗависимости(ВидЗадания.Ссылка, ЗависимостьВидовЗаданий, 0, ПорядокЗаданийОчереди);
	КонецЦикла;
	
	НомерОчередиОтраженияДокументов = 0;
	Для каждого ВидЗадания Из СписокВидовЗаданий Цикл
		НомерОчереди = ПорядокЗаданийОчереди.Получить(ВидЗадания.Ссылка);
		НомерОчередиОтраженияДокументов = Макс(НомерОчередиОтраженияДокументов, НомерОчереди);
		Если Не НомерОчереди = ВидЗадания.НомерОчереди Тогда
			ВидЗаданияОбъект = ВидЗадания.Ссылка.ПолучитьОбъект();
			ВидЗаданияОбъект.НомерОчереди = НомерОчереди;
			ВидЗаданияОбъект.ОбменДанными.Загрузка = Истина;
			ВидЗаданияОбъект.Записать();
		КонецЕсли;
	КонецЦикла;
	
	//++ НЕ УТ
	
	//++ Локализация
	
	// Отражение документов в регламентированном учете
	ВидЗаданияОбъект = Справочники.ВидыНеоперативныхЗаданий.ОтражениеВРегламентированномУчете.ПолучитьОбъект();
	ВидЗаданияОбъект.НомерОчереди = НомерОчередиОтраженияДокументов + 1;
	ВидЗаданияОбъект.ОбменДанными.Загрузка = Истина;
	ВидЗаданияОбъект.Записать();
	
	//-- Локализация
	
	//-- НЕ УТ
	
	//++ НЕ УТКА
	
	// Отражение документов в международном учете
	ВидЗаданияОбъект = Справочники.ВидыНеоперативныхЗаданий.ОтражениеВМеждународномУчете.ПолучитьОбъект();
	ВидЗаданияОбъект.НомерОчереди = НомерОчередиОтраженияДокументов + 1;
	ВидЗаданияОбъект.ОбменДанными.Загрузка = Истина;
	ВидЗаданияОбъект.Записать();
	
	//-- НЕ УТКА
	
КонецПроцедуры

Процедура ОпределитьНомерЗаданияПоЗависимости(ВидЗадания, ЗависимостьВидовЗаданий, НомерЗадания, ПорядокЗаданийОчереди) 
	
	СтруктураОтбора = Новый Структура;
	СтруктураОтбора.Вставить("ВидЗаданияЧтение", ВидЗадания);
	
	НайденнаяЗависимость = ЗависимостьВидовЗаданий.НайтиСтроки(СтруктураОтбора);
	Если НайденнаяЗависимость.Количество() > 0 Тогда
		
		Для каждого ТекЗависимость Из НайденнаяЗависимость Цикл
			
			НомерЗадания = ПорядокЗаданийОчереди.Получить(ТекЗависимость.ВидЗаданияЗапись);
			ОпределитьНомерЗаданияПоЗависимости(ТекЗависимость.ВидЗаданияЗапись, ЗависимостьВидовЗаданий, НомерЗадания, ПорядокЗаданийОчереди);
			ПорядокЗаданийОчереди.Вставить(ТекЗависимость.ВидЗаданияЗапись, НомерЗадания);
			НомерЗадания = НомерЗадания + 1;
			
			Если ПорядокЗаданийОчереди.Получить(ВидЗадания) < НомерЗадания Тогда
				ПорядокЗаданийОчереди.Вставить(ВидЗадания, НомерЗадания);
			КонецЕсли;
			
		КонецЦикла;
			
	Иначе
		
		НомерЗадания = 1;
		ПорядокЗаданийОчереди.Вставить(ВидЗадания, НомерЗадания); 
		
	КонецЕсли;
	
КонецПроцедуры 

