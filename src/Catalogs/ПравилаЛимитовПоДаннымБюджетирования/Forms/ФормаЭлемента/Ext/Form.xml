									<v8:item>
										<v8:lang>ru</v8:lang>
										<v8:content>Дополнительно ограничивающий</v8:content>
									</v8:item>
									<v8:item>
										<v8:lang>en</v8:lang>
										<v8:content>Additionally limiting</v8:content>
									</v8:item>
