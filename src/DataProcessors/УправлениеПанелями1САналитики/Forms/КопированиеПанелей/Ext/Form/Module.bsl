Процедура УстановитьВидимостьЭлементовПриПереключенииИсточника(Знач ЗначениеПереключателя)
	
	Если ЗначениеПереключателя = 0 Тогда
		Элементы.АдресСервераГиперссылка.Видимость = Истина;
		Элементы.ФайлПанелей.Видимость = Ложь;
		Элементы.ЗагрузитьПанелиИзСервиса.Видимость = Ложь;
		Элементы.ПанелиПриемникКартинкаСтатуса.Видимость = Ложь;
		Элементы.СостоянияВладельцевПриемника.Видимость = Ложь;
		Элементы.ПодменюСохранитьВФайл.Видимость = Истина;
		
	ИначеЕсли ЗначениеПереключателя = 1 Тогда
		Элементы.АдресСервераГиперссылка.Видимость = Ложь;
		Элементы.ФайлПанелей.Видимость = Истина;
		Элементы.ЗагрузитьПанелиИзСервиса.Видимость = Ложь;
		Элементы.ПанелиПриемникКартинкаСтатуса.Видимость = Истина;
		Элементы.СостоянияВладельцевПриемника.Видимость = Истина;
		Элементы.ПодменюСохранитьВФайл.Видимость = Ложь;
		
	// ИнтернетПоддержкаПользователей
	ИначеЕсли ЗначениеПереключателя = 2 Тогда
		
		Если ПодключенИТС() Тогда
			Элементы.АдресСервераГиперссылка.Видимость = Ложь;
			Элементы.ФайлПанелей.Видимость = Ложь;
			Элементы.ЗагрузитьПанелиИзСервиса.Видимость = Истина;
			Элементы.ПанелиПриемникКартинкаСтатуса.Видимость = Истина;
			Элементы.СостоянияВладельцевПриемника.Видимость = Истина;
			Элементы.ПодменюСохранитьВФайл.Видимость = Ложь;
		Иначе
			ИсточникИзФайла = ИсточникИзФайлаДоИзменения;
			УстановитьВидимостьЭлементовПриПереключенииИсточника(ИсточникИзФайла);
			
			ОповещениеОЗавершении = Новый ОписаниеОповещения(
				"ПослеПодключенияПоддержкиПользователей", ЭтотОбъект, ЗначениеПереключателя);
			ИнтернетПоддержкаПользователейКлиент.ПодключитьИнтернетПоддержкуПользователей(ОповещениеОЗавершении);
		КонецЕсли;
	// Конец ИнтернетПоддержкаПользователей
		
	КонецЕсли;
	
КонецПроцедуры

