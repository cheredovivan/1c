&НаКлиенте
Процедура УдалитьПанельПриемник(Команда)
	
	ПроверитьЗаполнение();
	
	ВыбранныйОбъект = Элементы.ПанелиПриемник.ТекущиеДанные;
	
	Если ВыбранныйОбъект = Неопределено
		Или ВыбранныйОбъект.Тип <> "chart" И ВыбранныйОбъект.Тип <> "dashboard" И ВыбранныйОбъект.Тип <> "folder" Тогда
		ОбщегоНазначенияКлиент.СообщитьПользователю(НСтр("ru = 'Выберите панель или папку в источнике';
														|en = 'Select a panel or folder in the source'"));
		Возврат;
	КонецЕсли;
	
	ИдентификаторСтроки = ВыбранныйОбъект.ИдентификаторСтроки;
	
	Если Не МожноУдалить(ИдентификаторСтроки) Тогда
		ОбщегоНазначенияКлиент.СообщитьПользователю(НСтр("ru = 'Удаление невозможно, объект подключен через общий доступ или через вышестоящую папку';
														|en = 'Cannot delete as the object is connected via shared access or the parent folder'"));
		Возврат;
	КонецЕсли;

	Если ВыбранныйОбъект.Тип = "folder" Тогда
		ТекстВопроса = НСтр("ru = 'Папка и вложенные объекты будут удалены, продолжить?';
							|en = 'The folder and nested objects will be deleted. Continue?'");
	Иначе
		ТекстВопроса = НСтр("ru = 'Объект будет удален, продолжить?';
							|en = 'The object will be deleted. Continue?'");
	КонецЕсли;

	Оповещение = Новый ОписаниеОповещения("ПослеПодтвержденияУдаленияПанели", ЭтотОбъект, ИдентификаторСтроки);
	ПоказатьВопрос(Оповещение, ТекстВопроса, РежимДиалогаВопрос.ДаНет);
	
КонецПроцедуры

&НаКлиенте
Процедура ЗагрузитьПанелиИзСервиса(Команда)
	
	// ИнтернетПоддержкаПользователей
	ДанныеПанелей = ДанныеПанелейИзСервиса();
	
	ОшибкаЗагрузки = (ТипЗнч(ДанныеПанелей) = Тип("Строка"));
	
	Если ТипЗнч(ДанныеПанелей) = Тип("Массив") И ДанныеПанелей.Количество() Тогда
		ЗагружатьНастройкиПанелей = Истина;
	Иначе
		ЗагружатьНастройкиПанелей = Ложь;
	КонецЕсли;
	
	Если ОшибкаЗагрузки Или Не ЗагружатьНастройкиПанелей Тогда
		СообщениеОбОшибке = НСтр("ru = 'В текущий момент из сервиса 1С информацию получить невозможно. Попробуйте позднее.';
								|en = 'It is currently impossible to get information from the 1C service. Try again later.'");
		ПоказатьПредупреждение(, СообщениеОбОшибке);
		Возврат;
	КонецЕсли;
	
	ПараметрыОткрытия = Новый Структура("ДанныеПанелей", ДанныеПанелей);
	ОписаниеОповещенияОЗакрытии = Новый ОписаниеОповещения("ПослеЗакрытияАктуализацияПанелейИзИнтернетСервиса", ЭтотОбъект);
	ОткрытьФорму("Обработка.УправлениеПанелями1САналитики.Форма.АктуализацияПанелейИзИнтернетСервиса",
				ПараметрыОткрытия, , , , ,
				ОписаниеОповещенияОЗакрытии,
				РежимОткрытияОкнаФормы.БлокироватьОкноВладельца);
	// Конец ИнтернетПоддержкаПользователей
	
КонецПроцедуры

