&НаСервере
Процедура ЗаполнитьТаблицуСумм()
	
	РаспознанныеДокументы = ВыбранныеДокументы();
	ПропуститьЗаполнение = ДокументыПропуститьЗаполнение.ВыгрузитьЗначения();
	ПропуститьТаблицу = ДокументыПропуститьТаблицу.ВыгрузитьЗначения();
	Для Каждого ДокументСсылка Из ПропуститьТаблицу Цикл
		ПропуститьЗаполнение.Добавить(ДокументСсылка);
	КонецЦикла;
	
	УчитыватьДоходыРасходы = ПолучитьФункциональнуюОпцию("ИспользоватьУчетПрочихДоходовРасходов");
	УчитыватьАктивыПассивы = ПолучитьФункциональнуюОпцию("ИспользоватьУчетПрочихАктивовПассивов");
	ИспользоватьСкладыВТЧ = ПолучитьФункциональнуюОпцию("ИспользоватьСкладыВТабличнойЧастиДокументовПродажи");
	
	Запрос = Новый Запрос;
	Запрос.УстановитьПараметр("РаспознанныеДокументы", РаспознанныеДокументы);
	Запрос.УстановитьПараметр("ПропуститьЗаполнение", ПропуститьЗаполнение);
	Запрос.УстановитьПараметр("НаОтдельныеПозиции", РаспознаваниеДокументовСлужебныйКлиентСервер.ВидыСкидок().НаОтдельныеПозиции);
	Запрос.УстановитьПараметр("ПустаяНоменклатура", Справочники.Номенклатура.ПустаяСсылка());
	Запрос.УстановитьПараметр("НеСопоставлена", НСтр("ru = 'Не сопоставлена: ';
													|en = 'Not mapped: '"));
	Запрос.УстановитьПараметр("УчетПрочихДоходовРасходов", УчитыватьДоходыРасходы);
	Запрос.УстановитьПараметр("УчетПрочихАктивовПассивов", УчитыватьАктивыПассивы);
	Запрос.УстановитьПараметр("ИспользоватьСкладыВТЧ", ИспользоватьСкладыВТЧ);
	Запрос.УстановитьПараметр("ИспользоватьИмпортныеТовары", ИспользоватьИмпортныеТовары);
	
