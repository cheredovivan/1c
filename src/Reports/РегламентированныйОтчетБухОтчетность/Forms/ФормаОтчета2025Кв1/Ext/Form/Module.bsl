&НаСервере
Процедура ПроверкаКС_20_01_код()
	
	ИменаПоказателей = Новый Структура;
		
	Для каждого КодСтроки Из НастройкиКС.МассивСтрокОИК Цикл 
		ИменаПоказателей.Вставить("Ф3_" + КодСтроки + "_40", "П000010" + КодСтроки 
			+ НастройкиКС.ГрафаОИК);
	КонецЦикла;
	
	ИмяПоказателяБаланса1 = "Ф1_" + НастройкиКС.СтрокаБаланса + "_40";
	ИменаПоказателей.Вставить(ИмяПоказателяБаланса1, "П000010" + НастройкиКС.СтрокаБаланса 
				+ НастройкиКС.ГрафаБаланса);
	ИмяПоказателяБаланса2 = "Ф1_" + НастройкиКС.СтрокаБаланса + "_4А";
	ИменаПоказателей.Вставить(ИмяПоказателяБаланса2, "П10001А" + НастройкиКС.СтрокаБаланса 
				+ НастройкиКС.ГрафаБаланса + "_");
	Показатели = СформироватьПоказатели(ИменаПоказателей);
	
	МногоПоказателей = НастройкиКС.МассивСтрокОИК.Количество() > 1;
	ШаблонДетализации2 = "%4 (%1 + %2 + %3)"; 
	
	ЗарегистрироватьПроверку();
	РасчетнаяСуммаОИК = 0;
	РасчетнаяСуммаБаланса = 0;
	Счетчик = 0;
	
	Для каждого КодСтроки Из НастройкиКС.МассивСтрокОИК Цикл 
		
		ИмяПоказателяОИК = "Ф3_" + КодСтроки + "_40";
		ЗаполнитьИРасшифроватьПоказатель(Показатели[ИмяПоказателяОИК], ДанныеКС.ДанныеОтчета.ОИК);
		РасчетнаяСуммаОИК = РасчетнаяСуммаОИК + Показатели[ИмяПоказателяОИК].Значение; 
		
		Счетчик = Счетчик + 1;
		Если МногоПоказателей Тогда
			ШаблонДетализации2 = СтрЗаменить(ШаблонДетализации2, "%" + Счетчик, Показатели[ИмяПоказателяОИК].Значение); 
		КонецЕсли;
	КонецЦикла; 
	
	Если МногоПоказателей Тогда
		ШаблонДетализации2 = СтрЗаменить(ШаблонДетализации2, "%4", РасчетнаяСуммаОИК); 
	КонецЕсли;
	
	ЗаполнитьИРасшифроватьПоказатель(Показатели[ИмяПоказателяБаланса1], ДанныеКС.ДанныеОтчета.Баланс);
	РасчетнаяСуммаБаланса = РасчетнаяСуммаБаланса + Показатели[ИмяПоказателяБаланса1].Значение;
	Если НастройкиКС.МногоСтрок Тогда
		КолвоСтрок = ДанныеКС.ДанныеОтчета.Баланс["КолвоСтрокП10001А" + НастройкиКС.СтрокаБаланса];
		Для Счетчик = 1 По КолвоСтрок Цикл
			ЗаполнитьИРасшифроватьПоказатель(Показатели[ИмяПоказателяБаланса2], ДанныеКС.ДанныеОтчета.Баланс, 
				Счетчик);
		КонецЦикла; 
		РасчетнаяСуммаБаланса = РасчетнаяСуммаБаланса + Показатели[ИмяПоказателяБаланса2].Итог;
	КонецЕсли;
		
	НастройкиКС.ЭтоОшибка = РавенствоНеВыполнено(РасчетнаяСуммаОИК, РасчетнаяСуммаБаланса);  
	Если МногоПоказателей Тогда
		НастройкиКС.Детализация = СтрШаблон(НастройкиКС.ШаблонДетализации,
			ШаблонДетализации2, РасчетнаяСуммаБаланса);
	Иначе
		НастройкиКС.Детализация = СтрШаблон(НастройкиКС.ШаблонДетализации,
			РасчетнаяСуммаОИК, РасчетнаяСуммаБаланса);
	КонецЕсли;
	ДополнитьТаблицуРезультата();
		
КонецПроцедуры

