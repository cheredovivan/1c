	Для каждого КодСтроки Из МассивКодовСтрок Цикл 
		
		ИмяПоказателя1 = НастройкиКС.ПрефиксРаздела + "_" + КодСтроки + "_40";
		ЗаполнитьИРасшифроватьПоказатель(Показатели[ИмяПоказателя1], ДанныеКС.ДанныеОтчета[НастройкиКС.ИмяРаздела]);
	
		Счетчик = Счетчик + 1;
		НастройкиКС.ШаблонДетализации2 = СтрЗаменить(НастройкиКС.ШаблонДетализации2, "%" + Счетчик, 
			?(Показатели[ИмяПоказателя1].Значение >= 0, Показатели[ИмяПоказателя1].Значение, 
			"(" + Показатели[ИмяПоказателя1].Значение + ")"));
		
	КонецЦикла;
	
	РасчетнаяСумма = СуммаПоказателей(Показатели, ИмяПоказателяИтога);
	НастройкиКС.ЭтоОшибка = РавенствоНеВыполнено(Показатели[ИмяПоказателяИтога].Значение, РасчетнаяСумма);
	НастройкиКС.Детализация = СтрШаблон(НастройкиКС.ШаблонДетализации,
		Показатели[ИмяПоказателяИтога].Значение, РасчетнаяСумма, НастройкиКС.ШаблонДетализации2);
	ДополнитьТаблицуРезультата();
		
