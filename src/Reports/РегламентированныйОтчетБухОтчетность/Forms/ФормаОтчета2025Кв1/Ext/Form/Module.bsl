&НаСервере
Процедура ПроверкаКС_01_01_код()
	
	ЕстьПрочие = НастройкиКС.Свойство("ПоказательПрочие");
	ИмяПоказателяИтога 	= "Ф1_" + НастройкиКС.ПоказательИтога 	+ "_40";
	Если ЕстьПрочие Тогда
		ИмяПоказателяПрочие = "Ф1_" + НастройкиКС.ПоказательПрочие 	+ "_40";
	КонецЕсли;
	
	ИменаПоказателей = Новый Структура;
	ИменаПоказателей.Вставить(ИмяПоказателяИтога, "П000010" + НастройкиКС.ПоказательИтога + НастройкиКС.Графа);
	
	МассивКодовСтрок 	= СтрРазделить(
		НастройкиКС.Показатели, ","); 
	Для каждого КодСтроки Из МассивКодовСтрок Цикл 
		ИменаПоказателей.Вставить("Ф1_" + КодСтроки + "_40", "П000010" + КодСтроки + НастройкиКС.Графа);
		ИменаПоказателей.Вставить("Ф1_" + КодСтроки + "_4А", "П10001А" + КодСтроки + НастройкиКС.Графа + "_");
	КонецЦикла;
	
	Если ЕстьПрочие Тогда
		ИменаПоказателей.Вставить(ИмяПоказателяПрочие, "П000010" + НастройкиКС.ПоказательПрочие + НастройкиКС.Графа);
	КонецЕсли;
	Показатели = СформироватьПоказатели(ИменаПоказателей); 
	
	ЗарегистрироватьПроверку();
	ЗаполнитьИРасшифроватьПоказатель(Показатели[ИмяПоказателяИтога], ДанныеКС.ДанныеОтчета.Баланс);
	Счетчик = 0;
	
	Для каждого КодСтроки Из МассивКодовСтрок Цикл 
		
		ИмяПоказателя1 = "Ф1_" + КодСтроки + "_40";
		ЗаполнитьПоказатель(Показатели[ИмяПоказателя1], ДанныеКС.ДанныеОтчета.Баланс);
		Если ЗначениеЗаполнено(Показатели[ИмяПоказателя1].Значение) Тогда 
			РасшифроватьПоказатель(Показатели[ИмяПоказателя1]);
		КонецЕсли;
	
		ИмяПоказателя2 = "Ф1_" + КодСтроки + "_4А";
		КолвоСтрок = ДанныеКС.ДанныеОтчета.Баланс["КолвоСтрокП10001А" + КодСтроки];
		Для Счетчик2 = 1 По КолвоСтрок Цикл
			ЗаполнитьПоказатель(Показатели[ИмяПоказателя2], ДанныеКС.ДанныеОтчета.Баланс, Счетчик2);
			Если ЗначениеЗаполнено(Показатели[ИмяПоказателя2].Значение) Тогда 
				РасшифроватьПоказатель(Показатели[ИмяПоказателя2]);
			КонецЕсли;
		КонецЦикла;
		
		Счетчик = Счетчик + 1;
		РасчетнаяСуммаПоКоду = Показатели[ИмяПоказателя1].Значение + Показатели[ИмяПоказателя2].Итог;
		НастройкиКС.ШаблонДетализации2 = СтрЗаменить(НастройкиКС.ШаблонДетализации2, "%" + Счетчик, РасчетнаяСуммаПоКоду);
		
	КонецЦикла;
	
	Если ЕстьПрочие Тогда
		ЗаполнитьПоказатель(Показатели[ИмяПоказателяПрочие], ДанныеКС.ДанныеОтчета.Баланс);  
		Если ЗначениеЗаполнено(Показатели[ИмяПоказателяПрочие].Значение) Тогда
			РасшифроватьПоказатель(Показатели[ИмяПоказателяПрочие]);
		КонецЕсли;
	КонецЕсли;
	
	РасчетнаяСумма = СуммаПоказателей(Показатели, ИмяПоказателяИтога);
	НастройкиКС.ЭтоОшибка = РавенствоНеВыполнено(Показатели[ИмяПоказателяИтога].Значение, РасчетнаяСумма);
	Если ЕстьПрочие Тогда
		НастройкиКС.Детализация = СтрШаблон(НастройкиКС.ШаблонДетализации,
			Показатели[ИмяПоказателяИтога].Значение, РасчетнаяСумма, НастройкиКС.ШаблонДетализации2, 
			Показатели[ИмяПоказателяПрочие].Значение);
	Иначе
		НастройкиКС.Детализация = СтрШаблон(НастройкиКС.ШаблонДетализации,
			Показатели[ИмяПоказателяИтога].Значение, РасчетнаяСумма, НастройкиКС.ШаблонДетализации2);
	КонецЕсли;
	ДополнитьТаблицуРезультата();
		
КонецПроцедуры

