Процедура ПроверкаКС_21_01_код()
	
	ИмяПоказателяИтога 	= НастройкиКС.ПрефиксРаздела + "_" + НастройкиКС.ПоказательИтога 	+ "_40";
	
	ИменаПоказателей = Новый Структура;
	ИменаПоказателей.Вставить(ИмяПоказателяИтога, "П000010" + НастройкиКС.ПоказательИтога + НастройкиКС.Графа);
	
	МассивКодовСтрок 	= СтрРазделить(
		НастройкиКС.Показатели, ","); 
	Для каждого КодСтроки Из МассивКодовСтрок Цикл 
		ИменаПоказателей.Вставить(НастройкиКС.ПрефиксРаздела + "_" + КодСтроки + "_40", "П000010" + КодСтроки 
			+ НастройкиКС.Графа);
	КонецЦикла;
	
	Показатели = СформироватьПоказатели(ИменаПоказателей); 
	
	ЗарегистрироватьПроверку();
	ЗаполнитьИРасшифроватьПоказатель(Показатели[ИмяПоказателяИтога], ДанныеКС.ДанныеОтчета[НастройкиКС.ИмяРаздела]);
