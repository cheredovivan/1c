&НаСервере
Процедура СформироватьДанныеОтчета()
	
	СохраненныйДокумент = СтруктураРеквизитовФормы.мСохраненныйДок;
	СохраненныеДанные = ОбъектОтчета().СохраненныеДанные(СохраненныйДокумент);
	ДанныеОтчета = Новый Структура; 
	
	Для каждого СтрокаКоллекции Из СохраненныеДанные.ДеревоДанныхРазделов.Строки Цикл
		РазделОтчетности = Новый Структура;
		Для каждого СтрокаРаздела Из СтрокаКоллекции.Строки Цикл 
			
			Если СтрокаРаздела.Тип = "Показатель" Тогда 
				РазделОтчетности.Вставить(СтрокаРаздела.Идентификатор, СтрокаРаздела.Данные);
			КонецЕсли;
			
			Если СтрокаРаздела.Тип = "МногострочнаяЧасть" Тогда 
				СтруктураПараметров = Новый Структура;
				СтруктураПараметров.Вставить("ИдентификаторМнЧасти"	, СтрокаРаздела.Идентификатор);
				СтруктураПараметров.Вставить("ТипМнЧасти"			, Лев(СтрокаРаздела.Идентификатор, 7));
				ПолучитьДанныеМнЧасти(СтрокаРаздела, РазделОтчетности, СтруктураПараметров);
			КонецЕсли;
		КонецЦикла;
		ДанныеОтчета.Вставить(СтрокаКоллекции.Идентификатор, РазделОтчетности);
	КонецЦикла;
	
	ДанныеКС.Вставить("ДанныеОтчета", ДанныеОтчета);

КонецПроцедуры

&НаСервере
Процедура ПолучитьДанныеМнЧасти(МнЧасть, РазделОтчетности, Знач СтруктураПараметров) 
				
	Если СтруктураПараметров.ТипМнЧасти = "П100010" 
		ИЛИ СтруктураПараметров.ТипМнЧасти = "П10001А" Тогда
		РазделОтчетности.Вставить("КолвоСтрок" + СтруктураПараметров.ИдентификаторМнЧасти, МнЧасть.Строки.Количество());
		
		Для каждого СтрокаМнЧасти Из МнЧасть.Строки Цикл 
   			НомерСтроки = МнЧасть.Строки.Индекс(СтрокаМнЧасти) + 1; 
			СтруктураПараметров.Вставить("НомерСтроки", XMLСтрока(НомерСтроки));
			СтруктураПараметров.Вставить("НомерСтроки1", НомерСтроки);
			ПолучитьДанныеСтрокиМнЧасти(СтрокаМнЧасти, РазделОтчетности, СтруктураПараметров);			
		КонецЦикла;
	ИначеЕсли СтруктураПараметров.ТипМнЧасти = "П10001Б" Тогда
		РазделОтчетности.Вставить("КолвоСтрок" + СтруктураПараметров.ИдентификаторМнЧасти + "_" 
			+ СтруктураПараметров.НомерСтроки, МнЧасть.Строки.Количество());
		
		Для каждого СтрокаМнЧасти Из МнЧасть.Строки Цикл 
   			НомерСтроки = МнЧасть.Строки.Индекс(СтрокаМнЧасти) + 1;
			СтруктураПараметров.Вставить("НомерСтроки2", НомерСтроки);
			СтруктураПараметров.Вставить("НомерСтроки", XMLСтрока(СтруктураПараметров.НомерСтроки1) 
				+ "_" + XMLСтрока(СтруктураПараметров.НомерСтроки2));
			ПолучитьДанныеСтрокиМнЧасти(СтрокаМнЧасти, РазделОтчетности, СтруктураПараметров);			
		КонецЦикла;
	КонецЕсли;
					
КонецПроцедуры

&НаСервере
Процедура ПолучитьДанныеСтрокиМнЧасти(СтрокаМнЧасти, РазделОтчетности, СтруктураПараметров) 
	
	Если СтрокаМнЧасти.Тип = "Строка" Тогда 
		Для каждого Показатель Из СтрокаМнЧасти.Строки Цикл
			ПолучитьДанныеПоказателяСтрокиМнЧасти(Показатель, РазделОтчетности, СтруктураПараметров);
		КонецЦикла;
	КонецЕсли;
	
КонецПроцедуры

&НаСервере
Процедура ПолучитьДанныеПоказателяСтрокиМнЧасти(Показатель, РазделОтчетности, Знач СтруктураПараметров) 
	
	Если Показатель.Тип = "Показатель" Тогда 
		РазделОтчетности.Вставить(Показатель.Идентификатор + "_" + СтруктураПараметров.НомерСтроки, Показатель.Данные);
	ИначеЕсли Показатель.Тип = "МногострочнаяЧасть" Тогда
		СтруктураПараметров.Вставить("ИдентификаторМнЧасти"	, Показатель.Идентификатор);
		СтруктураПараметров.Вставить("ТипМнЧасти"			, Лев(Показатель.Идентификатор, 7));
		ПолучитьДанныеМнЧасти(Показатель, РазделОтчетности, СтруктураПараметров);
	КонецЕсли;
	
КонецПроцедуры

