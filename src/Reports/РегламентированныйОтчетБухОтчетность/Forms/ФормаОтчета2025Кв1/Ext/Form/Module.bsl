&НаСервере
Процедура ПроверкаКС_27_01_код()
	
	ИмяПоказателяИтога 	= НастройкиКС.ПрефиксРаздела + "_" + НастройкиКС.ПоказательИтога 	+ "_40";
	
	ИменаПоказателей = Новый Структура;
	ИменаПоказателей.Вставить(ИмяПоказателяИтога, "П000010" + НастройкиКС.ПоказательИтога + НастройкиКС.Графа);
	
	МассивКодовСтрок 	= СтрРазделить(
		НастройкиКС.Показатели, ","); 
	Для каждого КодСтроки Из МассивКодовСтрок Цикл 
		ИменаПоказателей.Вставить(НастройкиКС.ПрефиксРаздела + "_" + КодСтроки + "_40", "П000010" + КодСтроки 
			+ НастройкиКС.Графа);
	КонецЦикла;
		
	ИмяПоказателяТЧ		= НастройкиКС.ПрефиксРаздела + "_" + НастройкиКС.ПоказательИтога 	+ "_4А";
	ИменаПоказателей.Вставить(ИмяПоказателяТЧ, "П10001А" + НастройкиКС.ПоказательИтога + НастройкиКС.Графа + "_");
	
	Показатели = СформироватьПоказатели(ИменаПоказателей); 
	
	ЗарегистрироватьПроверку();
	ЗаполнитьИРасшифроватьПоказатель(Показатели[ИмяПоказателяИтога], ДанныеКС.ДанныеОтчета[НастройкиКС.ИмяРаздела]);
	Счетчик = 0;
	
	Для каждого КодСтроки Из МассивКодовСтрок Цикл 
		
		ИмяПоказателя1 = НастройкиКС.ПрефиксРаздела + "_" + КодСтроки + "_40";
		ЗаполнитьИРасшифроватьПоказатель(Показатели[ИмяПоказателя1], ДанныеКС.ДанныеОтчета[НастройкиКС.ИмяРаздела]);
	
		Счетчик = Счетчик + 1;
		НастройкиКС.ШаблонДетализации2 = СтрЗаменить(НастройкиКС.ШаблонДетализации2, "%" + Счетчик, 
			?(Показатели[ИмяПоказателя1].Значение >= 0, Показатели[ИмяПоказателя1].Значение, 
			"(" + Показатели[ИмяПоказателя1].Значение + ")"));
		
	КонецЦикла;    
		
	КолвоСтрок = ДанныеКС.ДанныеОтчета[НастройкиКС.ИмяРаздела]["КолвоСтрокП10001А" + НастройкиКС.ПоказательИтога];
	Для Счетчик2 = 1 По КолвоСтрок Цикл
		ЗаполнитьПоказатель(Показатели[ИмяПоказателяТЧ], ДанныеКС.ДанныеОтчета[НастройкиКС.ИмяРаздела], 
			Счетчик2);
		Если ЗначениеЗаполнено(Показатели[ИмяПоказателяТЧ].Значение) Тогда
			РасшифроватьПоказатель(Показатели[ИмяПоказателяТЧ]);
		КонецЕсли;
	КонецЦикла;
	
	РасчетнаяСумма = СуммаПоказателей(Показатели, ИмяПоказателяИтога); 
	КодСравнения = ?(НастройкиКС.БольшеМеньше = ">=", 2, 3);
	НастройкиКС.ЭтоОшибка = РавенствоНеВыполнено(Показатели[ИмяПоказателяИтога].Значение, РасчетнаяСумма, КодСравнения);
	НастройкиКС.Детализация = СтрШаблон(НастройкиКС.ШаблонДетализации,
		Показатели[ИмяПоказателяИтога].Значение, РасчетнаяСумма, НастройкиКС.ШаблонДетализации2, 
		?(Показатели[ИмяПоказателяТЧ].Итог >= 0, Показатели[ИмяПоказателяТЧ].Итог, "(" + Показатели[ИмяПоказателяТЧ].Итог 
		+ ")"), НастройкиКС.БольшеМеньше);
	ДополнитьТаблицуРезультата();
		
КонецПроцедуры

