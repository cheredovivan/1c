		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗЛИЧНЫЕ
	СтруктураЗаказа.КлючПартия КАК КлючПартия
ПОМЕСТИТЬ ВтКлючиПартий
ИЗ
	РегистрСведений.СтруктураЗаказа КАК СтруктураЗаказа
ГДЕ
	СтруктураЗаказа.ЗаказНаПроизводство = &amp;ЗаказНаПроизводство
	И (СтруктураЗаказа.Запланировано &lt;&gt; 0
			ИЛИ СтруктураЗаказа.Запущено &lt;&gt; 0)
	И СтруктураЗаказа.КлючСвязиПеремещение = &amp;ПустойКлючСвязи
{ГДЕ
	СтруктураЗаказа.Номенклатура.*,
	СтруктураЗаказа.Характеристика.*}
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	СтруктураЗаказа.Номенклатура КАК Номенклатура,
	СтруктураЗаказа.Характеристика КАК Характеристика,
	ЕСТЬNULL(ОсновноеИзделие.Номенклатура, СтруктураЗаказа.Номенклатура) КАК ОсновноеИзделие,
	ЕСТЬNULL(ОсновноеИзделие.Характеристика, ОсновноеИзделие.Характеристика) КАК ХарактеристикаОсновногоИзделия,
	СтруктураЗаказа.КлючПартия КАК КлючПартия,
	ВЫБОР
		КОГДА СтруктураЗаказа.Этап ССЫЛКА Документ.ЭтапПроизводства2_2
			ТОГДА СтруктураЗаказа.Этап
		ИНАЧЕ СтруктураЗаказа.ЗаказНаПроизводство
	КОНЕЦ КАК Документ,
	ВЫБОР
		КОГДА СтруктураЗаказа.Спецификация = ЗНАЧЕНИЕ(Справочник.РесурсныеСпецификации.ПустаяСсылка)
			ТОГДА &amp;ТекстБезСпецификации
		ИНАЧЕ СтруктураЗаказа.Спецификация
	КОНЕЦ КАК Спецификация,
	СтруктураЗаказа.Номенклатура.ЕдиницаИзмерения КАК ЕдиницаИзмерения,
	СтруктураЗаказа.Назначение КАК Назначение,
	СтруктураЗаказа.Склад КАК Склад,
	СтруктураЗаказа.Запланировано + СтруктураЗаказа.Запущено КАК Количество,
	ВЫБОР
		КОГДА СтруктураЗаказа.Запланировано &lt;&gt; 0
			ТОГДА &amp;ТекстЗапланировано
		КОГДА СтруктураЗаказа.Запущено &lt;&gt; 0
			ТОГДА &amp;ТекстЗапущено
	КОНЕЦ КАК Состояние,
	ДатыГрафика.Начало КАК ДатаЗапуска,
	ДатыГрафика.Окончание КАК ДатаВыпуска,
	ЕСТЬNULL(ВЫБОР
			КОГДА СтруктураЗаказа.Спецификация.ВыпускПроизвольнымиПорциями
				ТОГДА 0
			ИНАЧЕ СпецификацииИзделий.КоличествоУпаковокНаЕдиницуПартииЗапуска
		КОНЕЦ, 0) КАК Кратность,
	ЕСТЬNULL(СтруктураЗаказа.Спецификация.МинимальнаяПартияВыпуска, 0) * ЕСТЬNULL(СпецификацииИзделий.КоличествоУпаковокНаЕдиницуПартииЗапуска, 0) КАК МинимальнаяПартия,
	ОсновноеИзделие.Номенклатура ЕСТЬ NULL
		ИЛИ ОсновноеИзделие.Номенклатура В (СтруктураЗаказа.Номенклатура, ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка)) КАК ЭтоОсновноеИзделие,
	СтруктураЗаказа.КлючНоменклатура КАК КлючНоменклатура
{ВЫБРАТЬ
	Номенклатура.*,
	Характеристика.*,
	Документ.*,
	Спецификация.*,
	ЕдиницаИзмерения.*,
	Назначение.*,
	Склад.*,
	Количество,
	Состояние,
	ДатаЗапуска,
	ДатаВыпуска,
	Кратность,
	МинимальнаяПартия,
	ОсновноеИзделие.*,
	ХарактеристикаОсновногоИзделия.*,
	КлючПартия,
	ЭтоОсновноеИзделие,
	КлючНоменклатура,
	ЭтоОсновноеИзделие}
ИЗ
	РегистрСведений.СтруктураЗаказа КАК СтруктураЗаказа
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СпецификацииИзделий КАК ОсновноеИзделие
		ПО СтруктураЗаказа.Спецификация = ОсновноеИзделие.Спецификация
			И (ОсновноеИзделие.Порядок = 1)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СпецификацииИзделий КАК СпецификацииИзделий
		ПО СтруктураЗаказа.Спецификация = СпецификацииИзделий.Спецификация
			И СтруктураЗаказа.Номенклатура.ВидНоменклатуры = СпецификацииИзделий.ВидНоменклатуры
			И (СпецификацииИзделий.Номенклатура В (СтруктураЗаказа.Номенклатура, ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка)))
			И (СпецификацииИзделий.Характеристика В (СтруктураЗаказа.Характеристика, ЗНАЧЕНИЕ(Справочник.ХарактеристикиНоменклатуры.ПустаяСсылка)))
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ГрафикЭтаповПроизводства2_2 КАК ДатыГрафика
		ПО СтруктураЗаказа.ЗаказНаПроизводство = ДатыГрафика.ЗаказНаПроизводство
			И СтруктураЗаказа.КлючПартия = ДатыГрафика.КлючПартия
			И (ДатыГрафика.СтатусГрафика = 0)
			И (ДатыГрафика.Этап = ЗНАЧЕНИЕ(Справочник.ЭтапыПроизводства.ПустаяСсылка))
ГДЕ
	СтруктураЗаказа.КлючПартия В
			(ВЫБРАТЬ
				ВтКлючиПартий.КлючПартия КАК КлючПартия
			ИЗ
				ВтКлючиПартий КАК ВтКлючиПартий)
	И (СтруктураЗаказа.Запланировано &lt;&gt; 0
			ИЛИ СтруктураЗаказа.Запущено &lt;&gt; 0)
	И (&amp;ПоказыватьПобочныйВыпуск
			ИЛИ НЕ ЕСТЬNULL(СпецификацииИзделий.ПобочныйВыход, ИСТИНА))
{ГДЕ
	(ОсновноеИзделие.Номенклатура ЕСТЬ NULL
			ИЛИ ОсновноеИзделие.Номенклатура В (СтруктураЗаказа.Номенклатура, ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка))) КАК ЭтоОсновноеИзделие}</query>
		<autoFillFields>false</autoFillFields>
	</dataSet>
	<calculatedField>
		<dataPath>Пустое</dataPath>
		<expression/>
		<useRestriction>
			<condition>true</condition>
			<group>true</group>
			<order>true</order>
		</useRestriction>
	</calculatedField>
	<calculatedField>
		<dataPath>НоменклатураХарактеристика</dataPath>
