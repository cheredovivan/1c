	<restrictionTemplate>
		<name>ДляРегистра_ЭлектронныеПодписи</name>
		<condition>// Отдельный запрос для РС ЭлектронныеПодписи.
// Пример вызова:
//	#ДляРегистра_ЭлектронныеПодписи() - без параметров

#Если &amp;ДокументооборотИспользоватьОграниченияПравДоступа #Тогда

	ЭлектронныеПодписи ИЗ РегистрСведений.ЭлектронныеПодписи КАК ЭлектронныеПодписи 

ГДЕ
	ЭлектронныеПодписи.ПодписанныйОбъект Ссылка Справочник.ВерсииФайлов
	И ИСТИНА В (
		ВЫБРАТЬ ПЕРВЫЕ 1 ИСТИНА
		ИЗ
			РегистрСведений.ДескрипторыДляОбъектов КАК ДескрипторыДляОбъектов
			ЛЕВОЕ СОЕДИНЕНИЕ
			РегистрСведений.ПраваПоДескрипторамДоступаОбъектов КАК Права
				ПО ДескрипторыДляОбъектов.Дескриптор = Права.Дескриптор
		ГДЕ
			ДескрипторыДляОбъектов.Объект = ЭлектронныеПодписи.ПодписанныйОбъект.Владелец.ВладелецФайла
			И Права.Пользователь = &amp;ТекущийПользователь
	) // Справочник.ВерсииФайлов
	
	ИЛИ (
		ЭлектронныеПодписи.ПодписанныйОбъект Ссылка Справочник.ВизыСогласования
		ИЛИ ЭлектронныеПодписи.ПодписанныйОбъект Ссылка Справочник.Резолюции
	)
	И ИСТИНА В (
		ВЫБРАТЬ ПЕРВЫЕ 1 ИСТИНА
		ИЗ
			РегистрСведений.ДескрипторыДляОбъектов КАК ДескрипторыДляОбъектов
			ЛЕВОЕ СОЕДИНЕНИЕ
			РегистрСведений.ПраваПоДескрипторамДоступаОбъектов КАК Права
				ПО ДескрипторыДляОбъектов.Дескриптор = Права.Дескриптор
		ГДЕ
			ДескрипторыДляОбъектов.Объект = ЭлектронныеПодписи.ПодписанныйОбъект.Документ
			И Права.Пользователь = &amp;ТекущийПользователь
	) // Справочник.ВизыСогласования Или Справочник.Резолюции.
	
	ИЛИ ЭлектронныеПодписи.ПодписанныйОбъект Ссылка Справочник.ДанныеОзнакомленияПодПодпись
	И ИСТИНА В (
		ВЫБРАТЬ ПЕРВЫЕ 1 ИСТИНА
		ИЗ
			РегистрСведений.ДескрипторыДляОбъектов КАК ДескрипторыДляОбъектов
			ЛЕВОЕ СОЕДИНЕНИЕ
			РегистрСведений.ПраваПоДескрипторамДоступаОбъектов КАК Права
				ПО ДескрипторыДляОбъектов.Дескриптор = Права.Дескриптор
		ГДЕ
			ДескрипторыДляОбъектов.Объект = ЭлектронныеПодписи.ПодписанныйОбъект.Документ
			И Права.Пользователь = &amp;ТекущийПользователь
	) // Справочник.ДанныеОзнакомленияПодПодпись.
	
	ИЛИ (
		НЕ ЭлектронныеПодписи.ПодписанныйОбъект Ссылка Справочник.ВерсииФайлов
		И НЕ ЭлектронныеПодписи.ПодписанныйОбъект Ссылка Справочник.ВизыСогласования
		И НЕ ЭлектронныеПодписи.ПодписанныйОбъект Ссылка Справочник.Резолюции
		И НЕ ЭлектронныеПодписи.ПодписанныйОбъект Ссылка Справочник.ДанныеОзнакомленияПодПодпись
	)
	И ИСТИНА В (
		ВЫБРАТЬ ПЕРВЫЕ 1 ИСТИНА
		ИЗ
			РегистрСведений.ДескрипторыДляОбъектов КАК ДескрипторыДляОбъектов
			ЛЕВОЕ СОЕДИНЕНИЕ
			РегистрСведений.ПраваПоДескрипторамДоступаОбъектов КАК Права
				ПО ДескрипторыДляОбъектов.Дескриптор = Права.Дескриптор
		ГДЕ
			ДескрипторыДляОбъектов.Объект = ЭлектронныеПодписи.ПодписанныйОбъект
			И Права.Пользователь = &amp;ТекущийПользователь
	) // Не ВерсииФайлов, ВизыСогласования, Резолюции, ДанныеОзнакомленияПодПодпись
	
#КонецЕсли</condition>
	</restrictionTemplate>
	<restrictionTemplate>
		<name>ПоДескрипторуОбъекта</name>
		<condition>// Параметр(1) - отбор по полям Изменение, Добавление:
//	по умолчанию нет условий, проверяется право чтения
//  "Изменение" - условие Изменение = ИСТИНА
//	"ИзменениеИлиДобавление" - условие (Изменение = ИСТИНА ИЛИ Добавление = ИСТИНА)
//
// Параметр(2) - имя поля, указывающее на объект, чьи права будут проанализированы
//	по умолчанию "Ссылка"
// 
// Параметр(3) - дополнительные условия:
//  "БезРуководителейИДелегатов" - не включать руководителей и делегатов в права
//  "БезРуководителей" - не включать руководителей в права
//  "БезДелегатов" - не включать делегатов в права
// 
// Параметр(4) - оставлять пустым (зарезервирован, не используется)
// 
// Примеры использования шаблона:
//	#ПоДескрипторуОбъекта("", "")
//	#ПоДескрипторуОбъекта("Изменение", "")
//	#ПоДескрипторуОбъекта("", "Документ")
//	#ПоДескрипторуОбъекта("ИзменениеИлиДобавление", "Мероприятие")
//	#ПоДескрипторуОбъекта("ИзменениеИлиДобавление", "Мероприятие", "БезРуководителейИДелегатов")

#Если &amp;ДокументооборотИспользоватьОграниченияПравДоступа #Тогда
	
	ТекущаяТаблица ИЗ #ТекущаяТаблица КАК ТекущаяТаблица
	
ГДЕ
	ИСТИНА В (
		ВЫБРАТЬ ПЕРВЫЕ 1 ИСТИНА
		ИЗ
			РегистрСведений.ДескрипторыДляОбъектов КАК ДескрипторыДляОбъектов
			ЛЕВОЕ СОЕДИНЕНИЕ
			РегистрСведений.ПраваПоДескрипторамДоступаОбъектов КАК Права
				ПО Права.Дескриптор = ДескрипторыДляОбъектов.Дескриптор
					И Права.Пользователь = &amp;ТекущийПользователь
		ГДЕ
			#Если "#Параметр(2)" = "" #Тогда
				ДескрипторыДляОбъектов.Объект = ТекущаяТаблица.Ссылка
			#Иначе
				ДескрипторыДляОбъектов.Объект = ТекущаяТаблица.#Параметр(2)
			#КонецЕсли
			
			И НЕ Права.Дескриптор ЕСТЬ NULL
			
			#Если "#Параметр(1)" = "Изменение" #Тогда
				И Права.Изменение
			#ИначеЕсли "#Параметр(1)" = "ИзменениеИлиДобавление" #Тогда
				И (Права.Изменение ИЛИ Права.Добавление)
			#КонецЕсли
			
			#Если "#Параметр(3)" = "БезРуководителейИДелегатов" #Тогда
				И Права.ОбъектОснование = НЕОПРЕДЕЛЕНО
			#КонецЕсли
	)
