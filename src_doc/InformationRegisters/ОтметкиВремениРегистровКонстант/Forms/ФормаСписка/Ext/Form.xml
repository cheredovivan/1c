	ВЫБОР
		КОГДА Т.Граница &gt;= 63082281600000
			ТОГДА ДОБАВИТЬКДАТЕ(ДАТАВРЕМЯ(2000, 1, 1), СЕКУНДА, (ВЫРАЗИТЬ((Т.Граница - 63082281600000) / 1000 - 0.5 КАК ЧИСЛО(12, 0))) + &amp;СмещениеВремени)
		ИНАЧЕ ДАТАВРЕМЯ(1, 1, 1)
	КОНЕЦ КАК ГраницаДата,
	Т.ИдентификаторКлюча КАК ИдентификаторКлюча,
	Т.ТипКлюча КАК ТипКлюча,
