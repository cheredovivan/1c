#Область УведомлениеОЗадолженности

// Показывает сведения о задолженности пользователю
//
// Параметры:
//  Форма - ФормаКлиентскогоПриложения
Процедура ПоказатьСведенияОЗадолженности(Форма) Экспорт
	
	Задолженность = ИнтерфейсДокументовЭДОВызовСервера.СведенияОЗадолженности();
	
	Если Задолженность.Количество() = 0 Тогда
		ПоказатьПредупреждение(, НСтр("ru='Сведения о задолженности были обновлены, задолженность отсутствует'"));
		Форма.Элементы.ПоказатьСведенияОЗадолженности.Видимость = Задолженность.Количество() > 0;
		Возврат;
	ИначеЕсли Задолженность.Количество() = 1 Тогда
		ОткрытьPDFФайлОЗадолженности(Форма, Задолженность[0].СсылкаНаPDF);
	Иначе
		ОткрытьФорму("Обработка.ИнтерфейсДокументовЭДО.Форма.СведенияОЗадолженности",
			Новый Структура("Задолженность", Задолженность),
			,,,,,
			РежимОткрытияОкнаФормы.БлокироватьОкноВладельца);
	КонецЕсли;
	
КонецПроцедуры

// Открывает pdf файл о задолженности
//
// Параметры:
//  Форма - ФормаКлиентскогоПриложения
//  СсылкаНаPDF - Строка
Процедура ОткрытьPDFФайлОЗадолженности(Форма, СсылкаНаPDF) Экспорт
	
	ДлительнаяОперация = ИнтерфейсДокументовЭДОВызовСервера.ДлительнаяОперацияФайлСДаннымиОЗадолженности(
		Форма.УникальныйИдентификатор, СсылкаНаPDF);
	ОписаниеОповещения = Новый ОписаниеОповещения(
		"ОткрытьPDFФайлОЗадолженностиОбработатьРезультат", ЭтотОбъект, Форма.УникальныйИдентификатор);
	
	ПараметрыОжидания = ДлительныеОперацииКлиент.ПараметрыОжидания(Форма);
	ДлительныеОперацииКлиент.ОжидатьЗавершение(ДлительнаяОперация, ОписаниеОповещения, ПараметрыОжидания);
	
КонецПроцедуры

// Открывает pdf файл после длительной операции
// 
// Параметры:
//  Результат - См. ДлительныеОперацииКлиент.НовыйРезультатДлительнойОперации
//  УникальныйИдентификатор - Строка - идентификатор формы
Процедура ОткрытьPDFФайлОЗадолженностиОбработатьРезультат(Результат, УникальныйИдентификатор) Экспорт

	Если Результат = Неопределено Или ЗначениеЗаполнено(Результат.КраткоеПредставлениеОшибки) Тогда
		ШаблонСтроки = "Не удалось открыть файл. Попробуйте позже или перейдите в <a href=""https://portal.1c.ru/application/list/1C-Edo"">личный кабинет</a> для просмотра информации о задолженности.";
		ФорматированнаяСтрока = СтроковыеФункцииКлиент.ФорматированнаяСтрока(ШаблонСтроки);
		ПоказатьПредупреждение(, ФорматированнаяСтрока);
		Возврат;
	КонецЕсли;
	
	ИмяФайла = СтрШаблон("%1.pdf", Формат(ОбщегоНазначенияКлиент.ДатаУниверсальная(), "ДФ=ггММддЧЧммсс"));
	ФайловаяСистемаКлиент.ОткрытьФайл(Результат.АдресРезультата, , ИмяФайла);
	
КонецПроцедуры

