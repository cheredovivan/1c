// Параметры:
//  Контрагент - ОпределяемыйТип.КонтрагентБЭД
// 
// Возвращаемое значение:
//  Булево
Функция ЕстьНастройкиПоКонтрагенту(Контрагент) Экспорт
	
	Запросы = Новый Массив;
	
	Отбор = НастройкиЭДО.НовыйОтборНастроекОтправки();
	Отбор.Получатель = "&Контрагент";
	ЗапросНастроек = НастройкиЭДО.ЗапросНастроекОтправки("НастройкиОтправки", Отбор);
	Запросы.Добавить(ЗапросНастроек);
	
	Запрос = Новый Запрос;
	Запрос.Текст = "ВЫБРАТЬ ПЕРВЫЕ 1 Отправитель ИЗ НастройкиОтправки";
	ИтоговыйЗапрос = ОбщегоНазначенияБЭД.СоединитьЗапросы(Запрос, Запросы);
	ИтоговыйЗапрос.УстановитьПараметр("Контрагент", Контрагент);
	
	Возврат Не ИтоговыйЗапрос.Выполнить().Пустой();
	
КонецФункции

// Параметры:
//  Контрагенты - Массив Из ОпределяемыйТип.УчастникЭДО
// 
// Возвращаемое значение:
//  Соответствие Из КлючИЗначение:
//  * Ключ - ОпределяемыйТип.УчастникЭДО
//  * Значение - Булево
Функция ЕстьНастройкиПоКонтрагентам(Контрагенты) Экспорт
	Запросы = Новый Массив;
	
	Отбор = НастройкиЭДО.НовыйОтборНастроекОтправки();
	Отбор.Получатель = "&Контрагенты";
	ЗапросНастроек = НастройкиЭДО.ЗапросНастроекОтправки("НастройкиОтправки", Отбор);
	Запросы.Добавить(ЗапросНастроек);
	
	Запрос = Новый Запрос;
	Запрос.Текст =
	"ВЫБРАТЬ
	|	УчастникиЭДО.Ссылка КАК Ссылка
	|ПОМЕСТИТЬ
	|	УчастникиЭДО
	|ИЗ
	|	&УчастникиЭДО КАК УчастникиЭДО
	|ИНДЕКСИРОВАТЬ ПО
	|	Ссылка;
	|
	|ВЫБРАТЬ РАЗЛИЧНЫЕ
	|УчастникиЭДО.Ссылка КАК УчастникЭДО,
	|НастройкиОтправки.Получатель ЕСТЬ НЕ NULL КАК ЕстьНастройка
	|ИЗ
	|	УчастникиЭДО КАК УчастникиЭДО
	|	ЛЕВОЕ СОЕДИНЕНИЕ НастройкиОтправки КАК НастройкиОтправки
	|	ПО УчастникиЭДО.Ссылка = НастройкиОтправки.Получатель";
	ИтоговыйЗапрос = ОбщегоНазначенияБЭД.СоединитьЗапросы(Запрос, Запросы);
	УчастникиЭДО = Новый ТаблицаЗначений;
	УчастникиЭДО.Колонки.Добавить("Ссылка", Метаданные.ОпределяемыеТипы.УчастникЭДО.Тип);
	Для Каждого Ссылка Из Контрагенты Цикл
		УчастникиЭДО.Добавить().Ссылка = Ссылка;
	КонецЦикла;
	ИтоговыйЗапрос.УстановитьПараметр("УчастникиЭДО", УчастникиЭДО);
	ИтоговыйЗапрос.УстановитьПараметр("Контрагенты", Контрагенты);
	
	Выборка = ИтоговыйЗапрос.Выполнить().Выбрать();
	Результат = Новый Соответствие;
	Пока Выборка.Следующий() Цикл
		Результат.Вставить(Выборка.УчастникЭДО, Выборка.ЕстьНастройка);
	КонецЦикла;
	Возврат Результат;
КонецФункции

