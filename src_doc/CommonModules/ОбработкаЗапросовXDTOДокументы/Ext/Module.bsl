	Если ТипЗнч(ДанныеСтраныМира) = Тип("Структура") И ЗначениеЗаполнено(ДанныеСтраныМира.Ссылка) Тогда
		ДанныеБанка.Вставить("country", ДанныеСтраныМира.Ссылка);
	КонецЕсли;
	
	ОбработкаЗапросовXDTO.ЗаполнитьОбъектXDTOИзДанныхОбъектаДО(Банк, ДанныеБанка, ОбъектXDTO, НаборКолонок);
	
